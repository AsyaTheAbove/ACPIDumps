                /* 0198 */  0x0D, 0xEF, 0xC1, 0x87, 0x04, 0x3E, 0x44, 0x8F,  // .....>D.
                /* 01A0 */  0xF5, 0x18, 0x02, 0x1E, 0x22, 0x3B, 0x25, 0x18,  // ....";%.
                /* 01A8 */  0x10, 0xEF, 0xFD, 0x5A, 0x40, 0x26, 0x63, 0x40,  // ...Z@&c@
