                    Method (_CRS, 0, NotSerialized)  // _CRS: Current Resource Settings
                    {
                        Name (BUF1, ResourceTemplate ()
                        {
                            IO (Decode16,
                                0x0240,             // Range Minimum
                                0x0240,             // Range Maximum
                                0x00,               // Alignment
                                0x10,               // Length
                                )
                            IRQ (Edge, ActiveHigh, Shared, )
                                {3}
                            IO (Decode16,
                                0x0250,             // Range Minimum
                                0x0250,             // Range Maximum
                                0x00,               // Alignment
                                0x10,               // Length
                                )
                        })
                        Return (BUF1) /* \_SB_.PCI0.SBRG.CIR_._CRS.BUF1 */
                    }

                    Method (_SRS, 1, NotSerialized)  // _SRS: Set Resource Settings
                    {
                        ENF1 ()
                        LDN1 = 0x06
                        CreateByteField (Arg0, 0x02, IOLA)
                        CreateByteField (Arg0, 0x03, IOHA)
                        CreateWordField (Arg0, 0x09, IRQE)
                        IOL1 = IOLA /* \_SB_.PCI0.SBRG.CIR_._SRS.IOLA */
                        IOH1 = IOHA /* \_SB_.PCI0.SBRG.CIR_._SRS.IOHA */
                        If (IRQE)
                        {
                            FindSetRightBit (IRQE, Local0)
                            INT1 = (Local0 - One)
                        }
                        Else
                        {
                            INT1 = Zero
                        }

                        ACT1 = One
                        Local0 = GLO1 /* \_SB_.PCI0.SBRG.GLO1 */
                        Local0 &= 0xBC
                        Local0 |= 0x41
                        GLO1 = Local0
                        Local0 = MFS1 /* \_SB_.PCI0.SBRG.MFS1 */
                        Local0 &= 0xBF
                        MFS1 = Local0
                        Local0 = DPD1 /* \_SB_.PCI0.SBRG.DPD1 */
                        Local0 |= 0x04
                        DPD1 = Local0
                        LDN1 = 0x0F
                        Local0 = PPO5 /* \_SB_.PCI0.SBRG.PPO5 */
                        Local0 |= 0x80
                        PPO5 = Local0
                        LDN1 = 0x09
                        Local0 = ACT1 /* \_SB_.PCI0.SBRG.ACT1 */
                        Local0 |= 0x08
                        ACT1 = Local0
                        Local0 = MSL5 /* \_SB_.PCI0.SBRG.MSL5 */
                        Local0 &= 0x1F
                        MSL5 = Local0
                        Local0 = IOR5 /* \_SB_.PCI0.SBRG.IOR5 */
                        Local0 &= 0x1F
                        Local0 |= 0x60
                        IOR5 = Local0
                        Local0 = DAR5 /* \_SB_.PCI0.SBRG.DAR5 */
                        Local0 |= 0x80
                        DAR5 = Local0
                        LDN1 = 0x0E
                        CreateByteField (Arg0, 0x0E, IOLB)
                        CreateByteField (Arg0, 0x0F, IOHB)
                        IOL1 = IOLB /* \_SB_.PCI0.SBRG.CIR_._SRS.IOLB */
                        IOH1 = IOHB /* \_SB_.PCI0.SBRG.CIR_._SRS.IOHB */
                        ACT1 = One
                        EXF1 ()
                    }

                    Name (_PRS, ResourceTemplate ()  // _PRS: Possible Resource Settings
                    {
                        StartDependentFn (0x00, 0x00)
                        {
                            IO (Decode16,
                                0x0240,             // Range Minimum
                                0x0240,             // Range Maximum
                                0x10,               // Alignment
                                0x10,               // Length
                                )
                            IRQNoFlags ()
                                {10}
                        }
                        StartDependentFn (0x00, 0x00)
                        {
                            IO (Decode16,
                                0x0240,             // Range Minimum
                                0x0240,             // Range Maximum
                                0x10,               // Alignment
                                0x10,               // Length
                                )
                            IRQNoFlags ()
                                {3,4,5,6,7,9,10,11,12}
                        }
                        EndDependentFn ()
                    })
                    Method (NCAP, 0, NotSerialized)
                    {
                        Local0 = 0x02
                        Local0 |= 0xF0000000
                        Return (Local0)
                    }

                    Method (NRXL, 0, NotSerialized)
                    {
                        Local0 = Zero
                        Local0 <<= 0x08
                        Local0 |= 0x80
                        Local0 <<= 0x08
                        Local0 |= 0xF5
                        Local0 <<= 0x08
                        Local0 |= 0x09
                        Return (Local0)
                    }

                    Method (NJKP, 0, NotSerialized)
                    {
                        Return (One)
                    }

                    Method (NTXP, 1, NotSerialized)
                    {
                        ENF1 ()
                        Local0 = MFP1 /* \_SB_.PCI0.SBRG.MFP1 */
                        Local0 &= 0xE7
                        Arg0 &= 0x03
                        If ((Arg0 == One))
                        {
                            Local0 |= 0x10
                        }
                        ElseIf ((Arg0 == Zero)){}
                        Else
                        {
                            Local0 |= 0x10
                        }

                        MFP1 = Local0
                        EXF1 ()
                    }

                    Method (NLED, 0, NotSerialized)
                    {
                        ENF1 ()
                        LDN1 = 0x09
                        Local0 = DAR5 /* \_SB_.PCI0.SBRG.DAR5 */
                        Local1 = 0x0A
                        While (Local1)
                        {
                            DAR5 = (0x7F & Local0)
                            Sleep (0x32)
                            DAR5 = (0x80 | Local0)
                            Sleep (0x32)
                            Local1--
                        }

                        EXF1 ()
                    }
                }

                Device (PIC)
