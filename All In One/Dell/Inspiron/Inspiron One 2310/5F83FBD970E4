                /* 02F8 */  0xE3, 0xF1, 0xBD, 0x8F, 0x4D, 0xF8, 0x69, 0x8F,  // ....M.i.
                /* 0300 */  0x0C, 0x02, 0x75, 0x5C, 0xE1, 0xA3, 0x3C, 0xAD,  // ..u\..<.
                /* 0308 */  0x27, 0x00, 0x1F, 0xFF, 0x4C, 0xE0, 0xA3, 0x87,  // '...L...
                /* 0310 */  0x0F, 0x4E, 0xE0, 0x18, 0x0F, 0xFC, 0x8B, 0xC9,  // .N......
                /* 0318 */  0x53, 0x86, 0x6F, 0x0D, 0x9E, 0xAF, 0x8F, 0x0F,  // S.o.....
                /* 0320 */  0x3A, 0xCD, 0x78, 0x3C, 0xA8, 0x81, 0x18, 0xFA,  // :.x<....
                /* 0328 */  0x15, 0x20, 0xEE, 0x3B, 0xC2, 0x7B, 0xC7, 0x0B,  // . .;.{..
                /* 0330 */  0xC7, 0x7B, 0x87, 0x41, 0x1E, 0x63, 0xF8, 0x79,  // .{.A.c.y
                /* 0338 */  0x8D, 0x3D, 0xD2, 0x7C, 0x0E, 0xF0, 0x79, 0x0D,  // .=.|..y.
                /* 0340 */  0x78, 0xA2, 0xFA, 0x68, 0x01, 0x8E, 0xB9, 0x3D,  // x..h...=
                /* 0348 */  0x53, 0xF8, 0x14, 0x11, 0xEC, 0x2D, 0xD0, 0xF7,  // S....-..
                /* 0350 */  0x41, 0x7E, 0x3E, 0xE4, 0x07, 0x4A, 0x76, 0xAD,  // A~>..Jv.
                /* 0358 */  0xC1, 0x4C, 0xE8, 0xED, 0xC2, 0x87, 0x1B, 0x76,  // .L.....v
                /* 0360 */  0xA0, 0x01, 0x9B, 0xC0, 0x03, 0x0D, 0x28, 0x0E,  // ......(.
                /* 0368 */  0x08, 0x1E, 0x57, 0xE8, 0x58, 0x6F, 0x1D, 0x9E,  // ..W.Xo..
                /* 0370 */  0xDC, 0xFF, 0x3F, 0x9E, 0x6F, 0x01, 0xEC, 0x4C,  // ..?.o..L
                /* 0378 */  0x03, 0x8C, 0xDE, 0x04, 0x84, 0x40, 0x71, 0xDE,  // .....@q.
                /* 0380 */  0x02, 0x8A, 0x7C, 0xA6, 0xA1, 0x30, 0x3E, 0xD3,  // ..|..0>.
                /* 0388 */  0x00, 0xAE, 0x02, 0x9C, 0x14, 0x40, 0x33, 0xEB,  // .....@3.
                /* 0390 */  0xB7, 0x52, 0x78, 0x87, 0x10, 0x86, 0x15, 0xFC,  // .Rx.....
                /* 0398 */  0x59, 0x96, 0xC3, 0x3C, 0x2F, 0x19, 0xE5, 0xD0,  // Y..</...
                /* 03A0 */  0x7C, 0xAA, 0xF0, 0x69, 0x94, 0x5D, 0x11, 0x30,  // |..i.].0
                /* 03A8 */  0xF7, 0x2A, 0xDC, 0xB9, 0x06, 0xF6, 0x3D, 0xD4,  // .*....=.
                /* 03B0 */  0x63, 0x7B, 0x2A, 0x38, 0xB7, 0xA7, 0x82, 0x47,  // c{*8...G
                /* 03B8 */  0x99, 0xB7, 0x87, 0xF7, 0x99, 0x47, 0x50, 0xE3,  // .....GP.
                /* 03C0 */  0xF8, 0x5C, 0xE3, 0x3B, 0x4D, 0xC0, 0x23, 0x78,  // .\.;M.#x
