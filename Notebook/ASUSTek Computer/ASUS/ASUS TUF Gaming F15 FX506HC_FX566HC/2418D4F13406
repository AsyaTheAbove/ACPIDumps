                                0x1C
                            }
                        })
                        Return (WGDY) /* \_SB_.PC00.RP13.PXSX.WGDS.WGDY */
                    }
                }
                Else
                {
                    Method (MTDS, 0, Serialized)
                    {
                        Name (MDSB, Package (0x12)
                        {
                            0x4D, 
                            0x54, 
                            0x44, 
                            0x53, 
                            One, 
                            0x02, 
                            One, 
                            0x18, 
                            0x15, 
                            0x15, 
                            0x15, 
                            0x15, 
