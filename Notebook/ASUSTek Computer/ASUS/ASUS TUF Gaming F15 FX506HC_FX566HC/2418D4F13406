                            Zero
                        }
                    }
                })
                Method (GPC, 0, Serialized)
                {
                    Return (GPCX) /* \_SB_.PC00.RP14.PXSX.GPCX */
                }

                If ((WIST () == One))
                {
                    Method (WRDS, 0, Serialized)
                    {
                        Name (WRDY, Package (0x02)
                        {
                            Zero, 
                            Package (0x0C)
                            {
                                0x07, 
                                One, 
                                0x78, 
                                0x6C, 
                                0x6C, 
                                0x6C, 
                                0x6C, 
                                0x78, 
                                0x6C, 
                                0x6C, 
                                0x6C, 
                                0x6C
                            }
                        })
                        Return (WRDY) /* \_SB_.PC00.RP14.PXSX.WRDS.WRDY */
                    }

                    Method (EWRD, 0, Serialized)
                    {
                        Name (EWRY, Package (0x02)
                        {
                            Zero, 
                            Package (0x21)
                            {
                                0x07, 
                                0x80, 
                                0x80, 
                                0x80, 
                                0x80, 
                                0x80, 
                                0x80, 
                                0x80, 
                                0x80, 
                                0x80, 
                                0x80, 
