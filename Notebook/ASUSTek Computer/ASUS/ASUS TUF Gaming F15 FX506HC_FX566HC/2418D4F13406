                            }
                        })
                        Return (WRDY) /* \_SB_.PC00.RP16.PXSX.WRDS.WRDY */
                    }

                    Method (EWRD, 0, Serialized)
                    {
                        Name (EWRY, Package (0x02)
                        {
                            Zero, 
                            Package (0x21)
                            {
                                0x07, 
                                0x80, 
                                0x80, 
                                0x80, 
                                0x80, 
