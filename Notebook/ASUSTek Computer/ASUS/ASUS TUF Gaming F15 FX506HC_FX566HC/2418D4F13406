            Method (PGOF, 1, Serialized)
            {
                If (\_SB.PC00.LPCB.EC0.ECOK)
                {
                    Acquire (\_SB.PC00.LPCB.EC0.MUT0, 0x2000)
                    Local0 = \_SB.PC00.LPCB.EC0.VIUF /* External reference */
                    Release (\_SB.PC00.LPCB.EC0.MUT0)
                }

                If ((Local0 == One))
                {
                    Return (Zero)
                }

                If (\_SB.PC00.LPCB.EC0.ECOK)
                {
                    Acquire (\_SB.PC00.LPCB.EC0.MUT0, 0x2000)
                    \_SB.PC00.LPCB.EC0.VGAT = Zero
                    Release (\_SB.PC00.LPCB.EC0.MUT0)
