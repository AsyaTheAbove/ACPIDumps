                DEMC,   16
            }

            OperationRegion (ECDL, SystemIO, 0x62, 0x06)
            Field (ECDL, ByteAcc, Lock, Preserve)
            {
                ECDP,   8, 
                Offset (0x04), 
                ECCP,   8
            }

            OperationRegion (IOWT, SystemIO, 0xED, One)
            Field (IOWT, ByteAcc, NoLock, Preserve)
            {
                IODY,   8
            }

            OperationRegion (CMOS, SystemIO, 0x70, 0x04)
            Field (CMOS, ByteAcc, NoLock, Preserve)
            {
                IDX0,   7, 
                Offset (0x01), 
                DAT0,   8, 
                IDX1,   7, 
                Offset (0x03), 
                DAT1,   8
            }

            IndexField (IDX1, DAT1, ByteAcc, NoLock, Preserve)
            {
                Offset (0x42)
            }

            OperationRegion (OEDG, SystemMemory, 0xFD6D0900, 0x03FF)
            Field (OEDG, AnyAcc, NoLock, Preserve)
            {
                Offset (0xD0), 
                PWEB,   1, 
                Offset (0xE0), 
                HRST,   1
            }

            Mutex (MUT0, 0x00)
            Name (CNT0, Zero)
            Method (IRDY, 0, NotSerialized)
            {
                CNT0 = Zero
                While ((CNT0 < 0x1770))
                {
                    Local0 = ECCP /* \_SB_.PC00.LPCB.EC0_.ECCP */
                    If (((Local0 & 0x02) == Zero))
                    {
                        Break
                    }

                    IODY = 0xFF
                    If ((CNT0 == 0x1770))
                    {
                        Local0 = ECDP /* \_SB_.PC00.LPCB.EC0_.ECDP */
                        IODY = 0xFF
                        CNT0 = Zero
                        While ((CNT0 < 0x03E8))
                        {
                            Local0 = ECCP /* \_SB_.PC00.LPCB.EC0_.ECCP */
                            If (((Local0 & 0x02) == Zero))
                            {
                                Return (Zero)
                            }

                            IODY = 0xFF
                            CNT0++
                        }
                    }

                    CNT0++
                }
            }

            Method (ORDY, 0, NotSerialized)
            {
                CNT0 = Zero
                While ((CNT0 < 0x1770))
                {
                    Local0 = ECCP /* \_SB_.PC00.LPCB.EC0_.ECCP */
                    If (((Local0 & One) != Zero))
                    {
                        Local0 = ECDP /* \_SB_.PC00.LPCB.EC0_.ECDP */
                        Return (Local0)
                    }

                    IODY = 0xFF
                    CNT0++
                }

                Local0 = ECDP /* \_SB_.PC00.LPCB.EC0_.ECDP */
                Return (Local0)
            }

            Method (ECMD, 1, Serialized)
            {
                Acquire (MUT0, 0x2000)
                IRDY ()
                ECCP = Arg0
                IRDY ()
                Release (MUT0)
            }

            Method (_PS0, 0, NotSerialized)  // _PS0: Power State 0
            {
                If (OKEC)
                {
                    ECOK = One
                }
            }

            Method (_PS3, 0, NotSerialized)  // _PS3: Power State 3
            {
                ECOK = Zero
            }

            Method (SELE, 0, NotSerialized)
            {
                Acquire (MUT0, 0x0100)
                BATN = DBAT /* \_SB_.PC00.LPCB.EC0_.DBAT */
                Release (MUT0)
                BATF = Zero
                If ((0xC0 & BATN))
                {
                    BATF |= One
                }

                If ((0x0C00 & BATN))
                {
                    BATF |= 0x04
                }

                Local0 = (BATN & 0x0F01)
                Local1 = (BATO & 0x0F01)
                If (~(Local0 == Local1))
                {
                    BATF |= 0x40
                }

                Local0 = (BATN & 0xC0)
                Local1 = (BATO & 0xC0)
                If (~(Local0 == Local1))
                {
                    BATF |= 0x02
                }

                If ((0x04 & BATF))
                {
                    BATF |= 0x10
                }

                BATO = BATN /* \_SB_.PC00.LPCB.EC0_.BATN */
            }

            Name (DFLG, 0xD1)
            Method (DNOT, 1, Serialized)
            {
                If (CondRefOf (\_SB.PC00.PEG1.PEGP))
                {
                    Acquire (MUT0, 0x1000)
                    If ((SDNT != One))
                    {
                        Release (MUT0)
                        Return (Zero)
                    }

                    Local0 = Arg0
                    If (((Local0 >= 0xD1) && (Local0 <= 0xD5)))
                    {
                        If ((DFLG != Local0))
                        {
                            P80B = Local0
                            ECMD (0xAE)
                            Notify (^^^PEG1.PEGP, Local0)
                            ECMD (0xAD)
                            DFLG = Local0
                        }
                    }

                    Release (MUT0)
                }
            }

            Name (BATO, 0xC0)
            Name (BATN, Zero)
            Name (BATF, 0xC0)
            Method (_Q2E, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
            {
                P80B = 0x2E
                If (ATKP)
                {
                    ^^^^ATKD.IANE (0x4F)
                }
            }

            Method (_Q3E, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
            {
                P80B = 0x3E
            }

            Method (_Q3F, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
            {
                P80B = 0x3F
                SELE ()
                ^^^^BAT1.CHBP (BATN)
                If ((0x02 & BATF))
                {
                    Acquire (MUT0, 0x0500)
                    Local0 = NOAC /* \_SB_.PC00.LPCB.EC0_.NOAC */
                    Local2 = TRSC /* \_SB_.PC00.LPCB.EC0_.TRSC */
                    If ((Local0 == One))
                    {
                        PWRS = Zero
                    }
                    Else
                    {
                        PWRS = ACIN /* \_SB_.PC00.LPCB.EC0_.ACIN */
                    }

                    Release (MUT0)
                    Notify (ACAD, 0x80) // Status Change
                    If (ATKP)
                    {
                        If ((PWRS == One))
                        {
                            ^^^^ATKD.IANE (0x58)
                            If (Local2)
                            {
                                Acquire (MUT0, 0x0200)
                                TRSC = Zero
                                Release (MUT0)
                            }
                        }
                        Else
                        {
                            ^^^^ATKD.IANE (0x57)
                        }
                    }

                    PNOT ()
                }

                If ((0x40 & BATF))
                {
                    Notify (BAT1, 0x81) // Information Change
                }

                Notify (BAT1, 0x80) // Status Change
            }

            Method (_Q40, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
            {
                P80B = 0x40
                Notify (\_TZ.THRM, 0x80) // Thermal Status Change
            }

            Method (_Q41, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
            {
                P80B = 0x41
                Notify (\_TZ.THRM, 0x80) // Thermal Status Change
            }

            Method (_Q79, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
            {
                P80B = 0x79
                If ((USTC == One))
                {
                    ^^^^UBTC.QUCM ()
                    Notify (UBTC, 0x80) // Status Change
                }
            }

            Method (_Q7A, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
            {
                P80B = 0x7A
                Notify (^^^PEG1.PEGP, 0x81) // Information Change
            }

            Method (_Q84, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
            {
                P80B = 0x84
                If (ATKP)
                {
                    ^^^^ATKD.IANE (0x6E)
                }
            }

            Method (_Q85, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
            {
                P80B = 0x85
                Notify (BAT1, 0x81) // Information Change
            }

            Method (_QA6, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
            {
                P80B = 0xA6
                If (ATKP)
                {
                    ^^^^ATKD.IANE (0x4F)
                }
            }

            Method (_QAE, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
            {
                P80B = 0xAE
                ^^^^ATKD.IANE (0xE5)
            }

            Method (_QB9, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
            {
                P80B = 0xB9
                If (ATKP)
                {
                    ^^^^ATKD.IANE (0x6E)
                }
            }

            Method (_QBA, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
            {
                P80B = 0xBA
                If (ATKP)
                {
                    ^^^^ATKD.IANE (0x6E)
                }
            }

            Method (_QBB, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
            {
                P80B = 0xBB
                Notify (^^^PEG1.PEGP, 0x81) // Information Change
                If (ATKP)
                {
                    ^^^^ATKD.IANE (0xC0)
                }
            }

            Method (_QBC, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
            {
                P80B = 0xBC
                DNOT (NVDX)
            }

            Method (_QC2, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
            {
                P80B = 0xC2
                If (ATKP)
                {
                    ^^^^ATKD.IANE (0xC0)
                }
            }

            Method (_QCE, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
            {
                P80B = 0xCE
                If (^^^^HIDD.BTLD)
                {
                    Notify (^^^^HIDD, 0xCE) // Hardware-Specific
                    P80B = 0xDE
                }
            }

            Method (_QCF, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
            {
                P80B = 0xCF
                If (^^^^HIDD.BTLD)
                {
                    Notify (^^^^HIDD, 0xCF) // Hardware-Specific
                    P80B = 0xDF
                }
            }

            Name (FMTG, Package (0x03)
            {
                Zero, 
                0x51, 
                0x57
            })
            Method (DGPS, 1, NotSerialized)
            {
                ^^^PEG1.PEGP.NLIM = One
                ^^^PEG1.PEGP.TGPU = DerefOf (FMTG [Arg0])
                Notify (^^^PEG1.PEGP, 0xC0) // Hardware-Specific
            }

            Method (_QDD, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
            {
                P80B = 0xDD
                If (ECOK)
                {
                    Acquire (MUT0, 0x1000)
                    If ((FTBL == Zero))
                    {
                        DNOT (0xD1)
                        ODV0 = Zero
                        Notify (TCPU, 0x83) // Device-Specific Change
                        Notify (IETM, 0x85) // Device-Specific
                        Notify (IETM, 0x88) // Device-Specific
                        DGPS (0x02)
                    }
                    ElseIf ((FTBL == 0x02))
                    {
                        DNOT (0xD1)
                        If ((UMAF != One))
                        {
                            ODV0 = One
                        }

                        If ((UMAF == One))
                        {
                            ODV0 = 0x03
                        }

                        Notify (TCPU, 0x83) // Device-Specific Change
                        Notify (IETM, 0x85) // Device-Specific
                        Notify (IETM, 0x88) // Device-Specific
                        DGPS (One)
                    }
                    ElseIf ((FTBL == One))
                    {
                        DNOT (0xD1)
                        ODV0 = 0x02
                        Notify (TCPU, 0x83) // Device-Specific Change
                        Notify (IETM, 0x85) // Device-Specific
                        Notify (IETM, 0x88) // Device-Specific
                        DGPS (0x02)
                    }

                    Release (MUT0)
                }
            }

            Method (CPUP, 1, NotSerialized)
            {
                ^^^^NPCF.ATPP = Arg0
                ^^^^NPCF.DTPP = Arg0
                Notify (NPCF, 0xC0) // Hardware-Specific
            }

            Method (_QD4, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
            {
                P80B = 0xD4
                QD4F = Zero
                If ((TGSF == One))
                {
                    TGSF = Zero
                    If ((FTBL == 0x02))
                    {
                        CPUP (0xC8)
                        DGPS (One)
                    }
                    Else
                    {
                        If ((FTBL == Zero))
                        {
                            CPUP (0x0118)
                        }
                        Else
                        {
                            CPUP (0x0168)
                        }

                        DGPS (0x02)
                    }
                }
                Else
                {
                    If ((FGWM == One))
                    {
                        If (((^^^^NPCF.WM2M & One) == Zero))
                        {
                            ^^^^NPCF.WM2M = 0x0D
                            Acquire (MUT0, 0x1000)
                            NTWF = One
                            Release (MUT0)
                        }
                    }
                    ElseIf ((FGWM == Zero))
                    {
                        If (((^^^^NPCF.WM2M & One) == One))
                        {
                            ^^^^NPCF.WM2M = Zero
                            Acquire (MUT0, 0x1000)
                            NTWF = One
                            Release (MUT0)
                        }
                    }

                    If ((NTWF == One))
                    {
                        Notify (NPCF, 0xC1) // Hardware-Specific
                        Acquire (MUT0, 0x1000)
                        NTWF = Zero
                        Release (MUT0)
                    }

                    Return (One)
                }
            }

            Method (NVON, 0, NotSerialized)
            {
                HRST = Zero
                Sleep (One)
                PWEB = One
                Sleep (0x07)
                HRST = One
                Sleep (0x64)
            }

            Method (_QA4, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
            {
                P80B = 0xA4
                QA4F = Zero
                If ((^^^PEG1.PEGP.WAT2 () == One))
                {
                    Return (Zero)
                }
            }

            Method (_QA9, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
            {
                P80B = 0xA9
                FGA9 = Zero
                Sleep (0x1770)
                Notify (^^^PEG1.PEGP, Zero) // Bus Check
            }

            Method (_QA8, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
            {
                P80B = 0xA8
                If ((DB2F == Zero))
                {
                    ^^^^NPCF.DBAC = One
                    ^^^^NPCF.DBDC = One
                }

                If ((DB2F == One))
                {
                    ^^^^NPCF.DBAC = Zero
                    ^^^^NPCF.DBDC = Zero
                }

                Notify (NPCF, 0xC0) // Hardware-Specific
            }
        }
    }

    Scope (_SB)
    {
        Device (ATKD)
        {
            Name (_HID, "PNP0C14" /* Windows Management Instrumentation Device */)  // _HID: Hardware ID
            Name (_UID, "ATK")  // _UID: Unique ID
            Name (ATKQ, Package (0x10)
            {
                0xFFFFFFFF, 
                0xFFFFFFFF, 
                0xFFFFFFFF, 
                0xFFFFFFFF, 
                0xFFFFFFFF, 
                0xFFFFFFFF, 
                0xFFFFFFFF, 
                0xFFFFFFFF, 
                0xFFFFFFFF, 
                0xFFFFFFFF, 
                0xFFFFFFFF, 
                0xFFFFFFFF, 
                0xFFFFFFFF, 
                0xFFFFFFFF, 
                0xFFFFFFFF, 
                0xFFFFFFFF
            })
            Name (AQHI, Zero)
            Name (AQTI, 0x0F)
            Name (AQNO, Zero)
            Name (WAPF, Zero)
            Name (PCDV, Zero)
            Name (SWKP, Zero)
            Method (IANQ, 1, Serialized)
            {
                P8XH (Zero, 0xD3)
                If ((AQNO >= 0x10))
                {
                    Local0 = 0x64
                    While ((Local0 && (AQNO >= 0x10)))
                    {
                        Local0--
                        Sleep (0x0A)
                    }

                    If ((Local0 && (AQNO >= 0x10)))
                    {
                        Return (Zero)
                    }
                }

                AQTI++
                AQTI &= 0x0F
                ATKQ [AQTI] = Arg0
                AQNO++
                Return (One)
            }

            Method (GANQ, 0, Serialized)
            {
                P8XH (Zero, 0xF2)
                If (AQNO)
                {
                    AQNO--
                    Local0 = DerefOf (ATKQ [AQHI])
                    AQHI++
                    AQHI &= 0x0F
                    Return (Local0)
                }

                Return (Ones)
            }

            Method (IANE, 1, Serialized)
            {
                P8XH (Zero, 0xF1)
                IANQ (Arg0)
                Notify (ATKD, 0xFF) // Hardware-Specific
            }

            Name (_WDG, Buffer (0x3C)
            {
                /* 0000 */  0xD0, 0x5E, 0x84, 0x97, 0x6D, 0x4E, 0xDE, 0x11,  // .^..mN..
                /* 0008 */  0x8A, 0x39, 0x08, 0x00, 0x20, 0x0C, 0x9A, 0x66,  // .9.. ..f
                /* 0010 */  0x4E, 0x42, 0x01, 0x02, 0x35, 0xBB, 0x3C, 0x0B,  // NB..5.<.
                /* 0018 */  0xC2, 0xE3, 0xED, 0x45, 0x91, 0xC2, 0x4C, 0x5A,  // ...E..LZ
                /* 0020 */  0x6D, 0x19, 0x5D, 0x1C, 0xFF, 0x00, 0x01, 0x08,  // m.].....
                /* 0028 */  0x21, 0x12, 0x90, 0x05, 0x66, 0xD5, 0xD1, 0x11,  // !...f...
                /* 0030 */  0xB2, 0xF0, 0x00, 0xA0, 0xC9, 0x06, 0x29, 0x10,  // ......).
                /* 0038 */  0x4D, 0x4F, 0x01, 0x00                           // MO..
            })
            Method (OFBD, 1, NotSerialized)
            {
                Name (FBDT, Package (0x53)
                {
                    0x88, 
                    0x89, 
                    0x8A, 
                    0x8B, 
                    0x8C, 
                    0x8D, 
                    0x8E, 
                    0x8F, 
                    0x70, 
                    0x71, 
                    0x72, 
                    0x73, 
                    0x74, 
                    0x75, 
                    0x76, 
                    0x77, 
                    0x78, 
