            }
        }
        ElseIf ((I2SB == 0x03))
        {
            Scope (_SB.PC00.I2C3)
            {
                Device (HDAC)
                {
                    Name (_HID, "INT00000")  // _HID: Hardware ID
                    Name (_CID, "INT00000")  // _CID: Compatible ID
                    Name (_DDN, "Intel(R) Smart Sound Technology Audio Codec")  // _DDN: DOS Device Name
                    Name (_UID, One)  // _UID: Unique ID
                    Name (CADR, Zero)
                    Name (CDIS, Zero)
