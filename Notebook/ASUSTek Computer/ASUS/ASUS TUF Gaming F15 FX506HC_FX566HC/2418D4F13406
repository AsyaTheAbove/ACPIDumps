                                0x80
                            }
                        })
                        Return (EWRY) /* \_SB_.PC00.RP13.PXSX.EWRD.EWRY */
                    }

                    Method (WGDS, 0, Serialized)
                    {
                        Name (WGDY, Package (0x02)
                        {
                            Zero, 
                            Package (0x13)
                            {
                                0x07, 
                                0x78, 
                                Zero, 
                                Zero, 
                                0x6C, 
                                Zero, 
                                Zero, 
                                0x84, 
                                0x0C, 
                                0x0C, 
                                0x88, 
                                0x1C, 
