                PA8V = AFL8 /* \_SB_.PC00.AFL8 */
                PA9V = AFL9 /* \_SB_.PC00.AFL9 */
                PAAV = AFLA /* \_SB_.PC00.AFLA */
                PABV = AFLB /* \_SB_.PC00.AFLB */
                PACV = AFLC /* \_SB_.PC00.AFLC */
                PADV = AFLD /* \_SB_.PC00.AFLD */
                PAEV = AFLE /* \_SB_.PC00.AFLE */
                PAFV = AFLF /* \_SB_.PC00.AFLF */
                Local0 = Zero
                PRST = One
                While ((PRST == One))
                {
                    Sleep (One)
                    Local0++
                    If ((Local0 == 0x64))
                    {
                        Break
                    }
                }
            }

