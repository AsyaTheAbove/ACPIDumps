    }

    Method (_WAK, 1, NotSerialized)  // _WAK: Wake
    {
        \_SB.PC00.LPCB.SWAK (Arg0)
        RWAK (Arg0)
        XWAK (Arg0)
        Return (AM00) /* \AM00 */
    }

    Scope (_SB.PC00)
