                }

                If ((GCAC == 0x31))
                {
                    Local2 |= 0x02
                }

                DerefOf (Local0 [0x02]) [0x02] = Local2
                DerefOf (Local0 [0x02]) [0x03] = Local2
                DerefOf (Local0 [0x02]) [0x04] = Local2
