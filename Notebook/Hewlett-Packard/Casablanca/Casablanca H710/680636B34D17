                        Local1 |= 0x08
                    }

                    DerefOf (Local0 [0x02]) [((Local2 * 0x10) + 
                        0x0B)] = Local1
                    DerefOf (Local0 [0x02]) [((Local2 * 0x10) + 
                        0x0C)] = Zero
                    DerefOf (Local0 [0x02]) [((Local2 * 0x10) + 
                        0x0D)] = Zero
                    DerefOf (Local0 [0x02]) [((Local2 * 0x10) + 
                        0x0E)] = Zero
                    DerefOf (Local0 [0x02]) [((Local2 * 0x10) + 
                        0x0F)] = Zero
                }

                If (^^PCI0.LPCB.EC.WWS)
                {
                    Local2 += One
                    DerefOf (Local0 [0x02]) [0x07] = Local2
                    DerefOf (Local0 [0x02]) [(Local2 * 0x10)] = 
                        0x02
                    DerefOf (Local0 [0x02]) [((Local2 * 0x10) + 
                        One)] = One
                    Local1 = (WWVD & 0xFF)
                    DerefOf (Local0 [0x02]) [((Local2 * 0x10) + 
                        0x02)] = Local1
                    Local1 = (WWVD >> 0x08)
                    Local1 &= 0xFF
                    DerefOf (Local0 [0x02]) [((Local2 * 0x10) + 
                        0x03)] = Local1
                    Local1 = (WWDD & 0xFF)
                    DerefOf (Local0 [0x02]) [((Local2 * 0x10) + 
                        0x04)] = Local1
                    Local1 = (WWDD >> 0x08)
                    Local1 &= 0xFF
                    DerefOf (Local0 [0x02]) [((Local2 * 0x10) + 
                        0x05)] = Local1
                    DerefOf (Local0 [0x02]) [((Local2 * 0x10) + 
                        0x06)] = Zero
                    DerefOf (Local0 [0x02]) [((Local2 * 0x10) + 
                        0x07)] = Zero
                    DerefOf (Local0 [0x02]) [((Local2 * 0x10) + 
                        0x08)] = Zero
                    DerefOf (Local0 [0x02]) [((Local2 * 0x10) + 
                        0x09)] = Zero
                    DerefOf (Local0 [0x02]) [((Local2 * 0x10) + 
                        0x0A)] = One
                    Local1 = Zero
                    If (^^PCI0.LPCB.EC.OWAN)
                    {
                        Local1 |= One
                    }
                    ElseIf (WAPT)
                    {
                        If (^^PCI0.LPCB.EC.BATP)
                        {
                            If (^^PCI0.LPCB.EC.WWD)
                            {
                                Local1 |= One
                            }
                        }
                        Else
                        {
                            Local1 &= 0xFE
                        }
                    }
                    ElseIf (^^PCI0.LPCB.EC.WWD)
                    {
                        Local1 |= One
                    }

                    If (WWWI)
                    {
                        Local1 |= 0x02
                    }

                    If (^^PCI0.LPCB.EC.WWSL)
                    {
                        Local1 |= 0x04
                    }

                    If (BTNS)
                    {
                        Local1 |= 0x08
                    }

                    DerefOf (Local0 [0x02]) [((Local2 * 0x10) + 
                        0x0B)] = Local1
                    Local1 = Zero
                    If (!^^PCI0.LPCB.EC.OWAN)
                    {
                        If (SIMD)
                        {
                            Local1 |= One
                        }
                    }

                    If (UNDA)
                    {
                        Local1 |= 0x02
                    }

                    If (UNDB)
                    {
                        Local1 |= 0x04
                    }

                    If (UNDC)
                    {
                        Local1 |= 0x08
                    }

                    DerefOf (Local0 [0x02]) [((Local2 * 0x10) + 
                        0x0C)] = Local1
                    DerefOf (Local0 [0x02]) [((Local2 * 0x10) + 
                        0x0D)] = Zero
                    DerefOf (Local0 [0x02]) [((Local2 * 0x10) + 
                        0x0E)] = Zero
                    DerefOf (Local0 [0x02]) [((Local2 * 0x10) + 
                        0x0F)] = Zero
                }

                If (^^PCI0.LPCB.EC.BTS)
                {
                    Local2 += One
                    DerefOf (Local0 [0x02]) [0x07] = Local2
                    DerefOf (Local0 [0x02]) [(Local2 * 0x10)] = 
                        One
                    DerefOf (Local0 [0x02]) [((Local2 * 0x10) + 
                        One)] = One
                    Local1 = (BTVD & 0xFF)
                    DerefOf (Local0 [0x02]) [((Local2 * 0x10) + 
                        0x02)] = Local1
                    Local1 = (BTVD >> 0x08)
                    Local1 &= 0xFF
                    DerefOf (Local0 [0x02]) [((Local2 * 0x10) + 
                        0x03)] = Local1
                    Local1 = (BTDD & 0xFF)
                    DerefOf (Local0 [0x02]) [((Local2 * 0x10) + 
                        0x04)] = Local1
                    Local1 = (BTDD >> 0x08)
                    Local1 &= 0xFF
                    DerefOf (Local0 [0x02]) [((Local2 * 0x10) + 
                        0x05)] = Local1
                    DerefOf (Local0 [0x02]) [((Local2 * 0x10) + 
                        0x06)] = Zero
                    DerefOf (Local0 [0x02]) [((Local2 * 0x10) + 
                        0x07)] = Zero
                    DerefOf (Local0 [0x02]) [((Local2 * 0x10) + 
                        0x08)] = Zero
                    DerefOf (Local0 [0x02]) [((Local2 * 0x10) + 
                        0x09)] = Zero
                    If (WBCB)
                    {
                        DerefOf (Local0 [0x02]) [((Local2 * 0x10) + 
                            0x0A)] = Zero
                    }
                    Else
                    {
                        DerefOf (Local0 [0x02]) [((Local2 * 0x10) + 
                            0x0A)] = 0x02
                    }

                    Local1 = Zero
                    If (WBCB)
                    {
                        If (^^PCI0.LPCB.EC.WLD)
                        {
                            Local1 |= One
                        }

                        If (WLWI)
                        {
                            Local1 |= 0x02
                        }

                        If (^^PCI0.LPCB.EC.WLSL)
                        {
                            Local1 |= 0x04
                        }
                    }
                    Else
                    {
                        If (^^PCI0.LPCB.EC.BTD)
                        {
                            Local1 |= One
                        }

                        If (BTWI)
                        {
                            Local1 |= 0x02
                        }

                        If (^^PCI0.LPCB.EC.BTSL)
                        {
                            Local1 |= 0x04
                        }
                    }

                    If (BTNS)
                    {
                        Local1 |= 0x08
                    }

                    DerefOf (Local0 [0x02]) [((Local2 * 0x10) + 
                        0x0B)] = Local1
                    DerefOf (Local0 [0x02]) [((Local2 * 0x10) + 
                        0x0C)] = Zero
                    DerefOf (Local0 [0x02]) [((Local2 * 0x10) + 
                        0x0D)] = Zero
                    DerefOf (Local0 [0x02]) [((Local2 * 0x10) + 
                        0x0E)] = Zero
                    DerefOf (Local0 [0x02]) [((Local2 * 0x10) + 
                        0x0F)] = Zero
                }

                Return (Local0)
            }

            Method (SWDS, 1, NotSerialized)
            {
                If (((^^PCI0.LPCB.EC.WLS | ^^PCI0.LPCB.EC.BTS) | ^^PCI0.LPCB.EC.WWS))
                {
                    Local3 = Arg0
                    Local0 = (Local3 & 0xFF)
                    Local1 = (Local3 >> 0x08)
                    Local1 &= 0xFF
                    Local2 = (Local3 >> 0x10)
                    Local2 &= 0xFF
                    Local3 >>= 0x18
                    Local3 &= 0xFF
                    If ((Local3 == One))
                    {
                        If ((Local2 == Zero))
                        {
                            If (WBCB)
                            {
                                SWLS (One)
                                SBTS (One)
                            }
                            Else
                            {
                                SWLS (One)
                            }
                        }

                        If ((Local2 == One))
                        {
                            SWWS (One)
                        }

                        If ((Local2 == 0x02))
                        {
                            SBTS (One)
                        }

                        If ((Local2 == 0xFE))
                        {
                            SWLS (One)
                            SBTS (One)
                            SWWS (One)
                        }

                        If ((Local2 == 0xFF))
                        {
                            BTNS = One
                            SWLS (One)
                            SBTS (One)
                            SWWS (One)
                        }
                    }
                    Else
                    {
                        If ((Local2 == Zero))
                        {
                            If (WBCB)
                            {
                                SWLS (Zero)
                                SBTS (Zero)
                            }
                            Else
                            {
                                SWLS (Zero)
                            }
                        }

                        If ((Local2 == One))
                        {
                            SWWS (Zero)
                        }

                        If ((Local2 == 0x02))
                        {
                            SBTS (Zero)
                        }

                        If ((Local2 == 0xFE))
                        {
                            SWLS (Zero)
                            SBTS (Zero)
                            SWWS (Zero)
                        }

                        If ((Local2 == 0xFF))
                        {
                            BTNS = Zero
                            SWLS (Zero)
                            SBTS (Zero)
                            SWWS (Zero)
                        }
                    }
                }
                Else
                {
                    Return (Package (0x02)
                    {
                        0x0D, 
                        Zero
                    })
                }

                Sleep (0xC8)
                WMIU = 0x05
                WMIV = Zero
                Notify (WMID, 0x80) // Status Change
                P80H = 0x85
                Return (Package (0x02)
                {
                    Zero, 
                    Zero
                })
            }

            Method (SBTS, 1, NotSerialized)
            {
                Local0 = Arg0
                Local0 &= One
                If ((^^PCI0.LPCB.EC.BTS && ^^PCI0.LPCB.EC.BTSL))
                {
                    BTWI = Local0
                    ^^PCI0.LPCB.EC.BTD = Local0
                    Local1 = ~Local0
                    BTPR = Local1
                    BTBK = Local0
                }
            }

            Method (SWLS, 1, NotSerialized)
            {
                Local0 = Arg0
                Local0 &= One
                If ((^^PCI0.LPCB.EC.WLS && ^^PCI0.LPCB.EC.WLSL))
                {
                    WLWI = Local0
                    ^^PCI0.LPCB.EC.WLD = Local0
                    WLBK = Local0
                }
            }

            Method (SWWS, 1, NotSerialized)
            {
                Local0 = Arg0
                Local0 &= One
                If ((^^PCI0.LPCB.EC.WWS && ^^PCI0.LPCB.EC.WWSL))
                {
                    WWWI = Local0
                    ^^PCI0.LPCB.EC.WWD = Local0
                    WWBK = Local0
                }
            }

            Method (GDLC, 0, NotSerialized)
            {
                Local0 = Package (0x03)
                    {
                        Zero, 
                        0x04, 
                        Buffer (0x04)
                        {
                             0x00, 0x00, 0x00, 0x00                           // ....
                        }
                    }
                HWCT = 0x1D
                HWBC = One
                SSMP = 0xC3
                CreateField (DerefOf (Local0 [0x02]), Zero, 0x20, BUFS)
                Local1 = Buffer (0x80){}
                Local1 = HWBF /* \HWBF */
                CreateDWordField (Local1, Zero, HWD1)
                BUFS = HWD1 /* \_SB_.WMID.GDLC.HWD1 */
                Return (Local0)
            }

            Method (SDLC, 1, NotSerialized)
            {
                Local0 = Buffer (0x80){}
                CreateDWordField (Local0, Zero, HWD1)
                HWD1 = Arg0
                HWBF = Local0
                HWCT = 0x1D
                HWBC = 0x02
                SSMP = 0xC3
                Return (Package (0x02)
                {
                    Zero, 
                    Zero
                })
            }

            Method (GBUS, 0, NotSerialized)
            {
                Local2 = Package (0x03)
                    {
                        Zero, 
                        0x04, 
                        Buffer (0x04){}
                    }
                If ((HBUC && One))
                {
                    Local0 = One
                }
                Else
                {
                    Local0 = Zero
                }

                Local1 = HBUS /* \HBUS */
                DerefOf (Local2 [0x02]) [Zero] = Local0
                DerefOf (Local2 [0x02]) [One] = Local1
                Return (Local2)
            }

            Method (SBUS, 1, NotSerialized)
            {
                HBUC = Arg0
                Return (Package (0x02)
                {
                    Zero, 
                    Zero
                })
            }

            Method (WMBG, 0, NotSerialized)
            {
                Local0 = Package (0x03)
                    {
                        Zero, 
                        0x04, 
                        Buffer (0x04)
                        {
                             0x00, 0x00, 0x00, 0x00                           // ....
                        }
                    }
                If (^^PCI0.LPCB.EC.BATP)
                {
                    If (^^PCI0.LPCB.EC.STPC)
                    {
                        DerefOf (Local0 [0x02]) [Zero] = 0x03
                    }
                    ElseIf (^^PCI0.LPCB.EC.ADDC)
                    {
                        DerefOf (Local0 [0x02]) [Zero] = 0x02
                    }
                    ElseIf ((^^PCI0.LPCB.EC.BST & 0x02))
                    {
                        DerefOf (Local0 [0x02]) [Zero] = One
                    }
                    Else
                    {
                        DerefOf (Local0 [0x02]) [Zero] = Zero
                    }
                }
                Else
                {
                    DerefOf (Local0 [0x02]) [Zero] = 0xFF
                }

                DerefOf (Local0 [0x02]) [One] = 0xFF
                Return (Local0)
            }

            Method (WMSG, 1, NotSerialized)
            {
                Local1 = Arg0
                Local0 = (Local1 & 0xFF)
                Local1 >>= 0x08
                Local1 &= 0xFF
                If ((Local0 == Zero))
                {
                    If (^^PCI0.LPCB.EC.BATP)
                    {
                        If ((Local1 == Zero))
                        {
                            ^^PCI0.LPCB.EC.ADDC = Zero
                            ^^PCI0.LPCB.EC.STPC = Zero
                            Local0 = Zero
                            Return (Local0)
                        }

                        If ((Local1 == One))
                        {
                            ^^PCI0.LPCB.EC.ADDC = Zero
                            ^^PCI0.LPCB.EC.STPC = Zero
                            Local0 = Zero
                            Return (Local0)
                        }

                        If ((Local1 == 0x02))
                        {
                            ^^PCI0.LPCB.EC.ADDC = One
                            ^^PCI0.LPCB.EC.STPC = Zero
                            Local0 = Zero
                            Return (Local0)
                        }
                    }
                    Else
                    {
                        Local0 = 0xFF
                        Return (Local0)
                    }
                }

                If ((Local0 == One))
                {
                    Local0 = 0xFF
                    Return (Local0)
                }

                If ((Local0 == 0x63))
                {
                    If (^^PCI0.LPCB.EC.BATP)
                    {
                        If ((Local1 == Zero))
                        {
                            ^^PCI0.LPCB.EC.ADDC = Zero
                            ^^PCI0.LPCB.EC.STPC = Zero
                            Local0 = Zero
                            Return (Local0)
                        }

                        If ((Local1 == One))
                        {
                            ^^PCI0.LPCB.EC.ADDC = One
                            ^^PCI0.LPCB.EC.STPC = Zero
                            Local0 = Zero
                            Return (Local0)
                        }

                        If ((Local1 == 0x02))
                        {
                            ^^PCI0.LPCB.EC.ADDC = Zero
                            ^^PCI0.LPCB.EC.STPC = One
                            Local0 = Zero
                            Return (Local0)
                        }
                    }
                    Else
                    {
                        Local0 = 0xFF
                        Return (Local0)
                    }
                }

                Return (Local0)
            }

            Method (GROC, 0, NotSerialized)
            {
                Local0 = Package (0x03)
                    {
                        Zero, 
                        0x04, 
                        Buffer (0x04)
                        {
                             0x00, 0x00, 0x00, 0x00                           // ....
                        }
                    }
                DerefOf (Local0 [0x02]) [Zero] = Zero
                DerefOf (Local0 [0x02]) [One] = Zero
                DerefOf (Local0 [0x02]) [0x02] = Zero
                DerefOf (Local0 [0x02]) [0x03] = Zero
                Return (Local0)
            }

            Method (GTDC, 1, NotSerialized)
            {
                Local0 = Buffer (0x80){}
                CreateDWordField (Local0, Zero, HWD1)
                HWD1 = Arg0
                HWBF = Local0
                HWCT = 0x28
                HWBC = One
                SSMP = 0xC3
                Local1 = Package (0x03)
                    {
                        Zero, 
                        0x80, 
                        Buffer (0x80){}
                    }
                CreateField (DerefOf (Local1 [0x02]), Zero, 0x0400, BUFS)
                Local2 = Buffer (0x80){}
                Local2 = HWBF /* \HWBF */
                BUFS = Local2
                Return (Local1)
            }

            Method (STDC, 1, NotSerialized)
            {
                Local0 = Buffer (0x80){}
                CreateField (Arg0, 0x80, 0x0400, BUFS)
                Local0 = BUFS /* \_SB_.WMID.STDC.BUFS */
                HWBF = Local0
                HWCT = 0x28
                HWBC = 0x02
                SSMP = 0xC3
                Return (Package (0x02)
                {
                    Zero, 
                    Zero
                })
            }

            Method (GFCC, 0, NotSerialized)
            {
                Local0 = Package (0x03)
                    {
                        Zero, 
                        0x04, 
                        Buffer (0x04)
                        {
                             0x00, 0x00, 0x00, 0x00                           // ....
                        }
                    }
                DerefOf (Local0 [0x02]) [Zero] = FACM /* \FACM */
                Return (Local0)
            }

            Method (SFCC, 1, NotSerialized)
            {
                HWCT = 0x29
                HWBC = 0x02
                WMBF [Zero] = Arg0
                HWBF = WMBF /* \_SB_.WMID.WMBF */
                SSMP = 0xC3
                FACM = Arg0
                Return (Package (0x02)
                {
                    Zero, 
                    Zero
                })
            }

            Method (GPES, 0, NotSerialized)
            {
                Local0 = Package (0x03)
                    {
                        Zero, 
                        0x04, 
                        Buffer (0x04)
                        {
                             0x00, 0x00, 0x00, 0x00                           // ....
                        }
                    }
                HWCT = 0x2A
                HWBC = One
                SSMP = 0xC3
                WMBF = HWBF /* \HWBF */
                DerefOf (Local0 [0x02]) [Zero] = WB00 /* \_SB_.WMID.WB00 */
                Return (Local0)
            }

            Method (SPES, 1, NotSerialized)
            {
                HWCT = 0x2A
                HWBC = 0x02
                WMBF [Zero] = Arg0
                HWBF = WMBF /* \_SB_.WMID.WMBF */
                SSMP = 0xC3
                Return (Package (0x02)
                {
                    Zero, 
                    Zero
                })
            }

            Method (GBCO, 0, NotSerialized)
            {
                Local0 = Package (0x03)
                    {
                        Zero, 
                        0x04, 
                        Buffer (0x04)
                        {
                             0x00, 0x00, 0x00, 0x00                           // ....
                        }
                    }
                Local1 = Zero
                If (^^PCI0.LPCB.EC.BATP)
                {
                    DerefOf (Local0 [0x02]) [Zero] = ^^PCI0.LPCB.EC.BATS /* \_SB_.PCI0.LPCB.EC__.BATS */
                }
                Else
                {
                    DerefOf (Local0 [0x02]) [Zero] = 0xFF
                }

                DerefOf (Local0 [0x02]) [One] = 0xFF
                Return (Local0)
            }

            Method (SBCO, 1, NotSerialized)
            {
                Local1 = Arg0
                Local0 = (Local1 & 0xFF)
                Local1 >>= 0x08
                Local1 &= 0xFF
                If ((Local0 == Zero))
                {
                    If ((Local1 <= 0x05))
                    {
                        ^^PCI0.LPCB.EC.BATC = Local1
                        Sleep (0x64)
                        Return (^^PCI0.LPCB.EC.BATR) /* \_SB_.PCI0.LPCB.EC__.BATR */
                    }
                    Else
                    {
                        Return (0x03)
                    }
                }
                Else
                {
                    Return (0x34)
                }
            }

            Method (GTCS, 0, NotSerialized)
            {
                Local0 = Package (0x03)
                    {
                        Zero, 
                        0x04, 
                        Buffer (0x04)
                        {
                             0x00, 0x00, 0x00, 0x00                           // ....
                        }
                    }
                If (^^PCI0.LPCB.EC.ECOK)
                {
                    DerefOf (Local0 [0x02]) [Zero] = ^^PCI0.LPCB.EC.GSTA /* \_SB_.PCI0.LPCB.EC__.GSTA */
                    DerefOf (Local0 [0x02]) [One] = ^^PCI0.LPCB.EC.MMOS /* \_SB_.PCI0.LPCB.EC__.MMOS */
                    DerefOf (Local0 [0x02]) [0x02] = ^^PCI0.LPCB.EC.DMOS /* \_SB_.PCI0.LPCB.EC__.DMOS */
                    STPS ()
                    Return (Local0)
                }
                Else
                {
                    Local0 = Package (0x02)
                        {
                            0x0D, 
                            Zero
                        }
                    Return (Local0)
                }
            }

            Method (STCS, 1, NotSerialized)
            {
                If (^^PCI0.LPCB.EC.ECOK)
                {
                    Local2 = Arg0
                    Local0 = (Local2 & 0xFF)
                    Local2 >>= 0x08
                    Local1 = (Local2 & 0xFF)
                    Local2 >>= 0x08
                    Local2 &= 0xFF
                    ^^PCI0.LPCB.EC.MMOS = Local1
                    ^^PCI0.LPCB.EC.DMOS = Local2
                    STPS ()
                    Return (Package (0x02)
                    {
                        Zero, 
                        Zero
                    })
                }
                Else
                {
                    Local0 = Package (0x02)
                        {
                            0x0D, 
                            Zero
                        }
                    Return (Local0)
                }
            }

            Method (STPS, 0, NotSerialized)
            {
                If ((^^PCI0.LPCB.EC.TURS == One))
                {
                    Sleep (0x64)
                    If (((^^PCI0.LPCB.EC.GSTA & One) == One))
                    {
                        If (((^^PCI0.LPCB.EC.GSTA & 0x02) == 0x02))
                        {
                            If ((^^PCI0.LPCB.EC.MMOS == Zero))
                            {
                                ^^PCI0.LPCB.EC.SPST (Zero)
                            }
                            Else
                            {
                                ^^PCI0.LPCB.EC.SPST (One)
                            }
                        }
                        ElseIf ((^^PCI0.LPCB.EC.DMOS == Zero))
                        {
                            ^^PCI0.LPCB.EC.SPST (Zero)
                        }
                        Else
                        {
                            ^^PCI0.LPCB.EC.SPST (One)
                        }
                    }
                    ElseIf ((^^PCI0.LPCB.EC.DMOS == Zero))
                    {
                        ^^PCI0.LPCB.EC.SPST (Zero)
                    }
                    Else
                    {
                        ^^PCI0.LPCB.EC.SPST (One)
                    }
                }
                Else
                {
                    ^^PCI0.LPCB.EC.SPST (Zero)
                }
            }

            Method (_WED, 1, NotSerialized)  // _Wxx: Wake Event, xx=0x00-0xFF
            {
                If ((Arg0 == 0xA0))
                {
                    If ((DerefOf (ETYP [Zero]) == Zero))
                    {
                        BF2S = S001 /* \_SB_.WMID.S001 */
                        DerefOf (EVNT [Zero]) [One] = BF2S /* \_SB_.WMID.BF2S */
                        Return (DerefOf (EVNT [Zero]))
                    }

                    If ((DerefOf (ETYP [Zero]) == One))
                    {
                        Return (DerefOf (EVNT [One]))
                    }
                    Else
                    {
                        Return (DerefOf (EVNT [0x02]))
                    }
                }
                Else
                {
                    Local0 = Buffer (0x08)
                        {
                             0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00   // ........
                        }
                    CreateDWordField (Local0, Zero, EVID)
                    CreateDWordField (Local0, 0x04, EVDA)
                    EVID = WMIU /* \_SB_.WMID.WMIU */
                    EVDA = WMIV /* \_SB_.WMID.WMIV */
                    If ((WMIY != Zero))
                    {
                        WMIU = WMIY /* \_SB_.WMID.WMIY */
                        WMIV = WMIZ /* \_SB_.WMID.WMIZ */
                        EVID = WMIU /* \_SB_.WMID.WMIU */
                        EVDA = WMIV /* \_SB_.WMID.WMIV */
                        WMIY = Zero
                        WMIZ = Zero
                        If ((WEVT == Zero))
                        {
                            WEVT = One
                            Notify (WMID, 0x80) // Status Change
                        }
                    }
                    Else
                    {
                        WMIU = Zero
                        WMIV = Zero
                    }

                    Return (Local0)
                }
            }

            Name (WQAE, Buffer (0x08A9)
            {
                /* 0000 */  0x46, 0x4F, 0x4D, 0x42, 0x01, 0x00, 0x00, 0x00,  // FOMB....
                /* 0008 */  0x99, 0x08, 0x00, 0x00, 0x8A, 0x3A, 0x00, 0x00,  // .....:..
