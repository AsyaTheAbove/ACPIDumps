                    If ((WMI8 == 0x2C))
                    {
                        Local2 = GTCS ()
                        WMIB = Zero
                    }
                }

                If ((WMI7 == 0x02))
                {
                    WMIB = 0x04
                    If (((WMI8 > Zero) && (WMI8 <= 0x32)))
                    {
                        If ((WMI9 < DerefOf (WMI4 [(WMI8 - One)])))
                        {
                            WMIB = 0x05
                        }
                        Else
                        {
                            CreateDWordField (Arg1, 0x10, WMIL)
                            If ((WMI8 == 0x09))
                            {
                                Local2 = WMIN (WMIL)
                                WMIB = Zero
                            }

