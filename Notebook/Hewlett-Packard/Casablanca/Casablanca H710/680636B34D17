                                WMIB = Zero
                            }

                            If ((WMI8 == 0x2C))
                            {
                                Local2 = STCS (WMIL)
                                WMIB = Zero
                            }
                        }
                    }
                }

                If ((WMI7 == 0x00020002))
                {
                    WMIB = 0x04
                    If (((WMI8 > Zero) && (WMI8 <= 0x03)))
                    {
                        If ((WMI9 < DerefOf (WMI5 [(WMI8 - One)])))
                        {
                            WMIB = 0x05
                        }
                        Else
                        {
                            CreateDWordField (Arg1, 0x10, WMCT)
                            If ((WMI8 == One))
                            {
                                Local2 = Package (0x03)
                                    {
                                        Zero, 
                                        0x04, 
                                        Buffer (0x04)
                                        {
                                             0x00, 0x00, 0x00, 0x00                           // ....
                                        }
                                    }
                                Local2 = CSTA ()
                                WMIB = Zero
                            }

                            If ((WMI8 == 0x02))
                            {
                                Local2 = Package (0x03)
                                    {
                                        Zero, 
                                        Zero, 
                                        Zero
                                    }
                                Local2 [Zero] = IAOR /* \IAOR */
                                Local2 = CACT (WMCT)
                                WMIB = Zero
                            }

                            If ((WMI8 == 0x03))
                            {
                                Local2 = Package (0x03)
                                    {
                                        Zero, 
                                        Zero, 
                                        Zero
                                    }
                                Local2 = CDAC (WMCT)
                                WMIB = Zero
                            }
                        }
                    }
                }

                If ((WMIB == Zero))
                {
                    WMIB = DerefOf (Local2 [Zero])
                    If ((WMIB == Zero))
                    {
                        If ((DerefOf (Local2 [One]) <= Local0))
                        {
                            Local0 = Zero
                            While ((Local0 < DerefOf (Local2 [One])))
                            {
                                Local1 [(Local0 + 0x08)] = DerefOf (DerefOf (
                                    Local2 [0x02]) [Local0])
                                Local0++
                            }

                            WMIA = 0x53534150
                        }
                        Else
                        {
                            WMIB = 0x05
                        }
                    }
                }

                Return (Local1)
            }

