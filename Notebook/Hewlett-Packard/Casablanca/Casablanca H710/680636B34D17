            Method (HDWC, 2, NotSerialized)
            {
                CreateDWordField (Arg1, 0x04, WMI7)
                CreateDWordField (Arg1, 0x08, WMI8)
                CreateDWordField (Arg1, 0x0C, WMI9)
                If ((Arg0 == One))
                {
                    Local0 = Zero
                }

                If ((Arg0 == 0x02))
                {
                    Local0 = 0x04
                }

                If ((Arg0 == 0x03))
                {
                    Local0 = 0x80
                }

                If ((Arg0 == 0x04))
                {
                    Local0 = 0x0400
                }

                If ((Arg0 == 0x05))
                {
                    Local0 = 0x1000
                }

                Local1 = Buffer ((0x08 + Local0)){}
                CreateDWordField (Local1, Zero, WMIA)
                CreateDWordField (Local1, 0x04, WMIB)
                WMIA = 0x4C494146
                WMIB = 0x02
                WMIB = 0x03
                If ((WMI7 == One))
                {
                    WMIB = 0x04
                    If ((WMI8 == 0x07))
                    {
                        If (WMI9)
                        {
                            Local3 = DerefOf (Arg1 [0x10])
                            Local2 = GTBI (Local3)
                            WMIB = Zero
                        }
                        Else
                        {
                            WMIB = 0x05
                        }
