                        }
                    })
                    MXIF [Zero] = NDID /* External reference */
                    DerefOf (MXIF [One]) [Zero] = DID1 /* External reference */
                    DerefOf (MXIF [0x02]) [Zero] = DID2 /* External reference */
                    DerefOf (MXIF [0x03]) [Zero] = DID3 /* External reference */
                    DerefOf (MXIF [0x04]) [Zero] = DID4 /* External reference */
                    DerefOf (MXIF [0x05]) [Zero] = DID5 /* External reference */
                    DerefOf (MXIF [0x06]) [Zero] = DID6 /* External reference */
                    DerefOf (MXIF [0x07]) [Zero] = DID7 /* External reference */
                    DerefOf (MXIF [0x08]) [Zero] = DID8 /* External reference */
                    DerefOf (MXIF [One]) [One] = MXVL (\_SB.PCI0.PEG0.PEGP.MXD1)
                    DerefOf (MXIF [0x02]) [One] = MXVL (\_SB.PCI0.PEG0.PEGP.MXD2)
                    DerefOf (MXIF [0x03]) [One] = MXVL (\_SB.PCI0.PEG0.PEGP.MXD3)
                    DerefOf (MXIF [0x04]) [One] = MXVL (\_SB.PCI0.PEG0.PEGP.MXD4)
                    DerefOf (MXIF [0x05]) [One] = MXVL (\_SB.PCI0.PEG0.PEGP.MXD5)
                    DerefOf (MXIF [0x06]) [One] = MXVL (\_SB.PCI0.PEG0.PEGP.MXD6)
                    DerefOf (MXIF [0x07]) [One] = MXVL (\_SB.PCI0.PEG0.PEGP.MXD7)
                    DerefOf (MXIF [0x08]) [One] = MXVL (\_SB.PCI0.PEG0.PEGP.MXD8)
                    If ((\_SB.PCI0.PEG0.PEGP.SGFL & One))
                    {
                        DerefOf (MXIF [NDID]) [Zero] = 0x80010306
                        DerefOf (MXIF [NDID]) [One] = MXVL (0x0101010E)
                    }

                    Return (MXIF) /* \_SB_.PCI0.GFX0._DSM.MXIF */
                }
            }

            If (CondRefOf (\_SB.PCI0.GFX0.HDSM))
            {
                Return (\_SB.PCI0.GFX0.HDSM) /* External reference */
                Arg0
                Arg1
                Arg2
                Arg3
            }

            Return (One)
        }

        Method (IDAB, 0, Serialized)
        {
            If (((\_SB.PCI0.PEG0.PEGP.SGMD & 0x0F) == One))
            {
                If ((\_SB.PCI0.PEG0.PEGP.SGFL == One))
                {
                    WIID ()
                }
            }
            Else
            {
                NDID = Zero
                If ((DIDL != Zero))
                {
                    DIDL = SDDL /* External reference */
                    DID1
                }

                If ((DDL2 != Zero))
                {
                    DDL2 = SDDL /* External reference */
                    DID2
                }

                If ((DDL3 != Zero))
                {
                    DDL3 = SDDL /* External reference */
                    DID3
                }

                If ((DDL4 != Zero))
                {
                    DDL4 = SDDL /* External reference */
                    DID4
                }

                If ((DDL5 != Zero))
                {
                    DDL5 = SDDL /* External reference */
                    DID5
                }

                If ((DDL6 != Zero))
                {
                    DDL6 = SDDL /* External reference */
                    DID6
                }

                If ((DDL7 != Zero))
                {
                    DDL7 = SDDL /* External reference */
                    DID7
                }

                If ((DDL8 != Zero))
                {
                    DDL8 = SDDL /* External reference */
                    DID8
                }
            }

            Return (Zero)
        }

        Method (HWID, 0, Serialized)
        {
            Name (TMP9, Package (0x09)
            {
                Ones, 
                Ones, 
                Ones, 
                Ones, 
                Ones, 
                Ones, 
                Ones, 
                Ones, 
                Ones
            })
            TMP9 [Zero] = (0x00010000 | DID1)
            TMP9 [One] = (0x00010000 | DID2)
            TMP9 [0x02] = (0x00010000 | DID3)
            TMP9 [0x03] = (0x00010000 | DID4)
            TMP9 [0x04] = (0x00010000 | DID5)
            TMP9 [0x05] = (0x00010000 | DID6)
            TMP9 [0x06] = (0x00010000 | DID7)
            TMP9 [0x07] = (0x00010000 | DID8)
            TMP9 [0x08] = Zero
            If ((\_SB.PCI0.PEG0.PEGP.SGFL & One))
            {
                TMP9 [0x08] = 0x80010306
            }

            Return (TMP9) /* \_SB_.PCI0.GFX0.HWID.TMP9 */
        }

        Method (WIID, 0, Serialized)
        {
            If (((DIDL & 0x0F0F) == 0x0306))
            {
                Return (Zero)
            }

            If (((DID2 & 0x0F0F) == 0x0306))
            {
                Return (Zero)
            }

            If (((DID3 & 0x0F0F) == 0x0306))
            {
                Return (Zero)
            }

            If (((DID4 & 0x0F0F) == 0x0306))
            {
                Return (Zero)
            }

            If (((DID5 & 0x0F0F) == 0x0306))
            {
                Return (Zero)
            }

            If (((DID6 & 0x0F0F) == 0x0306))
            {
                Return (Zero)
            }

            If (((DID7 & 0x0F0F) == 0x0306))
            {
                Return (Zero)
            }

            If (((DID8 & 0x0F0F) == 0x0306))
            {
                Return (Zero)
            }

            Switch (NDID)
            {
                Case (One)
                {
                    DID2 = 0x80010306
                    NDID = 0x02
                }
                Case (0x02)
                {
                    DID3 = 0x80010306
                    NDID = 0x03
                }
                Case (0x03)
                {
                    DID4 = 0x80010306
                    NDID = 0x04
                }
                Case (0x04)
                {
                    DID5 = 0x80010306
                    NDID = 0x05
                }
                Case (0x05)
                {
                    DID6 = 0x80010306
                    NDID = 0x06
                }
                Case (0x06)
                {
                    DID7 = 0x80010306
                    NDID = 0x07
                }
                Case (0x07)
                {
                    DID8 = 0x80010306
                    NDID = 0x08
                }
                Case (0x08)
                {
                    NDID = 0x09
                }

            }

            Return (Zero)
        }
    }

    Scope (\_SB.PCI0.PEG0)
    {
        OperationRegion (RPCI, PCI_Config, Zero, 0xF0)
        Field (RPCI, DWordAcc, Lock, Preserve)
        {
            Offset (0xB0), 
            ASPM,   2, 
                ,   2, 
            LNKD,   1
        }

        OperationRegion (RPCX, SystemMemory, \_SB.PCI0.PEG0.PEGP.XBAS, 0x8400)
        Field (RPCX, DWordAcc, NoLock, Preserve)
        {
            Offset (0x8214), 
            Offset (0x8216), 
            LNKS,   4
        }

        Device (PEGA)
        {
            Name (_ADR, One)  // _ADR: Address
            Name (_PRW, Package (0x02)  // _PRW: Power Resources for Wake
            {
                0x09, 
                0x04
            })
            OperationRegion (ACAP, PCI_Config, \_SB.PCI0.PEG0.PEGP.EECP, 0x14)
            Field (ACAP, DWordAcc, NoLock, Preserve)
            {
                Offset (0x10), 
                LCT1,   16
            }
        }
    }

    Scope (\_SB.PCI0.PEG0.PEGP)
    {
        Name (ELCT, Zero)
        Name (HVID, Zero)
        Name (HDID, Zero)
        OperationRegion (PCIS, PCI_Config, Zero, 0x0100)
        Field (PCIS, DWordAcc, Lock, Preserve)
        {
            DVID,   16, 
            Offset (0x2C), 
            SVID,   16, 
            SDID,   16, 
            Offset (0x4C), 
            WVID,   16, 
            WDID,   16
        }

        Field (PCIS, ByteAcc, NoLock, Preserve)
        {
            Offset (0x04), 
            CMDR,   8, 
            VGAR,   2008
        }

        Name (VGAB, Buffer (0xFB)
        {
             0x00                                             // .
        })
        OperationRegion (GPIO, SystemIO, GBAS, 0x60)
        Field (GPIO, ByteAcc, Lock, Preserve)
        {
            Offset (0x0C), 
            LVL0,   32, 
            Offset (0x38), 
            LVL1,   32, 
            Offset (0x48), 
            LVL2,   32
        }

        OperationRegion (PCAP, PCI_Config, EECP, 0x14)
        Field (PCAP, DWordAcc, NoLock, Preserve)
        {
            Offset (0x10), 
            LCTL,   16
        }

        Method (_INI, 0, NotSerialized)  // _INI: Initialize
        {
            \_SB.PCI0.PEG0.PEGP._ADR = Zero
        }

        Method (_ON, 0, Serialized)  // _ON_: Power On
        {
            GLSC ()
            SGPO (HLRS, One)
            SGPO (PWEN, One)
            Sleep (APDT)
            SGPO (HLRS, Zero)
            Sleep (IHDT)
            GLSR ()
            LNKD = Zero
            While ((LNKS < 0x07))
            {
                Sleep (One)
            }

            CMDR = Zero
            VGAR = VGAB /* \_SB_.PCI0.PEG0.PEGP.VGAB */
            CMDR = 0x06
            WVID = HVID /* \_SB_.PCI0.PEG0.PEGP.HVID */
            WDID = HDID /* \_SB_.PCI0.PEG0.PEGP.HDID */
            LCTL = ((ELCT & 0x43) | (LCTL & 0xFFBC))
            \_SB.PCI0.PEG0.PEGA.LCT1 = ((ELCT & 0x43) | (\_SB.PCI0.PEG0.PEGA.LCT1 & 0xFFBC))
            Notify (\_SB.PCI0.PEG0, Zero) // Bus Check
            Return (One)
        }

        Method (_OFF, 0, Serialized)  // _OFF: Power Off
        {
            ELCT = LCTL /* \_SB_.PCI0.PEG0.PEGP.LCTL */
            HVID = SVID /* \_SB_.PCI0.PEG0.PEGP.SVID */
            HDID = SDID /* \_SB_.PCI0.PEG0.PEGP.SDID */
            VGAB = VGAR /* \_SB_.PCI0.PEG0.PEGP.VGAR */
            LNKD = One
            While ((LNKS != Zero))
            {
                Sleep (One)
            }

            GLSC ()
            SGPO (HLRS, One)
            SGPO (PWEN, Zero)
            GLSR ()
            Notify (\_SB.PCI0.PEG0, Zero) // Bus Check
            Return (Zero)
        }

        Method (_STA, 0, Serialized)  // _STA: Status
        {
            If ((SGMD & 0x0F))
            {
                If ((SGPI (PWOK) == One))
                {
                    Return (0x0F)
                }

                Return (Zero)
            }

            If ((DVID != 0xFFFF))
            {
                Return (0x0F)
            }

            Return (Zero)
        }

        Method (_DOD, 0, NotSerialized)  // _DOD: Display Output Devices
        {
            Return (\_SB.PCI0.GFX0._DOD) /* External reference */
        }

        Device (DD01)
