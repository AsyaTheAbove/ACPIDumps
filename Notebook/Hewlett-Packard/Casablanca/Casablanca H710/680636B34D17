                    }

                    Return (NDDS (DID2))
                }

                Method (_DSS, 1, NotSerialized)  // _DSS: Device Set State
                {
                    If (((Arg0 & 0xC0000000) == 0xC0000000))
                    {
                        CSTE = NSTE /* \NSTE */
                    }
                }

                Method (_BCL, 0, NotSerialized)  // _BCL: Brightness Control Levels
                {
                    BTM0 = 0x8010
                    BTM1 = 0x8A16
                    BTM2 = 0x941C
                    BTM3 = 0x9E25
                    BTM4 = 0xA831
                    BTM5 = 0xB241
                    BTM6 = 0xBC56
                    BTM7 = 0xC671
                    BTM8 = 0xD094
                    BTM9 = 0xDAC3
                    BTMA = 0xE4FF
                    If ((LCDD == 0x15A2AF0D))
                    {
                        BTM0 = 0x8012
                        BTM1 = 0x8A18
                        BTM2 = 0x941F
                        BTM3 = 0x9E29
                        BTM4 = 0xA836
                        BTM5 = 0xB248
                        BTM6 = 0xBC5F
                        BTM7 = 0xC67E
                        BTM8 = 0xD0A6
                        BTM9 = 0xDADA
                        BTMA = 0xE4FF
                    }

                    Return (Package (0x0D)
                    {
                        0x64, 
                        0x32, 
                        Zero, 
                        0x0A, 
                        0x14, 
                        0x1E, 
                        0x28, 
                        0x32, 
                        0x3C, 
                        0x46, 
                        0x50, 
                        0x5A, 
                        0x64
                    })
                }

                Name (LGP1, Package (0x0B)
                {
                    0x06, 
                    0x08, 
                    0x0A, 
                    0x0E, 
                    0x13, 
                    0x19, 
                    0x21, 
                    0x2C, 
                    0x3A, 
                    0x4C, 
                    0x64
                })
                Name (CMO1, Package (0x0B)
                {
                    0x07, 
                    0x09, 
                    0x0C, 
                    0x0F, 
                    0x14, 
                    0x1B, 
                    0x24, 
                    0x2F, 
                    0x3E, 
                    0x52, 
                    0x64
                })
                Method (_BCM, 1, NotSerialized)  // _BCM: Brightness Control Method
                {
                    If (((Arg0 >= Zero) && (Arg0 <= 0x64)))
                    {
                        BRTL = Arg0
                        Divide (Arg0, 0x0A, Local2, Local1)
                        Local0 = DerefOf (LGP1 [Local1])
                        If ((LCDD == 0x15A2AF0D))
                        {
                            Local0 = DerefOf (CMO1 [Local1])
                        }

                        AINT (One, Local0)
                    }

                    ^^^LPCB.EC.STBL ()
                }

                Method (_BQC, 0, NotSerialized)  // _BQC: Brightness Query Current
                {
                    Return (BRTL) /* \BRTL */
                }
            }

            Device (DD03)
            {
                Method (_ADR, 0, Serialized)  // _ADR: Address
                {
                    If ((DID3 == Zero))
                    {
                        Return (0x03)
                    }
                    Else
                    {
                        Return ((0xFFFF & DID3))
