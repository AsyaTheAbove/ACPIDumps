            Method (_DSS, 1, NotSerialized)  // _DSS: Device Set State
            {
            }
        }

        Method (SGPI, 1, Serialized)
        {
            Local1 = (Arg0 >> 0x07)
            Arg0 &= 0x7F
            If ((Arg0 < 0x20))
            {
                Local0 = \_SB.PCI0.PEG0.PEGP.LVL0
                Local0 >>= Arg0
            }
            ElseIf ((Arg0 < 0x40))
            {
                Local0 = \_SB.PCI0.PEG0.PEGP.LVL1
                Local0 >>= (Arg0 - 0x20)
            }
            Else
            {
                Local0 = \_SB.PCI0.PEG0.PEGP.LVL2
                Local0 >>= (Arg0 - 0x40)
            }

            If ((Local1 == Zero))
            {
                Local0 = ~Local0
            }

            Return ((Local0 & One))
        }

        Method (SGPO, 2, Serialized)
        {
            Local3 = (Arg0 >> 0x07)
            Arg0 &= 0x7F
            If ((Local3 == Zero))
            {
                Local3 = ~Arg1
                Local3 &= One
            }

            If ((Arg0 < 0x20))
            {
                Local0 = (Local3 << Arg0)
                Local1 = (One << Arg0)
                Local2 = (\_SB.PCI0.PEG0.PEGP.LVL0 & ~Local1)
                \_SB.PCI0.PEG0.PEGP.LVL0 = (Local2 | Local0)
            }
            ElseIf ((Arg0 < 0x40))
            {
                Local0 = (Local3 << (Arg0 - 0x20))
                Local1 = (One << (Arg0 - 0x20))
                Local2 = (\_SB.PCI0.PEG0.PEGP.LVL1 & ~Local1)
                \_SB.PCI0.PEG0.PEGP.LVL1 = (Local2 | Local0)
            }
            Else
            {
                Local0 = (Local3 << (Arg0 - 0x40))
                Local1 = (One << (Arg0 - 0x40))
                Local2 = (\_SB.PCI0.PEG0.PEGP.LVL2 & ~Local1)
                \_SB.PCI0.PEG0.PEGP.LVL2 = (Local2 | Local0)
            }

            Return (One)
        }
    }

    Scope (\_SB.PCI0.GFX0)
    {
        Method (ATPX, 2, Serialized)
        {
            If ((Arg0 == Zero))
            {
                Name (TMP1, Buffer (0x0100)
                {
                     0x00                                             // .
                })
                CreateWordField (TMP1, Zero, F0SS)
                CreateWordField (TMP1, 0x02, F0IV)
                CreateDWordField (TMP1, 0x04, F0SF)
                F0SS = 0x08
                F0IV = One
                F0SF = 0xBF
                If (((\_SB.PCI0.PEG0.PEGP.SGMD & 0x0F) == Zero))
                {
                    F0SF = Zero
                    Return (TMP1) /* \_SB_.PCI0.GFX0.ATPX.TMP1 */
                }

                If (((\_SB.PCI0.PEG0.PEGP.SGMD & 0x0F) == 0x03))
                {
                    F0SF = Zero
                    Return (TMP1) /* \_SB_.PCI0.GFX0.ATPX.TMP1 */
                }

                If (((\_SB.PCI0.PEG0.PEGP.SGMD & 0x0F) == 0x02))
                {
                    F0SF = 0x33
                }

                If ((\_SB.PCI0.PEG0.PEGP.PXDY == One))
                {
                    F0SF &= 0xFFFFFFFD
                }

                If ((\_SB.PCI0.PEG0.PEGP.PXFD == One))
                {
                    F0SF |= 0x02
                }

                Return (TMP1) /* \_SB_.PCI0.GFX0.ATPX.TMP1 */
            }

            If ((Arg0 == One))
            {
                Name (TMP2, Buffer (0x0100)
                {
                     0x00                                             // .
                })
                CreateWordField (TMP2, Zero, F1SS)
                CreateDWordField (TMP2, 0x02, F1VM)
                CreateDWordField (TMP2, 0x06, F1FG)
                F1SS = 0x0A
                F1VM = 0x7F
                If (((\_SB.PCI0.PEG0.PEGP.SGMD & 0x0F) == 0x02))
                {
                    F1FG = Zero
                    F1VM = Zero
                    If ((\_SB.PCI0.PEG0.PEGP.PXDY == One))
                    {
                        F1FG = 0x80
                        F1VM = 0x80
                    }
                }
                Else
                {
                    F1FG = 0x40
                    F1VM = 0x40
                }

                Return (TMP2) /* \_SB_.PCI0.GFX0.ATPX.TMP2 */
            }

            If ((Arg0 == 0x02))
            {
                CreateWordField (Arg1, Zero, FN2S)
                CreateByteField (Arg1, 0x02, DGPR)
                If ((DGPR == Zero))
                {
                    \_SB.PCI0.PEG0.PEGP._OFF ()
                }

                If ((DGPR == One))
                {
                    \_SB.PCI0.PEG0.PEGP._ON ()
                }

                Return (Zero)
            }

            If ((Arg0 == 0x03))
            {
                CreateWordField (Arg1, Zero, FN3S)
                CreateWordField (Arg1, 0x02, SDMG)
                GLSC ()
                If ((SDMG == Zero))
                {
                    \_SB.PCI0.PEG0.PEGP.SGPO (\_SB.PCI0.PEG0.PEGP.DSEL, Zero)
                }

                If ((SDMG == One))
                {
                    \_SB.PCI0.PEG0.PEGP.SGPO (\_SB.PCI0.PEG0.PEGP.DSEL, One)
                }

                GLSR ()
                Return (Zero)
            }

            If ((Arg0 == 0x04))
            {
                CreateWordField (Arg1, Zero, FN4S)
                CreateWordField (Arg1, 0x02, SIMG)
                GLSC ()
                If ((SIMG == Zero))
                {
                    \_SB.PCI0.PEG0.PEGP.SGPO (\_SB.PCI0.PEG0.PEGP.ESEL, Zero)
                }

                If ((SIMG == One))
                {
                    \_SB.PCI0.PEG0.PEGP.SGPO (\_SB.PCI0.PEG0.PEGP.ESEL, One)
                }

                GLSR ()
                Return (Zero)
            }

            If ((Arg0 == 0x05))
            {
                CreateWordField (Arg1, Zero, FN5S)
                CreateWordField (Arg1, 0x02, TGFX)
                \_SB.PCI0.PEG0.PEGP.TGXA = TGFX /* \_SB_.PCI0.GFX0.ATPX.TGFX */
                \_SB.PCI0.PEG0.PEGP.GSTP = One
                Return (Zero)
            }

            If ((Arg0 == 0x06))
            {
                CreateWordField (Arg1, Zero, FN6S)
                CreateWordField (Arg1, 0x02, AGFX)
                \_SB.PCI0.PEG0.PEGP.AGXA = AGFX /* \_SB_.PCI0.GFX0.ATPX.AGFX */
                \_SB.PCI0.PEG0.PEGP.GSTP = Zero
                Return (Zero)
            }

            If ((Arg0 == 0x08))
            {
                Name (TMP3, Buffer (0x0100)
                {
                    /* 0000 */  0x0E, 0x00, 0x46, 0x00, 0x07, 0x01, 0x00, 0x00,  // ..F.....
                    /* 0008 */  0x01, 0x07, 0x01, 0x01, 0x00, 0x01, 0x05, 0x00,  // ........
                    /* 0010 */  0x00, 0x00, 0x04, 0x05, 0x00, 0x01, 0x10, 0x01,  // ........
                    /* 0018 */  0x07, 0x03, 0x00, 0x00, 0x03, 0x07, 0x03, 0x01,  // ........
                    /* 0020 */  0x10, 0x02, 0x07, 0x07, 0x00, 0x01, 0x03, 0x07,  // ........
                    /* 0028 */  0x07, 0x01, 0x10, 0x02, 0x07, 0x09, 0x00, 0x02,  // ........
                    /* 0030 */  0x03, 0x07, 0x09, 0x01, 0x20, 0x02, 0x07, 0x0A,  // .... ...
                    /* 0038 */  0x00, 0x03, 0x03, 0x07, 0x0A, 0x01, 0x30, 0x02,  // ......0.
                    /* 0040 */  0x07, 0x0B, 0x00, 0x04, 0x03, 0x07, 0x0B, 0x01,  // ........
                    /* 0048 */  0x30, 0x02, 0x07, 0x0C, 0x00, 0x06, 0x03         // 0......
                })
                CreateWordField (TMP3, Zero, ATNO)
                CreateWordField (TMP3, 0x02, ATSZ)
                TMP3 [0x0E] = ADPM (\_SB.PCI0.PEG0.PEGP.MXD2, One)
                TMP3 [0x13] = ADPM (\_SB.PCI0.PEG0.PEGP.MXD2, One)
                TMP3 [0x18] = ADPM (\_SB.PCI0.PEG0.PEGP.MXD3, One)
                TMP3 [0x1D] = ADPM (\_SB.PCI0.PEG0.PEGP.MXD3, 0x02)
                TMP3 [0x22] = ADPM (\_SB.PCI0.PEG0.PEGP.MXD4, One)
                TMP3 [0x27] = ADPM (\_SB.PCI0.PEG0.PEGP.MXD4, 0x02)
                TMP3 [0x2C] = ADPM (\_SB.PCI0.PEG0.PEGP.MXD5, One)
                TMP3 [0x31] = ADPM (\_SB.PCI0.PEG0.PEGP.MXD5, 0x02)
                TMP3 [0x36] = ADPM (\_SB.PCI0.PEG0.PEGP.MXD6, One)
                TMP3 [0x3B] = ADPM (\_SB.PCI0.PEG0.PEGP.MXD6, 0x02)
                TMP3 [0x40] = ADPM (\_SB.PCI0.PEG0.PEGP.MXD7, One)
                TMP3 [0x45] = ADPM (\_SB.PCI0.PEG0.PEGP.MXD7, 0x02)
                If ((\_SB.PCI0.PEG0.PEGP.SGFL & One))
                {
                    ATNO = (ATNO + One)
                    ATSZ = (ATSZ + 0x05)
                }

                Return (TMP3) /* \_SB_.PCI0.GFX0.ATPX.TMP3 */
            }

            Return (Zero)
        }

        Method (ADPM, 2, Serialized)
        {
            Local1 = Zero
            Local0 = (Arg0 >> 0x10)
            If ((Arg1 == One))
            {
                Local1 |= (Local0 & One)
            }
            Else
            {
                Local1 |= ((Local0 & 0x02) >> One)
            }

            Local1 <<= One
            Local0 = (Arg0 >> 0x18)
            If ((Arg1 == One))
            {
                Local1 |= (Local0 & One)
            }
            Else
            {
                Local1 |= ((Local0 & 0x02) >> One)
            }

            Local1 <<= One
            Local0 = (Arg0 >> 0x08)
            If ((Arg1 == One))
            {
                Local1 |= (Local0 & One)
            }
            Else
            {
                Local1 |= ((Local0 & 0x02) >> One)
            }

            Return (Local1)
        }

        Method (ATRM, 2, Serialized)
        {
            Local0 = Arg0
            Local1 = Arg1
            If ((Local1 > 0x1000))
            {
                Local1 = 0x1000
            }

            If ((Local0 > 0x00010000))
            {
                Return (Buffer (Local1)
                {
                     0x00                                             // .
                })
            }

            If ((Local0 > \_SB.PCI0.PEG0.PEGP.RVBS))
            {
                Return (Buffer (Local1)
                {
                     0x00                                             // .
                })
            }

            Local3 = (Local1 * 0x08)
            Name (ROM1, Buffer (0x8000)
            {
                 0x00                                             // .
            })
            Name (ROM2, Buffer (Local1)
            {
                 0x00                                             // .
            })
            If ((Local0 < 0x8000))
            {
                ROM1 = \_SB.PCI0.PEG0.PEGP.RBF1
            }
            Else
            {
                Local0 -= 0x8000
                ROM1 = \_SB.PCI0.PEG0.PEGP.RBF2
            }

            Local2 = (Local0 * 0x08)
            CreateField (ROM1, Local2, Local3, TMPB)
            ROM2 = TMPB /* \_SB_.PCI0.GFX0.ATRM.TMPB */
            Return (ROM2) /* \_SB_.PCI0.GFX0.ATRM.ROM2 */
        }
    }

    Name (BDFR, 0x000F804C)
    Name (GPLS, Zero)
    OperationRegion (GPCR, SystemMemory, BDFR |= PEBS /* \BDFR */, One)
    Field (GPCR, AnyAcc, NoLock, Preserve)
    {
        GPCT,   1
    }

    Method (GLSC, 0, Serialized)
    {
        GPLS = GPCT /* \GPCT */
        If ((GPCT == One))
        {
            GPCT = Zero
        }
    }

    Method (GLSR, 0, Serialized)
    {
        GPCT = GPLS /* \GPLS */
    }

    Method (HGAS, 0, NotSerialized)
    {
        \_SB.PCI0.PEG0.PEGP.PXGS = One
        Notify (\_SB.PCI0.GFX0, 0x81) // Information Change
    }

    Method (HBRT, 1, Serialized)
    {
        If ((0x04 & DSEN))
        {
            If ((Arg0 == 0x04))
            {
                Notify (\_SB.PCI0.PEG0.PEGP.DD02, 0x87) // Device-Specific
            }

            If ((Arg0 == 0x03))
            {
                Notify (\_SB.PCI0.PEG0.PEGP.DD02, 0x86) // Device-Specific
            }
        }
    }

    Method (HPFS, 0, Serialized)
    {
        If ((\_SB.PCI0.PEG0.PEGP.GSTP != One))
        {
            If ((\_SB.PCI0.PEG0.PEGP.AGXA == Zero))
            {
                \_SB.PCI0.GFX0.AINT (0x02, Zero)
            }
            Else
            {
                \_SB.PCI0.PEG0.PEGP.EMDR = One
                If ((\_SB.PCI0.PEG0.PEGP.EXPM == 0x02))
                {
                    \_SB.PCI0.PEG0.PEGP.EXPM = Zero
                }
                Else
                {
                    \_SB.PCI0.PEG0.PEGP.EXPM++
                }

                Notify (\_SB.PCI0.GFX0, 0x81) // Information Change
            }
        }
    }

    Method (HNOT, 1, Serialized)
    {
        Name (TMP0, Zero)
        While (One)
        {
            TMP0 = Arg0
            If ((TMP0 == One))
            {
                If ((\_SB.PCI0.PEG0.PEGP.GSTP != One))
                {
                    If ((\_SB.PCI0.PEG0.PEGP.SGMD & 0x02))
                    {
                        If ((\_SB.PCI0.PEG0.PEGP.AGXA == Zero))
                        {
                            Notify (\_SB.PCI0.GFX0, 0x80) // Status Change
                        }

                        If ((\_SB.PCI0.PEG0.PEGP.AGXA == One))
                        {
                            Notify (\_SB.PCI0.PEG0.PEGP, 0x80) // Status Change
                        }
                    }
                    Else
                    {
                        \_SB.PCI0.PEG0.PEGP.DSWR = TMP0 /* \HNOT.TMP0 */
                        Notify (\_SB.PCI0.GFX0, 0x81) // Information Change
                    }
                }
            }
            ElseIf ((TMP0 == 0x02))
            {
                If ((\_SB.PCI0.PEG0.PEGP.SGMD & 0x02))
                {
                    If ((\_SB.PCI0.PEG0.PEGP.AGXA == Zero))
                    {
                        Notify (\_SB.PCI0.GFX0, 0x80) // Status Change
                    }

                    If ((\_SB.PCI0.PEG0.PEGP.AGXA == One))
                    {
                        Notify (\_SB.PCI0.PEG0.PEGP, 0x80) // Status Change
                    }
                }
                Else
                {
                    Notify (\_SB.PCI0.GFX0, 0x80) // Status Change
                }
            }
            ElseIf ((TMP0 == 0x03))
            {
                If ((\_SB.PCI0.PEG0.PEGP.SGMD & 0x02))
                {
                    If ((\_SB.PCI0.PEG0.PEGP.AGXA == Zero))
                    {
                        Notify (\_SB.PCI0.GFX0, 0x80) // Status Change
                    }

                    If ((\_SB.PCI0.PEG0.PEGP.AGXA == One))
                    {
                        Notify (\_SB.PCI0.PEG0.PEGP, 0x80) // Status Change
                    }
                }
                Else
                {
                    Notify (\_SB.PCI0.GFX0, 0x80) // Status Change
                }
            }
            Else
            {
                Notify (\_SB.PCI0.GFX0, 0x80) // Status Change
            }

            Break
        }
    }

    Scope (\_SB.PCI0.GFX0)
    {
        Method (ATIF, 2, Serialized)
        {
            If ((Arg0 == Zero))
            {
                Name (TMP0, Buffer (0x0100)
                {
                     0x00                                             // .
                })
                CreateWordField (TMP0, Zero, F0SS)
                CreateWordField (TMP0, 0x02, F0IV)
                CreateDWordField (TMP0, 0x04, F0SN)
                CreateDWordField (TMP0, 0x08, F0SF)
                F0SS = 0x0C
                F0IV = One
                F0SN = 0x41
                F0SF = 0x07
                If (((\_SB.PCI0.PEG0.PEGP.SGMD & 0x0F) == 0x02))
                {
                    F0SN = Zero
                    F0SF = Zero
                }

                Return (TMP0) /* \_SB_.PCI0.GFX0.ATIF.TMP0 */
            }

            If ((Arg0 == One))
            {
                Name (TMP1, Buffer (0x0100)
                {
                     0x00                                             // .
                })
                CreateWordField (TMP1, Zero, F1SS)
                CreateDWordField (TMP1, 0x02, F1VF)
                CreateDWordField (TMP1, 0x06, F1FG)
                F1SS = 0x0A
                F1VF = 0x03
                F1FG = One
                Return (TMP1) /* \_SB_.PCI0.GFX0.ATIF.TMP1 */
            }

            If ((Arg0 == 0x02))
            {
                Name (TMP2, Buffer (0x0100)
                {
                     0x00                                             // .
                })
                CreateWordField (TMP2, Zero, F2SS)
                CreateDWordField (TMP2, 0x02, F2PR)
                CreateByteField (TMP2, 0x06, F2EM)
                CreateByteField (TMP2, 0x07, F2TG)
                CreateByteField (TMP2, 0x08, F2SI)
                CreateByteField (TMP2, 0x09, F2FG)
                CreateByteField (TMP2, 0x0A, F2FI)
                CreateByteField (TMP2, 0x0B, F2SP)
                F2SS = 0x0A
                Name (STS2, Zero)
                STS2 |= \_SB.PCI0.PEG0.PEGP.DSWR /* \_SB_.PCI0.GFX0.ATIF.STS2 */
                STS2 |= (\_SB.PCI0.PEG0.PEGP.PXGS << 0x06) /* \_SB_.PCI0.GFX0.ATIF.STS2 */
                F2PR = STS2 /* \_SB_.PCI0.GFX0.ATIF.STS2 */
                \_SB.PCI0.PEG0.PEGP.DSWR = Zero
                \_SB.PCI0.PEG0.PEGP.PXGS = Zero
                Return (TMP2) /* \_SB_.PCI0.GFX0.ATIF.TMP2 */
            }

            If ((Arg0 == 0x03))
            {
                Name (TMP3, Buffer (0x0100)
                {
                     0x00                                             // .
                })
                CreateWordField (TMP3, Zero, F3SS)
                CreateWordField (TMP3, 0x02, F3SD)
                CreateWordField (Arg1, Zero, AI3S)
                CreateWordField (Arg1, 0x02, SLDS)
                CreateWordField (Arg1, 0x04, CODS)
                \_SB.PCI0.PEG0.PEGP.CACD = SLDS /* \_SB_.PCI0.GFX0.ATIF.SLDS */
                \_SB.PCI0.PEG0.PEGP.CCND = CODS /* \_SB_.PCI0.GFX0.ATIF.CODS */
                If (\ECON)
                {
                    If ((\_SB.PCI0.LPCB.H_EC.LSTE == One))
                    {
                        \_SB.PCI0.PEG0.PEGP.CCND |= One
                    }
                }

                F3SS = 0x04
                \_SB.PCI0.PEG0.PEGP.TLSN = CTOI (\_SB.PCI0.PEG0.PEGP.CACD)
                Local1 = \_SB.PCI0.PEG0.PEGP.CACD
                Local0 = \_SB.PCI0.PEG0.PEGP.NTLE
                While (Local0)
                {
                    Local1 = NATL (\_SB.PCI0.PEG0.PEGP.TLSN)
                    If ((Local1 != Zero))
                    {
                        If (((Local1 & \_SB.PCI0.PEG0.PEGP.CCND) == Local1))
                        {
                            Local0 = One
                        }
                    }

                    Local0--
                    \_SB.PCI0.PEG0.PEGP.TLSN++
                    If ((\_SB.PCI0.PEG0.PEGP.TLSN > \_SB.PCI0.PEG0.PEGP.NTLE))
                    {
                        \_SB.PCI0.PEG0.PEGP.TLSN = One
                    }
                }

                SNXD (Local1)
                \_SB.PCI0.PEG0.PEGP.NACD = Local1
                F3SD = \_SB.PCI0.PEG0.PEGP.NACD
                Return (TMP3) /* \_SB_.PCI0.GFX0.ATIF.TMP3 */
            }

            Return (Zero)
        }

        Method (CTOI, 1, NotSerialized)
        {
            If ((\_SB.PCI0.PEG0.PEGP.NTLE != Zero))
            {
                If ((\_SB.PCI0.PEG0.PEGP.TLE1 == Arg0))
                {
                    Return (One)
                }

                If ((\_SB.PCI0.PEG0.PEGP.TLE2 == Arg0))
                {
                    Return (0x02)
                }

                If ((\_SB.PCI0.PEG0.PEGP.TLE3 == Arg0))
                {
                    Return (0x03)
                }

                If ((\_SB.PCI0.PEG0.PEGP.TLE4 == Arg0))
                {
                    Return (0x04)
                }

                If ((\_SB.PCI0.PEG0.PEGP.TLE5 == Arg0))
                {
                    Return (0x05)
                }

                If ((\_SB.PCI0.PEG0.PEGP.TLE6 == Arg0))
                {
                    Return (0x06)
                }

                If ((\_SB.PCI0.PEG0.PEGP.TLE7 == Arg0))
                {
                    Return (0x07)
                }

                If ((\_SB.PCI0.PEG0.PEGP.TLE8 == Arg0))
                {
                    Return (0x08)
                }

                If ((\_SB.PCI0.PEG0.PEGP.TLE9 == Arg0))
                {
                    Return (0x09)
                }

                If ((\_SB.PCI0.PEG0.PEGP.TL10 == Arg0))
                {
                    Return (0x0A)
                }

                If ((\_SB.PCI0.PEG0.PEGP.TL11 == Arg0))
                {
                    Return (0x0B)
                }

                If ((\_SB.PCI0.PEG0.PEGP.TL12 == Arg0))
                {
                    Return (0x0C)
                }

                If ((\_SB.PCI0.PEG0.PEGP.TL13 == Arg0))
                {
                    Return (0x0D)
                }

                If ((\_SB.PCI0.PEG0.PEGP.TL14 == Arg0))
                {
                    Return (0x0E)
                }

                If ((\_SB.PCI0.PEG0.PEGP.TL15 == Arg0))
                {
                    Return (0x0F)
                }
            }

            Return (Zero)
        }

        Method (NATL, 1, NotSerialized)
        {
            If ((\_SB.PCI0.PEG0.PEGP.NTLE != Zero))
            {
                If ((Arg0 == One))
                {
                    Return (\_SB.PCI0.PEG0.PEGP.TLE2)
                }

                If ((Arg0 == 0x02))
                {
                    Return (\_SB.PCI0.PEG0.PEGP.TLE3)
                }

                If ((Arg0 == 0x03))
                {
                    Return (\_SB.PCI0.PEG0.PEGP.TLE4)
                }

                If ((Arg0 == 0x04))
                {
                    Return (\_SB.PCI0.PEG0.PEGP.TLE5)
                }

                If ((Arg0 == 0x05))
                {
                    Return (\_SB.PCI0.PEG0.PEGP.TLE6)
                }

                If ((Arg0 == 0x06))
                {
                    Return (\_SB.PCI0.PEG0.PEGP.TLE7)
                }

                If ((Arg0 == 0x07))
                {
                    Return (\_SB.PCI0.PEG0.PEGP.TLE8)
                }

                If ((Arg0 == 0x08))
                {
                    Return (\_SB.PCI0.PEG0.PEGP.TLE9)
                }

                If ((Arg0 == 0x09))
                {
                    Return (\_SB.PCI0.PEG0.PEGP.TL10)
                }

                If ((Arg0 == 0x0A))
                {
                    Return (\_SB.PCI0.PEG0.PEGP.TL11)
                }

                If ((Arg0 == 0x0B))
                {
                    Return (\_SB.PCI0.PEG0.PEGP.TL12)
                }

                If ((Arg0 == 0x0C))
                {
                    Return (\_SB.PCI0.PEG0.PEGP.TL13)
                }

                If ((Arg0 == 0x0D))
                {
                    Return (\_SB.PCI0.PEG0.PEGP.TL14)
                }

                If ((Arg0 == 0x0E))
                {
                    Return (\_SB.PCI0.PEG0.PEGP.TL15)
                }

                If ((Arg0 == 0x0F))
                {
                    Return (\_SB.PCI0.PEG0.PEGP.TLE1)
                }
            }

            Return (Zero)
        }

        Method (INDL, 0, Serialized)
        {
            NXD1 = Zero
            NXD2 = Zero
            NXD3 = Zero
            NXD4 = Zero
            NXD5 = Zero
            NXD6 = Zero
            NXD7 = Zero
            NXD8 = Zero
        }

        Method (SNXD, 1, Serialized)
        {
            INDL ()
            Local0 = Arg0
            If ((Local0 & 0x02))
            {
                NXD1 = One
            }

            If ((Local0 & One))
            {
                NXD2 = One
            }

            If ((Local0 & 0x08))
            {
                NXD3 = One
            }

            If ((Local0 & 0x80))
            {
                NXD4 = One
            }

            If ((Local0 & 0x0200))
            {
                NXD5 = One
            }

            If ((Local0 & 0x0400))
            {
                NXD6 = One
            }

            If ((Local0 & 0x0800))
            {
                NXD7 = One
            }
        }
    }
}



