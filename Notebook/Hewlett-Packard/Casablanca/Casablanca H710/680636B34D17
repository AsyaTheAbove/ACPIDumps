            Method (GTBI, 1, NotSerialized)
            {
                Debug = "HP WMI Command 0x7 (BIOS Read)"
                If (Arg0)
                {
                    Local0 = Package (0x02)
                        {
                            0x06, 
                            Zero
                        }
                    Sleep (0x96)
                    Return (Local0)
                }

                If (^^PCI0.LPCB.EC.BATP)
                {
                    Local0 = Package (0x03)
                        {
                            Zero, 
                            0x80, 
                            Buffer (0x80){}
                        }
                    Local1 = ^^PCI0.LPCB.EC.BDC /* \_SB_.PCI0.LPCB.EC__.BDC_ */
                    Divide (Local1, 0x0100, Local2, DerefOf (Local0 [0x02]) [One])
                    DerefOf (Local0 [0x02]) [Zero] = Local2
                    Local1 = ^^PCI0.LPCB.EC.BFC /* \_SB_.PCI0.LPCB.EC__.BFC_ */
                    Divide (Local1, 0x0100, Local2, DerefOf (Local0 [0x02]) [0x03])
                    DerefOf (Local0 [0x02]) [0x02] = Local2
                    Local1 = ^^PCI0.LPCB.EC.BFC /* \_SB_.PCI0.LPCB.EC__.BFC_ */
                    Divide (Local1, 0x0100, Local2, DerefOf (Local0 [0x02]) [0x05])
                    DerefOf (Local0 [0x02]) [0x04] = Local2
                    Local1 = ^^PCI0.LPCB.EC.BTT /* \_SB_.PCI0.LPCB.EC__.BTT_ */
                    Local1 -= 0x0AAA
                    Divide (Local1, 0x0A, Local2, Local1)
                    Divide (Local1, 0x0100, Local2, DerefOf (Local0 [0x02]) [0x0B])
                    DerefOf (Local0 [0x02]) [0x0A] = Local2
                    Local1 = ^^PCI0.LPCB.EC.BPV /* \_SB_.PCI0.LPCB.EC__.BPV_ */
                    Divide (Local1, 0x0100, Local2, DerefOf (Local0 [0x02]) [0x0D])
                    DerefOf (Local0 [0x02]) [0x0C] = Local2
                    Local1 = ^^PCI0.LPCB.EC.BPR /* \_SB_.PCI0.LPCB.EC__.BPR_ */
                    Divide (Local1, 0x0100, Local2, DerefOf (Local0 [0x02]) [0x0F])
                    DerefOf (Local0 [0x02]) [0x0E] = Local2
                    Local1 = ^^PCI0.LPCB.EC.BDV /* \_SB_.PCI0.LPCB.EC__.BDV_ */
                    Divide (Local1, 0x0100, Local2, DerefOf (Local0 [0x02]) [0x11])
                    DerefOf (Local0 [0x02]) [0x10] = Local2
                    Local1 = ^^PCI0.LPCB.EC.BST /* \_SB_.PCI0.LPCB.EC__.BST_ */
                    Divide (Local1, 0x0100, Local2, DerefOf (Local0 [0x02]) [0x13])
                    DerefOf (Local0 [0x02]) [0x12] = Local2
                    Local1 = ^^PCI0.LPCB.EC.BC1 /* \_SB_.PCI0.LPCB.EC__.BC1_ */
                    Divide (Local1, 0x0100, Local2, DerefOf (Local0 [0x02]) [0x15])
                    DerefOf (Local0 [0x02]) [0x14] = Local2
                    Local1 = ^^PCI0.LPCB.EC.BC2 /* \_SB_.PCI0.LPCB.EC__.BC2_ */
                    Divide (Local1, 0x0100, Local2, DerefOf (Local0 [0x02]) [0x17])
                    DerefOf (Local0 [0x02]) [0x16] = Local2
                    Local1 = ^^PCI0.LPCB.EC.BC3 /* \_SB_.PCI0.LPCB.EC__.BC3_ */
                    Divide (Local1, 0x0100, Local2, DerefOf (Local0 [0x02]) [0x19])
                    DerefOf (Local0 [0x02]) [0x18] = Local2
                    Local1 = ^^PCI0.LPCB.EC.BC4 /* \_SB_.PCI0.LPCB.EC__.BC4_ */
                    Divide (Local1, 0x0100, Local2, DerefOf (Local0 [0x02]) [0x1B])
                    DerefOf (Local0 [0x02]) [0x1A] = Local2
                    Local1 = ^^BAT0.ITSN (ToBCD (^^PCI0.LPCB.EC.BSN), 0x05)
                    Concatenate (Local1, " ", Local4)
                    Local1 = ^^PCI0.LPCB.EC.BMD /* \_SB_.PCI0.LPCB.EC__.BMD_ */
                    Local2 = (Local1 & 0xFF)
                    Local3 = (Local1 & 0xFF00)
                    Local2 <<= 0x08
                    Local3 >>= 0x08
                    Local1 = (Local2 | Local3)
                    Local2 = (Local1 & 0x01E0)
                    Local2 >>= 0x05
                    Local2 = ^^BAT0.ITSN (ToBCD (Local2), 0x02)
                    Concatenate (Local4, Local2, Local3)
                    Concatenate (Local3, "/", Local4)
                    Local2 = (Local1 & 0x1F)
                    Local2 = ^^BAT0.ITSN (ToBCD (Local2), 0x02)
                    Concatenate (Local4, Local2, Local3)
                    Concatenate (Local3, "/", Local4)
                    Local2 = (Local1 & 0xFE00)
                    Local2 >>= 0x09
                    Local2 += 0x07BC
                    Local2 = ^^BAT0.ITSN (ToBCD (Local2), 0x04)
                    Concatenate (Local4, Local2, Local3)
                    Name (SNUM, Buffer (0x10){})
                    SNUM = Local3
                    Local3 = SNUM /* \_SB_.WMID.GTBI.SNUM */
                    Local2 = 0x1C
                    Local4 = Zero
                    Local1 = 0x10
                    While (Local1)
                    {
                        Local5 = DerefOf (Local3 [Local4])
                        ^^BAT0.PBFE (DerefOf (Local0 [0x02]), Local2, Local5)
                        Local1--
                        Local2++
                        Local4++
                    }

                    DerefOf (Local0 [0x02]) [Local2] = Zero
                    Local3 = Buffer (0x10)
                        {
                            "Hewlett-Packard"
                        }
                    Local3 = ^^PCI0.LPCB.EC.BTN /* \_SB_.PCI0.LPCB.EC__.BTN_ */
                    Local2 = 0x2C
                    Local4 = Zero
                    Local1 = 0x08
                    While (Local1)
                    {
                        Local5 = DerefOf (Local3 [Local4])
                        ^^BAT0.PBFE (DerefOf (Local0 [0x02]), Local2, Local5)
                        Local1--
                        Local2++
                        Local4++
                    }

                    DerefOf (Local0 [0x02]) [Local2] = Zero
                    Local3 = ^^PCI0.LPCB.EC.CTL /* \_SB_.PCI0.LPCB.EC__.CTL_ */
                    Local2 = 0x3E
                    Local4 = Zero
                    Local1 = 0x0F
                    While (Local1)
                    {
                        Local5 = DerefOf (Local3 [Local4])
                        DerefOf (Local0 [0x02]) [Local2] = Local5
                        Local1--
                        Local2++
                        Local4++
                    }

                    DerefOf (Local0 [0x02]) [Local2] = Zero
                    Local3 = ^^PCI0.LPCB.EC.BDNB /* \_SB_.PCI0.LPCB.EC__.BDNB */
                    Local2 = 0x4F
                    Local4 = Zero
                    Local1 = 0x07
                    While (Local1)
                    {
                        Local5 = DerefOf (Local3 [Local4])
                        DerefOf (Local0 [0x02]) [Local2] = Local5
                        Local1--
                        Local2++
                        Local4++
                    }

                    Local3 = Buffer (0x05)
                        {
                            "LION"
                        }
                    Local2 = 0x56
                    Local4 = Zero
                    Local1 = 0x04
                    While (Local1)
                    {
                        Local5 = DerefOf (Local3 [Local4])
                        DerefOf (Local0 [0x02]) [Local2] = Local5
                        Local1--
                        Local2++
                        Local4++
                    }

                    DerefOf (Local0 [0x02]) [0x5A] = Zero
                    DerefOf (Local0 [0x02]) [0x5B] = Zero
                    Local1 = ^^PCI0.LPCB.EC.BMD /* \_SB_.PCI0.LPCB.EC__.BMD_ */
                    Divide (Local1, 0x0100, Local2, DerefOf (Local0 [0x02]) [0x5D])
                    DerefOf (Local0 [0x02]) [0x5C] = Local2
                    Local1 = ^^PCI0.LPCB.EC.CHGC /* \_SB_.PCI0.LPCB.EC__.CHGC */
                    Divide (Local1, 0x0100, Local2, DerefOf (Local0 [0x02]) [0x5F])
                    DerefOf (Local0 [0x02]) [0x5E] = Local2
                    Local1 = ^^PCI0.LPCB.EC.CHGV /* \_SB_.PCI0.LPCB.EC__.CHGV */
                    Divide (Local1, 0x0100, Local2, DerefOf (Local0 [0x02]) [0x61])
                    DerefOf (Local0 [0x02]) [0x60] = Local2
                    Local1 = ^^PCI0.LPCB.EC.AVGC /* \_SB_.PCI0.LPCB.EC__.AVGC */
                    Divide (Local1, 0x0100, Local2, DerefOf (Local0 [0x02]) [0x63])
                    DerefOf (Local0 [0x02]) [0x62] = Local2
