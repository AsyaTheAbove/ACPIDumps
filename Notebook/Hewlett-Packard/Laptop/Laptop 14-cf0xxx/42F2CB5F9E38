            Method (GWDI, 0, NotSerialized)
            {
                Name (WBST, Package (0x01)
                {
                    0x1483
                })
                Name (WWST, Package (0x01)
                {
                    0xFFFF
                })
                Name (WMST, Package (0x01)
                {
                    0x1315
                })
                Name (WMDT, Package (0x01)
                {
                    0x0886
                })
                Local2 = Zero
                Local0 = 0x10
                Local6 = Buffer (0x10)
                    {
                        /* 0000 */  0x01, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00,  // ........
                        /* 0008 */  0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00   // ........
                    }
                CreateByteField (Local6, 0x02, GWL2)
                CreateByteField (Local6, 0x06, GWL6)
                CreateByteField (Local6, 0x07, GWL7)
                If ((((WLVD != 0xFFFF) || (WLDD != 0xFFFF)) && (
                    (WLVD != Zero) || (WLDD != Zero))))
                {
                    Local1 = Buffer (0x10)
                        {
                            /* 0000 */  0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00,  // ........
                            /* 0008 */  0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00   // ........
                        }
                    Local1 [0x02] = (WLVD & 0xFF)
                    Local1 [0x03] = ((WLVD >> 0x08) & 0xFF)
                    Local1 [0x04] = (WLDD & 0xFF)
                    Local1 [0x05] = ((WLDD >> 0x08) & 0xFF)
                    Local1 [0x06] = (WLSV & 0xFF)
                    Local1 [0x07] = ((WLSV >> 0x08) & 0xFF)
                    Local1 [0x08] = (WLSS & 0xFF)
                    Local1 [0x09] = ((WLSS >> 0x08) & 0xFF)
                    Local1 [0x0A] = Zero
                    If ((Match (WBST, MEQ, WLSS, MTR, Zero, Zero) != Ones))
                    {
                        Local1 [0x0A] = 0x05
                    }

                    If (((Match (WMST, MEQ, WLSS, MTR, Zero, Zero) != Ones) && 
                        (Match (WMDT, MEQ, WLDD, MTR, Zero, Zero) != Ones)))
                    {
                        Local1 [0x0A] = 0x08
                    }

                    Local4 = RDGO (WLRF)
                    Local4 |= (WLST << One)
                    Local4 |= 0x04
                    Local4 |= (BTLS << 0x03)
                    Local1 [0x0B] = Local4
                    If ((Local0 == 0x10))
                    {
                        Local2 = Local1
                    }
                    Else
                    {
                        Concatenate (Local2, Local1, Local3)
                        Local2 = Local3
                    }

                    Local0 += 0x10
                    GWL7++
                    GWL6++
                }

                If ((((BTVD != 0xFFFF) || (BTDD != 0xFFFF)) && (
                    (BTVD != Zero) || (BTDD != Zero))))
                {
                    Local1 = Buffer (0x10)
                        {
                            /* 0000 */  0x01, 0x01, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00,  // ........
                            /* 0008 */  0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00   // ........
                        }
                    Local1 [0x02] = (BTVD & 0xFF)
                    Local1 [0x03] = ((BTVD >> 0x08) & 0xFF)
                    Local1 [0x04] = (BTDD & 0xFF)
                    Local1 [0x05] = ((BTDD >> 0x08) & 0xFF)
                    Local1 [0x0A] = One
                    GWL6++
                    If ((Match (WBST, MEQ, WLSS, MTR, Zero, Zero) != Ones))
                    {
                        Local1 [0x0A] = 0x05
                        GWL6--
                    }

                    Local4 = BTON /* External reference */
                    Local4 |= (BLTH << One)
                    Local4 |= 0x04
                    Local4 |= (BTLS << 0x03)
                    Local1 [0x0B] = Local4
                    If ((Local0 == 0x10))
                    {
                        Local2 = Local1
                    }
                    Else
                    {
                        Concatenate (Local2, Local1, Local3)
                        Local2 = Local3
                    }

                    Local0 += 0x10
                    GWL7++
                }

                If ((((WWVD != 0xFFFF) || (WWDD != 0xFFFF)) && (
                    (WWVD != Zero) || (WWDD != Zero))))
                {
                    If (((WWVD == 0x03F0) && (WWDD == 0x241D)))
                    {
                        WWDD = 0x251D
                    }

                    Local1 = Buffer (0x10)
                        {
                            /* 0000 */  0x02, 0x01, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00,  // ........
                            /* 0008 */  0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00   // ........
                        }
                    Local1 [0x02] = (WWVD & 0xFF)
                    Local1 [0x03] = ((WWVD >> 0x08) & 0xFF)
                    Local1 [0x04] = (WWDD & 0xFF)
                    Local1 [0x05] = ((WWDD >> 0x08) & 0xFF)
                    Local1 [0x0A] = 0x02
                    GWL6++
                    If ((Match (WWST, MEQ, WLSS, MTR, Zero, Zero) != Ones))
                    {
                        Local1 [0x0A] = 0x07
                        GWL6--
                    }

                    Local4 = Local1 [0x0B]
                    Local5 = Local1 [0x0C]
                    Local4 = WAON /* External reference */
                    Local4 |= (WWAN << One)
                    Local4 |= 0x04
                    Local4 |= (BTLS << 0x03)
                    Local1 [0x0B] = Local4
                    If (\_SB.PCI0.LPCB.BTAP)
                    {
                        Local5 = (Local4 & 0xFE)
                        Local1 [0x0C] = Local5
                        If ((WWAN && BTLS))
                        {
                            Local4 &= 0x7F
                            Local5 &= 0xFE
                            Local1 [0x0B] = Local4
                            Local1 [0x0C] = Local5
                        }
                        Else
                        {
                            Local4 |= 0x80
                            Local1 [0x0B] = Local4
                            Local5 &= 0xFE
                            Local1 [0x0C] = Local5
                        }
                    }
                    Else
                    {
                        Local4 |= 0x80
                        Local5 |= One
                        Local1 [0x0B] = Local4
                        Local1 [0x0C] = Local5
                    }

                    If ((Local0 == 0x10))
                    {
                        Local2 = Local1
                    }
                    Else
                    {
                        Concatenate (Local2, Local1, Local3)
                        Local2 = Local3
                    }

                    Local0 += 0x10
                    GWL7++
                    GWL6++
                    GWL2 |= One
                }

                If ((((GPVD != 0xFFFF) || (GPDD != 0xFFFF)) && (
                    (GPVD != Zero) || (GPDD != Zero))))
                {
                    If (((GPVD == 0x03F0) && (GPDD == 0x241D)))
                    {
                        GPDD = 0x251D
                    }

                    Local1 = Buffer (0x10)
                        {
                            /* 0000 */  0x03, 0x01, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00,  // ........
                            /* 0008 */  0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00   // ........
                        }
                    Local1 [0x02] = (GPVD & 0xFF)
                    Local1 [0x03] = ((GPVD >> 0x08) & 0xFF)
                    Local1 [0x04] = (GPDD & 0xFF)
                    Local1 [0x05] = ((GPDD >> 0x08) & 0xFF)
                    Local1 [0x0A] = 0x02
                    Local4 = Local1 [0x0B]
                    Local5 = Local1 [0x0C]
                    Local4 = WAON /* External reference */
                    Local4 |= (WWAN << One)
                    Local4 |= 0x04
                    Local4 |= (BTLS << 0x03)
                    Local1 [0x0B] = Local4
                    If (\_SB.PCI0.LPCB.BTAP)
                    {
                        Local5 = (Local4 & 0xFE)
                        Local1 [0x0C] = Local5
                        If ((WWAN && BTLS))
                        {
                            Local4 &= 0x7F
                            Local5 &= 0xFE
                            Local1 [0x0B] = Local4
                            Local1 [0x0C] = Local5
                        }
                        Else
                        {
                            Local4 |= 0x80
                            Local1 [0x0B] = Local4
                            Local5 &= 0xFE
                            Local1 [0x0C] = Local5
                        }
                    }
                    Else
                    {
                        Local4 |= 0x80
                        Local5 |= One
                        Local1 [0x0B] = Local4
                        Local1 [0x0C] = Local5
                    }

                    If ((Local0 == 0x10))
                    {
                        Local2 = Local1
                    }
                    Else
                    {
                        Concatenate (Local2, Local1, Local3)
                        Local2 = Local3
                    }

                    Local0 += 0x10
                    GWL7++
                    GWL6++
                    GWL2 |= 0x02
                }

                If ((((WIVD != 0xFFFF) || (WIDD != 0xFFFF)) && (
                    (WIVD != Zero) || (WIDD != Zero))))
                {
                    Local1 = Buffer (0x10)
                        {
                            /* 0000 */  0x04, 0x01, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00,  // ........
                            /* 0008 */  0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00   // ........
                        }
                    Local1 [0x02] = (WIVD & 0xFF)
                    Local1 [0x03] = ((WIVD >> 0x08) & 0xFF)
                    Local1 [0x04] = (WIDD & 0xFF)
                    Local1 [0x05] = ((WIDD >> 0x08) & 0xFF)
                    Local1 [0x0A] = 0x04
                    GWL6++
                    If ((Match (WWST, MEQ, WLSS, MTR, Zero, Zero) != Ones))
                    {
                        Local1 [0x0A] = 0x08
                        GWL6--
                    }

                    If ((Local0 == 0x10))
                    {
                        Local2 = Local1
                    }
                    Else
                    {
                        Concatenate (Local2, Local1, Local3)
                        Local2 = Local3
                    }

                    Local0 += 0x10
                    GWL7++
                    GWL2 |= 0x02
                }

                If ((Local0 == 0x10))
                {
                    Local1 = Local6
                }
                Else
                {
                    Concatenate (Local6, Local2, Local1)
                }

                Local3 = Package (0x03){}
                Local3 [Zero] = Zero
                Local3 [One] = Local0
                Local3 [0x02] = Local1
                Return (Local3)
            }

            Method (GBCC, 0, NotSerialized)
            {
                Local0 = Package (0x03)
                    {
                        Zero, 
                        0x04, 
                        Buffer (0x04){}
                    }
                Local1 = \_SB.PCI0.LPCB.BST1 /* External reference */
                If (((\_SB.PCI0.LPCB.RPWR == One) && (\_SB.PCI0.LPCB.BTAP == One)))
                {
                    RTCC = Zero
                    Local2 = (Local1 & One)
                    Local3 = (Local1 & 0x80)
                    Local1 &= 0x02
                    Local1 |= (Local2 << 0x02)
                    Local1 |= (Local3 >> 0x07)
                    If ((Local3 == Zero))
                    {
                        Local1 = One
                    }
                    Else
                    {
                        Local1 &= 0x06
                    }

                    DerefOf (Local0 [0x02]) [Zero] = Local1
                    DerefOf (Local0 [0x02]) [One] = 0xFF
                }
                Else
                {
                    RTCC = 0x35
                }

                Return (Local0)
            }

            Method (GWLS, 0, NotSerialized)
            {
                Name (GWLS, Buffer (0x04)
                {
                     0x00, 0x00, 0x00, 0x00                           // ....
                })
                CreateByteField (GWLS, Zero, GWL0)
                CreateByteField (GWLS, One, GWL1)
                CreateByteField (GWLS, 0x02, GWL2)
                CreateByteField (GWLS, 0x03, GWL3)
                P80B = 0x52
                GWL0 = 0x20
                If ((((WLVD != 0xFFFF) || (WLDD != 0xFFFF)) && (
                    (WLVD != Zero) || (WLDD != Zero))))
                {
                    GWL0 |= One
                    GWL1 = RDGO (WLRF)
                    GWL1 |= (WLST << One) /* \_SB_.WMID.GWLS.GWL1 */
                    GWL1 |= 0x04 /* \_SB_.WMID.GWLS.GWL1 */
                    GWL1 |= (BTLS << 0x03) /* \_SB_.WMID.GWLS.GWL1 */
                    P80B = 0x54
                }

                If ((((BTVD != 0xFFFF) || (BTDD != 0xFFFF)) && (
                    (BTVD != Zero) || (BTDD != Zero))))
                {
                    GWL0 |= 0x02
                    GWL2 = BTON /* External reference */
                    GWL2 |= (BLTH << One) /* \_SB_.WMID.GWLS.GWL2 */
                    GWL2 |= 0x04 /* \_SB_.WMID.GWLS.GWL2 */
                    GWL2 |= (BTLS << 0x03) /* \_SB_.WMID.GWLS.GWL2 */
                    P80B = 0x55
                }

                If ((((WWVD != 0xFFFF) || (WWDD != 0xFFFF)) && (
                    (WWVD != Zero) || (WWDD != Zero))))
                {
                    GWL0 |= 0x6C
                    GWL3 = WWRF /* External reference */
                    GWL3 |= (WWAN << One) /* \_SB_.WMID.GWLS.GWL3 */
                    GWL3 |= 0x04 /* \_SB_.WMID.GWLS.GWL3 */
                    GWL3 |= (BTLS << 0x03) /* \_SB_.WMID.GWLS.GWL3 */
                }

                Local0 = Package (0x03)
                    {
                        Zero, 
                        0x04, 
                        Buffer (0x04)
                        {
                             0x01, 0x02, 0x03, 0x04                           // ....
                        }
                    }
                DerefOf (Local0 [0x02]) [Zero] = GWL0 /* \_SB_.WMID.GWLS.GWL0 */
                DerefOf (Local0 [0x02]) [One] = GWL1 /* \_SB_.WMID.GWLS.GWL1 */
                DerefOf (Local0 [0x02]) [0x02] = GWL2 /* \_SB_.WMID.GWLS.GWL2 */
                DerefOf (Local0 [0x02]) [0x03] = GWL3 /* \_SB_.WMID.GWLS.GWL3 */
                Return (Local0)
            }

            Method (GBIN, 1, NotSerialized)
            {
                If ((Arg0 < SizeOf (NBTI)))
                {
                    If ((Arg0 == Zero))
                    {
                        If (((\_SB.PCI0.LPCB.BTAP & One) == One))
                        {
                            RTCC = Zero
                            Return (\_SB.PCI0.LPCB.H_EC.GBTI (Arg0))
                        }
                        Else
                        {
                            RTCC = 0x34
                            Return (\_SB.PCI0.LPCB.H_EC.GBTI (Arg0))
                        }
                    }
                    ElseIf ((Arg0 == One))
                    {
                        If (((\_SB.PCI0.LPCB.BTAP & 0x02) == 0x02))
                        {
                            RTCC = Zero
                            Return (\_SB.PCI0.LPCB.H_EC.GBTI (Arg0))
                        }
                        Else
                        {
                            RTCC = 0x34
                            Return (\_SB.PCI0.LPCB.H_EC.GBTI (Arg0))
                        }
                    }
                    Else
                    {
                        RTCC = 0x34
                        Return (\_SB.PCI0.LPCB.H_EC.GBTI (Arg0))
                    }
                }
                Else
                {
                    Return (Package (0x02)
                    {
                        0x04, 
                        Zero
                    })
                }
            }

            Method (GBBH, 0, NotSerialized)
            {
                P80B = 0x18
                Debug = "Retrun Beze function table"
                Local0 = 0x14
                Local1 = Buffer (0x0A)
                    {
                        /* 0000 */  0x31, 0x01, 0xAE, 0x01, 0x9B, 0x01, 0xB6, 0x01,  // 1.......
                        /* 0008 */  0xB7, 0x01                                       // ..
                    }
                Concatenate (Local1, Buffer (0x0A)
                    {
                        /* 0000 */  0xFF, 0xFF, 0x9A, 0x21, 0x9B, 0x21, 0x9C, 0x21,  // ...!.!.!
                        /* 0008 */  0x00, 0x00                                       // ..
                    }, Local2)
                Local3 = Package (0x03){}
                Local3 [Zero] = Zero
                Local3 [One] = Local0
                Local3 [0x02] = Local2
                Return (Local3)
            }

            Method (GAOW, 0, NotSerialized)
            {
