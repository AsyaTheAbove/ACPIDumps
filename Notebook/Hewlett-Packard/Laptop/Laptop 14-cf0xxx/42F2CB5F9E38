            Method (STCS, 1, NotSerialized)
            {
                \_SB.PCI0.LPCB.MMST = ((Arg0 >> 0x08) & 0xFF)
                CMST = ((Arg0 >> 0x08) & 0xFF)
                \_SB.PCI0.LPCB.DMST = ((Arg0 >> 0x10) & 0xFF)
                CDST = ((Arg0 >> 0x10) & 0xFF)
                Local2 = Package (0x02)
                    {
                        Zero, 
                        Zero
                    }
                Return (Local2)
            }

            Method (GBCO, 0, NotSerialized)
            {
                Sleep (0x03E8)
                Local0 = Package (0x03)
                    {
                        Zero, 
                        0x04, 
                        Buffer (0x04){}
                    }
                Local2 = \_SB.PCI0.LPCB.BST0 /* External reference */
                If ((\_SB.PCI0.LPCB.BDNC == Zero))
                {
                    RTCC = Zero
                    If ((\_SB.PCI0.LPCB.BTAP == One))
                    {
                        Local1 = Zero
                        If ((Local2 & 0x02))
                        {
                            If ((Local2 & 0x08))
                            {
                                Local1 = 0x03
                            }
                            Else
                            {
                                Local1 = One
                            }
                        }

                        If ((Local2 & One))
                        {
                            Local1 = 0x02
                        }

                        If ((Local2 & 0x10))
                        {
                            Local1 = 0x04
                        }

                        If ((Local2 & 0x40))
                        {
                            Local1 = 0x05
                        }

                        If ((Local2 & 0x20))
                        {
                            Local1 = 0x06
                        }
                    }
                    Else
                    {
                        Local1 = 0xFF
                    }

                    DerefOf (Local0 [0x02]) [Zero] = Local1
                    DerefOf (Local0 [0x02]) [One] = 0xFF
                    DerefOf (Local0 [0x02]) [0x02] = One
                }
                Else
                {
                    RTCC = 0x37
                }

                Return (Local0)
            }

            Method (SBCO, 1, Serialized)
            {
                P80B = 0x2B
                If ((\_SB.PCI0.LPCB.BDNC == Zero))
                {
                    RTCC = Zero
                    If (((Arg0 & 0xFF) == Zero))
                    {
                        Switch ((Arg0 & 0xFF00))
                        {
                            Case (Zero)
                            {
                                BATD = Zero
                                BATC = 0x3A
                            }
                            Case (0x0100)
                            {
                                BATD = One
                                BATC = 0x3A
                            }
                            Case (0x0200)
                            {
                                \_SB.PCI0.LPCB.RPWR &= 0xFE
                                BATD = 0x02
                                BATC = 0x3A
                            }
                            Case (0x0300)
                            {
                                BATD = 0x03
                                BATC = 0x3A
                            }
                            Case (0x0400)
                            {
                                BATD = 0x04
                                BATC = 0x3A
                            }
                            Case (0x0500)
                            {
                                BATD = 0x05
                                BATC = 0x3A
                            }
                            Case (0x0600)
                            {
                                BATD = 0x55
                                BATC = 0x3C
                            }
                            Case (0x0700)
                            {
                                BATD = 0x44
                                BATC = 0x3C
                            }

                        }
                    }
                }
                Else
                {
                    RTCC = 0x37
                }

                Local2 = Package (0x02)
                    {
                        Zero, 
                        Zero
                    }
                Return (Local2)
            }

            Method (GHKS, 0, NotSerialized)
            {
                P80B = 0x19
                Local0 = Package (0x03)
                    {
                        Zero, 
                        0x04, 
                        Buffer (0x04){}
                    }
                DerefOf (Local0 [0x02]) [Zero] = 0x6E
                Return (Local0)
            }

            Method (SHKS, 1, NotSerialized)
            {
                P80B = 0x92
                \_SB.PCI0.LPCB.H_EC.SSHK (Arg0)
                Notify (\_SB.WMID, 0x80) // Status Change
                Return (Package (0x02)
                {
                    Zero, 
                    Zero
                })
            }

            Method (HKFR, 0, Serialized)
            {
                Local0 = Package (0x03)
                    {
                        Zero, 
                        0x04, 
                        Buffer (0x04){}
                    }
                Local1 = \HKNO /* External reference */
                Switch (Local1)
                {
                    Case (0x0D)
                    {
                        DerefOf (Local0 [0x02]) [Zero] = 0x31
                        DerefOf (Local0 [0x02]) [One] = One
                    }
                    Case (One)
                    {
                        DerefOf (Local0 [0x02]) [Zero] = 0x9B
                        DerefOf (Local0 [0x02]) [One] = One
                    }
                    Case (0x06)
                    {
                        DerefOf (Local0 [0x02]) [Zero] = 0x9D
                        DerefOf (Local0 [0x02]) [One] = One
                    }
                    Case (0x07)
                    {
                        DerefOf (Local0 [0x02]) [Zero] = 0xB6
                        DerefOf (Local0 [0x02]) [One] = One
                        Local3 = BRTL /* External reference */
                        Local3 *= 0x0A
                        DerefOf (Local0 [0x02]) [0x02] = Local3
                        DerefOf (Local0 [0x02]) [0x03] = 0x64
                    }
                    Case (0x08)
                    {
                        DerefOf (Local0 [0x02]) [Zero] = 0xB7
                        DerefOf (Local0 [0x02]) [One] = One
                        Local3 = BRTL /* External reference */
                        Local3 *= 0x0A
                        DerefOf (Local0 [0x02]) [0x02] = Local3
                        DerefOf (Local0 [0x02]) [0x03] = 0x64
                    }
                    Case (0x0B)
                    {
                        DerefOf (Local0 [0x02]) [Zero] = 0x9C
                        DerefOf (Local0 [0x02]) [One] = 0x21
                    }

                }

                \HKNO = Zero
                Return (Local0)
            }

            Method (GHKF, 0, NotSerialized)
            {
                P80B = 0x1A
                Return (HKFR ())
            }

            Method (SHKF, 1, Serialized)
            {
                P80B = 0x2A
                Switch (Arg0)
                {
                    Case (0x01AE)
                    {
                        \HKNO = 0x04
                    }
                    Case (0x0131)
                    {
                        \HKNO = 0x0D
                    }
                    Case (0x019D)
                    {
                        \HKNO = 0x06
                    }
                    Case (0x01B6)
                    {
                        \HKNO = 0x07
                    }
                    Case (0x01B7)
                    {
                        \HKNO = 0x08
                    }
                    Case (0x019B)
                    {
                        \HKNO = 0x08
                    }
                    Default
                    {
                        \HKNO = Zero
                    }

                }

                Return (HKFR ())
            }

            Method (GBBV, 0, Serialized)
            {
                P80B = 0x1C
                Local0 = Package (0x03)
                    {
                        Zero, 
                        0x04, 
                        Buffer (0x04){}
                    }
                Local1 = \HKNO /* External reference */
                Switch (Local1)
                {
                    Case (0x16)
                    {
                        DerefOf (Local0 [0x02]) [Zero] = 0x42
                        DerefOf (Local0 [0x02]) [One] = 0x21
                    }
                    Case (0xFF)
                    {
                        DerefOf (Local0 [0x02]) [Zero] = 0xE6
                        DerefOf (Local0 [0x02]) [One] = 0x20
                    }
                    Case (Package (0x03)
                        {
                            0x18, 
                            0x1A, 
                            0x19
                        }

)
                    {
                        DerefOf (Local0 [0x02]) [Zero] = 0x90
                        DerefOf (Local0 [0x02]) [One] = 0x21
                        If (Zero)
                        {
                            DerefOf (Local0 [0x02]) [0x02] = 0x02
                        }
                        Else
                        {
                            DerefOf (Local0 [0x02]) [0x02] = Zero
                        }
                    }
                    Case (Package (0x02)
                        {
                            0x1D, 
                            0x1B
                        }

)
                    {
                        DerefOf (Local0 [0x02]) [Zero] = 0x92
                        DerefOf (Local0 [0x02]) [One] = 0x21
                        If (Zero)
                        {
                            DerefOf (Local0 [0x02]) [0x02] = 0x02
                        }
                        Else
                        {
                            DerefOf (Local0 [0x02]) [0x02] = Zero
                        }
                    }
                    Case (0x1E)
                    {
                        DerefOf (Local0 [0x02]) [Zero] = 0x93
                        DerefOf (Local0 [0x02]) [One] = 0x21
                        If (Zero)
                        {
                            DerefOf (Local0 [0x02]) [0x02] = 0x02
                        }
                        Else
                        {
                            DerefOf (Local0 [0x02]) [0x02] = Zero
                        }
                    }
                    Case (0x1C)
                    {
                        DerefOf (Local0 [0x02]) [Zero] = 0x91
                        DerefOf (Local0 [0x02]) [One] = 0x21
                        If (Zero)
                        {
                            DerefOf (Local0 [0x02]) [0x02] = 0x02
                        }
                        Else
                        {
                            DerefOf (Local0 [0x02]) [0x02] = Zero
                        }
                    }
                    Case (0x78)
                    {
                        DerefOf (Local0 [0x02]) [Zero] = 0xA0
                        DerefOf (Local0 [0x02]) [One] = 0x21
                    }
                    Case (0x0B)
                    {
                        DerefOf (Local0 [0x02]) [Zero] = 0x9C
                        DerefOf (Local0 [0x02]) [One] = 0x21
                        \HKNO = Zero
                    }

                }

                Return (Local0)
            }

            Method (GSAS, 0, NotSerialized)
