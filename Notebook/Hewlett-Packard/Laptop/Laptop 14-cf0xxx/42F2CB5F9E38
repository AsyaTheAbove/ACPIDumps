SSDT15
------
DefinitionBlock ("", "SSDT", 2, "HPQOEM", "INSYDE  ", 0x00001000)
{
    External (_PR_.PR00, UnknownObj)
    External (_PR_.PR01, UnknownObj)
    External (_PR_.PR02, UnknownObj)
    External (_PR_.PR03, UnknownObj)
    External (_PR_.PR04, UnknownObj)
    External (_PR_.PR05, UnknownObj)
    External (_PR_.PR06, UnknownObj)
    External (_PR_.PR07, UnknownObj)
    External (_PR_.PR08, UnknownObj)
    External (_PR_.PR09, UnknownObj)
    External (_PR_.PR10, UnknownObj)
    External (_PR_.PR11, UnknownObj)
    External (_PR_.PR12, UnknownObj)
    External (_PR_.PR13, UnknownObj)
    External (_PR_.PR14, UnknownObj)
    External (_PR_.PR15, UnknownObj)
    External (_SB_.PCI0.LPCB.H_EC.CRT_, UnknownObj)
    External (_SB_.PCI0.LPCB.H_EC.DTMP, UnknownObj)
    External (_SB_.PCI0.LPCB.H_EC.ECRG, UnknownObj)
    External (_SB_.PCI0.LPCB.H_EC.FANE, UnknownObj)
    External (_SB_.PCI0.LPCB.H_EC.FANR, UnknownObj)
    External (_SB_.PCI0.LPCB.H_EC.HOTF, UnknownObj)
    External (_SB_.PCI0.LPCB.H_EC.LTMP, UnknownObj)
    External (_SB_.PCI0.LPCB.H_EC.PSV_, UnknownObj)
    External (_SB_.PCI0.LPCB.H_EC.SCP_, UnknownObj)
    External (_SB_.PCI0.LPCB.H_EC.VTMP, UnknownObj)
    External (CTYP, IntObj)
    External (TCNT, IntObj)
    External (VFN0, IntObj)

    Mutex (ECMM, 0x00)
    Scope (\_TZ)
    {
        PowerResource (FN00, 0x00, 0x0000)
        {
            Method (_STA, 0, Serialized)  // _STA: Status
            {
                Return (One)
            }

            Method (_ON, 0, Serialized)  // _ON_: Power On
            {
                VFN0 = One
            }

            Method (_OFF, 0, Serialized)  // _OFF: Power Off
            {
                VFN0 = Zero
            }
        }

        Device (FAN0)
        {
            Name (_HID, EisaId ("PNP0C0B") /* Fan (Thermal Solution) */)  // _HID: Hardware ID
            Name (_UID, Zero)  // _UID: Unique ID
            Name (_PR0, Package (0x01)  // _PR0: Power Resources for D0
            {
                FN00
            })
        }

        ThermalZone (TZ00)
        {
            Name (FMIN, Zero)
            Name (PTMP, 0x0BB8)
            Method (_SCP, 1, Serialized)  // _SCP: Set Cooling Policy
            {
                CTYP = Arg0
                \_SB.PCI0.LPCB.H_EC.SCP = Arg0
            }

            Name (HOTP, Zero)
            Name (HOTT, 0x6D)
            Method (HOTI, 0, Serialized)
            {
                HOTP = Zero
            }

            Method (_TMP, 0, Serialized)  // _TMP: Temperature
            {
                Acquire (ECMM, 0xFFFF)
                If (\_SB.PCI0.LPCB.H_EC.ECRG)
                {
                    Local0 = \_SB.PCI0.LPCB.H_EC.DTMP /* External reference */
                    If ((HOTP == One))
                    {
                        Local0 = HOTT /* \_TZ_.TZ00.HOTT */
                        Local0 -= One
                    }

                    If ((Local0 >= HOTT))
                    {
                        HOTP = One
                    }

                    If ((Local0 > 0x78))
                    {
                        Local0 = 0x32
                    }

                    Release (ECMM)
                    Return ((0x0AAC + (Local0 * 0x0A)))
                }

                Release (ECMM)
                Return (PTMP) /* \_TZ_.TZ00.PTMP */
            }

            Method (_AC0, 0, Serialized)  // _ACx: Active Cooling, x=0-9
            {
                Local0 = 0x46
                Return ((0x0AAC + (Local0 * 0x0A)))
            }

            Name (_AL0, Package (0x01)  // _ALx: Active List, x=0-9
            {
                FAN0
            })
            Method (XPSV, 0, Serialized)
            {
                Acquire (ECMM, 0xFFFF)
                If (\_SB.PCI0.LPCB.H_EC.ECRG)
                {
                    Local0 = \_SB.PCI0.LPCB.H_EC.PSV /* External reference */
                    Release (ECMM)
                    Return ((0x0AAC + (Local0 * 0x0A)))
                }

                Release (ECMM)
                Return (0x0FA2)
            }

            Method (XPSL, 0, Serialized)
            {
                If ((\TCNT == 0x10))
                {
                    Return (Package (0x10)
                    {
                        \_PR.PR00, 
                        \_PR.PR01, 
                        \_PR.PR02, 
                        \_PR.PR03, 
                        \_PR.PR04, 
                        \_PR.PR05, 
                        \_PR.PR06, 
                        \_PR.PR07, 
                        \_PR.PR08, 
                        \_PR.PR09, 
                        \_PR.PR10, 
                        \_PR.PR11, 
                        \_PR.PR12, 
                        \_PR.PR13, 
                        \_PR.PR14, 
                        \_PR.PR15
                    })
                }

                If ((\TCNT == 0x0E))
                {
                    Return (Package (0x0E)
