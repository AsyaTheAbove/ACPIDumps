            Name (BINC, Zero)
            Method (BINI, 0, NotSerialized)
            {
                Local4 = Buffer (0x28){}
                Local4 = ^^^GFX0.BCLM /* External reference */
                CreateByteField (Local4, Zero, BCL0)
                CreateByteField (Local4, 0x02, BCL1)
                CreateByteField (Local4, 0x04, BCL2)
                CreateByteField (Local4, 0x06, BCL3)
                CreateByteField (Local4, 0x08, BCL4)
                CreateByteField (Local4, 0x0A, BCL5)
                CreateByteField (Local4, 0x0C, BCL6)
                CreateByteField (Local4, 0x0E, BCL7)
                CreateByteField (Local4, 0x10, BCL8)
                CreateByteField (Local4, 0x12, BCL9)
                CreateByteField (Local4, 0x14, BCLA)
                If ((BINC == Zero))
                {
                    Local3 = BRTB ()
                    Local1 = Zero
                    Local0 = DerefOf (Local3 [Local1])
                    BCL0 = Local0
                    ^^^GFX0.BCLP = Local0
                    Local1++
                    Local0 = DerefOf (Local3 [Local1])
                    BCL1 = Local0
                    ^^^GFX0.BCLP = Local0
                    Local1++
                    Local0 = DerefOf (Local3 [Local1])
                    BCL2 = Local0
                    ^^^GFX0.BCLP = Local0
                    Local1++
                    Local0 = DerefOf (Local3 [Local1])
                    BCL3 = Local0
                    ^^^GFX0.BCLP = Local0
                    Local1++
                    Local0 = DerefOf (Local3 [Local1])
                    BCL4 = Local0
                    ^^^GFX0.BCLP = Local0
                    Local1++
                    Local0 = DerefOf (Local3 [Local1])
                    BCL5 = Local0
                    ^^^GFX0.BCLP = Local0
                    Local1++
                    Local0 = DerefOf (Local3 [Local1])
                    BCL6 = Local0
                    ^^^GFX0.BCLP = Local0
                    Local1++
                    Local0 = DerefOf (Local3 [Local1])
                    BCL7 = Local0
                    ^^^GFX0.BCLP = Local0
                    Local1++
                    Local0 = DerefOf (Local3 [Local1])
                    BCL8 = Local0
                    ^^^GFX0.BCLP = Local0
                    Local1++
                    Local0 = DerefOf (Local3 [Local1])
                    BCL9 = Local0
                    ^^^GFX0.BCLP = Local0
                    Local1++
                    Local0 = DerefOf (Local3 [Local1])
                    BCLA = Local0
                    ^^^GFX0.BCLP = Local0
                    BINC = One
                    ^^^GFX0.BCLM = Local4
                }
            }

            Method (OBCL, 0, NotSerialized)
