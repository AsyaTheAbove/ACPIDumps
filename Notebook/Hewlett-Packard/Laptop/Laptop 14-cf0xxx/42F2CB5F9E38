            Mutex (GC6M, 0x00)
            Method (ECNV, 1, NotSerialized)
            {
                Acquire (GC6M, 0xFFFF)
                If ((Arg0 == Zero))
                {
                    EC62 = Zero
                }

                If ((Arg0 == One))
                {
                    EC62 = One
                    Sleep (0x14)
                }

                Release (GC6M)
            }

            Method (KFCL, 2, NotSerialized)
            {
                If (ECRG)
                {
                    ^^FAON = Arg1
                    ^^FANS = Arg0
                }
            }

            Method (KSFS, 1, NotSerialized)
            {
                If (ECRG)
                {
                    ^^FANS = Arg0
                }
            }

            Method (KGFS, 0, NotSerialized)
            {
                Local0 = 0x14
                If (ECRG)
                {
                    Local0 = ^^FANS /* \_SB_.PCI0.LPCB.FANS */
                }

                Return (Local0)
            }

            Method (SSHK, 1, Serialized)
            {
                Acquire (ECMX, 0xFFFF)
                If (ECRG)
                {
                    P8XH (Zero, Arg0)
                    If ((SHTS == One))
                    {
                        ^^^^WMID.WGWE (0x0B, One)
                    }
                    Else
                    {
                        ^^^^WMID.WGWE (0x0B, Zero)
                    }

                    Sleep (0x64)
                    If ((MTST == Zero))
                    {
                        ^^^^WMID.WGWE (0x0A, Zero)
                    }
                    Else
                    {
                        ^^^^WMID.WGWE (0x0A, One)
                    }
                }

                Release (ECMX)
            }

            Method (GLID, 0, Serialized)
            {
                If (ECRG)
                {
                    Local0 = ^^LSTE /* \_SB_.PCI0.LPCB.LSTE */
                }
                Else
                {
                    Local0 = LIDS /* External reference */
                }

                Return (Local0)
            }

            Name (NGBF, 0xFF)
            Name (NGBT, 0xFF)
            Name (GACP, 0x07)
            Name (ACST, One)
            Name (SMAR, Zero)
            Name (NBAP, Zero)
            Name (NNBO, One)
            Name (NDCB, Zero)
            Name (NLB1, 0xC8)
            Name (NLB2, 0x64)
            Mutex (BTMX, 0x00)
            Name (NBTT, Package (0x08)
            {
                "Unknown", 
                "NiMH", 
                "LiIon"
            })
            Method (UPAD, 0, Serialized)
            {
                Acquire (BTMX, 0xFFFF)
                If ((GACP && One))
                {
                    GACP &= 0x06
                    Release (BTMX)
                    Local0 = One
                    Local1 = Zero
                    If (ECRG)
                    {
                        Local0 = ^^RPWR /* \_SB_.PCI0.LPCB.RPWR */
                        Local1 = ^^ADID /* \_SB_.PCI0.LPCB.ADID */
                    }

                    ACST = Local0
                    SMAR = Local1
                }
                Else
                {
                    Release (BTMX)
                }
            }

            Method (GACS, 0, Serialized)
            {
                UPAD ()
                Return (ACST) /* \_SB_.PCI0.LPCB.H_EC.ACST */
            }

            Method (GPID, 0, Serialized)
            {
                UPAD ()
                Return (SMAR) /* \_SB_.PCI0.LPCB.H_EC.SMAR */
            }

            Method (GBAP, 0, Serialized)
            {
                Acquire (BTMX, 0xFFFF)
                If ((GACP & 0x02))
                {
                    GACP &= 0x05
                    Release (BTMX)
                    If (ECRG)
                    {
                        NBAP = ^^BTAP /* \_SB_.PCI0.LPCB.BTAP */
                    }
                }
                Else
                {
                    Release (BTMX)
                }

                Return (NBAP) /* \_SB_.PCI0.LPCB.H_EC.NBAP */
            }

            Method (PWUP, 2, Serialized)
            {
                Local0 = Zero
                Acquire (BTMX, 0xFFFF)
                Local1 = (Arg0 | GACP) /* \_SB_.PCI0.LPCB.H_EC.GACP */
                GACP = (Local1 & 0x07)
                If ((GACP & 0x02))
                {
                    NGBF |= Arg1
                }

                If ((GACP & 0x04))
                {
                    If ((NGBT != 0xFF))
                    {
                        Local0 = One
                    }

                    NGBT |= Arg1
                }

                Release (BTMX)
                Return (Local0)
            }

            Method (BTDR, 1, Serialized)
            {
                If ((Arg0 == One))
                {
                    NNBO = One
                }
                ElseIf ((Arg0 == Zero))
                {
                    NNBO = Zero
                }

                Return (NNBO) /* \_SB_.PCI0.LPCB.H_EC.NNBO */
            }

            Method (BSTA, 1, Serialized)
            {
                BTDR (One)
                Local0 = GBAP ()
                Local1 = 0x0F
                If ((Local0 & Arg0))
                {
                    Local1 = 0x1F
                }

                Return (Local1)
            }

            Method (GBSS, 2, Serialized)
            {
                ToBCD (Arg0, Local0)
                Local3 = ISTR (Local0, 0x05)
                Concatenate (Local3, " ", Local4)
                Local0 = (Arg1 >> 0x05)
                Local1 = (Local0 & 0x0F)
                ToBCD (Local1, Local0)
                Local2 = ISTR (Local0, 0x02)
                Concatenate (Local4, Local2, Local3)
                Concatenate (Local3, "/", Local4)
                Local1 = (Arg1 & 0x1F)
                ToBCD (Local1, Local0)
                Local2 = ISTR (Local0, 0x02)
                Concatenate (Local4, Local2, Local3)
                Concatenate (Local3, "/", Local4)
                Local0 = (Arg1 >> 0x09)
                Local1 = (Local0 + 0x07BC)
                ToBCD (Local1, Local0)
                Local2 = ISTR (Local0, 0x04)
                Concatenate (Local4, Local2, Local3)
                Return (Local3)
            }

            Method (BTIF, 1, Serialized)
            {
                Local7 = (One << Arg0)
                BTDR (One)
                If ((BSTA (Local7) == 0x0F))
                {
                    Return (0xFF)
                }

                Acquire (BTMX, 0xFFFF)
                Local0 = NGBF /* \_SB_.PCI0.LPCB.H_EC.NGBF */
                Release (BTMX)
                If (((Local0 & Local7) == Zero))
                {
                    Return (Zero)
                }

                NBST [Arg0] = NDBS /* \_SB_.NDBS */
                Acquire (BTMX, 0xFFFF)
                NGBT |= Local7
                Release (BTMX)
                If (ECRG)
                {
                    ^^BSEL = Arg0
                    Switch (Arg0)
                    {
                        Case (Zero)
                        {
                            Local0 = ^^BFC0 /* \_SB_.PCI0.LPCB.BFC0 */
                            DerefOf (NBTI [Arg0]) [One] = Local0
                            DerefOf (NBTI [Arg0]) [0x02] = Local0
                            DerefOf (NBTI [Arg0]) [0x04] = ^^BDV0 /* \_SB_.PCI0.LPCB.BDV0 */
                        }
                        Case (One)
                        {
                            Local0 = ^^BFC1 /* \_SB_.PCI0.LPCB.BFC1 */
                            DerefOf (NBTI [Arg0]) [One] = Local0
                            DerefOf (NBTI [Arg0]) [0x02] = Local0
                            DerefOf (NBTI [Arg0]) [0x04] = ^^BDV1 /* \_SB_.PCI0.LPCB.BDV1 */
                        }

                    }

                    Local1 = (Local0 * NLB1) /* \_SB_.PCI0.LPCB.H_EC.NLB1 */
                    Divide ((Local1 + 0x63), 0x64, Local3, Local2)
                    DerefOf (NBTI [Arg0]) [0x05] = Local2
                    DerefOf (NBTI [Arg0]) [0x05] = NLB1 /* \_SB_.PCI0.LPCB.H_EC.NLB1 */
                    DerefOf (NBTI [Arg0]) [0x06] = NLB2 /* \_SB_.PCI0.LPCB.H_EC.NLB2 */
                    Switch (Arg0)
                    {
                        Case (Zero)
                        {
                            Local0 = ^^BSN0 /* \_SB_.PCI0.LPCB.BSN0 */
                            Local1 = ^^BMD0 /* \_SB_.PCI0.LPCB.BMD0 */
                        }
                        Case (One)
                        {
                            Local0 = ^^BSN1 /* \_SB_.PCI0.LPCB.BSN1 */
                            Local1 = ^^BMD1 /* \_SB_.PCI0.LPCB.BMD1 */
                        }

                    }
                }

                Local2 = GBSS (Local0, Local1)
                DerefOf (NBTI [Arg0]) [0x0A] = Local2
                Acquire (BTMX, 0xFFFF)
                NGBF &= ~Local7
                Release (BTMX)
                Return (Zero)
            }

            Method (BTST, 2, Serialized)
            {
                Local0 = ^^BPR0 /* \_SB_.PCI0.LPCB.BPR0 */
                If ((Local0 & 0x8000))
                {
                    If ((Local0 == 0xFFFF))
                    {
                        DerefOf (NBST [Arg0]) [One] = 0xFFFFFFFF
                    }
                    Else
                    {
                        Local1 = ~Local0
                        Local1++
                        Local3 = (Local1 & 0xFFFF)
                        DerefOf (NBST [Arg0]) [One] = Local3
                    }
                }
                Else
                {
                    DerefOf (NBST [Arg0]) [One] = Local0
                }

                DerefOf (NBST [Arg0]) [0x02] = ^^BRC0 /* \_SB_.PCI0.LPCB.BRC0 */
                DerefOf (NBST [Arg0]) [0x03] = ^^BPV0 /* \_SB_.PCI0.LPCB.BPV0 */
                Local2 = ^^BST0 /* \_SB_.PCI0.LPCB.BST0 */
                DerefOf (NBST [Arg0]) [Zero] = Local2
                If ((Local2 & 0x04))
                {
                    QQ70 = One
                }

                If ((^^^^WMID.BRTE == Zero))
                {
                    Local3 = 0xFFFFFFFF
                    DerefOf (NBST [Arg0]) [One] = Local3
                }

                Return (Zero)
            }

            Method (ITLB, 0, NotSerialized)
            {
                If (ECRG)
                {
                    Local1 = (^^BFC1 + ^^BFC0) /* \_SB_.PCI0.LPCB.BFC0 */
                    Divide ((Local1 + 0x63), 0x64, Local2, Local3)
                    NLB1 = (Local3 * ^^LB1) /* \_SB_.PCI0.LPCB.LB1_ */
                    NLB2 = (Local3 * ^^LB2) /* \_SB_.PCI0.LPCB.LB2_ */
                }
