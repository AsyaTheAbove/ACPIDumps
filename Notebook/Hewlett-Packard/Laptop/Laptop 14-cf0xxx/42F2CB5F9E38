            Method (GBTI, 1, NotSerialized)
            {
                If (ECRG)
                {
                    If ((^^BTAP & (One << Arg0)))
                    {
                        ^^BSEL = Arg0
                        Local0 = Package (0x03)
                            {
                                Zero, 
                                0x80, 
                                Buffer (0x80){}
                            }
                        DerefOf (Local0 [0x02]) [Zero] = ^^BDC0 /* \_SB_.PCI0.LPCB.BDC0 */
                        DerefOf (Local0 [0x02]) [One] = (^^BDC0 >> 0x08
                            )
                        DerefOf (Local0 [0x02]) [0x02] = ^^BFC0 /* \_SB_.PCI0.LPCB.BFC0 */
                        DerefOf (Local0 [0x02]) [0x03] = (^^BFC0 >> 0x08
                            )
                        DerefOf (Local0 [0x02]) [0x04] = ^^BRC0 /* \_SB_.PCI0.LPCB.BRC0 */
                        DerefOf (Local0 [0x02]) [0x05] = (^^BRC0 >> 0x08
                            )
                        DerefOf (Local0 [0x02]) [0x08] = ^^BCC0 /* \_SB_.PCI0.LPCB.BCC0 */
                        DerefOf (Local0 [0x02]) [0x09] = (^^BCC0 >> 0x08
                            )
                        Local1 = ^^CBT0 /* \_SB_.PCI0.LPCB.CBT0 */
                        Local1 -= 0x0AAC
                        Divide (Local1, 0x0A, Local2, Local3)
                        DerefOf (Local0 [0x02]) [0x0A] = Local3
                        DerefOf (Local0 [0x02]) [0x0B] = (Local3 >> 0x08
                            )
                        DerefOf (Local0 [0x02]) [0x0C] = ^^BPV0 /* \_SB_.PCI0.LPCB.BPV0 */
                        DerefOf (Local0 [0x02]) [0x0D] = (^^BPV0 >> 0x08
                            )
                        Local1 = ^^BPR0 /* \_SB_.PCI0.LPCB.BPR0 */
                        If (Local1)
                        {
                            If ((^^BST0 & One))
                            {
                                Local1 &= 0xFFFF
                            }
                        }

                        DerefOf (Local0 [0x02]) [0x0E] = Local1
                        DerefOf (Local0 [0x02]) [0x0F] = (Local1 >> 0x08
                            )
                        DerefOf (Local0 [0x02]) [0x10] = ^^BDV0 /* \_SB_.PCI0.LPCB.BDV0 */
                        DerefOf (Local0 [0x02]) [0x11] = (^^BDV0 >> 0x08
                            )
                        DerefOf (Local0 [0x02]) [0x12] = ^^BST0 /* \_SB_.PCI0.LPCB.BST0 */
                        DerefOf (Local0 [0x02]) [0x13] = (^^BST0 >> 0x08
                            )
                        DerefOf (Local0 [0x02]) [0x14] = ^^CV01 /* \_SB_.PCI0.LPCB.CV01 */
                        DerefOf (Local0 [0x02]) [0x15] = (^^CV01 >> 0x08
                            )
                        DerefOf (Local0 [0x02]) [0x16] = ^^CV02 /* \_SB_.PCI0.LPCB.CV02 */
                        DerefOf (Local0 [0x02]) [0x17] = (^^CV02 >> 0x08
                            )
                        DerefOf (Local0 [0x02]) [0x18] = ^^CV03 /* \_SB_.PCI0.LPCB.CV03 */
                        DerefOf (Local0 [0x02]) [0x19] = (^^CV03 >> 0x08
                            )
                        DerefOf (Local0 [0x02]) [0x1A] = ^^CV04 /* \_SB_.PCI0.LPCB.CV04 */
                        DerefOf (Local0 [0x02]) [0x1B] = (^^CV04 >> 0x08
                            )
                        CreateField (DerefOf (Local0 [0x02]), 0xE0, 0x80, BTSN)
                        BTSN = GBSS (^^BSN0, ^^BMD0)
                        Local1 = ^^BMN0 /* \_SB_.PCI0.LPCB.BMN0 */
                        CreateField (DerefOf (Local0 [0x02]), 0x0160, (SizeOf (Local1) * 0x08), 
                            BMAN)
                        BMAN = Local1
                        CreateField (DerefOf (Local0 [0x02]), 0x01F0, 0x80, CTN)
                        CTN = ^^BCT0 /* \_SB_.PCI0.LPCB.BCT0 */
                        CreateField (DerefOf (Local0 [0x02]), 0x0278, 0x38, BDN)
                        BDN = ^^BDN0 /* \_SB_.PCI0.LPCB.BDN0 */
                        CreateField (DerefOf (Local0 [0x02]), 0x02B0, 0x20, BTY)
                        BTY = ^^BTY0 /* \_SB_.PCI0.LPCB.BTY0 */
                        CreateField (DerefOf (Local0 [0x02]), 0x02E0, 0x10, BMD)
                        BMD = ^^BMD0 /* \_SB_.PCI0.LPCB.BMD0 */
                        CreateField (DerefOf (Local0 [0x02]), 0x02F0, 0x10, CRG)
                        If ((^^BST0 & One))
                        {
                            CRG = Zero
                        }
                        Else
                        {
                            CRG = ^^CRG0 /* \_SB_.PCI0.LPCB.CRG0 */
                        }

                        CreateField (DerefOf (Local0 [0x02]), 0x0300, 0x10, BCV)
                        BCV = ^^BCV0 /* \_SB_.PCI0.LPCB.BCV0 */
                        Local1 = ^^BPR0 /* \_SB_.PCI0.LPCB.BPR0 */
                        If (Local1)
                        {
                            If ((^^BST0 & One))
                            {
                                Local1 = (~Local1 + One)
                                Local1 &= 0xFFFF
                            }
                        }

                        CreateField (DerefOf (Local0 [0x02]), 0x0310, 0x10, CURT)
                        CURT = Local1
                        CreateField (DerefOf (Local0 [0x02]), 0x0350, 0x08, BNUM)
                        BNUM = One
                    }
                }
                Else
                {
                    Local0 = Package (0x02)
                        {
                            0x0D, 
                            Zero
                        }
                }

                Return (Local0)
            }

