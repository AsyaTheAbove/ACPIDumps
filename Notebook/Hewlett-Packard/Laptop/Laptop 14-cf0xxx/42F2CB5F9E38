            Method (SWLS, 1, NotSerialized)
            {
                Name (PWCN, 0xFF)
                PWCN = ((Arg0 & 0x00FF0000) >> 0x10)
                Name (RQDS, 0xFF)
                RQDS = ((Arg0 & 0xFF000000) >> 0x18)
                If ((PWCN == One))
                {
                    If (BTPE)
                    {
                        BLTH = RQDS /* \_SB_.WMID.SWLS.RQDS */
                        \_SB.SGOV (EDBT, RQDS)
                        BTON = RQDS /* \_SB_.WMID.SWLS.RQDS */
                    }
                    Else
                    {
                        BTON = Zero
                        BLTH = Zero
                        \_SB.SGOV (EDBT, Zero)
                    }
                }

                If ((PWCN == Zero))
                {
                    If (WDSP)
                    {
                        WTGP (WLRF, RQDS)
                        WARF = RQDS /* \_SB_.WMID.SWLS.RQDS */
                        WLON = RQDS /* \_SB_.WMID.SWLS.RQDS */
                        WLST = RQDS /* \_SB_.WMID.SWLS.RQDS */
                    }
                    Else
                    {
                        WTGP (WLRF, Zero)
                        WARF = Zero
                        WLON = Zero
                        WLST = Zero
                    }
                }

                If ((PWCN == 0x02))
                {
                    WWAN = RQDS /* \_SB_.WMID.SWLS.RQDS */
                    If ((\_SB.PCI0.LPCB.BTAP == One))
                    {
                        WWOF = !RQDS
                        WAON = RQDS /* \_SB_.WMID.SWLS.RQDS */
                        Sleep (0x32)
                        WWRF = RQDS /* \_SB_.WMID.SWLS.RQDS */
                    }
                }

                If ((PWCN == 0x03)){}
                If ((PWCN == 0x04))
                {
                    WTGP (WLRF, RQDS)
                    WARF = RQDS /* \_SB_.WMID.SWLS.RQDS */
                    WLON = RQDS /* \_SB_.WMID.SWLS.RQDS */
                    WLST = RQDS /* \_SB_.WMID.SWLS.RQDS */
                }

                If ((PWCN == 0x05))
                {
                    If (WDSP)
                    {
                        WTGP (WLRF, RQDS)
                        WARF = RQDS /* \_SB_.WMID.SWLS.RQDS */
                        WLON = RQDS /* \_SB_.WMID.SWLS.RQDS */
                        WLST = RQDS /* \_SB_.WMID.SWLS.RQDS */
                    }
                    Else
                    {
                        WTGP (WLRF, Zero)
                        WARF = Zero
                        WLON = Zero
                        WLST = Zero
                    }

                    If (BTPE)
                    {
                        BTON = RQDS /* \_SB_.WMID.SWLS.RQDS */
                        BLTH = RQDS /* \_SB_.WMID.SWLS.RQDS */
                        \_SB.SGOV (EDBT, RQDS)
                    }
                    Else
                    {
                        BTON = Zero
                        BLTH = Zero
                        \_SB.SGOV (EDBT, Zero)
                    }
                }

                If ((PWCN == 0x08))
                {
                    BLTH = RQDS /* \_SB_.WMID.SWLS.RQDS */
                    \_SB.SGOV (EDWL, RQDS)
                    WARF = RQDS /* \_SB_.WMID.SWLS.RQDS */
                    WLON = RQDS /* \_SB_.WMID.SWLS.RQDS */
                    \_SB.SGOV (EDBT, RQDS)
                    WLST = RQDS /* \_SB_.WMID.SWLS.RQDS */
                }

                If ((PWCN == 0x09)){}
                If ((PWCN == 0x07))
                {
                    \_SB.SGOV (EDWL, RQDS)
                    WARF = RQDS /* \_SB_.WMID.SWLS.RQDS */
                    WLON = RQDS /* \_SB_.WMID.SWLS.RQDS */
                    \_SB.SGOV (EDBT, RQDS)
                    WLST = RQDS /* \_SB_.WMID.SWLS.RQDS */
                    WWOF = !RQDS
                    WAON = RQDS /* \_SB_.WMID.SWLS.RQDS */
                    WWAN = RQDS /* \_SB_.WMID.SWLS.RQDS */
                    Sleep (0x32)
                    WWRF = RQDS /* \_SB_.WMID.SWLS.RQDS */
                }

                If (((PWCN == 0xFE) || (PWCN == 0xFF)))
                {
                    If (WDSP)
                    {
                        \_SB.SGOV (EDWL, RQDS)
                        WARF = RQDS /* \_SB_.WMID.SWLS.RQDS */
                        WLON = RQDS /* \_SB_.WMID.SWLS.RQDS */
                        WLST = RQDS /* \_SB_.WMID.SWLS.RQDS */
                    }
                    Else
                    {
                        \_SB.SGOV (EDWL, Zero)
                        WARF = Zero
                        WLON = Zero
                        WLST = Zero
                    }

                    If (BTPE)
                    {
                        BTON = RQDS /* \_SB_.WMID.SWLS.RQDS */
                        BLTH = RQDS /* \_SB_.WMID.SWLS.RQDS */
                        \_SB.SGOV (EDBT, RQDS)
                    }
                    Else
                    {
                        BTON = Zero
                        BLTH = Zero
                        \_SB.SGOV (EDBT, Zero)
                    }
                }

                Local2 = Package (0x02)
                    {
                        Zero, 
                        Zero
                    }
                \_SB.WMID.WGWE (0x05, Zero)
                Return (Local2)
            }

            Method (SBCC, 1, Serialized)
            {
                Acquire (\_SB.PCI0.LPCB.H_EC.ECMX, 0xFFFF)
                Switch ((Arg0 & 0xFF))
                {
                    Case (Zero)
                    {
                        RTCC = Zero
                        Switch ((Arg0 & 0xFF00))
                        {
                            Case (0x0100)
                            {
                                BATD = 0x10
                                BATC = 0x3D
                            }
                            Case (0x0200)
                            {
                                BATD = 0x10
                                BATC = 0x3D
                            }
                            Case (0x0400)
                            {
                                BATD = 0x02
                                BATC = 0x3D
                            }

                        }
                    }
                    Case (One)
                    {
                        RTCC = 0x34
                    }
                    Case (0x63)
                    {
                        RTCC = 0x03
                    }

                }

                Local2 = Package (0x02)
                    {
                        Zero, 
                        Zero
                    }
                Release (\_SB.PCI0.LPCB.H_EC.ECMX)
                Return (Local2)
            }

