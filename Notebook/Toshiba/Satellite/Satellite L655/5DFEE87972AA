                    OperationRegion (DCP0, SystemIO, 0x6C, One)
                    Field (DCP0, ByteAcc, NoLock, Preserve)
                    {
                        O6CB,   8
                    }

                    OperationRegion (DCP1, SystemIO, 0x68, One)
                    Field (DCP1, ByteAcc, NoLock, Preserve)
                    {
                        D68B,   8
                    }

                    Method (W4IE, 0, NotSerialized)
                    {
                        While ((O6CB & 0x02))
                        {
                            Stall (One)
                        }
                    }

                    Method (W4OF, 0, NotSerialized)
                    {
                        While (!(O6CB & One))
                        {
                            Stall (One)
                        }
                    }

                    Method (EC4R, 1, NotSerialized)
                    {
                        W4IE ()
                        O6CB = Arg0
                        W4OF ()
                        Local0 = D68B /* \_SB_.PCI0.LPCB.EC0_.D68B */
                        Return (Local0)
                    }

                    Method (EC4W, 1, NotSerialized)
                    {
                        W4IE ()
                        O6CB = Arg0
                        W4IE ()
                    }

                    Mutex (MUT1, 0x00)
                    Mutex (MUT0, 0x00)
                    Method (APOL, 1, NotSerialized)
                    {
                        DBPL = Arg0
                        EBPL = One
                    }

                    Name (PSTA, Zero)
                    Method (CPOL, 1, NotSerialized)
                    {
                        If ((PSTA == Zero))
                        {
                            If ((ECON != Zero))
                            {
                                APOL (Arg0)
                                PSTA = One
                            }
                        }
                    }

                    Method (_Q20, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        P80H = 0x20
                        If (ECON)
                        {
                            If ((SMST & 0x40))
                            {
                                Local0 = SMAA /* \_SB_.PCI0.LPCB.EC0_.SMAA */
                                If ((Local0 == 0x14))
                                {
                                    SMST &= 0xBF
                                    Local1 = PWRE /* \_SB_.PCI0.LPCB.EC0_.PWRE */
                                    If (Local1)
                                    {
                                        PWRE = Zero
                                        BFLG = 0x12
                                        If (ECON)
                                        {
                                            ^^^^BAT1.BSTA ()
                                            Notify (ACAD, 0x80) // Status Change
                                            Notify (BAT1, 0x80) // Status Change
                                            If (^^^^BAT1.BTCH)
                                            {
                                                ^^^^BAT1.UBIF ()
                                                Notify (BAT1, 0x81) // Information Change
                                                ^^^^BAT1.BTCH = Zero
                                            }
                                        }
                                    }
                                }

                                If ((Local0 == 0x16))
                                {
                                    SMST &= 0xBF
                                    ^^^^BAT1.BCRI = 0x04
                                    Notify (BAT1, 0x80) // Status Change
                                }
                                Else
                                {
                                    ^^^^BAT1.BCRI = Zero
                                }
                            }
                        }
                    }

                    Method (_Q09, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        P80H = 0x09
                        If (ECON)
                        {
                            PSTA = Zero
                            ^^^^BAT1.BSTA ()
                            Notify (ACAD, 0x80) // Status Change
                            Sleep (0x01F4)
                            PNOT ()
                            Notify (BAT1, 0x80) // Status Change
                            If (^^^^BAT1.BTCH)
                            {
                                ^^^^BAT1.UBIF ()
                                Notify (BAT1, 0x81) // Information Change
                                ^^^^BAT1.BTCH = Zero
                            }
                        }
                    }

                    Method (_Q9B, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        P80H = 0x9B
                        LDSS = Zero
                        Notify (LID, 0x80) // Status Change
                    }

                    Method (_Q9C, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        P80H = 0x9C
                        LDSS = One
                        Notify (LID, 0x80) // Status Change
                    }

                    Method (_QA0, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        P80H = 0xA0
                        FSMI (0x5A, Zero)
                    }

                    Method (_QA1, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        P80H = 0xA1
                        FSMI (0x5B, Zero)
                    }

                    Method (_Q0E, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        If (MID1)
                        {
                            ^^^GFX0.GHDS (Zero)
                        }
                        Else
                        {
                            ^^^P0P2.VGA.DRUL ()
                        }
                    }

                    Method (_Q0F, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        If (((OSYS > 0x07D0) && (OSYS < 0x07D6)))
                        {
                            Local0 = BLVL /* \_SB_.PCI0.LPCB.EC0_.BLVL */
                            If ((Local0 < Zero))
                            {
                                Local0 = Zero
                                BLVL = Zero
                            }

                            If ((Local0 == Zero))
                            {
                                Local1 = PL00 /* \PL00 */
                            }
                            ElseIf ((Local0 == One))
                            {
                                Local1 = PL01 /* \PL01 */
                            }
                            ElseIf ((Local0 == 0x02))
                            {
                                Local1 = PL02 /* \PL02 */
                            }
                            ElseIf ((Local0 == 0x03))
                            {
                                Local1 = PL03 /* \PL03 */
                            }
                            ElseIf ((Local0 == 0x04))
                            {
                                Local1 = PL04 /* \PL04 */
                            }
                            ElseIf ((Local0 == 0x05))
                            {
                                Local1 = PL05 /* \PL05 */
                            }
                            ElseIf ((Local0 == 0x06))
                            {
                                Local1 = PL06 /* \PL06 */
                            }
                            ElseIf ((Local0 == 0x07))
                            {
                                Local1 = PL07 /* \PL07 */
                            }

                            If (MID1)
                            {
                                ^^^GFX0.AINT (One, Local1)
                            }
                            Else
                            {
                                OWNS = Local1
                                FSMI (0x22, One)
                            }
                        }
                        ElseIf (MID1)
                        {
                            Notify (^^^GFX0.DD02, 0x87) // Device-Specific
                        }
                        Else
                        {
                            Notify (^^^P0P2.VGA.LCD, 0x87) // Device-Specific
                        }
                    }

                    Method (_Q10, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        If (((OSYS > 0x07D0) && (OSYS < 0x07D6)))
                        {
                            Local0 = BLVL /* \_SB_.PCI0.LPCB.EC0_.BLVL */
                            If ((Local0 > 0x07))
                            {
                                Local0 = 0x07
                                BLVL = 0x07
                            }

                            If ((Local0 == Zero))
                            {
                                Local1 = PL00 /* \PL00 */
                            }
                            ElseIf ((Local0 == One))
                            {
                                Local1 = PL01 /* \PL01 */
                            }
                            ElseIf ((Local0 == 0x02))
                            {
                                Local1 = PL02 /* \PL02 */
                            }
                            ElseIf ((Local0 == 0x03))
                            {
                                Local1 = PL03 /* \PL03 */
                            }
                            ElseIf ((Local0 == 0x04))
                            {
                                Local1 = PL04 /* \PL04 */
                            }
                            ElseIf ((Local0 == 0x05))
                            {
                                Local1 = PL05 /* \PL05 */
                            }
                            ElseIf ((Local0 == 0x06))
                            {
                                Local1 = PL06 /* \PL06 */
                            }
                            ElseIf ((Local0 == 0x07))
                            {
                                Local1 = PL07 /* \PL07 */
                            }

                            If (MID1)
                            {
                                ^^^GFX0.AINT (One, Local1)
                            }
                            Else
                            {
                                OWNS = Local1
                                FSMI (0x22, One)
                            }
                        }
                        ElseIf (MID1)
                        {
                            Notify (^^^GFX0.DD02, 0x86) // Device-Specific
                        }
                        Else
                        {
                            Notify (^^^P0P2.VGA.LCD, 0x86) // Device-Specific
                        }
                    }

                    Method (_Q11, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        P80H = 0x11
                    }

                    Method (_Q12, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        P80H = 0x12
                    }

                    Method (_Q8D, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        P80H = 0x8D
                    }

                    Method (_Q8E, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        P80H = 0x8E
                    }

                    Method (_Q88, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        INP4 = One
                        P80H = 0x8D
                        Sleep (0x64)
                        Notify (\_PR.CPU0, 0x80) // Performance Capability Change
                        Sleep (0x64)
                        Local0 = \_PR.CPU0._PPC /* External reference */
                        P80H = Local0
                    }

                    Method (_Q89, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        INP4 = Zero
                        P80H = 0x8E
                        Sleep (0x64)
                        If ((ISTM == Zero))
                        {
                            Notify (\_PR.CPU0, 0x80) // Performance Capability Change
                            Sleep (0x64)
                        }

                        Local0 = \_PR.CPU0._PPC /* External reference */
                        P80H = Local0
                    }

                    Method (_Q76, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        If ((^^^^TVAP.VZOK >= One))
                        {
                            P80H = 0x76
                            ^^^^TVAP.EVNT (0xC0)
                        }
