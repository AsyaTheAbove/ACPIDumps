                        Sleep (0x05)
                        ^^^^TVAP.EVNT (0x03)
                        Sleep (0x05)
                        ^^^^TVAP.EVNT (0x04)
                    }

                    Method (_QB0, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        Local0 = PECL /* \_SB_.PCI0.LPCB.EC0_.PECL */
                        If (Local0)
                        {
                            P80H = 0xB2
                            ^^^^TVAP.EVNT (0xB2)
                        }
                    }

                    Method (_QB1, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
