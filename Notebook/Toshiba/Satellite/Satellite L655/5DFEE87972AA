            Method (SPFC, 6, NotSerialized)
            {
                VALI [Zero] = Arg0
                VALI [One] = Arg1
                VALI [0x02] = Arg2
                VALI [0x03] = Arg3
                VALI [0x04] = Arg4
                VALI [0x05] = Arg5
                Local0 = Arg0
                Local1 = Arg1
                Local2 = Arg2
                Local3 = Arg3
                Local4 = Arg4
                Local5 = Arg5
                VALO [Zero] = Local0
                VALO [One] = Local1
                VALO [0x02] = Local2
                VALO [0x03] = Local3
                VALO [0x04] = Local4
                VALO [0x05] = Local5
                Return (VALO) /* \_SB_.QITR.VALO */
