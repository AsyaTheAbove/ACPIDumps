            Device (FRES)
            {
                Name (_ADR, Zero)  // _ADR: Address
                Method (_PRW, 0, NotSerialized)  // _PRW: Power Resources for Wake
                {
                    If ((MD04 && MD09))
                    {
                        Return (Package (0x02)
                        {
                            0x09, 
                            0x03
                        })
                    }
                    Else
                    {
                        Return (Package (0x02)
                        {
                            0x09, 
                            Zero
                        })
                    }
                }

                Method (_PSW, 1, NotSerialized)  // _PSW: Power State Wake
                {
                    If ((MD04 && MD09))
                    {
                        If (Arg0)
                        {
                            ^^^LPCB.EC0.USEP = One
                            If (OG03)
                            {
                                ^^^LPCB.EC0.USE2 = Zero
                            }
                            Else
                            {
                                ^^^LPCB.EC0.USE2 = One
                            }
                        }
                    }
                }
            }
        }

