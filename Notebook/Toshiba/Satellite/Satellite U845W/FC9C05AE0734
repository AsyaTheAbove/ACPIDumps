                    Method (APOL, 1, NotSerialized)
                    {
                        DBPL = Arg0
                        EBPL = One
                    }

                    Method (_Q20, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        P80H = 0x20
                        If (ECON)
                        {
                            If ((SMST & 0x40))
                            {
                                Local0 = SMAA /* \_SB_.PCI0.LPCB.EC0_.SMAA */
                                If ((Local0 == 0x14))
                                {
                                    SMST &= 0xBF
                                    Local1 = PWRE /* \_SB_.PCI0.LPCB.EC0_.PWRE */
                                    If (Local1)
                                    {
                                        PWRE = Zero
                                        If (ECON)
                                        {
                                            ^^^^BAT1.BSTA ()
                                            Notify (ACAD, 0x80) // Status Change
                                            Sleep (0x01F4)
                                            PNOT ()
                                            Notify (BAT1, 0x80) // Status Change
                                            If (^^^^BAT1.BTCH)
                                            {
                                                ^^^^BAT1.UBIF ()
                                                Notify (BAT1, 0x81) // Information Change
                                                ^^^^BAT1.BTCH = Zero
                                            }
                                        }
                                    }
                                }

                                If ((Local0 == 0x16))
                                {
                                    SMST &= 0xBF
                                    ^^^^BAT1.BCRI = 0x04
                                    Notify (BAT1, 0x80) // Status Change
                                }
                                Else
                                {
                                    ^^^^BAT1.BCRI = Zero
                                }
                            }
                        }
                    }

                    Method (_Q08, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        If (FKSF)
                        {
                            Local0 = (FKCS | 0x10)
                            OSMI (0x22, Local0)
                        }
                    }

                    Method (_Q09, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        P80H = 0x09
                        If (ECON)
                        {
                            ^^^^BAT1.BSTA ()
                            Notify (ACAD, 0x80) // Status Change
                            Notify (BAT1, 0x80) // Status Change
                            If (^^^^BAT1.BTCH)
                            {
                                ^^^^BAT1.UBIF ()
                                Notify (BAT1, 0x81) // Information Change
                                ^^^^BAT1.BTCH = Zero
                            }
                        }
                    }

                    Method (_Q9B, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        P80H = 0x9B
                        Notify (TVAP, 0x8F) // Device-Specific
                        Notify (LID, 0x80) // Status Change
                    }

                    Method (_Q9C, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        P80H = 0x9C
                        Notify (LID, 0x80) // Status Change
