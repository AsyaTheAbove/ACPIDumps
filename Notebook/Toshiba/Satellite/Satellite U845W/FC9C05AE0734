            Method (RQ1C, 1, NotSerialized)
            {
                Q000 = 0x08
                Q001 = ^^PCI0.LPCB.EC0.BLVL /* \_SB_.PCI0.LPCB.EC0_.BLVL */
                Q002 = One
            }

            Method (RS1C, 2, NotSerialized)
            {
                Q000 = Arg1
                ^^PCI0.LPCB.EC0.BLVL = Q000 /* \_SB_.QWMI.Q000 */
                If ((^^PCI0.LPCB.EC0.BLVL == Zero))
                {
                    Q000 = PL00 /* \PL00 */
                }
                ElseIf ((^^PCI0.LPCB.EC0.BLVL == One))
                {
                    Q000 = PL01 /* \PL01 */
                }
                ElseIf ((^^PCI0.LPCB.EC0.BLVL == 0x02))
                {
                    Q000 = PL02 /* \PL02 */
                }
                ElseIf ((^^PCI0.LPCB.EC0.BLVL == 0x03))
                {
                    Q000 = PL03 /* \PL03 */
                }
                ElseIf ((^^PCI0.LPCB.EC0.BLVL == 0x04))
                {
                    Q000 = PL04 /* \PL04 */
                }
                ElseIf ((^^PCI0.LPCB.EC0.BLVL == 0x05))
                {
                    Q000 = PL05 /* \PL05 */
                }
                ElseIf ((^^PCI0.LPCB.EC0.BLVL == 0x06))
                {
                    Q000 = PL06 /* \PL06 */
                }
                ElseIf ((^^PCI0.LPCB.EC0.BLVL == 0x07))
                {
                    Q000 = PL07 /* \PL07 */
                }
            }

            Method (RQ1D, 1, NotSerialized)
