                {
                    _PSC = 0x02
                }

                Method (_PS3, 0, NotSerialized)  // _PS3: Power State 3
                {
                    _PSC = 0x03
                }

                Name (NDSP, Zero)
                Name (TGLT, Package (0x06)
                {
                    Package (0x0A)
                    {
                        One, 
                        One, 
