                    {
                        INP4 = One
                        Sleep (0x64)
                        \_PR.CPU0._PCT ()
                        Notify (\_PR.CPU0, 0x80) // Performance Capability Change
                        Sleep (0x64)
                        P80H = 0x8D
                    }

                    Method (_Q8E, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        INP4 = Zero
                        Sleep (0x64)
                        If ((CPUD == Zero))
                        {
                            \_PR.CPU0._PCT ()
                            Notify (\_PR.CPU0, 0x80) // Performance Capability Change
                            Sleep (0x64)
                        }

                        P80H = 0x8E
                    }

                    Method (_Q8F, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
