                RPM2,   16
            }

            OperationRegion (ECF3, EmbeddedControl, 0x0100, 0x0100)
            Field (ECF3, ByteAcc, Lock, Preserve)
            {
                Offset (0xA1), 
                PRCN,   8, 
                PROH,   8, 
                PRST,   8, 
                AMPH,   8, 
                AMPF,   8, 
                AMPR,   16, 
                SOAI,   8, 
                Offset (0xAB), 
                BCFD,   8, 
                RBCF,   8, 
                Offset (0xD0), 
                SO20,   8, 
                ABC1,   8, 
                ABC2,   8, 
                ABST,   8, 
                NVC1,   8, 
                NVC2,   8, 
                NVST,   8, 
                WBTC,   8, 
                Offset (0xE0), 
                PWMV,   128, 
                WTDP,   8, 
                RTDP,   8, 
                WTP2,   8, 
                RTP2,   8, 
                WTP3,   16, 
                RTP3,   8, 
                WT3T,   8, 
                WT3D,   8, 
                Offset (0xFF), 
                WTP4,   8
            }

            OperationRegion (CMS2, SystemIO, 0x72, 0x02)
            Field (CMS2, ByteAcc, NoLock, Preserve)
            {
                XCMI,   8, 
                XCMD,   8
            }

            Method (WXCM, 2, NotSerialized)
            {
                XCMI = Arg0
                XCMD = Arg1
            }

            Method (RXCM, 1, NotSerialized)
            {
                XCMI = Arg0
                Return (XCMD) /* \_SB_.PCI0.LPCB.ECDV.XCMD */
            }

            Device (BAT1)
            {
                Name (_HID, EisaId ("PNP0C0A") /* Control Method Battery */)  // _HID: Hardware ID
                Name (_UID, One)  // _UID: Unique ID
                Method (_STA, 0, NotSerialized)  // _STA: Status
                {
                    Return (0x1F)
                }

                Method (_BIF, 0, NotSerialized)  // _BIF: Battery Information
                {
                    Name (BPKG, Package (0x0D)
                    {
                        One, 
                        0xFFFFFFFF, 
                        0xFFFFFFFF, 
                        One, 
                        0xFFFFFFFF, 
                        Zero, 
                        Zero, 
                        0x64, 
                        Zero, 
                        " ", 
                        " ", 
                        "LI-ION", 
                        "GIGABYTE"
                    })
                    BPKG [0x09] = "AORUS 15G"
                    BPKG [One] = B1DC /* \_SB_.PCI0.LPCB.ECDV.B1DC */
                    BPKG [0x02] = B1FC /* \_SB_.PCI0.LPCB.ECDV.B1FC */
                    BPKG [0x04] = B1DG /* \_SB_.PCI0.LPCB.ECDV.B1DG */
                    If (B1FC)
                    {
                        FBFC = B1FC /* \_SB_.PCI0.LPCB.ECDV.B1FC */
                        BPKG [0x05] = (B1FC / 0x0A)
                        BPKG [0x06] = (B1FC / 0x19)
                        BPKG [0x07] = (B1FC / 0x64)
                    }

                    Return (BPKG) /* \_SB_.PCI0.LPCB.ECDV.BAT1._BIF.BPKG */
                }

                Method (_BST, 0, NotSerialized)  // _BST: Battery Status
                {
                    Name (PKG1, Package (0x04)
                    {
                        0xFFFFFFFF, 
                        0xFFFFFFFF, 
                        0xFFFFFFFF, 
                        0xFFFFFFFF
                    })
                    Name (DGPU, Zero)
                    DGPU = RXCM (0x11)
                    If ((DGPU == One))
                    {
                        SGOV (0x030A0017, One)
                    }

                    PKG1 [Zero] = (B1ST & 0x07)
                    PKG1 [One] = B1CR /* \_SB_.PCI0.LPCB.ECDV.B1CR */
                    PKG1 [0x02] = B1RC /* \_SB_.PCI0.LPCB.ECDV.B1RC */
                    PKG1 [0x02] = ((B1RC * FBFC) / B1FC)
                    PKG1 [0x03] = B1FV /* \_SB_.PCI0.LPCB.ECDV.B1FV */
                    Return (PKG1) /* \_SB_.PCI0.LPCB.ECDV.BAT1._BST.PKG1 */
                }

                Method (_PCL, 0, NotSerialized)  // _PCL: Power Consumer List
                {
                    Return (_SB) /* \_SB_ */
                }
            }

            Mutex (ECMT, 0x00)
            Method (_REG, 2, NotSerialized)  // _REG: Region Availability
            {
                If (((Arg0 == 0x03) && (Arg1 == One)))
                {
                    If ((Zero == ACTT))
                    {
                        CFAN = Zero
                    }

                    ^^^GFX0.CLID = LSTE /* \_SB_.PCI0.LPCB.ECDV.LSTE */
                    LIDS = LSTE /* \_SB_.PCI0.LPCB.ECDV.LSTE */
                    Local0 = PWRS /* \PWRS */
                    PWRS = RPWR /* \_SB_.PCI0.LPCB.ECDV.RPWR */
                    PNOT ()
                }
            }

            Method (BATS, 0, NotSerialized)
            {
                B1SC = B1RC /* \_SB_.PCI0.LPCB.ECDV.B1RC */
                B1SS = B1ST /* \_SB_.PCI0.LPCB.ECDV.B1ST */
                Notify (BAT1, 0x80) // Status Change
                Notify (BAT1, 0x81) // Information Change
            }

            Method (ACCK, 0, NotSerialized)
            {
                Notify (ADP1, 0x80) // Status Change
                LPST ()
            }

            Method (BSCK, 0, NotSerialized)
            {
                BATS ()
                LPST ()
            }

            Method (LPST, 0, NotSerialized)
            {
            }

