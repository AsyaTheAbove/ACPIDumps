                    0x00B2,             // Range Minimum
                    0x00B2,             // Range Maximum
                    0x01,               // Alignment
                    0x02,               // Length
                    )
                IO (Decode16,
                    0x0680,             // Range Minimum
                    0x0680,             // Range Maximum
                    0x01,               // Alignment
                    0x20,               // Length
                    )
                IO (Decode16,
                    0x1800,             // Range Minimum
                    0x1800,             // Range Maximum
                    0x01,               // Alignment
                    0xFF,               // Length
                    )
                IO (Decode16,
                    0xF800,             // Range Minimum
                    0xF800,             // Range Maximum
                    0x01,               // Alignment
                    0x80,               // Length
                    )
                IO (Decode16,
                    0xF880,             // Range Minimum
                    0xF880,             // Range Maximum
                    0x01,               // Alignment
                    0x80,               // Length
                    )
                IO (Decode16,
                    0xFC00,             // Range Minimum
                    0xFC00,             // Range Maximum
                    0x01,               // Alignment
                    0x80,               // Length
                    )
                IO (Decode16,
                    0xFC80,             // Range Minimum
                    0xFC80,             // Range Maximum
                    0x01,               // Alignment
                    0x80,               // Length
                    )
                IO (Decode16,
                    0xFD00,             // Range Minimum
                    0xFD00,             // Range Maximum
                    0x01,               // Alignment
                    0x80,               // Length
                    )
                IO (Decode16,
                    0x164E,             // Range Minimum
                    0x164E,             // Range Maximum
                    0x01,               // Alignment
                    0x02,               // Length
                    )
                IO (Decode16,
                    0x0000,             // Range Minimum
                    0x0000,             // Range Maximum
                    0x01,               // Alignment
                    0x00,               // Length
                    _Y2D)
                IO (Decode16,
                    0x0000,             // Range Minimum
                    0x0000,             // Range Maximum
                    0x01,               // Alignment
                    0x00,               // Length
                    _Y2E)
                IO (Decode16,
                    0x0000,             // Range Minimum
                    0x0000,             // Range Maximum
                    0x01,               // Alignment
                    0x00,               // Length
                    _Y2F)
                IO (Decode16,
                    0x0000,             // Range Minimum
                    0x0000,             // Range Maximum
                    0x01,               // Alignment
                    0x00,               // Length
                    _Y30)
                IO (Decode16,
                    0x0000,             // Range Minimum
                    0x0000,             // Range Maximum
                    0x01,               // Alignment
                    0x00,               // Length
                    _Y31)
                IO (Decode16,
                    0x0000,             // Range Minimum
                    0x0000,             // Range Maximum
                    0x01,               // Alignment
                    0x00,               // Length
                    _Y32)
                IO (Decode16,
                    0x0000,             // Range Minimum
                    0x0000,             // Range Maximum
                    0x01,               // Alignment
                    0x00,               // Length
                    _Y33)
                IO (Decode16,
                    0x0000,             // Range Minimum
                    0x0000,             // Range Maximum
                    0x01,               // Alignment
                    0x00,               // Length
                    _Y34)
            })
            Method (_CRS, 0, Serialized)  // _CRS: Current Resource Settings
            {
                CreateWordField (BUF0, \_SB.PCI0.LPCB.LDRC._Y2D._MIN, RMN0)  // _MIN: Minimum Base Address
                CreateWordField (BUF0, \_SB.PCI0.LPCB.LDRC._Y2D._MAX, RMX0)  // _MAX: Maximum Base Address
                CreateByteField (BUF0, \_SB.PCI0.LPCB.LDRC._Y2D._LEN, LEN0)  // _LEN: Length
                CreateWordField (BUF0, \_SB.PCI0.LPCB.LDRC._Y2E._MIN, RMNA)  // _MIN: Minimum Base Address
                CreateWordField (BUF0, \_SB.PCI0.LPCB.LDRC._Y2E._MAX, RMXA)  // _MAX: Maximum Base Address
                CreateByteField (BUF0, \_SB.PCI0.LPCB.LDRC._Y2E._LEN, LENA)  // _LEN: Length
                If (IT0E)
                {
                    If ((IT0A != 0x18))
                    {
                        RMN0 = (IT0A << 0x02)
                        RMX0 = RMN0 /* \_SB_.PCI0.LPCB.LDRC._CRS.RMN0 */
                        Local0 = ((Local0 = (IT0M << 0x02)) | 0x03)
                        Local0 += One
                        If ((Local0 <= 0x80))
                        {
                            LEN0 = Local0
                        }
                        Else
                        {
                            LEN0 = 0x80
                            RMNA = (RMN0 + LEN0) /* \_SB_.PCI0.LPCB.LDRC._CRS.LEN0 */
                            RMXA = RMNA /* \_SB_.PCI0.LPCB.LDRC._CRS.RMNA */
                            LENA = (Local0 - 0x80)
                        }
                    }
                }

                CreateWordField (BUF0, \_SB.PCI0.LPCB.LDRC._Y2F._MIN, RMN1)  // _MIN: Minimum Base Address
                CreateWordField (BUF0, \_SB.PCI0.LPCB.LDRC._Y2F._MAX, RMX1)  // _MAX: Maximum Base Address
                CreateByteField (BUF0, \_SB.PCI0.LPCB.LDRC._Y2F._LEN, LEN1)  // _LEN: Length
                CreateWordField (BUF0, \_SB.PCI0.LPCB.LDRC._Y30._MIN, RMNB)  // _MIN: Minimum Base Address
                CreateWordField (BUF0, \_SB.PCI0.LPCB.LDRC._Y30._MAX, RMXB)  // _MAX: Maximum Base Address
                CreateByteField (BUF0, \_SB.PCI0.LPCB.LDRC._Y30._LEN, LENB)  // _LEN: Length
                If (IT1E)
                {
                    If ((IT1A != 0x18))
                    {
                        RMN1 = (IT1A << 0x02)
                        RMX1 = RMN1 /* \_SB_.PCI0.LPCB.LDRC._CRS.RMN1 */
                        Local0 = ((Local0 = (IT1M << 0x02)) | 0x03)
                        Local0 += One
                        If ((Local0 <= 0x80))
                        {
                            LEN1 = Local0
                        }
                        Else
                        {
                            LEN1 = 0x80
                            RMNB = (RMN1 + LEN1) /* \_SB_.PCI0.LPCB.LDRC._CRS.LEN1 */
                            RMXB = RMNB /* \_SB_.PCI0.LPCB.LDRC._CRS.RMNB */
                            LENB = (Local0 - 0x80)
                        }
                    }
                }

                CreateWordField (BUF0, \_SB.PCI0.LPCB.LDRC._Y31._MIN, RMN2)  // _MIN: Minimum Base Address
                CreateWordField (BUF0, \_SB.PCI0.LPCB.LDRC._Y31._MAX, RMX2)  // _MAX: Maximum Base Address
                CreateByteField (BUF0, \_SB.PCI0.LPCB.LDRC._Y31._LEN, LEN2)  // _LEN: Length
                CreateWordField (BUF0, \_SB.PCI0.LPCB.LDRC._Y32._MIN, RMNC)  // _MIN: Minimum Base Address
                CreateWordField (BUF0, \_SB.PCI0.LPCB.LDRC._Y32._MAX, RMXC)  // _MAX: Maximum Base Address
                CreateByteField (BUF0, \_SB.PCI0.LPCB.LDRC._Y32._LEN, LENC)  // _LEN: Length
                If (IT2E)
                {
                    If ((IT2A != 0x18))
                    {
                        RMN2 = (IT2A << 0x02)
                        RMX2 = RMN2 /* \_SB_.PCI0.LPCB.LDRC._CRS.RMN2 */
                        Local0 = ((Local0 = (IT2M << 0x02)) | 0x03)
                        Local0 += One
                        If ((Local0 <= 0x80))
                        {
                            LEN2 = Local0
                        }
                        Else
                        {
                            LEN2 = 0x80
                            RMNC = (RMN2 + LEN2) /* \_SB_.PCI0.LPCB.LDRC._CRS.LEN2 */
                            RMXC = RMNC /* \_SB_.PCI0.LPCB.LDRC._CRS.RMNC */
                            LENC = (Local0 - 0x80)
                        }
                    }
                }

                CreateWordField (BUF0, \_SB.PCI0.LPCB.LDRC._Y33._MIN, RMN3)  // _MIN: Minimum Base Address
                CreateWordField (BUF0, \_SB.PCI0.LPCB.LDRC._Y33._MAX, RMX3)  // _MAX: Maximum Base Address
                CreateByteField (BUF0, \_SB.PCI0.LPCB.LDRC._Y33._LEN, LEN3)  // _LEN: Length
                CreateWordField (BUF0, \_SB.PCI0.LPCB.LDRC._Y34._MIN, RMND)  // _MIN: Minimum Base Address
                CreateWordField (BUF0, \_SB.PCI0.LPCB.LDRC._Y34._MAX, RMXD)  // _MAX: Maximum Base Address
                CreateByteField (BUF0, \_SB.PCI0.LPCB.LDRC._Y34._LEN, LEND)  // _LEN: Length
                If (IT3E)
                {
                    If ((IT0A != 0x18))
                    {
                        RMN3 = (IT3A << 0x02)
                        RMX3 = RMN3 /* \_SB_.PCI0.LPCB.LDRC._CRS.RMN3 */
                        Local0 = ((Local0 = (IT3M << 0x02)) | 0x03)
                        Local0 += One
                        If ((Local0 <= 0x80))
                        {
                            LEN3 = Local0
                        }
                        Else
                        {
                            LEN3 = 0x80
                            RMND = (RMN3 + LEN3) /* \_SB_.PCI0.LPCB.LDRC._CRS.LEN3 */
                            RMXD = RMND /* \_SB_.PCI0.LPCB.LDRC._CRS.RMND */
                            LEND = (Local0 - 0x80)
                        }
                    }
                }

                Return (BUF0) /* \_SB_.PCI0.LPCB.LDRC.BUF0 */
            }
        }

