            0x4A, 
            0x4B, 
            0x4C, 
            0x4D, 
            0x4E, 
            0x4F, 
            0x50, 
            0x51, 
            0x52, 
            0x53, 
            0x54, 
            0x55, 
            0x56, 
            0x57, 
            0x58, 
            0x59, 
            0x5A, 
            0x5B, 
            0x5C, 
            0x5D, 
            0x5E, 
            0x5F, 
            0x60, 
            0x61, 
            0x62, 
            0x63, 
            0x64
        })
        Method (_BCL, 0, NotSerialized)  // _BCL: Brightness Control Levels
        {
            Return (BCL2) /* \_SB_.PCI0.GFX0.DD1F.BCL2 */
        }

        Method (_BCM, 1, NotSerialized)  // _BCM: Brightness Control Method
        {
            LCDL = Arg0
            Name (BBCT, Buffer (BLLM) {})
            BBCT = BLLT /* External reference */
            AINT (One, LERP (BCLP, BBCT, Arg0))
        }

        Method (_BQC, 0, NotSerialized)  // _BQC: Brightness Query Current
        {
            Return (LCDL) /* External reference */
        }

        Method (BLNF, 0, NotSerialized)
        {
            If ((BSWF == One))
            {
                Notify (\_SB.PCI0.GFX0.DD1F, 0x87) // Device-Specific
            }

            If ((BSWF == 0x02))
            {
                Notify (\_SB.PCI0.GFX0.DD1F, 0x86) // Device-Specific
            }

            BSWF = Zero
            AHKF |= One
        }

        Method (LERP, 3, NotSerialized)
        {
            If ((Arg2 == Zero))
            {
                Arg2 += One
            }

            Local0 = Zero
            While ((Local0 < BLLM))
            {
                Local1 = DerefOf (Arg0 [(Local0 + 0x02)])
                Local2 = DerefOf (Arg0 [(Local0 + 0x03)])
                If (((Arg2 >= Local1) && (Arg2 <= Local2)))
                {
                    Local3 = (0xFF - GBUF (Arg1, Local0))
                    Local4 = (0xFF - GBUF (Arg1, (Local0 + One)))
                    Local5 = ((((Local4 - Local3) * (Arg2 - Local1
                        )) / (Local2 - Local1)) + Local3)
                    Return (Local5)
                }

                Local0++
            }

            Return (Zero)
        }

        Method (GBUF, 2, NotSerialized)
        {
            CreateByteField (Arg0, Arg1, BLL0)
            Return (BLL0) /* \_SB_.PCI0.GFX0.DD1F.GBUF.BLL0 */
        }
    }

    Scope (\)
    {
        Method (MPLX, 4, Serialized)
        {
            CreateByteField (BPLD, 0x08, PLDV)
            CreateByteField (BPLD, 0x0C, PLDE)
            CreateWordField (BPLD, 0x0A, PLDG)
            CreateWordField (BPLD, 0x10, PLVO)
            CreateWordField (BPLD, 0x12, PLHO)
            PLDG = (Arg0 << 0x07)
            PLDV = Arg1
            PLVO = Arg2
            PLHO = Arg3
            If ((Arg1 & 0x03))
            {
                PLDE |= 0x03
            }
            Else
            {
                PLDE &= 0xFC
            }

            Return (BPLD) /* External reference */
        }
    }

    Scope (\_SB.PCI0.XHC.RHUB.HS01)
    {
    }

    Scope (\_SB.PCI0.XHC.RHUB.HS02)
    {
    }

    Scope (\_SB.PCI0.XHC.RHUB.HS03)
    {
    }

    Scope (\_SB.PCI0.XHC.RHUB.HS04)
    {
    }

    Scope (\_SB.PCI0.XHC.RHUB.HS05)
    {
    }

    Scope (\_SB.PCI0.XHC.RHUB.HS06)
    {
        Device (CM01)
        {
            Name (_ADR, 0x06)  // _ADR: Address
            Method (_PLD, 0, NotSerialized)  // _PLD: Physical Location of Device
            {
                Return (MPLX (0x06, 0x20, 0xC8, 0x96))
            }
        }
    }

    Scope (\_SB.PCI0.XHC.RHUB.HS07)
    {
    }

    Scope (\_SB.PCI0.XHC.RHUB.HS08)
    {
