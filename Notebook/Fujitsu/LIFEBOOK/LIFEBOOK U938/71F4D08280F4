        Scope (\)
        {
            Name (TCDT, 0x69)
            Name (TCUT, 0x64)
            Name (THCF, Zero)
            Name (GXLP, Zero)
        }

        Method (TINI, 0, NotSerialized)
        {
            If (TCOV)
            {
                TCUT = CUOV /* \CUOV */
                TCDT = CDOV /* \CDOV */
            }
        }

        Method (TCTR, 0, NotSerialized)
        {
            If (CondRefOf (\_PR.PR00._PPC, Local0))
            {
                If ((ASTT && !ACPW))
                {
                    TMIN ()
                }
                Else
                {
                    ASTT = Zero
                    If ((!LWMD && !TALM))
                    {
                        If ((PDAT >= TCDT))
                        {
                            TDWN ()
                            THCF = One
                        }
                        ElseIf (THCF)
                        {
                            If ((PDAT <= TCUT))
                            {
                                TUPC ()
                            }
                        }
                        Else
                        {
                            TMAX ()
                        }
                    }
                    ElseIf ((LWMD && TALM))
                    {
                        TMIN ()
                    }
                    Else
                    {
                        TDWN ()
                    }

                    If ((\_PR.CPPC == RPPC))
                    {
                        THCF = Zero
                        If (((\_PR.CPPC == PSTM) && (PDAT > TCUT))) {}
                        Else
                        {
                            GXNM ()
                        }
                    }
                }
            }
        }

        Method (GXLW, 0, NotSerialized)
        {
            If (!GXLP)
            {
                ^^^FEXT.FUNC (0x1006, 0x20, 0x800000E2, Zero)
                GXLP = One
            }
        }

        Method (GXNM, 0, NotSerialized)
        {
            If (GXLP)
            {
                ^^^FEXT.FUNC (0x1006, 0x20, 0xE2, Zero)
                GXLP = Zero
            }
        }

        Method (TMIN, 0, NotSerialized)
        {
            If ((PSTM != \_PR.CPPC))
            {
                \_PR.CPPC = PSTM /* External reference */
                PPCC ()
            }

            GXLW ()
        }

        Method (TMAX, 0, NotSerialized)
        {
            If ((RPPC != \_PR.CPPC))
            {
                \_PR.CPPC = RPPC /* \RPPC */
                PPCC ()
            }
        }

        Method (TDWN, 0, NotSerialized)
        {
            If ((\_PR.CPPC == PSTM))
            {
                GXLW ()
            }

            If ((\_PR.CPPC < PSTM))
            {
                \_PR.CPPC++
                If ((\_PR.CPPC < PSTM))
                {
                    \_PR.CPPC++
                }

                PPCC ()
                GXLW ()
            }
        }

        Method (TUPC, 0, NotSerialized)
        {
            If ((\_PR.CPPC > RPPC))
            {
                \_PR.CPPC--
                If ((\_PR.CPPC > RPPC))
                {
                    \_PR.CPPC--
                }

                PPCC ()
            }
        }

        Method (TPTS, 0, NotSerialized)
        {
            If (CondRefOf (\_PR.PR00._PPC, Local0))
            {
                \_PR.CPPC = Zero
                PPCC ()
                THCF = Zero
            }

            GXLP = Zero
        }

        Method (TWAK, 1, NotSerialized)
        {
            If (ASTT)
            {
                TMIN ()
            }
            ElseIf (CondRefOf (\_PR.PR00._PPC, Local0))
            {
                If ((PDAT >= TCDT))
                {
                    TDWN ()
                    THCF = One
                }
            }
        }

        Method (PPCC, 0, NotSerialized)
        {
            If ((PC00 & 0x08))
            {
                Notify (\_PR.PR00, 0x80) // Performance Capability Change
            }

            If ((PC01 & 0x08))
            {
                Notify (\_PR.PR01, 0x80) // Performance Capability Change
            }

            If ((PC02 & 0x08))
            {
                Notify (\_PR.PR02, 0x80) // Performance Capability Change
            }

            If ((PC03 & 0x08))
            {
                Notify (\_PR.PR03, 0x80) // Performance Capability Change
            }

            If ((PC04 & 0x08))
            {
                Notify (\_PR.PR04, 0x80) // Performance Capability Change
            }

            If ((PC05 & 0x08))
            {
                Notify (\_PR.PR05, 0x80) // Performance Capability Change
            }

            If ((PC06 & 0x08))
            {
                Notify (\_PR.PR06, 0x80) // Performance Capability Change
            }

            If ((PC07 & 0x08))
            {
                Notify (\_PR.PR07, 0x80) // Performance Capability Change
            }

            If ((PC08 & 0x08))
            {
                Notify (\_PR.PR08, 0x80) // Performance Capability Change
            }

            If ((PC09 & 0x08))
            {
                Notify (\_PR.PR09, 0x80) // Performance Capability Change
            }

            If ((PC10 & 0x08))
            {
                Notify (\_PR.PR10, 0x80) // Performance Capability Change
            }

            If ((PC11 & 0x08))
            {
                Notify (\_PR.PR11, 0x80) // Performance Capability Change
            }

            If ((PC12 & 0x08))
            {
                Notify (\_PR.PR12, 0x80) // Performance Capability Change
            }

            If ((PC13 & 0x08))
            {
                Notify (\_PR.PR13, 0x80) // Performance Capability Change
            }

            If ((PC14 & 0x08))
            {
                Notify (\_PR.PR14, 0x80) // Performance Capability Change
            }

            If ((PC15 & 0x08))
            {
                Notify (\_PR.PR15, 0x80) // Performance Capability Change
            }
        }

        Device (KBC)
        {
            Name (R101, 0x0303D041)
            Name (R106, 0x2003D041)
            Name (F300, 0x0003AA1A)
            Name (F309, 0x0903AA1A)
            Method (_HID, 0, NotSerialized)  // _HID: Hardware ID
            {
                If (JPKF)
                {
                    If ((OSYS >= 0x07DC))
                    {
                        Return (F309) /* \_SB_.PCI0.LPCB.KBC_.F309 */
                    }
                    Else
                    {
                        Return (R106) /* \_SB_.PCI0.LPCB.KBC_.R106 */
                    }
                }
                ElseIf ((OSYS >= 0x07DC))
                {
                    Return (F300) /* \_SB_.PCI0.LPCB.KBC_.F300 */
                }
                Else
                {
                    Return (R101) /* \_SB_.PCI0.LPCB.KBC_.R101 */
                }
            }

            Method (_CID, 0, NotSerialized)  // _CID: Compatible ID
            {
                If (JPKF)
                {
                    Return (R106) /* \_SB_.PCI0.LPCB.KBC_.R106 */
                }
                Else
                {
                    Return (R101) /* \_SB_.PCI0.LPCB.KBC_.R101 */
                }
            }

            Method (_STA, 0, NotSerialized)  // _STA: Status
            {
                Return (0x0F)
            }

            Name (_CRS, ResourceTemplate ()  // _CRS: Current Resource Settings
            {
                IO (Decode16,
                    0x0060,             // Range Minimum
                    0x0060,             // Range Maximum
                    0x01,               // Alignment
                    0x01,               // Length
                    )
                IO (Decode16,
                    0x0064,             // Range Minimum
                    0x0064,             // Range Maximum
                    0x01,               // Alignment
                    0x01,               // Length
                    )
                IRQNoFlags ()
                    {1}
            })
        }

        Device (PS2M)
        {
            Method (_HID, 0, NotSerialized)  // _HID: Hardware ID
            {
                If ((OSYS >= 0x07DC))
                {
                    Return (0x121F2E4F)
                }
                Else
                {
                    Return (0x121F2E4F)
                }
            }

            Method (_CID, 0, NotSerialized)  // _CID: Compatible ID
