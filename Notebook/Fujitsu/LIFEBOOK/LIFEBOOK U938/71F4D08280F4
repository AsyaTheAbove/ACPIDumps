    }

    Scope (_SB.PCI0.XHC.RHUB.SS01)
    {
        Method (_UPC, 0, Serialized)  // _UPC: USB Port Capabilities
        {
            Switch (ToInteger (GSPT (Zero)))
            {
                Case (One)
                {
                    Return (UPC3) /* \UPC3 */
                }
                Case (0x02)
                {
                    Return (UPNC) /* \UPNC */
                }
                Case (0x03)
                {
                    Return (UPCC) /* \UPCC */
                }
                Default
                {
                    Return (UPCH) /* \UPCH */
                }

            }
        }

        Method (_PLD, 0, Serialized)  // _PLD: Physical Location of Device
        {
            Switch (ToInteger (GSPT (Zero)))
            {
                Case (One)
                {
                    If (GCMP (Zero))
                    {
                        Return (MPLD (GCMP (Zero), One))
                    }
                    Else
                    {
                        Return (MPLD (0x11, One))
                    }
                }
                Case (0x03)
                {
                    If (GCMP (Zero))
                    {
                        Return (MPLD (GCMP (Zero), One))
                    }
                    Else
                    {
                        Return (MPLD (0x11, One))
                    }
                }
                Default
                {
                    If (GCMP (Zero))
                    {
                        Return (MPLD (GCMP (Zero), Zero))
                    }
                    Else
                    {
                        Return (MPLD (0x11, Zero))
                    }
                }

            }
        }
    }

    Scope (_SB.PCI0.XHC.RHUB.SS02)
    {
        Method (_UPC, 0, Serialized)  // _UPC: USB Port Capabilities
        {
            Switch (ToInteger (GSPT (One)))
            {
                Case (One)
                {
                    Return (UPC3) /* \UPC3 */
                }
                Case (0x02)
                {
                    Return (UPNC) /* \UPNC */
                }
                Case (0x03)
                {
                    Return (UPCC) /* \UPCC */
                }
                Default
                {
                    Return (UPCH) /* \UPCH */
                }

            }
        }

        Method (_PLD, 0, Serialized)  // _PLD: Physical Location of Device
        {
            Switch (ToInteger (GSPT (One)))
            {
                Case (One)
                {
                    If (GCMP (One))
                    {
                        Return (MPLD (GCMP (One), One))
                    }
                    Else
                    {
                        Return (MPLD (0x12, One))
                    }
                }
                Case (0x03)
                {
                    If (GCMP (One))
                    {
                        Return (MPLD (GCMP (One), One))
                    }
                    Else
                    {
                        Return (MPLD (0x12, One))
                    }
                }
                Default
                {
                    If (GCMP (One))
                    {
                        Return (MPLD (GCMP (One), Zero))
                    }
                    Else
                    {
                        Return (MPLD (0x12, Zero))
                    }
                }

            }
        }
    }

    Scope (_SB.PCI0.XHC.RHUB.SS03)
    {
        Method (_UPC, 0, Serialized)  // _UPC: USB Port Capabilities
        {
            Switch (ToInteger (GSPT (0x02)))
            {
                Case (One)
                {
                    Return (UPC3) /* \UPC3 */
                }
                Case (0x02)
                {
                    Return (UPNC) /* \UPNC */
                }
                Case (0x03)
                {
                    Return (UPCC) /* \UPCC */
                }
                Default
                {
                    Return (UPCH) /* \UPCH */
                }

            }
        }

        Method (_PLD, 0, Serialized)  // _PLD: Physical Location of Device
        {
            Switch (ToInteger (GSPT (0x02)))
            {
                Case (One)
                {
                    If (GCMP (0x02))
                    {
                        Return (MPLD (GCMP (0x02), One))
                    }
                    Else
                    {
                        Return (MPLD (0x13, One))
                    }
                }
                Case (0x03)
                {
                    If (GCMP (0x02))
                    {
                        Return (MPLD (GCMP (0x02), One))
                    }
                    Else
                    {
                        Return (MPLD (0x13, One))
                    }
                }
                Default
                {
                    If (GCMP (0x02))
                    {
                        Return (MPLD (GCMP (0x02), Zero))
                    }
                    Else
                    {
                        Return (MPLD (0x13, Zero))
                    }
                }

            }
        }
    }

    Scope (_SB.PCI0.XHC.RHUB.SS04)
    {
        Method (_UPC, 0, Serialized)  // _UPC: USB Port Capabilities
        {
            Switch (ToInteger (GSPT (0x03)))
            {
                Case (One)
                {
                    Return (UPC3) /* \UPC3 */
                }
                Case (0x02)
                {
                    Return (UPNC) /* \UPNC */
                }
                Case (0x03)
                {
                    Return (UPCC) /* \UPCC */
                }
                Default
                {
                    Return (UPCH) /* \UPCH */
                }

            }
        }

        Method (_PLD, 0, Serialized)  // _PLD: Physical Location of Device
        {
            Switch (ToInteger (GSPT (0x03)))
            {
                Case (One)
                {
                    If (GCMP (0x03))
                    {
                        Return (MPLD (GCMP (0x03), One))
                    }
                    Else
                    {
                        Return (MPLD (0x14, One))
                    }
                }
                Case (0x03)
                {
                    If (GCMP (0x03))
                    {
                        Return (MPLD (GCMP (0x03), One))
                    }
                    Else
                    {
                        Return (MPLD (0x14, One))
                    }
                }
                Default
                {
                    If (GCMP (0x03))
                    {
                        Return (MPLD (GCMP (0x03), Zero))
                    }
                    Else
                    {
                        Return (MPLD (0x14, Zero))
                    }
                }

            }
        }
    }

    Scope (_SB.PCI0.XHC.RHUB.SS05)
    {
        Method (_UPC, 0, Serialized)  // _UPC: USB Port Capabilities
