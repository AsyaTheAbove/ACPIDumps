    OperationRegion (FJRV, SystemMemory, 0x8278E018, 0x006D)
    Scope (\)
    {
        Field (FJRV, AnyAcc, Lock, Preserve)
        {
            WAPB,   8, 
            Offset (0x02), 
            Offset (0x06), 
            Offset (0x08), 
            OWET,   8, 
            PIID,   320, 
            Offset (0x32), 
            Offset (0x36), 
            Offset (0x3A), 
            Offset (0x3B), 
            SBWF,   8, 
            Offset (0x3D), 
            HYBT,   8, 
            IANF,   8, 
            CANF,   8, 
            KBCF,   8, 
            VBTT,   8, 
            Offset (0x43), 
            Offset (0x44), 
