                    {
                        Return (MPLD (GCMP (0x04), One))
                    }
                    Else
                    {
                        Return (MPLD (0x15, One))
                    }
                }
                Default
                {
                    If (GCMP (Zero))
                    {
                        Return (MPLD (GCMP (0x04), Zero))
                    }
                    Else
                    {
                        Return (MPLD (0x15, Zero))
                    }
                }

            }
        }
    }

    Scope (_SB.PCI0.XHC.RHUB.SS06)
    {
        Method (_UPC, 0, Serialized)  // _UPC: USB Port Capabilities
