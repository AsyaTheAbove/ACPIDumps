            TINI ()
        }

        Method (AINI, 0, NotSerialized)
        {
            If (AS34)
            {
                S3EN = One
            }

            If ((DCLW == 0x7F))
            {
                DCOL = 0x03
            }
            Else
            {
                DCOL = DCLW /* \DCLW */
            }

            If (GGIV (0x02030003))
            {
                SGOV (0x0205000A, Zero)
            }
            Else
            {
                SGOV (0x0205000A, One)
            }
        }

        OperationRegion (FJIO, SystemIO, 0xFD70, 0x10)
        Scope (\)
        {
            Field (\_SB.PCI0.LPCB.FJIO, ByteAcc, NoLock, Preserve)
            {
                PMID,   8, 
                Offset (0x02), 
                PMDR,   8, 
                Offset (0x04), 
                PMDT,   8, 
                Offset (0x06), 
                PMCR,   8
            }
        }

