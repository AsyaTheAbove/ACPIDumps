    Scope (_SB)
    {
        Scope (\)
        {
            Name (WBTN, Zero)
            Name (NGTM, Zero)
            Name (LSBL, Zero)
            Name (LEDI, Zero)
            Name (BTNI, Zero)
            Name (BNBF, Buffer (0x20) {})
            Name (BNSP, Zero)
            Name (BNGP, Zero)
            Name (BNCT, Zero)
            Name (LIDF, Zero)
            Name (WSEF, Zero)
            Name (DPCI, Zero)
            Name (DPCS, Zero)
            Name (BYCI, Zero)
            Name (BTCI, Zero)
        }

        Device (FEXT)
        {
            Name (_HID, "FUJ02E3")  // _HID: Hardware ID
            Method (_STA, 0, NotSerialized)  // _STA: Status
            {
                Return (0x0F)
            }

            Method (_INI, 0, NotSerialized)  // _INI: Initialize
            {
                LEDI = Zero
                LEDI |= 0x00020000
                BNSP = Zero
                BNGP = Zero
                BNCT = Zero
                IRBC = Zero
                IRBF = Zero
                BTNI = Zero
                If (SISA)
                {
                    BTNI |= 0x00010000
                    BTNI |= One
                }
                ElseIf (SISB)
                {
                    ECCI |= 0x08
                }

                If (BLEN)
                {
                    BTCI = 0x0400
                    DPCI |= BTCI /* \BTCI */
                }

                HYBT = Zero
            }

            Method (FUNC, 4, Serialized)
            {
                Local0 = 0x80000000
                Switch (Arg0)
                {
                    Case (0x1000)
                    {
                        Local0 = S000 (Arg1, Arg2, Arg3)
                    }
                    Case (0x1001)
                    {
                        Local0 = S001 (Arg1, Arg2, Arg3)
                    }
                    Case (0x1002)
                    {
                        Local0 = S002 (Arg1, Arg2, Arg3)
                    }
                    Case (0x1005)
                    {
                        Local0 = S005 (Arg1, Arg2, Arg3)
                    }
                    Case (0x1006)
                    {
                        Local0 = S006 (Arg1, Arg2, Arg3)
                    }
                    Case (0x80001006)
                    {
                        If ((Arg1 == 0x21))
                        {
                            If ((Arg2 == One))
                            {
                                ^^CMB1.EBIF ()
                            }
                        }
                    }
                    Case (0x1007)
                    {
                        Local0 = S007 (Arg1, Arg2, Arg3)
                    }
                    Case (0x1008)
                    {
                        Local0 = S008 (Arg1, Arg2, Arg3)
                    }
                    Case (0x1009)
                    {
                        Local0 = S009 (Arg1, Arg2, Arg3)
                    }
                    Case (0x100A)
                    {
                        Local0 = S00A (Arg1, Arg2, Arg3)
                    }
                    Case (0x1013)
                    {
                        Local0 = S013 (Arg1, Arg2, Arg3)
                    }
                    Case (0x1015)
                    {
                        Local0 = S015 (Arg1, Arg2, Arg3)
                    }

                }

                Return (Local0)
            }

            Method (SSMI, 4, NotSerialized)
            {
                QAG1 = Arg0
                QAG2 = Arg1
                QAG3 = Arg2
                Return (FSMI (0x87, Arg3, Zero))
            }

            Method (S000, 3, Serialized)
            {
                Local0 = Zero
                Switch (Arg0)
                {
                    Case (Zero)
                    {
                        Local0 |= 0x40000000
                        Local0 |= 0x20000000
                        Local0 |= 0x04000000
                        Local0 |= 0x02000000
                        Local0 |= 0x00020000
                        Local0 |= 0x0100
                        Local0 |= 0x20
                    }
                    Case (One)
                    {
                        If ((FLKS & 0x80))
                        {
                            Local0 |= 0x40000000
                            FLKS ^= 0x80
                        }

                        If ((AHKF & 0x02))
                        {
                            Local0 |= 0x20000000
                            AHKF ^= 0x02
                        }

                        If ((AHKF & One))
                        {
                            Local0 |= 0x02000000
                            AHKF ^= One
                        }

                        If ((AHKF & 0x08))
                        {
                            Local0 |= 0x04000000
                            AHKF ^= 0x08
                        }

                        Local0 |= (LIDF << 0x08)
                        LIDF = Zero
                        Local0 |= (WSEF << 0x05)
                        WSEF = Zero
                    }
                    Case (0x02)
                    {
                    }
                    Case (0x03)
                    {
                    }
                    Case (0x04)
                    {
                        Arg1 = ((Arg2 << 0x10) | (Arg1 & 0xFFFF))
                        Local0 = FSMI (0x91, Arg0, Arg1)
                        Local0 |= ((FLKS & One) << 0x1E)
                        If ((Local0 & 0x20))
                        {
                            RFSW = One
                        }
                        Else
                        {
                            RFSW = Zero
                        }

                        Local0 |= (RFSW << 0x05)
                        Local0 |= (^^LID._LID () << 0x08)
                    }
                    Case (0x05)
                    {
                        If ((Arg1 & 0x20))
                        {
                            If ((Arg2 & 0x20))
                            {
                                RFSW = One
                            }
                            Else
                            {
                                RFSW = Zero
                            }
                        }

                        Arg1 |= ((Arg1 & 0x40000000) >> 0x10)
                        Arg2 |= ((Arg2 & 0x40000000) >> 0x10)
                        Arg1 = ((Arg2 << 0x10) | (Arg1 & 0xFFFF))
                        Local0 = FSMI (0x91, Arg0, Arg1)
                    }
                    Default
                    {
                        Local0 = 0x80000000
                    }

                }

                Return (Local0)
            }

            Method (S001, 3, NotSerialized)
            {
                Local0 = 0x80000000
                If ((Arg0 == Zero))
                {
                    Local0 = LEDI /* \LEDI */
                }
                ElseIf (((Arg0 == One) | (Arg0 == 0x02)))
                {
                    Local2 = LEDI /* \LEDI */
                    Local2 &= 0xFFFFFFFFFFFDFFFF
                    If ((Local2 & 0x4000))
                    {
                        Local2 |= 0x00010000
                    }

                    If ((Arg1 & Local2))
                    {
                        Local3 = Arg0
                        Arg1 <<= 0x08
                        Local3 |= Arg1
                        Local4 = Arg2
                        Local0 = FSMI (0x7F, Local3, Local4)
                    }
                }

                Return (Local0)
            }

            Method (S002, 3, Serialized)
            {
                Local0 = 0x80000000
                Switch (Arg0)
                {
                    Case (Zero)
                    {
                        Local0 = BTNI /* \BTNI */
                    }
                    Case (One)
                    {
                        Local0 = GIRB ()
                    }
                    Case (0x02)
                    {
                        Local3 = 0x02
                        Local0 = FSMI (0x80, Local3, Zero)
                    }
                    Case (0x03)
                    {
                        If (Arg1)
                        {
                            Local1 = ~Arg1
                            Local2 = (Arg2 & Arg1)
                            WBTN = ((WBTN & Local1) | Local2)
                            Local3 = ((WBTN << 0x08) | 0x03)
                            FSMI (0x80, Local3, Zero)
                        }

                        Local0 = WBTN /* \WBTN */
                    }

                }

                Return (Local0)
            }

            Method (SIRB, 1, NotSerialized)
            {
                If ((BNCT < 0x10))
                {
                    CreateWordField (BNBF, BNSP, BNP1)
                    BNP1 = Arg0
                    BNCT++
                    BNSP += 0x02
                    If ((BNSP >= 0x20))
                    {
                        BNSP = Zero
                    }
                }
            }

            Method (GIRB, 0, NotSerialized)
            {
                If (BNCT)
                {
                    CreateWordField (BNBF, BNGP, BNP2)
                    Local0 = BNP2 /* \_SB_.FEXT.GIRB.BNP2 */
                    Local0 |= 0x40000000
                    BNCT--
                    BNGP += 0x02
                    If ((BNGP >= 0x20))
                    {
                        BNGP = Zero
                    }
                }
                Else
                {
                    Local0 = Zero
                }

                Local0 &= 0xFFFF0FFF
                Return (Local0)
            }

            Method (S005, 3, Serialized)
            {
                Local0 = 0x80000000
                Switch (Arg0)
                {
                    Case (Zero)
                    {
                        Local0 = DPCI /* \DPCI */
                    }
                    Case (One)
                    {
                        If ((Arg1 & BTCI))
                        {
                            If ((Arg2 & BTCI))
                            {
                                SGOV (0x02010015, Zero)
                                DPCS |= BTCI /* \BTCI */
                            }
                            Else
                            {
                                If (RFSW)
                                {
                                    SGOV (0x02010015, One)
                                }

                                DPCS &= ~BTCI
                            }
                        }

                        Local0 = (DPCS & DPCI) /* \DPCI */
                    }

                }

                Return (Local0)
            }

            Method (A2BB, 2, NotSerialized)
            {
                Local0 = (Arg0 - 0x30)
                Local1 = (Arg1 - 0x30)
                If ((((Local0 >= Zero) && (Local0 <= 0x09)) && (
                    (Local1 >= Zero) && (Local1 <= 0x09))))
                {
                    Local2 = ((Local0 << 0x04) | Local1)
                }
                Else
                {
                    Local2 = Zero
                }

                Return (Local2)
            }

            Method (S006, 3, Serialized)
            {
                Local0 = 0x80000000
                If (ECOK)
                {
                    Switch (Arg0)
                    {
                        Case (Zero)
                        {
                            Local0 = ECCI /* \ECCI */
                        }
                        Case (0x10)
                        {
                            Switch (Arg1)
                            {
                                Case (Zero)
                                {
                                    Local0 = 0xFD70
                                }
                                Case (0x0100)
                                {
                                    Local0 = ((TCST << 0x08) | PDAT) /* \PDAT */
                                }
                                Default
                                {
                                    Local2 = ((Arg1 >> 0x08) & 0xFF)
                                    ECG1 = (Arg1 & 0xFF)
                                    Switch (Local2)
                                    {
                                        Case (Zero)
                                        {
                                            ECGM = 0x23
                                        }
                                        Case (One)
                                        {
                                            ECGM = 0x43
                                        }
                                        Case (0x38)
                                        {
                                            ECG2 = ((Arg2 >> 0x08) & 0xFF)
                                            ECG3 = (Arg2 & 0xFF)
                                            ECGM = 0x39
                                        }
                                        Default
                                        {
                                            If (((Local2 >= 0x04) && (Local2 <= 0xFF)))
                                            {
                                                ECGM = (Local2 + One)
                                            }
                                            Else
                                            {
                                                Return (Local0)
                                            }
                                        }

                                    }

                                    Local1 = Zero
                                    While ((ECGM && (Local1 < 0x01F4)))
                                    {
                                        Sleep (0x02)
                                        Local1++
                                    }

                                    If ((Local1 >= 0x01F4))
                                    {
                                        Local0 = 0x80000001
                                    }
                                    Else
                                    {
                                        Local0 = ((ECG2 << 0x08) | ECG3) /* \ECG3 */
                                    }
                                }

                            }
                        }
                        Case (0x11)
                        {
                            Local2 = ((Arg1 >> 0x08) & 0xFF)
                            ECG1 = (Arg1 & 0xFF)
                            ECG2 = ((Arg2 >> 0x08) & 0xFF)
                            ECG3 = (Arg2 & 0xFF)
                            Switch (Local2)
                            {
                                Case (Zero)
                                {
                                    ECGM = 0x22
                                }
                                Case (One)
                                {
                                    ECGM = 0x42
                                }
                                Default
                                {
                                    If (((Local2 >= 0x04) && (Local2 <= 0xFF)))
                                    {
                                        ECGM = Local2
                                    }
                                }

                            }

                            Local1 = Zero
                            While ((ECGM && (Local1 < 0x01F4)))
                            {
                                Sleep (0x02)
                                Local1++
                            }

                            If ((Local1 >= 0x01F4))
                            {
                                Local0 = 0x80000001
                            }
                            Else
                            {
                                Local0 = Zero
                            }
                        }
                        Case (0x12)
                        {
                            Local0 = GINT /* \GINT */
                            GINT = Zero
                        }
                        Case (0x20)
                        {
                            ECD3 = ((Arg1 & 0xFF000000) >> 0x18)
                            ECD2 = ((Arg1 & 0x00FF0000) >> 0x10)
                            ECD1 = ((Arg1 & 0xFF00) >> 0x08)
                            ECCM = (Arg1 & 0xFF)
                            Local1 = Zero
                            Sleep (0x02)
                            While ((ECCM && (Local1 < 0x64)))
                            {
                                Sleep (0x02)
                                Local1++
                            }

                            If ((Local1 >= 0x64))
                            {
                                Local0 = 0x80000001
                            }
                            Else
                            {
                                Local0 = (((ECD3 << 0x10) | (ECD2 << 0x08)) | 
                                    ECD1) /* \ECD1 */
                            }
                        }
                        Case (0x21)
                        {
                            ECB3 = ((Arg1 & 0xFF000000) >> 0x18)
                            ECB2 = ((Arg1 & 0x00FF0000) >> 0x10)
                            ECB1 = ((Arg1 & 0xFF00) >> 0x08)
                            ECBM = (Arg1 & 0xFF)
                            Local1 = Zero
                            While ((ECBM && (Local1 < 0x64)))
                            {
                                Sleep (One)
                                Local1++
                            }

                            If ((Local1 >= 0x64))
                            {
                                Local0 = 0x80000001
                            }
                            ElseIf (((Arg1 != 0x03) && (Arg1 != 0x04)))
                            {
                                Local0 = (((ECB3 << 0x10) | (ECB2 << 0x08)) | 
                                    ECB1) /* \ECB1 */
                            }
                            ElseIf ((((Arg1 == 0x03) && B1P) || ((Arg1 == 0x04) && 
                                B2P)))
                            {
                                Local0 = (((A2BB (ECB7, ECB8) << 0x10) | (A2BB (ECB5, 
                                    ECB6) << 0x08)) | A2BB (ECB3, ECB4))
                            }
                            Else
                            {
                                Local0 = Zero
                            }
                        }
                        Case (0x30)
                        {
                            Local0 = 0x80000001
                            Local1 = ((Arg1 & 0xFF00) >> 0x08)
                            Local2 = ((Arg1 & 0xFFFF0000) >> 0x10)
                            If (((Local2 == 0xFD70) || (Local2 == 0xFD74)))
                            {
                                PMID = Local1
                                Local0 = PMDT /* \PMDT */
                            }
                        }
                        Case (0x31)
                        {
                            Local0 = 0x80000001
                            Local1 = (Arg1 & 0xFF)
                            Local2 = ((Arg1 & 0xFF00) >> 0x08)
                            Local3 = ((Arg1 & 0xFFFF0000) >> 0x10)
                            If (((Local3 == 0xFD70) || (Local3 == 0xFD74)))
                            {
                                PMID = Local2
                                PMDT = Local1
                                Local0 = Zero
                            }
                        }

                    }
                }

                Return (Local0)
            }

            Method (S007, 3, Serialized)
            {
                Local0 = 0x80000000
                Switch (Arg0)
                {
                    Case (Zero)
                    {
                        Switch (Arg1)
                        {
                            Case (Zero)
                            {
                                Local0 = One
                            }
                            Case (0x10)
                            {
                                Local0 = MINP ()
                            }

                        }
                    }
                    Case (One)
                    {
                        Switch (Arg1)
                        {
                            Case (Zero)
                            {
                                Local0 = RPPC /* \RPPC */
                            }
                            Case (One)
                            {
                                If ((Arg2 <= MINP ()))
                                {
                                    RPPC = (Arg2 & 0x0F)
                                    If (CondRefOf (\_PR.PR00._PPC, Local1))
                                    {
                                        \_PR.CPPC = RPPC /* \RPPC */
                                        ^^PCI0.LPCB.PPCC ()
                                    }

                                    Local0 = Zero
                                }
                            }

                        }
                    }

                }

                Return (Local0)
            }

            Method (MINP, 0, NotSerialized)
            {
                If ((PSTC == Zero))
                {
                    Local0 = PSTM /* External reference */
                }
                Else
                {
                    Local0 = ((PSTM + One) / 0x02)
                    If ((Local0 > 0x0F))
                    {
                        Local0 = 0x0F
                    }
                }

                Return (Local0)
            }

            Method (S008, 3, NotSerialized)
            {
                Local0 = 0x80000000
                If ((Arg0 == Zero))
                {
                    If ((Arg1 == Zero))
                    {
                        Local0 = Zero
                    }
                    ElseIf ((Arg1 == One))
                    {
                        If ((OSYS >= 0x07D6))
                        {
                            Local0 = 0x02
                        }
                        Else
                        {
                            Local0 = Zero
                        }
                    }
                    ElseIf (((Arg1 >= 0x02) && (Arg1 <= 0x05)))
                    {
                        Local0 = SSMI (Arg0, Arg1, Arg2, Zero)
                    }
                }
                ElseIf (((Arg0 >= One) && (Arg0 <= 0x02)))
                {
                    Local0 = SSMI (Arg0, Arg1, Arg2, Zero)
                }

                Return (Local0)
            }

            Method (S009, 3, NotSerialized)
            {
                Local0 = 0x80000000
                If ((Arg0 == 0x20))
                {
                    If ((Arg1 == One))
                    {
                        Local0 = HMAR ()
                    }
                    ElseIf ((Arg1 == 0x02))
                    {
                        Local0 = HMFX (Arg2)
                    }
                    Else
                    {
                        Local0 = 0x80000001
                    }
                }
                ElseIf ((Arg0 == 0x21))
                {
                    Local0 = HWMR (Arg1)
                }

                Return (Local0)
            }

            Method (HMAR, 0, NotSerialized)
            {
                Return (0x0101)
            }

            Method (HMFX, 1, NotSerialized)
            {
                If ((Arg0 == One))
                {
                    Local0 = 0x0100
                }
                ElseIf ((Arg0 == 0x02))
                {
                    Local0 = Zero
                }
                ElseIf ((Arg0 == 0x04))
                {
                    Local0 = Zero
                }
                ElseIf ((Arg0 == 0x08))
                {
                    Local0 = Zero
                }
                ElseIf ((Arg0 == 0x10))
                {
                    Local0 = Zero
                }
                ElseIf ((Arg0 == 0x20))
                {
                    Local0 = Zero
                }
                ElseIf ((Arg0 == 0x40))
                {
                    Local0 = Zero
                }
                ElseIf ((Arg0 == 0x80))
                {
                    Local0 = Zero
                }
                Else
                {
                    Local0 = 0x80000001
                }

                Return (Local0)
            }

            Method (HWMR, 1, NotSerialized)
            {
                Local2 = 0x80000001
                If ((Arg0 == One))
                {
                    Local1 = S006 (0x10, 0x0120, Zero)
                    S006 (0x11, 0x0120, 0xFF00)
                    Local3 = S006 (0x10, 0x0103, Zero)
                    S006 (0x11, 0x0103, (Local3 & 0xFFFFFFFC))
                    Sleep (0x0BB8)
                    ECG1 = 0x50
                    ECGM = 0x43
                    While ((ECGM != Zero))
                    {
                        Sleep (0x0A)
                    }

                    Local0 = Zero
                    Local0 = ((ECG2 << 0x08) | ECG3) /* \ECG3 */
                    If ((Local0 != Zero))
                    {
                        Local2 = (0x01C9C380 / Local0)
                    }
                    Else
                    {
                        Local2 = Zero
                    }

                    S006 (0x11, 0x0120, Local1)
                    S006 (0x11, 0x0103, Local3)
                }
                ElseIf ((Arg0 == 0x02))
                {
                    Local1 = S006 (0x10, 0x0120, Zero)
                    S006 (0x11, 0x0120, 0xFF00)
                    Local3 = S006 (0x10, 0x0103, Zero)
                    S006 (0x11, 0x0103, (Local3 & 0xFFFFFFFC))
                    Sleep (0x0BB8)
                    Local2 = S006 (0x10, 0x0151, Zero)
                    Local2 &= 0xFFFF
                    If ((Local2 != Zero))
                    {
                        Local2 = (0x01C9C380 / Local2)
                    }
                    Else
                    {
                        Local2 = Zero
                    }

                    S006 (0x11, 0x0120, Local1)
                    S006 (0x11, 0x0103, Local3)
                }
                Else
                {
                    If ((Arg0 == 0x0100))
                    {
                        Local2 = PDAT /* \PDAT */
                    }

                    If ((Arg0 == 0x0200))
                    {
                        Local2 = S006 (0x10, 0x0153, Zero)
                        Local2 &= 0xFF
                    }
                }

                Return (Local2)
            }

            Method (TMRR, 1, NotSerialized)
            {
                S006 (0x11, 0x0104, (Arg0 << 0x08))
                Local1 = Zero
                While (((S006 (0x10, 0x0100, Zero) & 0x8000) && (Local1 < 0x22)))
                {
                    Sleep (0x1E)
                    Local1++
                }

                Local2 = S006 (0x10, 0x0104, Zero)
                Local1 = Zero
                While (((S006 (0x10, 0x0100, Zero) & 0x8000) && (Local1 < 0x22)))
                {
                    Sleep (0x1E)
                    Local1++
                }

                Return (Local2)
            }

            Method (TMRW, 2, NotSerialized)
            {
                S006 (0x11, 0x0105, ((Arg0 << 0x08) | (Arg1 & 0xFF)
                    ))
                Local1 = Zero
                While (((S006 (0x10, 0x0100, Zero) & 0x8000) && (Local1 < 0x22)))
                {
                    Sleep (0x1E)
                    Local1++
                }
            }

            Method (S00A, 3, NotSerialized)
            {
                Local0 = 0x80000000
                Local1 = Arg0
                If ((AUCS == Zero))
                {
                    Return (Local0)
                }

                If ((Arg0 == Zero))
                {
                    Local0 = 0x0107
                }
                ElseIf (((Arg0 == One) | (Arg0 == 0x02)))
                {
                    Arg1 <<= 0x08
                    Local1 |= Arg1
                    Local0 = FSMI (0x88, Local1, Zero)
                }

                Return (Local0)
            }

            Method (S013, 3, Serialized)
            {
                Local0 = 0x80000000
                Switch (Arg0)
                {
                    Case (Zero)
                    {
                        Local0 = One
                    }
                    Case (One)
                    {
                        Local0 = Zero
                        Switch (Arg1)
                        {
                            Case (Zero)
                            {
                                If (HYBT)
                                {
                                    Local0 |= One
                                }
                            }
                            Case (One)
                            {
                                If ((Arg2 & One))
                                {
                                    HYBT = One
                                }
                                Else
                                {
                                    HYBT = Zero
                                }
                            }

                        }
                    }

                }

                Return (Local0)
            }

            Method (S015, 3, Serialized)
            {
                Local0 = 0x80000000
                Switch (Arg0)
                {
                    Case (0x04)
                    {
                    }
                    Case (0x05)
                    {
                    }

                }

                Return (Local0)
            }
        }

        Device (FJGI)
        {
            Name (_HID, EisaId ("FUJ0420"))  // _HID: Hardware ID
            Method (_DSM, 4, Serialized)  // _DSM: Device-Specific Method
            {
                If ((Arg0 == ToUUID ("6ab81a65-1149-4c2b-b0ef-a83e13f282f0") /* Unknown UUID */))
                {
                    Switch (ToInteger (Arg2))
                    {
                        Case (Zero)
                        {
                            Switch (ToInteger (Arg1))
                            {
                                Case (Zero)
                                {
                                    Return (Buffer (One)
                                    {
                                         0x03                                             // .
                                    })
                                }
                                Default
                                {
                                    Return (Buffer (One)
                                    {
                                         0x00                                             // .
                                    })
                                }

                            }
                        }
                        Case (One)
                        {
                            Switch (ToInteger (Arg1))
                            {
                                Case (Zero)
                                {
                                    CRRB = Arg3
                                    SYNC = Zero
                                    Local0 = Zero
                                    FSMI (0x96, Zero, Zero)
                                    While ((SYNC == Zero))
                                    {
                                        Sleep (One)
                                        Local0++
                                        If ((Local0 >= 0x1388))
                                        {
                                            Return (Ones)
                                        }
                                    }

                                    Return (Zero)
                                }
                                Default
                                {
                                    Return (Ones)
                                }

                            }
                        }
                        Default
                        {
                            Return (Ones)
                        }

                    }
                }
            }
        }

        Device (PWRB)
        {
            Name (_HID, EisaId ("PNP0C0C") /* Power Button Device */)  // _HID: Hardware ID
            Method (_STA, 0, NotSerialized)  // _STA: Status
            {
                Return (0x0F)
            }
        }

        Device (AC)
        {
            Name (_HID, "ACPI0003" /* Power Source Device */)  // _HID: Hardware ID
            Scope (\)
            {
                Name (ACPS, Ones)
            }

            Method (_INI, 0, NotSerialized)  // _INI: Initialize
            {
                ACPS = ACPW /* \ACPW */
                PWRS = ACPW /* \ACPW */
            }

            Method (_STA, 0, NotSerialized)  // _STA: Status
            {
                Return (0x0F)
            }

            Method (_PSR, 0, NotSerialized)  // _PSR: Power Source
            {
                _INI ()
                If (VBTT)
                {
                    Return (Zero)
                }

                If (ACPW)
                {
                    Return (One)
                }
                Else
                {
                    Return (Zero)
                }
            }

            Name (_PCL, Package (0x01)  // _PCL: Power Consumer List
            {
                _SB
            })
            Method (ACHK, 0, NotSerialized)
            {
                Local0 = ACPW /* \ACPW */
                If ((Local0 != ACPS))
                {
                    Sleep (0x28)
                    Sleep (0x0A)
                    Notify (AC, 0x80) // Status Change
                    PWRS = ACPW /* \ACPW */
                    PNOT ()
                    ACPS = Local0
                }
            }
        }

        Name (BIDT, Buffer (0x10)
        {
            /* 0000 */  0x00, 0x01, 0x02, 0x03, 0x00, 0x00, 0x00, 0x00,  // ........
            /* 0008 */  0x00, 0x00, 0x00, 0x00, 0x00, 0x04, 0x05, 0x06   // ........
        })
        Name (BMNT, Package (0x07)
        {
            "CP345705", 
            "CP345713", 
            "CP345721", 
            "CP345729", 
            "CP186215 / CP186216", 
            "CP245392", 
            "CP144820 / CP144821"
        })
        Name (BMST, Package (0x07)
        {
            "CP345705", 
            "CP345713", 
            "CP345721", 
            "CP345729", 
            "CP245376", 
            "CP245392", 
            "CP245408"
        })
        Device (CMB1)
        {
            Name (_HID, EisaId ("PNP0C0A") /* Control Method Battery */)  // _HID: Hardware ID
            Name (_UID, Zero)  // _UID: Unique ID
            Name (_SUN, Zero)  // _SUN: Slot User Number
            Name (_DDN, "0")  // _DDN: DOS Device Name
            Name (_PCL, Package (0x01)  // _PCL: Power Consumer List
            {
                _SB
            })
            Name (BUFF, Buffer (0x0C) {})
            Name (BUF2, Buffer (0x13) {})
            Scope (\)
            {
                Name (B1PS, Ones)
                Name (B1RS, Ones)
                Name (B1CS, Ones)
                Name (B1LS, Ones)
                Name (B1IS, Ones)
                Name (BC1S, Ones)
                Name (BIF1, Package (0x0D)
                {
                    One, 
                    Zero, 
                    Zero, 
                    One, 
                    Zero, 
                    Zero, 
                    Zero, 
                    One, 
                    One, 
                    "", 
                    "1", 
                    "LION", 
                    "Fujitsu"
                })
                Name (BIX1, Package (0x14)
                {
                    One, 
                    One, 
                    Zero, 
                    Zero, 
                    One, 
                    Zero, 
                    Zero, 
                    Zero, 
                    0xFFFFFFFF, 
                    0x00017318, 
                    0xFFFFFFFF, 
                    0xFFFFFFFF, 
                    0x88B8, 
                    0x61A8, 
                    One, 
                    One, 
                    "", 
                    "1", 
                    "LION", 
                    "Fujitsu"
                })
                Name (BST1, Package (0x04) {})
                Name (VBIF, Package (0x0D)
                {
                    One, 
                    0x16A8, 
                    0x16A8, 
                    One, 
                    0x3840, 
                    0x02CF, 
                    0x51, 
                    One, 
                    One, 
                    "Test Battery", 
                    "1", 
                    "LION", 
                    "Fujitsu"
                })
                Name (VBIX, Package (0x14)
                {
                    One, 
                    One, 
                    0x16A8, 
                    0x16A8, 
                    One, 
                    0x3840, 
                    0x02CF, 
                    0x51, 
                    0xFFFFFFFF, 
                    0x00017318, 
                    0xFFFFFFFF, 
                    0xFFFFFFFF, 
                    0x88B8, 
                    0x61A8, 
                    One, 
                    One, 
                    "Test Battery", 
                    "1", 
                    "LION", 
                    "Fujitsu"
                })
                Name (VRBS, Package (0x04)
                {
                    One, 
                    0x0CF0, 
                    0x1600, 
                    0x3B96
                })
            }

            Method (MKWD, 2, NotSerialized)
            {
                If ((Arg1 & 0x80))
                {
                    Local0 = (0xFFFF0000 | Arg0)
                    Local0 |= (Arg1 << 0x08)
                    Local0 = (Zero - Local0)
                }
                Else
                {
                    Local0 = Arg0
                    Local0 |= (Arg1 << 0x08)
                }

                Return (Local0)
            }

            Method (MBCD, 3, NotSerialized)
            {
                CreateByteField (Arg1, Arg2, BBF1)
                CreateByteField (Arg1, (Arg2 + One), BBF2)
                BBF1 = (0x30 + (Arg0 >> 0x04))
                BBF2 = (0x30 + (Arg0 & 0x0F))
            }

            Method (_INI, 0, NotSerialized)  // _INI: Initialize
            {
                B1PS = B1P /* \B1P_ */
                B1RS = B1CP /* \B1CP */
                B1CS = B1C /* \B1C_ */
                B1LS = MKWD (B1LL, B1LH)
                B1IS = B1ID /* \B1ID */
                BC1S = MKWD (BC1L, BC1H)
            }

            Method (_BIF, 0, NotSerialized)  // _BIF: Battery Information
            {
                If (VBTT)
                {
                    Return (VBIF) /* \VBIF */
                }

                BIF1 [One] = MKWD (B1DL, B1DH)
                BIF1 [0x02] = MKWD (B1LL, B1LH)
                BIF1 [0x04] = MKWD (B1OL, B1OH)
                Divide ((0x7C * DerefOf (BIF1 [One])), 0x03E8, Local0, 
                    BIF1 [0x05])
                If ((BIFL == 0xFF))
                {
                    Local1 = 0x03
                }
                Else
                {
                    Local1 = BIFL /* \BIFL */
                }

                Divide ((((Local1 * 0x0A) + 0x04) * DerefOf (BIF1 [
                    One])), 0x03E8, Local0, BIF1 [0x06])
                ^^FEXT.FUNC (0x80001006, 0x21, One, Zero)
                BIF1 [0x0A] = BUF2 /* \_SB_.CMB1.BUF2 */
                B1PS = B1P /* \B1P_ */
                B1LS = MKWD (B1LL, B1LH)
                B1IS = B1ID /* \B1ID */
                Return (BIF1) /* \BIF1 */
            }

            Method (_BIX, 0, NotSerialized)  // _BIX: Battery Information Extended
            {
                If (VBTT)
                {
                    Return (VBIX) /* \VBIX */
                }

                BIX1 [0x02] = MKWD (B1DL, B1DH)
                BIX1 [0x03] = MKWD (B1LL, B1LH)
                BIX1 [0x05] = MKWD (B1OL, B1OH)
                Divide ((0x7C * DerefOf (BIX1 [0x02])), 0x03E8, Local0, 
                    BIX1 [0x06])
                If ((BIFL == 0xFF))
                {
                    Local1 = 0x03
                }
                Else
                {
                    Local1 = BIFL /* \BIFL */
                }

                Divide ((((Local1 * 0x0A) + 0x04) * DerefOf (BIX1 [
                    0x02])), 0x03E8, Local0, BIX1 [0x07])
                ^^FEXT.FUNC (0x80001006, 0x21, One, Zero)
                BIX1 [0x11] = BUF2 /* \_SB_.CMB1.BUF2 */
                B1PS = B1P /* \B1P_ */
                B1LS = MKWD (B1LL, B1LH)
                B1IS = B1ID /* \B1ID */
                Return (BIX1) /* \BIX1 */
            }

            Method (_BST, 0, NotSerialized)  // _BST: Battery Status
            {
                If (VBTT)
                {
                    Return (VRBS) /* \VRBS */
                }

                B1RS = B1CP /* \B1CP */
                B1CS = B1C /* \B1C_ */
                Local0 = Zero
                If (B1P)
                {
                    If (B1C)
                    {
                        Local0 |= 0x02
                    }
                    ElseIf (!ACPS)
                    {
                        Local0 |= One
                    }

                    If ((B1CP <= One))
                    {
                        Local0 |= 0x04
                    }
                }

                Local1 = MKWD (B1CL, B1CH)
                If ((OSYS >= 0x07D6))
                {
                    Local2 = ((B1CP * MKWD (B1LL, B1LH)) + 0x32)
                    Local2 /= 0x64
                }
                Else
                {
                    Divide ((B1CP * MKWD (B1LL, B1LH)), 0x03E8, Local4, Local2)
                    If (Local4)
                    {
                        Local2++
                    }

                    Local2 *= 0x0A
                }

                Local3 = MKWD (B1VL, B1VH)
                BST1 [Zero] = Local0
                BST1 [One] = Local1
                BST1 [0x02] = Local2
                BST1 [0x03] = Local3
                Return (BST1) /* \BST1 */
            }

            Method (_STA, 0, NotSerialized)  // _STA: Status
            {
                If (VBTT)
                {
                    Return (0x1F)
                }

                If (B1P)
                {
                    Return (0x1F)
                }

                Return (0x0F)
            }

            Method (BCHK, 0, NotSerialized)
            {
                If ((B1P != B1PS))
                {
                    Notify (CMB1, 0x81) // Information Change
                    B1PS = B1P /* \B1P_ */
                }

                If (B1PS)
                {
                    Local0 = MKWD (B1LL, B1LH)
                    Local1 = MKWD (BC1L, BC1H)
                    If (((Local0 != B1LS) || (Local1 != BC1S)))
                    {
                        Notify (CMB1, 0x81) // Information Change
                    }

                    If (((B1C != B1CS) || (B1CP != B1RS)))
                    {
                        Notify (CMB1, 0x80) // Status Change
                    }

                    B1LS = Local0
                    B1CS = B1C /* \B1C_ */
                    B1RS = B1CP /* \B1CP */
                    BC1S = Local1
                }
            }

            Method (EBIF, 0, NotSerialized)
            {
                If (ECOK)
                {
                    ECB1 = Zero
                    ECBM = One
                    Local1 = Zero
                    While ((ECBM && (Local1 < 0x64)))
                    {
                        Sleep (One)
                        Local1++
                    }

                    If ((Local1 < 0x64))
                    {
                        If (ECB1)
                        {
                            BUF2 [Zero] = ECB1 /* \ECB1 */
                            BUF2 [One] = ECB2 /* \ECB2 */
                            BUF2 [0x02] = ECB3 /* \ECB3 */
                            BUF2 [0x03] = ECB4 /* \ECB4 */
                            BUF2 [0x04] = ECB5 /* \ECB5 */
                            BUF2 [0x05] = ECB6 /* \ECB6 */
                            BUF2 [0x06] = ECB7 /* \ECB7 */
                            BUF2 [0x07] = ECB8 /* \ECB8 */
                            BUF2 [0x08] = ECB9 /* \ECB9 */
                            BUF2 [0x09] = ECBA /* \ECBA */
                            BUF2 [0x0A] = ECBB /* \ECBB */
                            BUF2 [0x0B] = ECBC /* \ECBC */
                            BUF2 [0x0C] = ECBD /* \ECBD */
                            BUF2 [0x0D] = ECBE /* \ECBE */
                            BUF2 [0x0E] = ECBF /* \ECBF */
                            BUF2 [0x0F] = ECBG /* \ECBG */
                            BUF2 [0x10] = ECBH /* \ECBH */
                            BUF2 [0x11] = ECBI /* \ECBI */
                            BUF2 [0x12] = Zero
                            ECB1 = Zero
                            ECBM = 0x03
                            Local1 = Zero
                            While ((ECBM && (Local1 < 0x64)))
                            {
                                Sleep (One)
                                Local1++
                            }

                            If ((Local1 < 0x64))
                            {
                                BUFF [Zero] = ECB1 /* \ECB1 */
                                BUFF [One] = ECB2 /* \ECB2 */
                                BUFF [0x02] = ECB3 /* \ECB3 */
                                BUFF [0x03] = ECB4 /* \ECB4 */
                                BUFF [0x04] = ECB5 /* \ECB5 */
                                BUFF [0x05] = ECB6 /* \ECB6 */
                                BUFF [0x06] = ECB7 /* \ECB7 */
                                BUFF [0x07] = ECB8 /* \ECB8 */
                                BUFF [0x08] = ECB9 /* \ECB9 */
                                BUFF [0x09] = ECBA /* \ECBA */
                                BUFF [0x0A] = ECBB /* \ECBB */
                                BUFF [0x0B] = Zero
                                BIF1 [0x09] = BUFF /* \_SB_.CMB1.BUFF */
                                BIX1 [0x10] = BUFF /* \_SB_.CMB1.BUFF */
                            }
                        }
                    }

                    BIX1 [0x08] = MKWD (BC1L, BC1H)
                    BC1S = MKWD (BC1L, BC1H)
                }
            }
        }

        Device (LID)
        {
            Name (_HID, EisaId ("PNP0C0D") /* Lid Device */)  // _HID: Hardware ID
            Method (_LID, 0, NotSerialized)  // _LID: Lid Status
            {
                If (LCBM)
                {
                    ^^PCI0.GFX0.CLID = 0x03
                    Return (One)
                }

                If (!GGIV (0x02030003))
                {
                    ^^PCI0.GFX0.CLID = 0x03
                    Return (One)
                }
                Else
                {
                    ^^PCI0.GFX0.CLID = Zero
                    Return (Zero)
                }
            }

            Name (_PRW, Package (0x02)  // _PRW: Power Resources for Wake
            {
                0x23, 
                0x04
            })
            Method (_PSW, 1, NotSerialized)  // _PSW: Power State Wake
            {
                Noop
            }
        }
    }

