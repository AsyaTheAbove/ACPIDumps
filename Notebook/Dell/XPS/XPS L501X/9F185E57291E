            Device (P0P2)
            {
                Method (_ADR, 0, NotSerialized)  // _ADR: Address
                {
                    If ((PLMD == 0x02))
                    {
                        Return (0x00030000)
                    }
                    Else
                    {
                        Return (0x00010000)
                    }
                }

                Name (_PRW, Package (0x02)  // _PRW: Power Resources for Wake
                {
                    0x09, 
                    0x04
                })
                Method (_PRT, 0, NotSerialized)  // _PRT: PCI Routing Table
                {
                    If (PICM)
                    {
                        Return (AR82) /* \_SB_.AR82 */
                    }

                    Return (PR82) /* \_SB_.PR82 */
                }

                Device (PEGP)
                {
                    Name (_ADR, Zero)  // _ADR: Address
                    Name (_PRW, Package (0x02)  // _PRW: Power Resources for Wake
                    {
                        0x09, 
                        0x04
                    })
                    Name (NBL1, Package (0x10)
                    {
                        0x6E, 
                        0x94, 
                        0x16, 
                        0x5A, 
                        0x4E, 
                        0x9F, 
                        0x85, 
                        0xF7, 
                        0x3C, 
                        0x85, 
                        0x85, 
                        0xCF, 
                        0x42, 
                        0xF5, 
                        0xBC, 
                        0x14
                    })
                    Name (NBL2, Package (0x10)
                    {
                        0x16, 
                        0xED, 
                        0x3E, 
                        0x35, 
                        0x9E, 
                        0x7F, 
                        0xBC, 
                        0x5D, 
                        0x65, 
                        0x09, 
                        0x8E, 
                        0x0E, 
                        0xC0, 
                        0xFF, 
                        0x92, 
                        0xF9
                    })
                    Method (NV3D, 4, NotSerialized)
                    {
                        Name (_T_0, Zero)  // _T_x: Emitted by ASL Compiler, x=0-9, A-Z
                        If ((Arg1 != 0x0102))
                        {
                            Return (0x80000002)
                        }

                        _T_0 = ToInteger (Arg2)
                        If ((_T_0 == Zero))
                        {
                            P8XH (Zero, 0xE8)
                            Return (Buffer (0x04)
                            {
                                 0x01, 0x00, 0x40, 0x00                           // ..@.
                            })
                        }
                        ElseIf ((_T_0 == 0x16))
                        {
                            P8XH (Zero, 0xE9)
                            CreateWordField (Arg3, Zero, USRG)
                            If ((ToInteger (USRG) == 0x02))
                            {
                                If ((PLMD == One))
                                {
                                    Return (NBL1) /* \_SB_.PCI0.P0P2.PEGP.NBL1 */
                                }
                                Else
                                {
                                    Return (NBL2) /* \_SB_.PCI0.P0P2.PEGP.NBL2 */
                                }
                            }
                        }

                        Return (0x80000002)
                    }

                    Method (_DSM, 4, Serialized)  // _DSM: Device-Specific Method
                    {
                        If ((PLMD == One))
                        {
                            If ((Arg0 == ToUUID ("a486d8f8-0bda-471b-a72b-6042a6b5bee0") /* Unknown UUID */))
                            {
                                P8XH (Zero, 0xF5)
                                Return (NVOP) /* External reference */
                                Arg0
                                Arg1
                                Arg2
                                Arg3
                            }
                        }

                        If ((Arg0 == ToUUID ("d4a50b75-65c7-46f7-bfb7-41514cea0244") /* Unknown UUID */))
                        {
                            P8XH (Zero, 0xE7)
                            Return (NV3D (Arg0, Arg1, Arg2, Arg3))
                        }

                        Return (0x80000001)
                    }

                    Method (_DOD, 0, NotSerialized)  // _DOD: Display Output Devices
                    {
                        If ((PLMD == One))
                        {
                            Return (Package (0x01)
                            {
                                0x80007330
                            })
                        }
                        Else
                        {
                            Return (Package (0x03)
                            {
                                0x0110, 
                                0x80007330, 
                                0x80006340
                            })
                        }
                    }

                    Device (LCD)
                    {
                        Name (_ADR, 0x0110)  // _ADR: Address
                        Method (_DCS, 0, NotSerialized)  // _DCS: Display Current Status
                        {
                            If ((PLMD == One))
                            {
                                Return (Zero)
                            }
                            Else
                            {
                                Return (0x1F)
                            }
                        }

                        Name (_DGS, One)  // _DGS: Display Graphics State
                        Method (_DSS, 1, NotSerialized)  // _DSS: Device Set State
                        {
                            If ((PLMD == 0x02))
                            {
                                If ((Arg0 & 0x40000000))
                                {
                                    If ((Arg0 & One))
                                    {
                                        _DCS () = 0x1F
                                    }
                                    Else
                                    {
                                        _DCS () = 0x1D
                                    }
                                }
                            }
                        }

                        Method (_BCL, 0, NotSerialized)  // _BCL: Brightness Control Levels
                        {
                            Return (Package (0x12)
                            {
                                0x64, 
                                0x28, 
                                0x10, 
                                0x16, 
                                0x1C, 
                                0x22, 
                                0x28, 
                                0x2D, 
                                0x33, 
                                0x38, 
                                0x3E, 
                                0x43, 
                                0x49, 
                                0x4E, 
                                0x54, 
                                0x59, 
                                0x5F, 
                                0x64
                            })
                        }

                        Method (_BCM, 1, NotSerialized)  // _BCM: Brightness Control Method
                        {
                            If ((^^^^LPCB.EC0.EIDL == 0xDD)){}
                            Else
                            {
                                If (PWRS)
                                {
                                    If ((Arg0 == 0x10))
                                    {
                                        BRGA = Zero
                                    }

                                    If ((Arg0 == 0x16))
                                    {
                                        BRGA = One
                                    }

                                    If ((Arg0 == 0x1C))
                                    {
                                        BRGA = 0x02
                                    }

                                    If ((Arg0 == 0x22))
                                    {
                                        BRGA = 0x03
                                    }

                                    If ((Arg0 == 0x28))
                                    {
                                        BRGA = 0x04
                                    }

                                    If ((Arg0 == 0x2D))
                                    {
                                        BRGA = 0x05
                                    }

                                    If ((Arg0 == 0x33))
                                    {
                                        BRGA = 0x06
                                    }

                                    If ((Arg0 == 0x38))
                                    {
                                        BRGA = 0x07
                                    }

                                    If ((Arg0 == 0x3E))
                                    {
                                        BRGA = 0x08
                                    }

                                    If ((Arg0 == 0x43))
                                    {
                                        BRGA = 0x09
                                    }

                                    If ((Arg0 == 0x49))
                                    {
                                        BRGA = 0x0A
                                    }

                                    If ((Arg0 == 0x4E))
                                    {
                                        BRGA = 0x0B
                                    }

                                    If ((Arg0 == 0x54))
                                    {
                                        BRGA = 0x0C
                                    }

                                    If ((Arg0 == 0x59))
                                    {
                                        BRGA = 0x0D
                                    }

                                    If ((Arg0 == 0x5F))
                                    {
                                        BRGA = 0x0E
                                    }

                                    If ((Arg0 == 0x64))
                                    {
                                        BRGA = 0x0F
                                    }
                                }
                                Else
                                {
                                    If ((Arg0 == 0x10))
                                    {
                                        BRG1 = Zero
                                        BRG0 = Zero
                                    }

                                    If ((Arg0 == 0x16))
                                    {
                                        BRG1 = Zero
                                        BRG0 = One
                                    }

                                    If ((Arg0 == 0x1C))
                                    {
                                        BRG1 = Zero
                                        BRG0 = 0x02
                                    }

                                    If ((Arg0 == 0x22))
                                    {
                                        BRG1 = Zero
                                        BRG0 = 0x03
                                    }

                                    If ((Arg0 == 0x28))
                                    {
                                        BRG1 = One
                                        BRG0 = Zero
                                    }

                                    If ((Arg0 == 0x2D))
                                    {
                                        BRG1 = One
                                        BRG0 = One
                                    }

                                    If ((Arg0 == 0x33))
                                    {
                                        BRG1 = One
                                        BRG0 = 0x02
                                    }

                                    If ((Arg0 == 0x38))
                                    {
                                        BRG1 = One
                                        BRG0 = 0x03
                                    }

                                    If ((Arg0 == 0x3E))
                                    {
                                        BRG1 = 0x02
                                        BRG0 = Zero
                                    }

                                    If ((Arg0 == 0x43))
                                    {
                                        BRG1 = 0x02
                                        BRG0 = One
                                    }

                                    If ((Arg0 == 0x49))
                                    {
                                        BRG1 = 0x02
                                        BRG0 = 0x02
                                    }

                                    If ((Arg0 == 0x4E))
                                    {
                                        BRG1 = 0x02
                                        BRG0 = 0x03
                                    }

                                    If ((Arg0 == 0x54))
                                    {
                                        BRG1 = 0x03
                                        BRG0 = Zero
                                    }

                                    If ((Arg0 == 0x59))
                                    {
                                        BRG1 = 0x03
                                        BRG0 = One
                                    }

                                    If ((Arg0 == 0x5F))
                                    {
                                        BRG1 = 0x03
                                        BRG0 = 0x02
                                    }

                                    If ((Arg0 == 0x64))
                                    {
                                        BRG1 = 0x03
                                        BRG0 = 0x03
                                    }
                                }

                                PHSR (0x88)
                            }
                        }

                        Method (_BQC, 0, NotSerialized)  // _BQC: Brightness Query Current
                        {
                            If (PWRS)
                            {
                                If ((BRGA == Zero))
                                {
                                    Return (0x10)
                                }

                                If ((BRGA == One))
                                {
                                    Return (0x16)
                                }

                                If ((BRGA == 0x02))
                                {
                                    Return (0x1C)
                                }

                                If ((BRGA == 0x03))
                                {
                                    Return (0x22)
                                }

                                If ((BRGA == 0x04))
                                {
                                    Return (0x28)
                                }

                                If ((BRGA == 0x05))
                                {
                                    Return (0x2D)
                                }

                                If ((BRGA == 0x06))
                                {
                                    Return (0x33)
                                }

                                If ((BRGA == 0x07))
                                {
                                    Return (0x38)
                                }

                                If ((BRGA == 0x08))
                                {
                                    Return (0x3E)
                                }

                                If ((BRGA == 0x09))
                                {
                                    Return (0x43)
                                }

                                If ((BRGA == 0x0A))
                                {
                                    Return (0x49)
                                }

                                If ((BRGA == 0x0B))
                                {
                                    Return (0x4E)
                                }

                                If ((BRGA == 0x0C))
                                {
                                    Return (0x54)
                                }

                                If ((BRGA == 0x0D))
                                {
                                    Return (0x59)
                                }

                                If ((BRGA == 0x0E))
                                {
                                    Return (0x5F)
                                }

                                If ((BRGA == 0x0F))
                                {
                                    Return (0x64)
                                }
                            }
                            Else
                            {
                                Local0 = BRG1 /* \BRG1 */
                                Local0 <<= 0x02
                                Local0 |= BRG0 /* \BRG0 */
                                If ((Local0 == Zero))
                                {
                                    Return (0x10)
                                }

                                If ((Local0 == One))
                                {
                                    Return (0x16)
                                }

                                If ((Local0 == 0x02))
                                {
                                    Return (0x1C)
                                }

                                If ((Local0 == 0x03))
                                {
                                    Return (0x22)
                                }

                                If ((Local0 == 0x04))
                                {
                                    Return (0x28)
                                }

                                If ((Local0 == 0x05))
                                {
                                    Return (0x2D)
                                }

                                If ((Local0 == 0x06))
                                {
                                    Return (0x33)
                                }

                                If ((Local0 == 0x07))
                                {
                                    Return (0x38)
                                }

                                If ((Local0 == 0x08))
                                {
                                    Return (0x3E)
                                }

                                If ((Local0 == 0x09))
                                {
                                    Return (0x43)
                                }

                                If ((Local0 == 0x0A))
                                {
                                    Return (0x49)
                                }

                                If ((Local0 == 0x0B))
                                {
                                    Return (0x4E)
                                }

                                If ((Local0 == 0x0C))
                                {
                                    Return (0x54)
                                }

                                If ((Local0 == 0x0D))
                                {
                                    Return (0x59)
                                }

                                If ((Local0 == 0x0E))
                                {
                                    Return (0x5F)
                                }

                                If ((Local0 == 0x0F))
                                {
                                    Return (0x64)
                                }
                            }
                        }
                    }

                    Device (HDMI)
                    {
