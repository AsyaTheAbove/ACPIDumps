                        QA3E,   1, 
                        Offset (0x50), 
                        TOFS,   8, 
                        Offset (0x53), 
                        TTRT,   1, 
                        CCPU,   1, 
                        Offset (0x55), 
                        OTPC,   8, 
                        VGAT,   8, 
                        CHPT,   8, 
                        CPUT,   8, 
                        SYST,   8, 
                        DTS1,   8, 
                        DTS2,   8, 
                        Offset (0x5E), 
                        HWSN,   8, 
                        CTYP,   8, 
                        FCNT,   1, 
                        FTST,   1, 
                            ,   3, 
                        FADJ,   1, 
                        TCTL,   1, 
                        FATO,   1, 
