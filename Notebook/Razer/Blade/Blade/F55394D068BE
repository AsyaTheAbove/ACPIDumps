            If ((TBTS == One))
            {
                If (((SBNR == TBU0) || (SBNR == TBU1)))
                {
                    Return (Zero)
                }
                Else
                {
                    Return (HPCE) /* \_SB_.PCI0.RP17.HPCE */
                }
            }
            Else
            {
                Return (HPCE) /* \_SB_.PCI0.RP17.HPCE */
            }
        }
    }

