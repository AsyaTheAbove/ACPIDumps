            {
                Name (PSID, ToUUID ("bf60df52-56cd-454f-9a23-cb4dcfae77f8"))
                Name (PSEN, Package (0x02)
                {
                    "CecPsOnSwitchingEnabledInS0", 
                    One
                })
                Name (PSDS, Package (0x02)
                {
