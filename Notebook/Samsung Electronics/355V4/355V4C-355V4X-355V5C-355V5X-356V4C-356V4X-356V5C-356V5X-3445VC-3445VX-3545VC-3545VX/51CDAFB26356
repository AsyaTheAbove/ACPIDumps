            Mutex (FAMX, 0x00)
            Method (FANG, 1, NotSerialized)
            {
                Acquire (FAMX, 0xFFFF)
                ERIB = Arg0
                Local0 = ERBD /* \_SB_.PCI0.SBRG.EC0_.ERBD */
                Release (FAMX)
                Return (Local0)
            }

            Method (FANW, 2, NotSerialized)
            {
                Acquire (FAMX, 0xFFFF)
                ERIB = Arg0
                ERBD = Arg1
                Release (FAMX)
                Return (Arg1)
            }

            Method (TUVR, 1, NotSerialized)
            {
                Return (0x03)
            }

            Method (THRO, 1, NotSerialized)
            {
                ATHR (Arg0)
            }

            Method (CLCK, 1, NotSerialized)
            {
                Return (Zero)
            }

            Method (PCLK, 0, NotSerialized)
            {
                APCL ()
            }

            Method (ATHR, 1, NotSerialized)
            {
                If ((Arg0 == Zero))
                {
                    If ((\_PR.P000._PPC == Zero))
                    {
                        Return (One)
                    }
                    Else
                    {
                        Return (Zero)
                    }
                }
                Else
                {
                    If ((Arg0 == One))
                    {
                        Return (\_PR.P000._PPC) /* External reference */
                    }
                    Else
                    {
                        If ((Arg0 == 0x02))
                        {
                            Return (TTHR) /* \_SB_.PCI0.SBRG.EC0_.TTHR */
                        }
                        Else
                        {
                            Return (0xFF)
                        }
                    }
                }
            }

            Method (APCL, 0, NotSerialized)
            {
                Local0 = PTVL /* \_SB_.PCI0.SBRG.EC0_.PTVL */
                Local1 = PPC0 /* \PPC0 */
                Local2 = PPC1 /* \PPC1 */
                \_PR.P000.PPCV = Local0
                \_PR.P001.PPCV = Local0
                If ((MPEN >= 0x04))
                {
                    \_PR.P002.PPCV = Local0
                    \_PR.P003.PPCV = Local0
                }

                Notify (\_PR.P000, 0x80) // Performance Capability Change
            }

            Name (CMFP, Package (0x0B)
            {
                0x11, 
                0x12, 
                0x13, 
                0x14, 
                0x15, 
                0x16, 
                0x60, 
                0x61, 
                0x62, 
                0x63, 
                0x64
            })
            Mutex (CFMX, 0x00)
            Method (CFUN, 4, Serialized)
            {
                Name (ESRC, 0x05)
                If ((Match (CMFP, MEQ, DerefOf (Index (Arg0, Zero)), MTR, 
                    Zero, Zero) != Ones))
                {
                    Acquire (CFMX, 0xFFFF)
                    SMID = Arg0
                    SFNO = Arg1
                    BFDT = Arg2
                    SMIC = 0xCE
                    Release (CFMX)
                }
                Else
                {
                    If ((DerefOf (Index (Arg0, Zero)) == 0x10))
                    {
                        If ((DerefOf (Index (Arg1, Zero)) == One))
                        {
                            CreateByteField (Arg2, Zero, CAPV)
                            CAVR = CAPV /* \_SB_.PCI0.SBRG.EC0_.CFUN.CAPV */
                            STDT = One
                        }
                        Else
                        {
                            If ((DerefOf (Index (Arg1, Zero)) == 0x02))
                            {
                                Local0 = Buffer (0x80) {}
                                CreateByteField (Local0, Zero, BFD0)
                                BFD0 = 0x07
                                STDT = One
                                BFDT = Local0
                            }
                            Else
                            {
                                STDT = Zero
                            }
                        }
                    }
                    Else
                    {
                        If ((DerefOf (Index (Arg0, Zero)) == 0x18))
                        {
                            Acquire (CFMX, 0xFFFF)
                            If ((DerefOf (Index (Arg1, Zero)) == 0x02))
                            {
                                SMD0 = Zero
                                SMAD = DerefOf (Index (Arg2, One))
                                SMCM = DerefOf (Index (Arg2, 0x02))
                                SMPR = DerefOf (Index (Arg2, Zero))
                                While ((~(ESRC == Zero) && ~((SMST & 0x80
                                    ) == 0x80)))
                                {
                                    Sleep (0x14)
                                    ESRC -= One
                                }

                                Local2 = SMST /* \_SB_.PCI0.SBRG.EC0_.SMST */
                                If (((Local2 & 0x80) == 0x80))
                                {
                                    Local1 = Buffer (0x80) {}
                                    Index (Local1, Zero) = Local2
                                    If ((Local2 == 0x80))
                                    {
                                        P80H = 0xC4
                                        Index (Local1, One) = BCNT /* \_SB_.PCI0.SBRG.EC0_.BCNT */
                                        Local3 = SMD0 /* \_SB_.PCI0.SBRG.EC0_.SMD0 */
                                        Index (Local1, 0x02) = DerefOf (Index (Local3, Zero))
                                        Index (Local1, 0x03) = DerefOf (Index (Local3, One))
                                        Index (Local1, 0x04) = DerefOf (Index (Local3, 0x02))
                                        Index (Local1, 0x05) = DerefOf (Index (Local3, 0x03))
                                        Index (Local1, 0x06) = DerefOf (Index (Local3, 0x04))
                                        Index (Local1, 0x07) = DerefOf (Index (Local3, 0x05))
                                        Index (Local1, 0x08) = DerefOf (Index (Local3, 0x06))
                                        Index (Local1, 0x09) = DerefOf (Index (Local3, 0x07))
                                        Index (Local1, 0x0A) = DerefOf (Index (Local3, 0x08))
                                        Index (Local1, 0x0B) = DerefOf (Index (Local3, 0x09))
                                        Index (Local1, 0x0C) = DerefOf (Index (Local3, 0x0A))
                                        Index (Local1, 0x0D) = DerefOf (Index (Local3, 0x0B))
                                        Index (Local1, 0x0E) = DerefOf (Index (Local3, 0x0C))
                                        Index (Local1, 0x0F) = DerefOf (Index (Local3, 0x0D))
                                        Index (Local1, 0x10) = DerefOf (Index (Local3, 0x0E))
                                        Index (Local1, 0x11) = DerefOf (Index (Local3, 0x0F))
                                        Index (Local1, 0x12) = DerefOf (Index (Local3, 0x10))
                                        Index (Local1, 0x13) = DerefOf (Index (Local3, 0x11))
                                        Index (Local1, 0x14) = DerefOf (Index (Local3, 0x12))
                                        Index (Local1, 0x15) = DerefOf (Index (Local3, 0x13))
                                        Index (Local1, 0x16) = DerefOf (Index (Local3, 0x14))
                                        Index (Local1, 0x17) = DerefOf (Index (Local3, 0x15))
                                        Index (Local1, 0x18) = DerefOf (Index (Local3, 0x16))
                                        Index (Local1, 0x19) = DerefOf (Index (Local3, 0x17))
                                        Index (Local1, 0x1A) = DerefOf (Index (Local3, 0x18))
                                        Index (Local1, 0x1B) = DerefOf (Index (Local3, 0x19))
                                        Index (Local1, 0x1C) = DerefOf (Index (Local3, 0x1A))
                                        Index (Local1, 0x1D) = DerefOf (Index (Local3, 0x1B))
                                        Index (Local1, 0x1E) = DerefOf (Index (Local3, 0x1C))
                                        Index (Local1, 0x1F) = DerefOf (Index (Local3, 0x1D))
                                        Index (Local1, 0x20) = DerefOf (Index (Local3, 0x1E))
                                        Index (Local1, 0x21) = DerefOf (Index (Local3, 0x1F))
                                    }

                                    BFDT = Local1
                                    STDT = One
                                }
                                Else
                                {
                                    P80H = 0xC5
                                    STDT = Zero
                                }
                            }
                            Else
                            {
                                P80H = 0xC6
                                STDT = Zero
                            }

                            Release (CFMX)
                        }
                        Else
                        {
                            STDT = Zero
                        }
                    }
                }
            }

            Mutex (PFMX, 0x00)
            Method (PFUN, 4, Serialized)
            {
                Acquire (PFMX, 0xFFFF)
                SMID = Arg0
                SFNO = Arg1
                BFDT = Arg2
                SMIC = 0xCF
                Release (PFMX)
                STDT = Zero
            }

            Method (CMFC, 4, Serialized)
            {
                Name (RTVL, Package (0x04)
                {
                    Zero, 
                    Zero, 
                    Zero, 
                    Zero
                })
                If ((ObjectType (Arg0) == One))
                {
                    Index (RTVL, Zero) = 0x8000
                }
                Else
                {
                    If ((DerefOf (Index (Arg0, Zero)) < 0xFE))
                    {
                        Local1 = Buffer (0x82) {}
                        CreateWordField (Local1, Zero, RTST)
                        CreateField (Local1, 0x10, 0x0400, RTDT)
                        If ((ToInteger (Arg0) <= 0x7F))
                        {
                            CFUN (Arg0, Arg1, Arg2, Arg3)
                        }
                        Else
                        {
                            PFUN (Arg0, Arg1, Arg2, Arg3)
                        }

                        RTST = STDT /* \STDT */
                        RTDT = BFDT /* \BFDT */
                        Return (Local1)
                    }
                    Else
                    {
                        Index (RTVL, Zero) = 0x8000
                    }
                }

                Return (RTVL) /* \_SB_.PCI0.SBRG.EC0_.CMFC.RTVL */
            }
        }

        Scope (EC0)
        {
            Method (BSHK, 0, NotSerialized)
            {
            }
        }

        Device (BAT1)
        {
            Name (_HID, EisaId ("PNP0C0A") /* Control Method Battery */)  // _HID: Hardware ID
            Name (_UID, One)  // _UID: Unique ID
            Name (_PCL, Package (0x01)  // _PCL: Power Consumer List
            {
                _SB
            })
            Name (BMDL, Zero)
            Name (BASC, One)
            Method (_STA, 0, NotSerialized)  // _STA: Status
            {
                ^^EC0.BSHK ()
                If (ECOK ())
                {
                    If (^^EC0.BOL0)
                    {
                        Sleep (0x14)
                        Return (0x1F)
                    }
                    Else
                    {
                        Sleep (0x14)
                        Return (0x0F)
                    }
                }
                Else
                {
                    Sleep (0x14)
                    Return (0x1F)
                }
            }

            Method (_BIF, 0, NotSerialized)  // _BIF: Battery Information
            {
                Name (STAT, Package (0x0D)
                {
                    One, 
                    0x1770, 
                    0x1770, 
                    One, 
                    0x2A30, 
                    0x0258, 
                    0xB4, 
                    0x0108, 
                    0x0EC4, 
                    "PABAS0241231", 
                    "41167", 
                    "Li-Ion", 
                    "COMPAL "
                })
                Index (STAT, 0x02) = ^^EC0.BFC0 /* \_SB_.PCI0.SBRG.EC0_.BFC0 */
                Index (STAT, Zero) = ^^EC0.BAM0 /* \_SB_.PCI0.SBRG.EC0_.BAM0 */
                If (ECOK ())
                {
                    If ((^^EC0.BAM0 == Zero))
                    {
                        BASC = 0x0A
                    }

                    If ((^^EC0.ORRF == One))
                    {
                        ^^EC0.BFCB = ^^EC0.BFC0 /* \_SB_.PCI0.SBRG.EC0_.BFC0 */
                    }

                    Local0 = ^^EC0.BDN0 /* \_SB_.PCI0.SBRG.EC0_.BDN0 */
                    BMDL = Local0
                    Index (STAT, One) = (^^EC0.BDC0 * BASC)
                    Sleep (0x14)
                    Index (STAT, 0x04) = ^^EC0.BDV0 /* \_SB_.PCI0.SBRG.EC0_.BDV0 */
                    Sleep (0x14)
                    Local2 = (^^EC0.BFC0 * BASC)
                    Sleep (0x14)
                    Index (STAT, 0x02) = Local2
                    Divide (Local2, 0x64, Local0, Local1)
                    Local1 *= 0x0A
                    Index (STAT, 0x05) = Local1
                    Divide (Local2, 0x64, Local0, Local1)
                    Local1 *= 0x03
                    Index (STAT, 0x06) = Local1
                }

                Return (STAT) /* \_SB_.PCI0.SBRG.BAT1._BIF.STAT */
            }

            Method (_BST, 0, NotSerialized)  // _BST: Battery Status
            {
                Name (PBST, Package (0x04)
                {
                    Zero, 
                    0xFFFFFFFF, 
                    0xFFFFFFFF, 
                    0x1770
                })
                If (ECOK ())
                {
                    Index (PBST, Zero) = ^^EC0.BST0 /* \_SB_.PCI0.SBRG.EC0_.BST0 */
                    Sleep (0x14)
                    Local4 = ^^EC0.BAC0 /* \_SB_.PCI0.SBRG.EC0_.BAC0 */
                    Local4 -= 0xFFFF
                    Index (PBST, One) = Local4
                    Sleep (0x14)
                    If ((^^EC0.ORRF == One))
                    {
                        Index (PBST, 0x02) = (^^EC0.OBRC * BASC)
                        Sleep (0x14)
                    }
                    Else
                    {
                        Index (PBST, 0x02) = (^^EC0.BRC0 * BASC)
                        Sleep (0x14)
                    }

                    Index (PBST, 0x03) = ^^EC0.BPV0 /* \_SB_.PCI0.SBRG.EC0_.BPV0 */
                    Sleep (0x14)
                    If ((^^EC0.BDN0 != BMDL))
                    {
                        Notify (BAT1, 0x81) // Information Change
                    }
                }

                Return (PBST) /* \_SB_.PCI0.SBRG.BAT1._BST.PBST */
            }
        }

        Device (ACAD)
        {
            Name (_HID, "ACPI0003" /* Power Source Device */)  // _HID: Hardware ID
            Name (_PCL, Package (0x01)  // _PCL: Power Consumer List
            {
                _SB
            })
            Method (_PSR, 0, NotSerialized)  // _PSR: Power Source
            {
                Local0 = ^^EC0.ADPT /* \_SB_.PCI0.SBRG.EC0_.ADPT */
                AHKS (Local0)
                Return (Local0)
            }
        }
    }

    OperationRegion (SNVS, SystemMemory, 0x8EA1AA98, 0x034B)
    Field (SNVS, AnyAcc, Lock, Preserve)
    {
        SIGN,   64, 
        SVER,   8, 
        SREV,   8, 
        SIZE,   32, 
        CVIP,   16, 
        SDSP,   32, 
        OSTP,   16, 
        ISSG,   8, 
        VIGD,   16, 
        IGMM,   64, 
        VEGD,   16, 
        EGMM,   64
    }

    Scope (_SB.PCI0.SBRG)
    {
        OperationRegion (LPEC, SystemIO, 0xFD60, 0x04)
        Field (LPEC, ByteAcc, NoLock, Preserve)
        {
            ECFA,   8, 
            ECFB,   16, 
            ECFD,   8
        }

        Mutex (PHMX, 0x00)
        Method (PHMR, 1, NotSerialized)
        {
            Acquire (PHMX, 0xFFFF)
            If ((STPH != 0xAA))
            {
                ECFB = Arg0
                Local0 = ECFD /* \_SB_.PCI0.SBRG.ECFD */
                ECFB = 0x23F4
            }
            Else
            {
                Local0 = Zero
            }

            Release (PHMX)
            Return (Local0)
        }

        Method (PHMW, 2, NotSerialized)
        {
            Acquire (PHMX, 0xFFFF)
            If ((STPH != 0xAA))
            {
                ECFB = Arg0
                ECFD = Arg1
                ECFB = 0x23F4
            }

            Release (PHMX)
            Return (Arg1)
        }

        Method (EIOR, 3, NotSerialized)
        {
            Local1 = Arg0
            Local2 = Arg1
            Local3 = Arg2
            Local0 = Zero
            Local4 = (Local3 + 0x0100)
            Local5 = (Local1 << Local2)
            If ((Local5 & 0xFFFFFFFFFFFFFF00))
            {
                Local0 = (PHMR (Local4) << 0x08)
            }

            Local0 |= PHMR (Local3)
            Local0 = ((Local0 >> Local2) & Local1)
            Return (Local0)
        }

        Method (EIOW, 4, NotSerialized)
        {
            Local1 = Arg1
            Local2 = Arg2
            Local3 = Arg3
            Local4 = (Local3 + 0x0100)
            Local5 = (Local1 << Local2)
            Local6 = ((Arg0 << Local2) & Local5)
            If ((Local5 & 0xFFFFFFFFFFFFFF00))
            {
                Local0 = (PHMR (Local4) & ~(Local5 >> 0x08))
                Local0 |= (Local6 >> 0x08)
                PHMW (Local4, Local0)
            }

            Local0 = (((PHMR (Local3) & ~Local5) | Local6) & 
                0xFF)
            PHMW (Local3, Local0)
        }

        Method (ENUR, 2, NotSerialized)
        {
            Local0 = Zero
            Local2 = Arg1
            While (Local2)
            {
                Local2 -= One
                Local3 = (Local2 * 0x08)
                Local1 = ((Local2 << 0x08) + Arg0)
                Local0 |= (PHMR (Local1) << Local3)
            }

            Return (Local0)
        }

        Method (ENUW, 3, NotSerialized)
        {
            Local2 = Arg1
            While (Local2)
            {
                Local2 -= One
                Local3 = (Local2 << 0x08)
                Local1 = (Local3 + Arg0)
                Local4 = (Local2 * 0x08)
                Local5 = (0xFF << Local4)
                Local0 = ((Arg2 & Local5) >> Local4)
                PHMW (Local1, Local0)
            }

            Return (Local2)
        }

        Method (ERBK, 2, NotSerialized)
        {
            Local2 = Arg1
            Name (ERBF, Buffer (Local2) {})
            While (Local2)
            {
                Local2 -= One
                Local1 = ((Local2 << 0x08) + Arg0)
                Index (ERBF, Local2) = PHMR (Local1)
            }

            Return (ERBF) /* \_SB_.PCI0.SBRG.ERBK.ERBF */
        }
    }

    OperationRegion (TOSN, SystemMemory, 0x8EA1B918, 0x0400)
    Field (TOSN, AnyAcc, Lock, Preserve)
    {
        ONVS,   32, 
        BRAD,   8, 
        VVEN,   16, 
        CIRE,   8, 
        PJID,   8, 
        Offset (0x10), 
        HDPT,   8, 
        CEV1,   32, 
        CEV2,   32, 
        CEV3,   32, 
        CEVD,   8, 
        CEVN,   8, 
        CCM1,   32, 
        CCM2,   32, 
        CCM3,   32, 
        CCM4,   32, 
        CCMD,   8, 
        CCMN,   8, 
        CME1,   32, 
        CME2,   32, 
        CME3,   32, 
        CME4,   32, 
        CMEN,   8, 
        CEEV,   16, 
        CECM,   8, 
        Offset (0x50), 
        ALMF,   8, 
        ALMM,   8, 
        ALMH,   8, 
        ALMD,   8, 
        ALMO,   8, 
        ALMY,   8, 
        Offset (0x58), 
        TOUD,   8, 
        TPID,   8, 
        Offset (0x60), 
        ZPOD,   8, 
        BDMO,   8, 
        OEJL,   8, 
        DTPE,   8, 
        SATM,   8, 
        Offset (0x68), 
        BGU1,   8, 
        BST1,   8, 
        BFC1,   16, 
        BDN1,   8, 
        Offset (0x70), 
        OSYS,   16, 
        PPC0,   8, 
        PPC1,   8, 
        LPTP,   8, 
        FDCP,   8, 
        CSTE,   16, 
        CADL,   8, 
        BRTL,   8, 
        MPEN,   8, 
        Offset (0x80), 
        ODT1,   8, 
        ODT2,   8, 
        Offset (0x8C), 
        DTSW,   8, 
        WKLN,   8, 
        WAKF,   8, 
        US3W,   8, 
        PPCS,   8, 
        PPPM,   8, 
        CPUN,   8, 
        Offset (0x96), 
        IDMN,   16, 
        IDPC,   16, 
        HORZ,   16, 
        VERT,   16, 
        Offset (0xA0), 
        OES1,   8, 
        OES2,   8, 
        Offset (0xA8), 
        BTEN,   8, 
        BTON,   8, 
        Offset (0xAC), 
        LED1,   8, 
        LED2,   8, 
        Offset (0xB0), 
        SATN,   8, 
        TMFA,   8, 
        NV3D,   8, 
        DSMD,   8, 
        BAYS,   8, 
        JETD,   8, 
        STPH,   8, 
        HAPE,   8, 
        IINF,   8, 
        VBRI,   8, 
        BC5P,   8, 
        Offset (0xBC), 
        OA3S,   8, 
        Offset (0xBE), 
        WPDK,   8, 
        Offset (0xC0), 
        DPK1,   32, 
        DPK2,   32, 
        DPK3,   32, 
        DPK4,   32, 
        DPK5,   32, 
        DPK6,   32, 
        DPK7,   32, 
        DPK8,   32, 
        DPK9,   32, 
        DPKA,   32, 
        DPKB,   32, 
        DPKC,   32, 
        DPKD,   8, 
        DKEY,   32, 
        CHKS,   32, 
        Offset (0x100), 
        HWSF,   16, 
        HWSD,   16, 
        HWBD,   1024, 
        Offset (0x240), 
        SBMF,   32, 
        SBSF,   32, 
        SBCF,   32, 
        SBB1,   32, 
        SBB2,   32, 
        SBB3,   32, 
        Offset (0x300)
    }

    Scope (_SB.PCI0.SBRG.EC0)
    {
        Method (EHKS, 1, NotSerialized)
        {
            If ((Arg0 == 0x03))
            {
                ECRD = One
                OPEC ()
            }

            EIOW (Zero, One, Zero, 0x86F7)
        }
    }

    Scope (_SB.PCI0.SBRG.ACAD)
    {
        Name (XX00, Buffer (0x03) {})
        Method (AHKS, 1, NotSerialized)
        {
            CreateWordField (XX00, Zero, SSZE)
            CreateByteField (XX00, 0x02, ACST)
            SSZE = 0x03
            If (Arg0)
            {
                P80H = 0xAC
                ^^^VGA.AFN4 (One)
                ACST = Zero
            }
            Else
            {
                P80H = 0xDC
                ^^^VGA.AFN4 (0x02)
                ACST = One
            }

            ALIB (One, XX00)
            If ((Arg0 == Zero))
            {
                If (((G1EI == One) && (G1FI == One)))
                {
                    Notify (PCI0, Zero) // Bus Check
                }
            }
        }
    }

    Scope (_SB.PCI0.OHC1)
    {
        Method (_PRW, 0, NotSerialized)  // _PRW: Power Resources for Wake
        {
            If (US3W)
            {
                Return (Package (0x02)
                {
                    0x0B, 
                    0x03
                })
            }
            Else
            {
                Return (Package (0x02)
                {
                    0x0B, 
                    Zero
                })
            }
        }

        Method (_PSW, 1, NotSerialized)  // _PSW: Power State Wake
        {
            If (Arg0)
            {
                ^^SBRG.EIOW (One, One, 0x05, 0xA4F4)
            }
            Else
            {
                ^^SBRG.EIOW (Zero, One, 0x05, 0xA4F4)
            }
        }
    }

