    Scope (_TZ)
    {
        ThermalZone (TZ00)
        {
            Method (_TMP, 0, NotSerialized)  // _TMP: Temperature
            {
                If (BC5P)
                {
                    BC5P = Zero
                    Return (0x127C)
                }

                Local0 = \_SB.PCI0.SBRG.EIOR (0xFF, Zero, 0xB0F7)
                Return ((0x0AAC + (Local0 * 0x0A)))
            }

            Method (_HOT, 0, NotSerialized)  // _HOT: Hot Temperature
            {
                Return (0x1218)
            }

            Method (_CRT, 0, NotSerialized)  // _CRT: Critical Temperature
            {
                Return (0x12E0)
            }
        }
    }

    Scope (_SB)
    {
        Scope (\_SB)
        {
            OperationRegion (ASMI, SystemIO, 0xB0, 0x02)
            Field (ASMI, ByteAcc, NoLock, Preserve)
            {
                SMIC,   8, 
                SMID,   8
            }

            Mutex (CSMX, 0x00)
            Method (CSMI, 1, NotSerialized)
            {
                Acquire (CSMX, 0xFFFF)
                SMID = Arg0
                SMIC = 0xEE
                Release (CSMX)
            }
        }

        Scope (\_GPE)
        {
            Method (_L16, 0, NotSerialized)  // _Lxx: Level-Triggered GPE
            {
                If (GE22)
                {
                    E22C = Zero
                }
                Else
                {
                    E22C = One
                }

                E22S = One
                Notify (\_SB.LID, 0x80) // Status Change
            }
        }

        Scope (\_SB)
        {
            Device (LID)
            {
                Name (_HID, EisaId ("PNP0C0D") /* Lid Device */)  // _HID: Hardware ID
                Method (_LID, 0, NotSerialized)  // _LID: Lid Status
                {
                    Return (GE22) /* \GE22 */
                }

                Name (_PRW, Package (0x02)  // _PRW: Power Resources for Wake
                {
                    0x16, 
                    0x03
                })
            }
        }
    }

    Scope (_SB.PCI0.BR14.LOM)
    {
        Method (_PSW, 1, NotSerialized)  // _PSW: Power State Wake
        {
            If (Arg0)
            {
                ^^^SBRG.EIOW (One, One, 0x02, 0xA4F4)
            }
            Else
            {
                ^^^SBRG.EIOW (Zero, One, 0x02, 0xA4F4)
            }
        }
    }

    Scope (_SB.PCI0.SBRG)
    {
        Device (PS2M)
        {
            Name (_HID, EisaId ("PNP0F03") /* Microsoft PS/2-style Mouse */)  // _HID: Hardware ID
            Name (_CID, EisaId ("PNP0F13") /* PS/2 Mouse */)  // _CID: Compatible ID
            Method (_STA, 0, NotSerialized)  // _STA: Status
            {
                If ((IOST & 0x4000))
                {
                    Return (0x0F)
                }
                Else
                {
                    Return (Zero)
                }
            }

            Name (CRS1, ResourceTemplate ()
            {
                IRQNoFlags ()
                    {12}
            })
            Name (CRS2, ResourceTemplate ()
            {
                IO (Decode16,
                    0x0060,             // Range Minimum
                    0x0060,             // Range Maximum
                    0x00,               // Alignment
                    0x01,               // Length
                    )
                IO (Decode16,
                    0x0064,             // Range Minimum
                    0x0064,             // Range Maximum
                    0x00,               // Alignment
                    0x01,               // Length
                    )
                IRQNoFlags ()
                    {12}
            })
            Method (_CRS, 0, NotSerialized)  // _CRS: Current Resource Settings
            {
                If ((IOST & 0x0400))
                {
                    Return (CRS1) /* \_SB_.PCI0.SBRG.PS2M.CRS1 */
                }
                Else
                {
                    Return (CRS2) /* \_SB_.PCI0.SBRG.PS2M.CRS2 */
                }
            }

            Name (_PRS, ResourceTemplate ()  // _PRS: Possible Resource Settings
            {
                StartDependentFn (0x00, 0x00)
                {
                    IRQNoFlags ()
                        {12}
                }
                EndDependentFn ()
            })
            Method (_PSW, 1, NotSerialized)  // _PSW: Power State Wake
            {
                MSFG = Arg0
            }
        }

        Scope (\)
        {
            Name (MSFG, One)
        }

        Device (PS2K)
        {
            Name (_HID, EisaId ("PNP0303") /* IBM Enhanced Keyboard (101/102-key, PS/2 Mouse) */)  // _HID: Hardware ID
            Name (_CID, EisaId ("PNP030B"))  // _CID: Compatible ID
            Method (_STA, 0, NotSerialized)  // _STA: Status
            {
                Return (0x0F)
            }

            Name (_CRS, ResourceTemplate ()  // _CRS: Current Resource Settings
            {
                IO (Decode16,
                    0x0060,             // Range Minimum
                    0x0060,             // Range Maximum
                    0x00,               // Alignment
                    0x01,               // Length
                    )
                IO (Decode16,
                    0x0064,             // Range Minimum
                    0x0064,             // Range Maximum
                    0x00,               // Alignment
                    0x01,               // Length
                    )
                IRQNoFlags ()
                    {1}
            })
            Name (_PRS, ResourceTemplate ()  // _PRS: Possible Resource Settings
            {
                StartDependentFn (0x00, 0x00)
                {
                    FixedIO (
                        0x0060,             // Address
                        0x01,               // Length
                        )
                    FixedIO (
                        0x0064,             // Address
                        0x01,               // Length
                        )
                    IRQNoFlags ()
                        {1}
                }
                EndDependentFn ()
            })
            Method (_PSW, 1, NotSerialized)  // _PSW: Power State Wake
            {
                KBFG = Arg0
            }
        }

        Scope (\)
        {
            Name (KBFG, One)
        }
    }

    Scope (_SB.PCI0.SBRG.EC0)
    {
        Method (OPEC, 0, NotSerialized)
        {
            If ((OSYS >= 0x07D9))
            {
                EIOW (0x03, 0x07, Zero, 0xA3F4)
            }
            Else
            {
                If ((OSYS >= 0x07D6))
                {
                    EIOW (One, 0x07, Zero, 0xA3F4)
                    OSTY = One
                }
                Else
                {
                    If ((OSYS == 0x03E8))
                    {
                        EIOW (0x02, 0x07, Zero, 0xA3F4)
                    }
                    Else
                    {
                        If ((OSYS < 0x07D6))
                        {
                            If ((OSYS != Zero))
                            {
                                EIOW (Zero, 0x07, Zero, 0xA3F4)
                            }
                        }
                    }
                }
            }
        }

        Scope (\_SB)
        {
            Mutex (KSMX, 0x00)
            Method (KSMI, 1, NotSerialized)
            {
                Acquire (KSMX, 0xFFFF)
                SMID = Arg0
                SMIC = 0xD9
                Release (KSMX)
            }
        }

        Scope (\_SB.PCI0.SBRG.EC0)
        {
            Method (VAFC, 6, NotSerialized)
            {
                Return (SABI (Arg0, Arg1, Arg2, Arg3, Arg4, Arg5))
            }
        }

        Scope (\_SB)
        {
            Method (SABI, 6, NotSerialized)
            {
                Name (TSFR, Package (0x06) {})
                Index (TSFR, Zero) = Zero
                Index (TSFR, One) = Zero
                Index (TSFR, 0x02) = Zero
                Index (TSFR, 0x03) = Zero
                Index (TSFR, 0x04) = Zero
                Index (TSFR, 0x05) = Zero
                CreateDWordField (Arg0, Zero, SAB0)
                CreateDWordField (Arg1, Zero, SAB1)
                CreateDWordField (Arg2, Zero, SAB2)
                CreateDWordField (Arg3, Zero, SAB3)
                CreateDWordField (Arg4, Zero, SAB4)
                CreateDWordField (Arg5, Zero, SAB5)
                If ((SAB0 == 0x9900))
                {
                    ^PCI0.SBRG.EIOW (SAB1, 0xFF, Zero, 0x21FB)
                    ^PCI0.SBRG.EIOW (SAB2, 0xFF, Zero, 0x22FB)
                    ^PCI0.SBRG.EIOW (SAB3, 0xFF, Zero, 0x23FB)
                    KSMI (0x02)
                    Local2 = ^PCI0.SBRG.EIOR (0xFF, Zero, 0x20FB)
                    Local3 = ^PCI0.SBRG.EIOR (0xFF, Zero, 0x24FB)
                    Index (TSFR, Zero) = SAB0 /* \_SB_.SABI.SAB0 */
                    Index (TSFR, One) = SAB1 /* \_SB_.SABI.SAB1 */
                    Index (TSFR, 0x02) = Local2
                    Index (TSFR, 0x03) = Local3
                    Index (TSFR, 0x04) = SAB4 /* \_SB_.SABI.SAB4 */
                    Index (TSFR, 0x05) = SAB5 /* \_SB_.SABI.SAB5 */
                    Return (TSFR) /* \_SB_.SABI.TSFR */
                }

                SBMF = SAB0 /* \_SB_.SABI.SAB0 */
                SBSF = SAB1 /* \_SB_.SABI.SAB1 */
                SBCF = SAB2 /* \_SB_.SABI.SAB2 */
                SBB1 = SAB3 /* \_SB_.SABI.SAB3 */
                SBB2 = SAB4 /* \_SB_.SABI.SAB4 */
                SBB3 = SAB5 /* \_SB_.SABI.SAB5 */
                KSMI (Zero)
                Index (TSFR, Zero) = SBMF /* \SBMF */
                Index (TSFR, One) = SBSF /* \SBSF */
                Index (TSFR, 0x02) = SBCF /* \SBCF */
                Index (TSFR, 0x03) = SBB1 /* \SBB1 */
                Index (TSFR, 0x04) = SBB2 /* \SBB2 */
                Index (TSFR, 0x05) = SBB3 /* \SBB3 */
                Return (TSFR) /* \_SB_.SABI.TSFR */
            }
        }

        Method (_Q11, 0, NotSerialized)  // _Qxx: EC Query
        {
            Debug = "=====QUERY_11====="
            If ((OSYS >= 0x07D6))
            {
                Notify (^^^VGA.LCD, 0x87) // Device-Specific
            }
            Else
            {
                Local0 = ^^^VGA.LCD._BQC ()
                ^^^VGA.LCD._BCM (Local0)
            }
        }

        Method (_Q12, 0, NotSerialized)  // _Qxx: EC Query
        {
            Debug = "=====QUERY_12====="
            If ((OSYS >= 0x07D6))
            {
                Notify (^^^VGA.LCD, 0x86) // Device-Specific
            }
            Else
            {
                Local0 = ^^^VGA.LCD._BQC ()
                ^^^VGA.LCD._BCM (Local0)
            }
        }

        Method (_Q1D, 0, NotSerialized)  // _Qxx: EC Query
        {
            PCLK ()
        }

        Method (_Q1E, 0, NotSerialized)  // _Qxx: EC Query
        {
            PCLK ()
        }

        Method (_Q25, 0, NotSerialized)  // _Qxx: EC Query
        {
            Debug = "=====QUERY_25====="
            Sleep (0x03E8)
            Notify (BAT1, 0x81) // Information Change
            Sleep (0x03E8)
            Notify (BAT1, 0x80) // Status Change
        }

        Method (_Q37, 0, NotSerialized)  // _Qxx: EC Query
        {
            Notify (ACAD, 0x80) // Status Change
            Sleep (0x03E8)
            Notify (BAT1, 0x80) // Status Change
        }

        Method (_Q38, 0, NotSerialized)  // _Qxx: EC Query
        {
            Notify (ACAD, 0x80) // Status Change
            Sleep (0x03E8)
            Notify (BAT1, 0x80) // Status Change
        }

        Method (_Q41, 0, NotSerialized)  // _Qxx: EC Query
        {
            Debug = "=====QUERY_41====="
            Local0 = ^^^VGA.LCD._BQC ()
            ^^^VGA.LCD._BCM (Local0)
        }

        Method (_Q60, 0, NotSerialized)  // _Qxx: EC Query
        {
            Debug = "=====QUERY_60====="
        }

        Method (_Q2D, 0, NotSerialized)  // _Qxx: EC Query
        {
            BC5P = One
            Notify (\_TZ.TZ00, 0x80) // Thermal Status Change
        }
    }

