                Scope (\_SB.PCI0.SATA)
                {
                    OperationRegion (SATX, PCI_Config, Zero, 0x28)
                    Field (SATX, AnyAcc, NoLock, Preserve)
                    {
                        VIDI,   32, 
                        Offset (0x24), 
                        BA05,   32
                    }

                    Device (ODDZ)
                    {
                        Method (_ADR, 0, Serialized)  // _ADR: Address
                        {
                            If (G36I)
                            {
                                Return (One)
                            }
                            Else
                            {
                                Return (0x02)
                            }
                        }

                        Name (ODPS, Zero)
                        Method (_STA, 0, NotSerialized)  // _STA: Status
                        {
                            If ((^^^AFD.ODZP == 0x80000000))
                            {
                                Return (Zero)
                            }
                            Else
                            {
                                Return (0x0F)
                            }
                        }

                        Method (_PS0, 0, NotSerialized)  // _PS0: Power State 0
                        {
                            P80H = 0xDF00
                            If ((^^^AFD.ODZP == 0x80000001))
                            {
                                If ((VIDI == 0x78041022))
                                {
                                    Sleep (0x28)
                                    GAAO = Zero
                                    GAAE = Zero
                                    GABO = One
                                    GABE = Zero
                                    Sleep (0x28)
                                    P80H = 0xDE00
                                }
                            }

                            ODPS = Zero
                        }

                        Method (_PS3, 0, NotSerialized)  // _PS3: Power State 3
                        {
                            P80H = 0xDF03
                            If ((^^^AFD.ODZP == 0x80000001))
                            {
                                If ((VIDI == 0x78041022))
                                {
                                    Sleep (0x28)
                                    GAAO = Zero
                                    GAAE = Zero
                                    GABO = Zero
                                    GABE = Zero
                                    Sleep (0x28)
                                    P80H = 0xDE03
                                }
                            }

                            ODPS = 0x03
                        }

                        Method (_PSC, 0, NotSerialized)  // _PSC: Power State Current
                        {
                            Return (ODPS) /* \_SB_.PCI0.SATA.ODDZ.ODPS */
                        }

                        Device (ODD)
                        {
                            Name (_ADR, Zero)  // _ADR: Address
                            Method (_RMV, 0, NotSerialized)  // _RMV: Removal Status
                            {
                                Return (Zero)
                            }

                            Method (_EJX, 1, NotSerialized)
                            {
                                P80H = 0x89AB
                                Sleep (0x28)
                                GAAO = Zero
                                GAAE = Zero
                                GABO = Zero
                                GABE = Zero
                                Sleep (0x28)
                            }
                        }
                    }
                }

                Scope (\_GPE)
                {
                    Method (_L1D, 0, NotSerialized)  // _Lxx: Level-Triggered GPE
                    {
                        If ((\_SB.PCI0.AFD.ODZP == 0x80000001))
                        {
                            If (E1DC)
                            {
                                Sleep (0x14)
                                If (GE06)
                                {
                                    If (((\_SB.PCI0.SATA.VIDI == 0x78001022) || (\_SB.PCI0.SATA.VIDI == 0x78011022))) {}
                                    If ((\_SB.PCI0.SATA.VIDI == 0x78041022))
                                    {
                                        Notify (\_SB.PCI0.SATA.ODDZ, 0x80) // Status Change
                                    }

                                    P80H = 0xDF15
                                }
                            }
                            Else
                            {
                                Sleep (0x14)
                                If (!GE06)
                                {
                                    If (((\_SB.PCI0.SATA.VIDI == 0x78001022) || (\_SB.PCI0.SATA.VIDI == 0x78011022))) {}
                                    If ((\_SB.PCI0.SATA.VIDI == 0x78041022))
                                    {
                                        Notify (\_SB.PCI0.SATA.ODDZ, 0x80) // Status Change
                                    }

                                    P80H = 0xDF14
                                }
                            }

                            E06C = ~GE06
                        }
                    }
                }
            }

