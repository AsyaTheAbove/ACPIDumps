                0x384A, 
                0x21EC, 
                0x15A3, 
                0x05BB, 
                0x149E, 
                0x1729, 
                0x364D
            })
            Name (BCLJ, Package (0x08)
            {
                0x07, 
                0x0D, 
                0x14, 
                0x1E, 
                0x2D, 
                0x3C, 
                0x50, 
                0x64
            })
            Name (BCLC, Package (0x0A)
            {
                0x64, 
                0x2C, 
                0x07, 
                0x0D, 
                0x14, 
                0x1E, 
                0x2D, 
                0x3C, 
                0x50, 
                0x64
            })
            Method (_BCL, 0, NotSerialized)  // _BCL: Brightness Control Levels
            {
                Local0 = DerefOf (Index (BCLJ, 0x07))
                Index (BCLC, Zero) = Local0
                Local1 = DerefOf (Index (BCLJ, 0x03))
                Index (BCLC, One) = Local1
                Return (BCLC) /* \_SB_.PCI0.VGA_.LCD_.BCLC */
            }

            Method (_BCM, 1, NotSerialized)  // _BCM: Brightness Control Method
            {
                If (((Arg0 >= Zero) && (Arg0 <= 0x64)))
                {
                    Local1 = Match (BCLJ, MEQ, Arg0, MTR, Zero, Zero)
                    Local3 = Match (IDPT, MEQ, IDPC, MTR, Zero, Zero)
                    If ((Local1 != Ones))
                    {
                        ^^^SBRG.EC0.BRTS = Local1
                    }

                    If ((Local3 != Ones))
                    {
                        Local2 = DerefOf (Index (PBCL, Local3))
                    }
                    Else
                    {
                        Local2 = Package (0x08)
                            {
                                0x64, 
                                0x5A, 
                                0x50, 
                                0x46, 
                                0x3C, 
                                0x32, 
                                0x28, 
                                0x1E
                            }
                    }

                    Local3 = DerefOf (Index (Local2, Local1))
                }

                Local1 = (G1EI ^ G1FI) /* \G1FI */
                If ((Local1 == Zero))
                {
                    AINT (One, Local3)
                }
            }

            Method (_BQC, 0, NotSerialized)  // _BQC: Brightness Query Current
            {
                Local1 = ^^^SBRG.EC0.BRTS /* \_SB_.PCI0.SBRG.EC0_.BRTS */
                Local0 = DerefOf (Index (BCLJ, Local1))
                Return (Local0)
            }
        }

        Device (TV)
        {
            Method (_ADR, 0, NotSerialized)  // _ADR: Address
            {
                Return (0x0200)
            }
        }

        Device (DDI1)
        {
            Method (_ADR, 0, NotSerialized)  // _ADR: Address
            {
                Return (0x0210)
            }
        }

        Device (DDI2)
        {
            Method (_ADR, 0, NotSerialized)  // _ADR: Address
            {
                Return (0x0220)
            }
        }

        Method (AINT, 2, NotSerialized)
        {
            Local1 = ((Arg1 * 0xFF) / 0x64)
            AFN7 (Local1)
        }
    }

    Method (OWAK, 1, Serialized)
    {
        If ((Arg0 == 0x03))
        {
            \_SB.PCI0.SBRG.EC0.S3RM = One
            Notify (\_SB.PWRB, 0x02) // Device Wake
        }

        If ((Arg0 == 0x04))
        {
            \_SB.PCI0.SBRG.EC0.S4RM = One
        }
    }

    Method (OPTS, 1, Serialized)
    {
        If ((Arg0 == 0x03))
        {
            \_SB.PCI0.SBRG.EC0.S3ST = One
        }

        If ((Arg0 == 0x04))
        {
            \_SB.PCI0.SBRG.EC0.S4ST = One
        }

        If ((Arg0 == 0x05))
        {
            \_SB.PCI0.SBRG.EC0.S5ST = One
        }
    }

    Scope (_SB.PCI0)
    {
        Method (_INI, 0, NotSerialized)  // _INI: Initialize
        {
            If ((RVID <= 0x13)) {}
            OSYS = 0x07D0
            If (CondRefOf (_OSI, Local0))
            {
                If (_OSI ("Linux"))
                {
                    OSYS = 0x03E8
                }

                If (_OSI ("Windows 2001"))
                {
                    OSYS = 0x07D1
                }

                If (_OSI ("Windows 2001 SP1"))
                {
                    OSYS = 0x07D1
                }

                If (_OSI ("Windows 2001 SP2"))
                {
                    OSYS = 0x07D2
                }

                If (_OSI ("Windows 2001.1"))
                {
                    OSYS = 0x07D3
                }

                If (_OSI ("Windows 2006"))
                {
                    OSYS = 0x07D6
                }

                If (_OSI ("Windows 2009"))
                {
                    OSYS = 0x07D9
                }

                OSTP = OSYS /* \OSYS */
                KSMI (One)
            }
        }
    }

    Scope (_SB.PCI0.SATA.ODDZ)
    {
        Name (_UPC, Package (0x04)  // _UPC: USB Port Capabilities
        {
            0xFF, 
            0xFF, 
            Zero, 
            Zero
        })
        Name (_PLD, ToPLD ()  // _PLD: Physical Location of Device
    }

    Name (_S0, Package (0x04)  // _S0_: S0 System State
    {
        Zero, 
        Zero, 
        Zero, 
        Zero
    })
    If (SS3)
    {
        Name (_S3, Package (0x04)  // _S3_: S3 System State
        {
            0x03, 
            Zero, 
            Zero, 
            Zero
        })
    }

    If (SS4)
    {
        Name (_S4, Package (0x04)  // _S4_: S4 System State
        {
            0x04, 
            Zero, 
            Zero, 
            Zero
        })
    }

    Name (_S5, Package (0x04)  // _S5_: S5 System State
    {
        0x05, 
        Zero, 
        Zero, 
        Zero
    })
    Method (PTS, 1, NotSerialized)
    {
        If (Arg0)
        {
            SPTS (Arg0)
            \_SB.PCI0.NPTS (Arg0)
            OPTS (Arg0)
        }
    }

    Method (WAK, 1, NotSerialized)
    {
        SWAK (Arg0)
        \_SB.PCI0.NWAK (Arg0)
        PWAK (Arg0)
        OWAK (Arg0)
    }
}



