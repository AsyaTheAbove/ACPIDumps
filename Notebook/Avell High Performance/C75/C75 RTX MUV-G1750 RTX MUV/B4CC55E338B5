                Debug = "=== QD4 ==="
                Notify (^^^PEG0.PEGP, 0xD4) // Hardware-Specific
            }

            Method (_QD5, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
            {
                P8XH (Zero, 0xD5)
                Debug = "=== QD5 ==="
                Notify (^^^PEG0.PEGP, 0xD5) // Hardware-Specific
            }

            Name (OSDT, Zero)
            Method (_Q53, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
            {
                Sleep (0x14)
                OSDT = OSDF /* \_SB_.PCI0.LPCB.EC0_.OSDF */
                ^^^^AMW0.WMBC (Zero, 0x03, OSDT)
                OSDT = Zero
                Sleep (0x14)
                OSDF = Zero
            }

            Method (_Q6E, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
            {
                P8XH (Zero, 0x6E)
                S3WA = One
            }

            Method (_Q74, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
            {
                P8XH (Zero, 0x74)
            }

            Method (_Q75, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
            {
                P8XH (Zero, 0x75)
            }

            Method (_Q76, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
            {
                P8XH (Zero, 0x76)
                Notify (PEG0, 0x81) // Information Change
                Notify (^^^PEG0.PEGP, 0x81) // Information Change
            }

            Method (_Q77, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
            {
                P8XH (Zero, 0x77)
                ^^^^AMW0.WMBC (Zero, 0x03, 0xF0)
            }

            Method (_Q78, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
            {
                P8XH (Zero, 0x78)
                ^^^^AMW0.WMBC (Zero, 0x03, 0xF0)
            }
        }
    }

    Scope (_SB)
    {
        Mutex (MUEX, 0x00)
        Device (AC0)
        {
            Name (_HID, "ACPI0003" /* Power Source Device */)  // _HID: Hardware ID
            Name (_PCL, Package (0x01)  // _PCL: Power Consumer List
            {
                _SB
            })
            Method (_PSR, 0, NotSerialized)  // _PSR: Power Source
            {
                Acquire (MUEX, 0xFFFF)
                If (ECON)
                {
                    Local0 = ^^PCI0.LPCB.EC0.XCIN /* \_SB_.PCI0.LPCB.EC0_.XCIN */
                }
                Else
                {
                    Local0 = One
                }

                Release (MUEX)
                If (^^PCI0.LPCB.EC0.HPEN)
                {
                    Local0 = One
                }

                Return (Local0)
            }
        }

        Device (BAT0)
        {
            Name (_HID, EisaId ("PNP0C0A") /* Control Method Battery */)  // _HID: Hardware ID
            Name (_UID, Zero)  // _UID: Unique ID
            Name (_PCL, Package (0x01)  // _PCL: Power Consumer List
            {
                _SB
            })
            Name (PAK0, Package (0x0D)
            {
                One, 
                0x0C56, 
                0x0C56, 
                Zero, 
                0x2A30, 
                Zero, 
                Zero, 
                0x10, 
                0x08, 
                "standard", 
                "00001", 
                "LiON", 
                "OEM"
            })
            Method (ECBE, 0, NotSerialized)
            {
                Acquire (MUEX, 0xFFFF)
                Debug = "ECBE"
                PAK0 [One] = ^^PCI0.LPCB.EC0.XIF1 /* \_SB_.PCI0.LPCB.EC0_.XIF1 */
                If ((^^PCI0.LPCB.EC0.CYCN >= 0x32))
                {
                    PAK0 [0x02] = ^^PCI0.LPCB.EC0.XIF2 /* \_SB_.PCI0.LPCB.EC0_.XIF2 */
                }
                Else
                {
                    PAK0 [0x02] = ^^PCI0.LPCB.EC0.XIF1 /* \_SB_.PCI0.LPCB.EC0_.XIF1 */
                }

                PAK0 [0x03] = ^^PCI0.LPCB.EC0.XIF3 /* \_SB_.PCI0.LPCB.EC0_.XIF3 */
                PAK0 [0x04] = ^^PCI0.LPCB.EC0.XIF4 /* \_SB_.PCI0.LPCB.EC0_.XIF4 */
                PAK0 [0x07] = ^^PCI0.LPCB.EC0.XIF7 /* \_SB_.PCI0.LPCB.EC0_.XIF7 */
                Release (MUEX)
            }

            Method (_BIF, 0, NotSerialized)  // _BIF: Battery Information
            {
                If (ECON)
                {
                    ECBE ()
                }
                Else
                {
                    PAK0 [One] = 0xFFFFFFFF
                    PAK0 [0x02] = 0xFFFFFFFF
                    PAK0 [0x04] = 0xFFFFFFFF
                    PAK0 [0x09] = "Error"
                    PAK0 [0x0A] = "00000"
                    PAK0 [0x0B] = "Error"
                    PAK0 [0x0C] = "Error"
                }

                Return (PAK0) /* \_SB_.BAT0.PAK0 */
            }

            Name (BFB0, Package (0x04)
            {
                Zero, 
                Zero, 
                Zero, 
                Zero
            })
            Method (_BST, 0, NotSerialized)  // _BST: Battery Status
            {
                Acquire (MUEX, 0xFFFF)
                Debug = "BST1 Start"
                If ((^^PCI0.LPCB.EC0.BSOK == One))
                {
                    BFB0 [Zero] = ^^PCI0.LPCB.EC0.XST0 /* \_SB_.PCI0.LPCB.EC0_.XST0 */
                    BFB0 [One] = ^^PCI0.LPCB.EC0.XST1 /* \_SB_.PCI0.LPCB.EC0_.XST1 */
                    If ((^^PCI0.LPCB.EC0.CYCN >= 0x32))
                    {
                        BFB0 [0x02] = ^^PCI0.LPCB.EC0.XST2 /* \_SB_.PCI0.LPCB.EC0_.XST2 */
                    }
                    Else
                    {
                        Local0 = ((^^PCI0.LPCB.EC0.XST2 * 0x64) / ^^PCI0.LPCB.EC0.XIF2)
                        Local1 = (^^PCI0.LPCB.EC0.XIF1 / 0x64)
                        BFB0 [0x02] = (Local0 * Local1)
                    }

                    BFB0 [0x03] = ^^PCI0.LPCB.EC0.XST3 /* \_SB_.PCI0.LPCB.EC0_.XST3 */
                    Debug = "BST1 End"
                }

                Release (MUEX)
                Return (BFB0) /* \_SB_.BAT0.BFB0 */
            }

            Method (_STA, 0, NotSerialized)  // _STA: Status
            {
                Debug = "BAT0_STA Start"
                If (((BAEX == Zero) || (DA18 == One)))
                {
                    Return (Zero)
                }
                Else
                {
                    If (ECON)
                    {
                        Acquire (MUEX, 0xFFFF)
                        Local0 = ^^PCI0.LPCB.EC0.BPST /* \_SB_.PCI0.LPCB.EC0_.BPST */
                        Release (MUEX)
                        If ((Local0 == One))
                        {
                            Return (0x1F)
                        }
                        Else
                        {
                            Return (0x0F)
                        }
                    }
                    Else
                    {
                        Return (0x0F)
                    }

                    Release (MUEX)
                }

                Debug = "BAT0_STA END"
            }
        }

        ThermalZone (ECTZ)
        {
            Method (_CRT, 0, Serialized)  // _CRT: Critical Temperature
            {
                Return (0x0E94)
            }

            Method (_TMP, 0, Serialized)  // _TMP: Temperature
            {
                If ((ECON == Zero))
                {
                    Return (0x0BC2)
                }

                If ((^^PCI0.LPCB.EC0.THOT != Zero))
                {
                    ^^PCI0.LPCB.EC0.THOT = Zero
                    Return (0x0FA0)
                }
                Else
                {
                    Return ((0x0AAC + (^^PCI0.LPCB.EC0.XHPP * 0x0A)))
                }
            }
        }
    }

