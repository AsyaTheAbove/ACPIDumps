                CPPC = 0x10
                PNOT ()
            }

            Method (_Q47, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
            {
                P8XH (Zero, 0x47)
                CPPC = 0x13
                PNOT ()
            }

            Method (_Q70, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
            {
                P8XH (Zero, 0x70)
                OperationRegion (SMIT, SystemIO, 0xB2, One)
                Field (SMIT, ByteAcc, NoLock, Preserve)
                {
                    SMIP,   8
                }

                PWL1 = PL1L /* \_SB_.PCI0.LPCB.EC0_.PL1L */
                PWL2 = PL2L /* \_SB_.PCI0.LPCB.EC0_.PL2L */
                PWL3 = PL3L /* \_SB_.PCI0.LPCB.EC0_.PL3L */
                PWL4 = PL4L /* \_SB_.PCI0.LPCB.EC0_.PL4L */
                If ((PWL2 == Zero))
                {
                    PWL2 |= 0x80
                }

                SMIP = 0xAA
            }

            Method (_Q71, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
            {
                P8XH (Zero, 0x71)
                OperationRegion (SMIT, SystemIO, 0xB2, One)
                Field (SMIT, ByteAcc, NoLock, Preserve)
                {
                    SMIP,   8
                }

                TCCO = TCOS /* \_SB_.PCI0.LPCB.EC0_.TCOS */
                SMIP = 0x99
            }

            Method (_Q72, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
            {
                P8XH (Zero, 0x72)
                OperationRegion (SMIT, SystemIO, 0xB2, One)
                Field (SMIT, ByteAcc, NoLock, Preserve)
                {
                    SMIP,   8
                }

                TRBO = TURB /* \_SB_.PCI0.LPCB.EC0_.TURB */
                SMIP = 0x9A
            }

            Method (_QD1, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
            {
                P8XH (Zero, 0xD1)
                Debug = "=== QD1 ==="
                Notify (^^^PEG0.PEGP, 0xD1) // Hardware-Specific
            }

            Method (_QD2, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
            {
