                    {
                        DBG8 = 0x8A
                        CTSD = One
                        Notify (\_TZ.THRM, 0x80) // Thermal Status Change
                    }

                    Method (_Q8B, 0, NotSerialized)  // _Qxx: EC Query
                    {
                        DBG8 = 0x8B
                    }

                    Method (_Q8C, 0, NotSerialized)  // _Qxx: EC Query
                    {
                        DBG8 = 0x8C
                    }

                    Method (_Q90, 0, NotSerialized)  // _Qxx: EC Query
                    {
                        DBG8 = 0x90
                    }

                    Method (_QB4, 0, NotSerialized)  // _Qxx: EC Query
                    {
                        DBG8 = 0xB4
                        APMC = 0xB5
                        If ((OSFL () == Zero))
                        {
                            Notify (PCI0, Zero) // Bus Check
                        }
                        Else
                        {
                            Notify (GFX0, Zero) // Bus Check
                        }

                        Sleep (0x02EE)
                        Notify (GFX0, 0x80) // Status Change
                        Sleep (0x01F4)
                        ^^^GFX0.GNOT (One, Zero)
                    }

                    Method (_QB5, 0, NotSerialized)  // _Qxx: EC Query
                    {
                        DBG8 = 0xB5
                    }

                    Method (_QB6, 0, NotSerialized)  // _Qxx: EC Query
                    {
                        DBG8 = 0xB6
                    }

                    Method (_QB7, 0, NotSerialized)  // _Qxx: EC Query
                    {
                        DBG8 = 0xB7
                        If (_OSI ("Windows 2006"))
                        {
                            Notify (^^^GFX0.DD04, 0x87) // Device-Specific
                        }
                        Else
                        {
                            While (!IRDY ())
                            {
                                Sleep (One)
                            }

                            CMD1 = 0x11
                            While (!IRDY ())
                            {
                                Sleep (One)
                            }

                            DAT1 = 0x82
                        }
                    }

                    Method (_QB8, 0, NotSerialized)  // _Qxx: EC Query
                    {
                        DBG8 = 0xB8
                        If (_OSI ("Windows 2006"))
                        {
                            Notify (^^^GFX0.DD04, 0x86) // Device-Specific
                        }
                        Else
                        {
                            While (!IRDY ())
                            {
                                Sleep (One)
                            }

                            CMD1 = 0x11
                            While (!IRDY ())
                            {
                                Sleep (One)
                            }

                            DAT1 = 0x81
                        }
                    }

                    Method (_QB9, 0, NotSerialized)  // _Qxx: EC Query
                    {
                        DBG8 = 0xB9
                        If (_OSI ("Windows 2006"))
                        {
                            Notify (MBTN, 0x80) // Status Change
                            HSKS = Zero
                        }
                    }

                    Method (MSWK, 1, NotSerialized)
                    {
                        ^^^WAWA.PDC1 = One
                        ^^^WAWA.HPCS = One
                        Notify (P0P4, Zero) // Bus Check
                        Notify (\_PR.P001, 0x80) // Performance Capability Change
                        Sleep (0x0A)
                        Notify (\_PR.P002, 0x80) // Performance Capability Change
                        If (_OSI ("Windows 2006"))
                        {
                            If ((HSKS == 0x04))
                            {
                                Notify (MBTN, 0x02) // Device Wake
                                HSKS = Zero
                            }
                        }
                    }
                }
