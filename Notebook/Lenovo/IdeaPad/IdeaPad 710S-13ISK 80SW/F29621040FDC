                    0x0064,             // Range Minimum
                    0x0064,             // Range Maximum
                    0x01,               // Alignment
                    0x01,               // Length
                    )
                IRQ (Edge, ActiveHigh, Exclusive, )
                    {1}
            })
        }

        Device (PS2M)
        {
            Method (_HID, 0, NotSerialized)  // _HID: Hardware ID
            {
                Local0 = ^^EC0.TPTP /* \_SB_.PCI0.LPCB.EC0_.TPTP */
                If ((Local0 == One))
                {
                    Return (0x632B2E4F)
                }

                If ((Local0 == 0x02))
                {
                    Return (0x37068416)
                }

                If ((Local0 == 0x04))
                {
                    Return (0x4116A906)
                }

                If ((Local0 == 0x08))
                {
                    Return (0x43C08C4E)
                }

                If ((Local0 == 0x10))
                {
                    Return (0x0100330F)
                }

                Return (0x27068416)
            }

            Method (_CID, 0, NotSerialized)  // _CID: Compatible ID
            {
                Local0 = ^^EC0.TPTP /* \_SB_.PCI0.LPCB.EC0_.TPTP */
                If ((Local0 == One))
                {
                    Return (Package (0x03)
                    {
                        0x130FD041, 
                        0x002B2E4F, 
                        0x02002E4F
                    })
                }

                If ((Local0 == 0x02))
                {
                    Return (Package (0x02)
                    {
                        0x130FD041, 
                        0x8416
                    })
                }

                If ((Local0 == 0x04))
                {
                    Return (Package (0x01)
                    {
                        0x130FD041
                    })
                }

                If ((Local0 == 0x08))
                {
                    Return (Package (0x01)
                    {
                        0x130FD041
                    })
                }

                If ((Local0 == 0x10))
                {
                    Return (Package (0x05)
                    {
                        0x130FD041, 
                        0x030FD041, 
                        0x0E0FD041, 
                        0x0B0FD041, 
                        0x120FD041
                    })
                }

                Return (Package (0x02)
                {
                    0x130FD041, 
                    0x8416
                })
            }

            Name (_CRS, ResourceTemplate ()  // _CRS: Current Resource Settings
            {
                IRQ (Edge, ActiveHigh, Exclusive, )
                    {12}
            })
        }
