                     0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00   // ........
                }, 

                Buffer (0x08)
                {
                     0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00   // ........
                }
            })
            If ((POSB == Zero))
            {
                DerefOf (BCFV [0x02]) [Zero] = BAI0 /* \_SB_.PCI0.LPCB.EC0_.BAI0 */
                DerefOf (BCFV [0x02]) [One] = BAI1 /* \_SB_.PCI0.LPCB.EC0_.BAI1 */
