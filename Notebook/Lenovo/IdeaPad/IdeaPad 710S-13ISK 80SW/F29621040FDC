        Method (GSBI, 1, NotSerialized)
        {
            If ((Arg0 == Zero))
            {
                Local0 = (NB0S & One)
                If ((Local0 == One))
                {
                    CreateWordField (SBI0, Zero, DECA)
                    DECA = B0DC /* \_SB_.PCI0.LPCB.EC0_.B0DC */
                    CreateWordField (SBI0, 0x02, FUCC)
                    FUCC = B0FC /* \_SB_.PCI0.LPCB.EC0_.B0FC */
                    CreateWordField (SBI0, 0x04, RECA)
                    RECA = B0RC /* \_SB_.PCI0.LPCB.EC0_.B0RC */
                    CreateWordField (SBI0, 0x06, ATTE)
                    ATTE = B0AE /* \_SB_.PCI0.LPCB.EC0_.B0AE */
                    CreateWordField (SBI0, 0x08, ATTF)
                    ATTF = B0AF /* \_SB_.PCI0.LPCB.EC0_.B0AF */
                    CreateWordField (SBI0, 0x0A, VOLT)
                    VOLT = B0VO /* \_SB_.PCI0.LPCB.EC0_.B0VO */
                    CreateWordField (SBI0, 0x0C, CURR)
                    CURR = B0CU /* \_SB_.PCI0.LPCB.EC0_.B0CU */
                    CreateWordField (SBI0, 0x0E, TMPK)
                    TMPK = B0TE /* \_SB_.PCI0.LPCB.EC0_.B0TE */
                    CreateWordField (SBI0, 0x10, MADA)
                    MADA = B0MD /* \_SB_.PCI0.LPCB.EC0_.B0MD */
                    CreateWordField (SBI0, 0x12, FIUD)
                    FIUD = B0FU /* \_SB_.PCI0.LPCB.EC0_.B0FU */
                    CreateWordField (SBI0, 0x14, DEVO)
                    DEVO = B0DV /* \_SB_.PCI0.LPCB.EC0_.B0DV */
                    CreateQWordField (SBI0, 0x16, DECH)
                    DECH = B0DY /* \_SB_.PCI0.LPCB.EC0_.B0DY */
                    CreateWordField (SBI0, 0x1E, DEC1)
                    DEC1 = B0Y1 /* \_SB_.PCI0.LPCB.EC0_.B0Y1 */
                    CreateQWordField (SBI0, 0x20, DENA)
                    DENA = B0DN /* \_SB_.PCI0.LPCB.EC0_.B0DN */
                    CreateQWordField (SBI0, 0x28, MANA)
                    MANA = B0MN /* \_SB_.PCI0.LPCB.EC0_.B0MN */
                    CreateDWordField (SBI0, 0x30, MAN1)
                    MAN1 = B0M1 /* \_SB_.PCI0.LPCB.EC0_.B0M1 */
                    CreateQWordField (SBI0, 0x34, BCNU)
                    BCNU = B0B0 /* \_SB_.PCI0.LPCB.EC0_.B0B0 */
                    CreateQWordField (SBI0, 0x3C, BCN1)
                    BCN1 = B0B1 /* \_SB_.PCI0.LPCB.EC0_.B0B1 */
                    CreateDWordField (SBI0, 0x44, BCN2)
                    BCN2 = B0B2 /* \_SB_.PCI0.LPCB.EC0_.B0B2 */
                    CreateWordField (SBI0, 0x48, BCN3)
                    BCN3 = B0B3 /* \_SB_.PCI0.LPCB.EC0_.B0B3 */
                    CreateByteField (SBI0, 0x4A, BCN4)
                    BCN4 = B0B4 /* \_SB_.PCI0.LPCB.EC0_.B0B4 */
                    SBI0 [0x4B] = BAI0 /* \_SB_.PCI0.LPCB.EC0_.BAI0 */
                    SBI0 [0x4C] = BAI1 /* \_SB_.PCI0.LPCB.EC0_.BAI1 */
                    SBI0 [0x4D] = BAI2 /* \_SB_.PCI0.LPCB.EC0_.BAI2 */
                    SBI0 [0x4E] = BAI3 /* \_SB_.PCI0.LPCB.EC0_.BAI3 */
                    SBI0 [0x4F] = BAI4 /* \_SB_.PCI0.LPCB.EC0_.BAI4 */
                    SBI0 [0x50] = BAI5 /* \_SB_.PCI0.LPCB.EC0_.BAI5 */
                    SBI0 [0x51] = BAI6 /* \_SB_.PCI0.LPCB.EC0_.BAI6 */
                    SBI0 [0x52] = BAI7 /* \_SB_.PCI0.LPCB.EC0_.BAI7 */
                }

                Return (SBI0) /* \_SB_.PCI0.LPCB.EC0_.VPC0.SBI0 */
            }

            If ((Arg0 == One))
            {
                Local0 = (NB1S & One)
                If ((Local0 == One)){}
                Return (SBI1) /* \_SB_.PCI0.LPCB.EC0_.VPC0.SBI1 */
            }
        }
    }

    Scope (_SB.PCI0.LPCB.EC0.VPC0)
    {
        Method (HODD, 0, NotSerialized)
        {
            If ((SZPO == Zero))
            {
                ZPOF = Zero
                Return (0xFF)
            }
            Else
            {
                OSTP = GOST ()
                If (((OSTP != 0x04) && (OSTP != 0x05)))
                {
                    ZPOF = One
                }
                Else
                {
                    ZPOF = Zero
                    Return (0xFF)
                }
            }
        }
    }

    Name (_S0, Package (0x04)  // _S0_: S0 System State
    {
        Zero, 
        Zero, 
        Zero, 
        Zero
    })
    If (SS1)
    {
        Name (_S1, Package (0x04)  // _S1_: S1 System State
        {
            One, 
            Zero, 
            Zero, 
            Zero
        })
    }

    If (SS3)
    {
        Name (_S3, Package (0x04)  // _S3_: S3 System State
        {
            0x05, 
            Zero, 
            Zero, 
            Zero
        })
    }

    If (SS4)
    {
        Name (_S4, Package (0x04)  // _S4_: S4 System State
        {
            0x06, 
            Zero, 
            Zero, 
            Zero
        })
    }

    Name (_S5, Package (0x04)  // _S5_: S5 System State
    {
        0x07, 
        Zero, 
        Zero, 
        Zero
    })
    Method (PTS, 1, NotSerialized)
    {
        If (Arg0){}
    }

    Method (WAK, 1, NotSerialized)
    {
    }
}



