                DerefOf (BCFV [0x02]) [0x03] = BAI3 /* \_SB_.PCI0.LPCB.EC0_.BAI3 */
                DerefOf (BCFV [0x02]) [0x04] = BAI4 /* \_SB_.PCI0.LPCB.EC0_.BAI4 */
                DerefOf (BCFV [0x02]) [0x05] = BAI5 /* \_SB_.PCI0.LPCB.EC0_.BAI5 */
