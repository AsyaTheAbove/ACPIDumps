    Scope (\)
    {
        Mutex (MSGF, 0x00)
    }

    Scope (_SB.PCI0.LPCB.EC0)
    {
        Name (ACIO, Zero)
        Name (DCIO, Zero)
        Method (_Q1C, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x1C
            Local0 = (SWBL & One)
            If ((Local0 == Zero))
            {
                Local1 = (SWBL | One)
                SWBL = Local1
            }
            Else
            {
                Acquire (MSGF, 0xFFFF)
                BRTN (0x86)
                Release (MSGF)
            }

            If ((OSYS == 0x07D9))
            {
                Sleep (0x64)
                VP1D |= 0x10
                Notify (VPC0, 0x80) // Status Change
            }
        }

        Method (_Q1D, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x1D
            Local0 = (^^^GFX0.CBLV & 0xFF)
            If ((Local0 <= One))
            {
                Local1 = (SWBL & 0xFE)
                SWBL = Local1
            }

            Acquire (MSGF, 0xFFFF)
            BRTN (0x87)
            Release (MSGF)
            If ((OSYS == 0x07D9))
            {
                Sleep (0x64)
                VP1D |= 0x10
                Notify (VPC0, 0x80) // Status Change
            }
        }

        Method (_Q2C, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x2C
            Acquire (MSGF, 0xFFFF)
            Notify (AMW1, 0xB0) // Device-Specific
            Release (MSGF)
        }

        Method (_Q36, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x36
            KOSD = One
            Sleep (0x01F4)
            NTMR ()
        }

        Method (_Q40, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x40
            DCIO = One
            Notify (BAT0, 0x81) // Information Change
        }

        Method (_Q41, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x41
            DCIO = One
            Notify (BAT0, 0x81) // Information Change
        }

        Method (_Q48, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x48
            Notify (BAT0, 0x81) // Information Change
        }

        Method (_Q4C, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x4C
            If (B0ST)
            {
                Notify (BAT0, 0x80) // Status Change
            }
        }

        Method (_Q50, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x50
            ACIO = One
            VP1D |= 0x02
            Notify (VPC0, 0x80) // Status Change
            Notify (ADP1, 0x80) // Status Change
        }

        Method (_Q51, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x51
            ACIO = One
            VP1D |= 0x02
            Notify (VPC0, 0x80) // Status Change
            Notify (ADP1, 0x80) // Status Change
        }

        Method (_Q52, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x52
            LIDS () = MLID /* \_SB_.PCI0.LPCB.EC0_.MLID */
            ^^^GFX0.GLID (LIDS ())
            Notify (LID0, 0x80) // Status Change
        }

        Method (_Q53, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x53
            ^^^GFX0.CLID = MLID /* \_SB_.PCI0.LPCB.EC0_.MLID */
            LIDS () = MLID /* \_SB_.PCI0.LPCB.EC0_.MLID */
            ^^^GFX0.GLID (LIDS ())
            Notify (LID0, 0x80) // Status Change
        }

        Method (NTMR, 0, NotSerialized)
        {
            Notify (\_TZ.TZS0, 0x80) // Thermal Status Change
            Notify (\_TZ.TZS1, 0x80) // Thermal Status Change
        }
    }

    Scope (_SB.PCI0.LPCB.EC0)
    {
        Field (ECMB, ByteAcc, Lock, Preserve)
        {
            Offset (0x20), 
            THS0,   8, 
            THS1,   8, 
            THS2,   8, 
            THS3,   8, 
            THS4,   8, 
            THS5,   8, 
            THS6,   8, 
