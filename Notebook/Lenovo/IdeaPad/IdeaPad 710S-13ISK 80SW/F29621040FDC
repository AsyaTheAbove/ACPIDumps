            /* 0318 */  0xA3, 0x9E, 0x18, 0xF8, 0x11, 0xCB, 0x88, 0x6F,  // .......o
            /* 0320 */  0x4E, 0x1E, 0x8D, 0x4F, 0x5B, 0x0C, 0x94, 0x9D,  // N..O[...
            /* 0328 */  0x23, 0x38, 0x20, 0x16, 0xEA, 0x9D, 0x8B, 0xC1,  // #8 .....
            /* 0330 */  0xF8, 0xCA, 0xC5, 0x4F, 0x43, 0xAF, 0x60, 0x3E,  // ...OC.`>
            /* 0338 */  0x50, 0xF8, 0x1E, 0xE7, 0x03, 0x85, 0x67, 0x19,  // P.....g.
            /* 0340 */  0xE6, 0x49, 0xC0, 0x18, 0x8F, 0x07, 0x3D, 0x61,  // .I....=a
            /* 0348 */  0x49, 0xD7, 0xA8, 0xC8, 0xA9, 0x02, 0xE6, 0xB5,  // I.......
            /* 0350 */  0xE0, 0xA9, 0x02, 0xF8, 0xFE, 0xFF, 0x4F, 0x15,  // ......O.
            /* 0358 */  0x00, 0x2F, 0x20, 0x7D, 0xAA, 0x00, 0xCE, 0x43,  // ./ }...C
            /* 0360 */  0x7E, 0xAA, 0x00, 0xB6, 0xFF, 0xFF, 0x53, 0x05,  // ~.....S.
            /* 0368 */  0x18, 0xD4, 0x9E, 0x2A, 0x00, 0x22, 0xC2, 0xF2,  // ...*."..
            /* 0370 */  0x53, 0x05, 0x70, 0xF9, 0xFF, 0x9F, 0x02, 0xC0,  // S.p.....
            /* 0378 */  0x71, 0x61, 0x78, 0xAA, 0x00, 0xD6, 0xF2, 0x4F,  // qax....O
            /* 0380 */  0x15, 0x28, 0x0E, 0x4B, 0x3E, 0x55, 0x20, 0x4E,  // .(.K>U N
            /* 0388 */  0x01, 0x2F, 0x0A, 0x9E, 0x88, 0x2F, 0xBB, 0x6F,  // ./.../.o
            /* 0390 */  0x15, 0xB8, 0x63, 0x05, 0xC0, 0x98, 0xFF, 0xFF,  // ..c.....
            /* 0398 */  0xB1, 0x02, 0x8E, 0x84, 0x63, 0x05, 0xE8, 0x2C,  // ....c..,
            /* 03A0 */  0x1C, 0x2B, 0x80, 0x4E, 0x84, 0x53, 0x00, 0x68,  // .+.N.S.h
            /* 03A8 */  0x6E, 0x0C, 0x98, 0xF3, 0x04, 0xBF, 0x46, 0x60,  // n.....F`
            /* 03B0 */  0x46, 0xE1, 0x6B, 0xB6, 0xCF, 0xC1, 0xFC, 0x94,  // F.k.....
            /* 03B8 */  0xCD, 0x81, 0x9F, 0x2A, 0x3C, 0xFD, 0xE8, 0x07,  // ...*<...
            /* 03C0 */  0xF3, 0x36, 0x81, 0x39, 0x51, 0x18, 0xCD, 0x18,  // .6.9Q...
            /* 03C8 */  0xA7, 0xE2, 0xFB, 0x84, 0x03, 0x9C, 0x27, 0x40,  // ......'@
            /* 03D0 */  0x61, 0xED, 0x3C, 0x01, 0x28, 0xFC, 0xFF, 0x9F,  // a.<.(...
            /* 03D8 */  0x27, 0x00, 0x1F, 0x22, 0xCE, 0x13, 0xA0, 0xD3,  // '.."....
            /* 03E0 */  0x70, 0x9E, 0x00, 0x3A, 0x21, 0xCE, 0x13, 0x20,  // p..:!.. 
            /* 03E8 */  0xC6, 0x64, 0x17, 0x08, 0x5F, 0x22, 0x7C, 0xA0,  // .d.._"|.
            /* 03F0 */  0x00, 0xFF, 0xA1, 0x0C, 0x77, 0xA0, 0x80, 0x77,  // ....w..w
            /* 03F8 */  0xEA, 0x7F, 0xA0, 0x00, 0x06, 0xFF, 0xFF, 0x03,  // ........
            /* 0400 */  0x05, 0xC0, 0x07, 0x39, 0x07, 0x0A, 0xD0, 0x1D,  // ...9....
            /* 0408 */  0xF0, 0x7D, 0xA0, 0x00, 0x2E, 0x71, 0x4E, 0x01,  // .}...qN.
            /* 0410 */  0xA0, 0x01, 0x8B, 0xF2, 0xCC, 0x6D, 0xF4, 0x27,  // .....m.'
            /* 0418 */  0x32, 0x8F, 0xE5, 0x71, 0xCC, 0x97, 0x0A, 0xCC,  // 2..q....
            /* 0420 */  0x91, 0xE7, 0xD0, 0xEC, 0xFD, 0x4E, 0x81, 0x38,  // .....N.8
            /* 0428 */  0x57, 0x60, 0x51, 0x0D, 0xFE, 0xEE, 0xC3, 0xFE,  // W`Q.....
            /* 0430 */  0xFF, 0x47, 0x32, 0x78, 0x97, 0x0A, 0x18, 0x04,  // .G2x....
            /* 0438 */  0xA3, 0x2E, 0xDB, 0x04, 0x8A, 0x36, 0x72, 0x85,  // .....6r.
            /* 0440 */  0x83, 0x41, 0x9D, 0x48, 0x7C, 0x74, 0xC0, 0x0F,  // .A.H|t..
            /* 0448 */  0xFE, 0xD8, 0x0F, 0xF9, 0x28, 0x99, 0x24, 0x38,  // ....(.$8
            /* 0450 */  0xD4, 0xB9, 0xC0, 0xE3, 0x6B, 0x7E, 0x24, 0x1A,  // ....k~$.
            /* 0458 */  0xCF, 0x4B, 0x80, 0x87, 0x15, 0x92, 0x1D, 0xD1,  // .K......
            /* 0460 */  0x0E, 0x17, 0x23, 0x05, 0x42, 0x47, 0x83, 0x47,  // ..#.BG.G
            /* 0468 */  0x0F, 0x5F, 0x3F, 0x0E, 0x8B, 0xC1, 0x24, 0xF0,  // ._?...$.
            /* 0470 */  0x89, 0x80, 0xA1, 0x71, 0x82, 0x61, 0xC6, 0x0E,  // ...q.a..
            /* 0478 */  0x9A, 0x13, 0xC3, 0x2B, 0x89, 0xD1, 0xDE, 0xEF,  // ...+....
            /* 0480 */  0x7C, 0x7D, 0xE7, 0x40, 0x87, 0x6A, 0x08, 0xDF,  // |}.@.j..
            /* 0488 */  0x2A, 0x0C, 0xFE, 0x64, 0x62, 0x30, 0x5F, 0x4C,  // *..db0_L
            /* 0490 */  0x8C, 0xE2, 0x73, 0x1E, 0xBF, 0x08, 0x18, 0xEA,  // ..s.....
            /* 0498 */  0x05, 0xE3, 0x0D, 0xE2, 0x05, 0x83, 0x03, 0xBD,  // ........
            /* 04A0 */  0x60, 0x30, 0x94, 0xB7, 0x95, 0x50, 0x6F, 0x79,  // `0...Poy
            /* 04A8 */  0x3E, 0x96, 0xBC, 0x01, 0x18, 0x2F, 0x46, 0x20,  // >..../F 
            /* 04B0 */  0x8F, 0x11, 0x3B, 0x58, 0xC3, 0x7B, 0x38, 0x6F,  // ..;X.{8o
            /* 04B8 */  0x06, 0x3E, 0x9A, 0xB0, 0x69, 0x3F, 0x4D, 0x94,  // .>..i?M.
            /* 04C0 */  0x7B, 0xD1, 0xD0, 0x25, 0x30, 0x6A, 0x8C, 0xE6,  // {..%0j..
            /* 04C8 */  0xA0, 0x08, 0x5C, 0x68, 0xCC, 0xB1, 0xC1, 0x53,  // ..\h...S
            /* 04D0 */  0x79, 0xD1, 0xF0, 0xB1, 0x81, 0xB9, 0x3F, 0x5C,  // y.....?\
            /* 04D8 */  0x48, 0x21, 0x82, 0x86, 0x18, 0xB7, 0x46, 0x01,  // H!....F.
            /* 04E0 */  0xDE, 0x2E, 0x1B, 0x3A, 0x14, 0x16, 0x77, 0xBE,  // ...:..w.
            /* 04E8 */  0x10, 0x56, 0x63, 0x28, 0x82, 0x88, 0x10, 0x34,  // .Vc(...4
            /* 04F0 */  0x8A, 0xF1, 0x22, 0x84, 0x0A, 0x11, 0x25, 0xEA,  // .."...%.
            /* 04F8 */  0x59, 0x04, 0x79, 0x70, 0x89, 0x1A, 0x24, 0x5A,  // Y.yp..$Z
            /* 0500 */  0x30, 0x23, 0x30, 0xFB, 0x07, 0x35, 0xF6, 0xCC,  // 0#0..5..
            /* 0508 */  0xD0, 0xE9, 0xC0, 0x91, 0x20, 0xF4, 0xFF, 0x1F,  // .... ...
            /* 0510 */  0x0D, 0x3F, 0x7C, 0xF8, 0x84, 0xE0, 0xD3, 0xC1,  // .?|.....
            /* 0518 */  0x21, 0x7A, 0xA0, 0x06, 0x79, 0x43, 0xB4, 0xCE,  // !z..yC..
            /* 0520 */  0xA3, 0x01, 0x19, 0x1E, 0x3B, 0xAE, 0xF8, 0xD4,  // ....;...
            /* 0528 */  0xE1, 0xB3, 0x02, 0xDE, 0x35, 0xA0, 0x2E, 0x09,  // ....5...
            /* 0530 */  0x0F, 0x1A, 0x60, 0x39, 0xA6, 0xC0, 0x1F, 0x8F,  // ..`9....
            /* 0538 */  0x0F, 0x08, 0x3E, 0x0B, 0xF8, 0xC4, 0x48, 0x06,  // ..>...H.
            /* 0540 */  0x81, 0x1A, 0x99, 0xA1, 0x3D, 0xD2, 0xD3, 0x7A,  // ....=..z
            /* 0548 */  0x0C, 0xE2, 0x04, 0x3E, 0xA2, 0x3F, 0xE5, 0xB0,  // ...>.?..
            /* 0550 */  0xF1, 0x80, 0xFF, 0xD0, 0xF1, 0x68, 0x10, 0xE1,  // .....h..
            /* 0558 */  0xCD, 0xC0, 0xF3, 0xF5, 0x13, 0x88, 0x9D, 0x7C,  // .......|
            /* 0560 */  0x70, 0xD0, 0x06, 0x7D, 0x75, 0xE0, 0x07, 0x16,  // p..}u...
            /* 0568 */  0x16, 0xF7, 0xC0, 0x47, 0x45, 0x1D, 0x5E, 0x50,  // ...GE.^P
            /* 0570 */  0x07, 0x0C, 0x9F, 0x2D, 0xD8, 0x5D, 0x87, 0xC3,  // ...-.]..
            /* 0578 */  0xB2, 0x03, 0x91, 0x47, 0x06, 0x1F, 0x10, 0x7F,  // ...G....
            /* 0580 */  0x42, 0x39, 0x67, 0x1F, 0x29, 0x0C, 0xC8, 0xE4,  // B9g.)...
            /* 0588 */  0x9D, 0x36, 0x40, 0x33, 0x34, 0xCC, 0x24, 0x1E,  // .6@34.$.
            /* 0590 */  0x83, 0xF8, 0x80, 0xE1, 0xFC, 0xFF, 0x4F, 0x41,  // ......OA
            /* 0598 */  0x70, 0xB1, 0x9E, 0x82, 0xC0, 0x80, 0x12, 0xFD,  // p.......
            /* 05A0 */  0xC9, 0xA1, 0xCF, 0x2B, 0x95, 0x20, 0xA2, 0xBD,  // ...+. ..
            /* 05A8 */  0x72, 0x84, 0x0A, 0x64, 0x8C, 0x38, 0xAF, 0x41,  // r..d.8.A
            /* 05B0 */  0x81, 0xC2, 0x04, 0x0B, 0xE4, 0x91, 0x24, 0xB0,  // ......$.
            /* 05B8 */  0xC8, 0xF1, 0xA1, 0x07, 0xC1, 0x01, 0x1F, 0x8A,  // ........
            /* 05C0 */  0x18, 0xF6, 0x7B, 0x91, 0xE7, 0x71, 0x4C, 0x3E,  // ..{..qL>
            /* 05C8 */  0xA9, 0x18, 0xE1, 0x98, 0x1E, 0x3D, 0x1E, 0x58,  // .....=.X
            /* 05D0 */  0x7C, 0x15, 0x39, 0xF2, 0xF3, 0x7A, 0x2D, 0x78,  // |.9..z-x
            /* 05D8 */  0xA1, 0xF0, 0x68, 0x7D, 0x38, 0x42, 0x0D, 0x01,  // ..h}8B..
            /* 05E0 */  0x35, 0x80, 0x47, 0x93, 0x53, 0xF4, 0x74, 0x4E,  // 5.G.S.tN
            /* 05E8 */  0x0C, 0x77, 0x26, 0xE0, 0x93, 0xC6, 0x1C, 0x96,  // .w&.....
            /* 05F0 */  0x70, 0x0A, 0x6D, 0xFA, 0xD4, 0x68, 0xD4, 0xAA,  // p.m..h..
            /* 05F8 */  0x41, 0x99, 0x1A, 0x65, 0x1A, 0xD4, 0xEA, 0x53,  // A..e...S
            /* 0600 */  0xA9, 0x31, 0x63, 0x87, 0x05, 0xCB, 0x7B, 0x22,  // .1c...{"
            /* 0608 */  0xE8, 0x8C, 0xE0, 0x70, 0x20, 0x54, 0xF2, 0x43,  // ...p T.C
            /* 0610 */  0x41, 0x46, 0x4E, 0x8D, 0x06, 0x11, 0x90, 0x03,  // AFN.....
            /* 0618 */  0xBE, 0x96, 0x04, 0xE4, 0xB8, 0x20, 0x02, 0xB2,  // ..... ..
            /* 0620 */  0x54, 0x15, 0x40, 0x4C, 0x38, 0x88, 0x80, 0x1C,  // T.@L8...
            /* 0628 */  0xFA, 0x79, 0x20, 0x20, 0x4B, 0xB0, 0x01, 0xC4,  // .y  K...
            /* 0630 */  0x02, 0xF8, 0x00, 0x62, 0x71, 0x40, 0x04, 0xE4,  // ...bq@..
            /* 0638 */  0xF8, 0x34, 0x02, 0xB2, 0x18, 0x25, 0x40, 0x4C,  // .4...%@L
            /* 0640 */  0xFE, 0xE3, 0x41, 0x40, 0x0E, 0x03, 0x22, 0x20,  // ..A@.." 
            /* 0648 */  0x0B, 0x5F, 0xB7, 0x40, 0x1C, 0x1B, 0x84, 0x9B,  // ._.@....
            /* 0650 */  0x01, 0x61, 0xFF, 0x7F                           // .a..
        })
    }

    Scope (\)
    {
        OperationRegion (CMIO, SystemIO, 0x72, 0x02)
        Field (CMIO, ByteAcc, Lock, Preserve)
        {
            CINX,   8, 
            CDAT,   8
        }

        IndexField (CINX, CDAT, ByteAcc, NoLock, Preserve)
        {
            Offset (0x10), 
            CMX0,   32, 
            CMX1,   32, 
            CMX2,   32, 
            CMX3,   32, 
            CMX4,   32, 
            CMX5,   32, 
            CMX6,   32, 
            CMX7,   32, 
            TYOS,   8, 
            W7WL,   8, 
            W7BT,   8
        }
    }

    Scope (_SB.PCI0)
    {
        Method (GOST, 0, NotSerialized)
        {
            Local0 = Zero
            TYOS = Zero
            If (((OSYS > 0x07D0) || (OSYS < 0x07D6)))
            {
                Local0 = One
            }

            If ((OSYS == 0x07D6))
            {
                Local0 = 0x02
            }

            If ((OSYS == 0x07D9))
            {
                Local0 = 0x03
                TYOS = 0x37
            }

            If ((OSYS == 0x07DC))
            {
                Local0 = 0x04
            }

            If ((OSYS == 0x07DD))
            {
                Local0 = 0x04
            }

            If ((OSYS == 0x07DF))
            {
                Local0 = 0x05
            }

            If ((OSYS == 0x03E8))
            {
                Local0 = 0x40
            }

            Return (Local0)
        }
    }

    Scope (_SB.PCI0.LPCB.EC0)
    {
        Method (LVKI, 0, NotSerialized)
        {
        }

        Method (_Q5C, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x5C
            Notify (ADP1, 0x80) // Status Change
            VP1D |= 0x04
            Notify (VPC0, 0x80) // Status Change
        }

        Method (_Q28, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x28
            If (One)
            {
                VP2D |= 0x20
                Notify (VPC0, 0x80) // Status Change
            }
            Else
            {
            }
        }

        Method (_Q14, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x14
            If ((Zero == OPST))
            {
                OPST = One
            }
            Else
            {
                OPST = Zero
            }

            Notify (^^^PEG0.PEGP, 0xDF) // Hardware-Specific
        }

        Method (_Q22, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x22
            VP1D |= 0x20
            VP2D |= 0x04
            Notify (VPC0, 0x80) // Status Change
        }

        Method (_Q2B, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x2B
            If ((GWSS == Zero)){}
            Else
            {
                If (WLPS)
                {
                    WLEN = WRST /* \WRST */
                }

                If (BTPS)
                {
                    BTEN = BTST /* \BTST */
                    SGOV (0x02060000, BTST)
                }

                If (WNPS)
                {
                    WWAN = WNST /* \WNST */
                }
            }

            VP2D |= 0x02
            Notify (VPC0, 0x80) // Status Change
        }

        Method (_Q2E, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            OSTP = GOST ()
            P80H = 0x2E
            If (CAMC)
            {
                CAMC = Zero
            }
            Else
            {
                CAMC = One
            }

            VP1D |= 0x80
            Notify (VPC0, 0x80) // Status Change
        }

        Method (_Q70, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x70
            LAMN (0x7A)
        }

        Method (_Q72, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x72
            LAMN (0x7C)
        }

        Method (_Q74, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x74
            LAMN (0x7E)
        }

        Method (_Q75, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x75
            LAMN (0x7F)
        }

        Method (_Q27, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x27
        }

        Method (_Q3D, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x3D
            VP2D |= One
            Notify (VPC0, 0x80) // Status Change
        }
    }

    Scope (_SB.PCI0.LPCB.EC0)
    {
        Name (VCMD, Zero)
        Name (VDAT, Zero)
        Name (NOVS, Zero)
        Name (VP1D, Zero)
        Name (VP2D, Zero)
        Name (GSPS, Zero)
        Name (APDT, Zero)
        Name (OKAD, Zero)
        Name (ZPOS, Zero)
        Name (AMSV, Zero)
        Name (SPKP, Zero)
        Name (SMKP, Zero)
        Name (KBLT, Zero)
        Name (BRLV, Buffer (0x0B)
        {
             0x00                                             // .
        })
        Name (RLV2, Package (0x0B)
        {
            0x64, 
            0x5A, 
            0x50, 
            0x46, 
            0x3C, 
            0x32, 
            0x28, 
            0x1E, 
            0x14, 
            0x0A, 
            Zero
        })
        Name (BPWM, Buffer (0x0B)
        {
             0x00                                             // .
        })
        Name (PWM2, Buffer (0x0B)
        {
            /* 0000 */  0x10, 0x28, 0x3D, 0x50, 0x69, 0x82, 0x9B, 0xB4,  // .(=Pi...
            /* 0008 */  0xCD, 0xE6, 0xFF                                 // ...
        })
        Name (BBLM, Buffer (0x16)
        {
             0x00                                             // .
        })
        Device (VPC0)
        {
            Name (_HID, "VPC2004")  // _HID: Hardware ID
            Name (_UID, Zero)  // _UID: Unique ID
            Name (VCFG, 0x000D0440)
            Name (VPCD, Zero)
            Method (_STA, 0, NotSerialized)  // _STA: Status
            {
                Return (0x0F)
            }

            Method (_CFG, 0, NotSerialized)
            {
                Local0 = 0x00080000
                Local0 |= 0x80000000
                If ((CENS == One))
                {
                    Local0 |= 0x00800000
                }

                Local0 |= 0x00700000
                Local0 |= 0x00040000
                Local0 |= 0x00010000
                If ((WNPS == One))
                {
                    Local0 |= 0x00020000
                }

                If (SGEN)
                {
                    If ((SKUT == Zero))
                    {
                        Local0 |= 0x0100
                    }
                    Else
                    {
                        Local0 |= 0x0500
                        If ((^^^^GFX0.CGPU == One))
                        {
                            Local0 &= 0xFFFFEFFF
                        }
                        Else
                        {
                            Local0 |= 0x1000
                        }
                    }
                }

                Local0 |= 0x02
                If (SVCU)
                {
                    Local0 |= 0x04
                }

                Local0 |= 0x03000000
                Local0 |= 0x10000000
                Local0 |= 0x40000000
                Local0 |= 0x20000000
                Local0 |= 0x10
                Local0 |= 0x04000000
                Local0 |= 0x40000000
                Return (Local0)
            }

            Method (APPC, 1, Serialized)
            {
                Return (One)
            }

            Method (DBSL, 0, NotSerialized)
            {
                If ((OSYS <= 0x07D2))
                {
                    Return (RLV2) /* \_SB_.PCI0.LPCB.EC0_.RLV2 */
                }
                Else
                {
                    Return (BRLV) /* \_SB_.PCI0.LPCB.EC0_.BRLV */
                }
            }

            Method (SBSL, 0, NotSerialized)
            {
            }

            Method (STHT, 1, Serialized)
            {
                If (Arg0){}
                Else
                {
                }
            }

            Method (SHDC, 1, Serialized)
            {
                If (Arg0){}
                Else
                {
                }
            }

            Method (SVCR, 1, Serialized)
            {
                If ((Arg0 == 0x02))
                {
                    AMCB = Zero
                }
                ElseIf ((Arg0 == 0x03))
                {
                    AMCB = One
                }

                If ((Arg0 == 0x04)){}
            }

            Method (VPCR, 1, Serialized)
            {
                If ((Arg0 == One))
                {
                    VPCD = VCMD /* \_SB_.PCI0.LPCB.EC0_.VCMD */
                }
                ElseIf ((Arg0 == Zero))
                {
                    VPCD = VDAT /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                }
                Else
                {
                    VPCD = Zero
                }

                Return (VPCD) /* \_SB_.PCI0.LPCB.EC0_.VPC0.VPCD */
            }

            Method (VPCW, 2, Serialized)
            {
                VCMD = One
                If ((Arg0 == One))
                {
                    XCMD (Arg1)
                }
                Else
                {
                    VDAT = Arg1
                }

                VCMD = Zero
                Return (Zero)
            }

            Method (XCMD, 1, Serialized)
            {
                Switch (Arg0)
                {
                    Case (0x10)
                    {
                        VDAT = VP1D /* \_SB_.PCI0.LPCB.EC0_.VP1D */
                        VP1D = Zero
                    }
                    Case (0x1A)
                    {
                        VDAT = VP2D /* \_SB_.PCI0.LPCB.EC0_.VP2D */
                        VP2D = Zero
                    }
                    Case (0x11)
                    {
                        If ((OSYS <= 0x07D2))
                        {
                            VDAT = SizeOf (RLV2)
                        }
                        Else
                        {
                            VDAT = SizeOf (BRLV)
                        }
                    }
                    Case (0x12)
                    {
                        If ((OSYS < 0x07DC))
                        {
                            Local0 = (^^^^GFX0.CBLV & 0xFF)
                            If ((OSYS > 0x07D2))
                            {
                                ToBuffer (^^^^GFX0.BCLM, BBLM) /* \_SB_.PCI0.LPCB.EC0_.BBLM */
                                Local2 = Zero
                                While ((Local2 < SizeOf (BBLM)))
                                {
                                    Local1 = DerefOf (BBLM [Local2])
                                    Divide (Local2, 0x02, Local5, Local6)
                                    If ((Zero == Local5))
                                    {
                                        BPWM [Local6] = Local1
                                    }
                                    Else
                                    {
                                        BRLV [Local6] = (Local1 & 0x7F)
                                    }

                                    Local2++
                                }
                            }

                            Local2 = Zero
                            If ((OSYS <= 0x07D2))
                            {
                                Local3 = 0x0A
                            }
                            Else
                            {
                                Local3 = SizeOf (BPWM)
                            }

                            While ((Local2 <= Local3))
                            {
                                If ((OSYS <= 0x07D2))
                                {
                                    Local1 = DerefOf (PWM2 [Local2])
                                }
                                Else
                                {
                                    Local1 = DerefOf (BPWM [Local2])
                                }

                                Local4 = (Local1 * 0x64)
                                Divide (Local4, 0xFF, Local5, Local6)
                                If ((Local0 == Local6))
                                {
                                    VDAT = Local2
                                    Break
                                }

                                Local2++
                            }
                        }
                        Else
                        {
                            VDAT = Zero
                        }
                    }
                    Case (0x13)
                    {
                    }
                    Case (0x14)
                    {
                        VDAT = WLEN /* \_SB_.PCI0.LPCB.EC0_.WLEN */
                        WRST = VDAT /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                        W7WL = VDAT /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                        If ((VDAT == Zero))
                        {
                            W7WL = 0x02
                        }
                    }
                    Case (0x15)
                    {
                        WLEN = VDAT /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                        WRST = VDAT /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                        W7WL = VDAT /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                        If ((VDAT == Zero))
                        {
                            W7WL = 0x02
                        }
                    }
                    Case (0x16)
                    {
                        VDAT = GGOV (0x02060000)
                        BTST = VDAT /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                        W7BT = VDAT /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                        If ((VDAT == Zero))
                        {
                            W7BT = 0x02
                        }
                    }
                    Case (0x17)
                    {
                        BTEN = VDAT /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                        BTST = VDAT /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                        SGOV (0x02060000, BTST)
                        W7BT = VDAT /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                        If ((VDAT == Zero))
                        {
                            W7BT = 0x02
                        }
                    }
                    Case (0x18)
                    {
                        VDAT = BLST /* \_SB_.PCI0.LPCB.EC0_.BLST */
                    }
                    Case (0x33)
                    {
                        SWBL = VDAT /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                    }
                    Case (0x19)
                    {
                        VDAT = NOVS /* \_SB_.PCI0.LPCB.EC0_.NOVS */
                    }
                    Case (0x1B)
                    {
                        VDAT = TPEN /* \_SB_.PCI0.LPCB.EC0_.TPEN */
                        If ((TPEN == One)){}
                        Else
                        {
                        }
                    }
                    Case (0x1C)
                    {
                        TPCL = VDAT /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                    }
                    Case (0x1D)
                    {
                        VDAT = CAMC /* \_SB_.PCI0.LPCB.EC0_.CAMC */
                    }
                    Case (0x1E)
                    {
                        CAMC = VDAT /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                    }
                    Case (0x1F)
                    {
                        VDAT = WWAN /* \_SB_.PCI0.LPCB.EC0_.WWAN */
                        WNST = VDAT /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                    }
                    Case (0x20)
                    {
                        WWAN = VDAT /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                        WNST = VDAT /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                    }
                    Case (0x21)
                    {
                        VDAT = ZPOS /* \_SB_.PCI0.LPCB.EC0_.ZPOS */
                        ZPOS = Zero
                    }
                    Case (0x22)
                    {
                        DUST = VDAT /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                    }
                    Case (0x23)
                    {
                        VDAT = GWSS /* \_SB_.PCI0.LPCB.EC0_.GWSS */
                    }
                    Case (0x24)
                    {
                        BTEN = VDAT /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                        BTST = VDAT /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                        SGOV (0x02060000, BTST)
                        W7BT = VDAT /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                        If ((VDAT == Zero))
                        {
                            W7BT = 0x02
                        }

                        WWAN = VDAT /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                        WNST = VDAT /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                        WLEN = VDAT /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                        WRST = VDAT /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                        W7WL = VDAT /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                        If ((VDAT == Zero))
                        {
                            W7WL = 0x02
                        }
                    }
                    Case (0x25)
                    {
                        VDAT = One
                    }
                    Case (0x27)
                    {
                        VDAT = CBST /* \_SB_.PCI0.LPCB.EC0_.CBST */
                    }
                    Case (0x29)
                    {
                        VDAT = GSPS /* \_SB_.PCI0.LPCB.EC0_.GSPS */
                    }
                    Case (0x2B)
                    {
                        VDAT = One
                        If ((DUST == One))
                        {
                            VDAT |= 0x02 /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                        }
                        ElseIf (((DUS1 == One) && (DUS2 == Zero)))
                        {
                            VDAT |= 0x84 /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                        }
                        Else
                        {
                            VDAT |= 0x04 /* \_SB_.PCI0.LPCB.EC0_.VDAT */
                        }
                    }
                    Case (0x2F)
                    {
                        Local0 = Zero
                        If ((ACIO == One))
                        {
                            Local0 |= 0x08
                            ACIO = Zero
                        }

                        If ((DCIO == One))
                        {
                            Local0 |= 0x80
                            DCIO = Zero
                        }

                        If ((KBLT == One))
                        {
                            Local0 |= 0x02
                            KBLT = Zero
                        }

                        If ((AMSV == One))
                        {
                            If ((RSBM == Zero))
                            {
                                Local0 |= 0x04
                            }

                            AMSV = Zero
                        }
                        ElseIf ((AMSV == 0x02))
                        {
                            Local0 |= 0x02
                        }

                        VDAT = Local0
                    }
                    Case (0x31)
                    {
                        VDAT = OKAD /* \_SB_.PCI0.LPCB.EC0_.OKAD */
                        OKAD = Zero
                    }
                    Case (0x38)
                    {
                        Local0 = Zero
                        If ((MACS == One))
                        {
                            Local0 |= One
                        }
                        Else
                        {
                            Local0 |= 0x02
                        }

                        If ((DCS1 == One))
                        {
                            Local0 |= 0x04
                        }
                        Else
                        {
                            Local0 |= 0x08
                        }

                        If ((DCS2 == One))
                        {
                            Local0 |= 0x10
                        }
                        Else
                        {
                            Local0 |= 0x20
                        }

                        VDAT = Local0
                    }
                    Default
                    {
                        Noop
                    }

                }
            }
        }

        Method (_Q29, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x29
            NOVS = One
            VP1D |= 0x08
            Notify (VPC0, 0x80) // Status Change
        }

        Method (_Q76, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x76
            NOVS = Zero
            VP1D |= 0x08
            Notify (VPC0, 0x80) // Status Change
        }

