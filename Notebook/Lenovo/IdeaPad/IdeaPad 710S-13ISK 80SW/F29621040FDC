                KVTP,   1, 
                KCP1,   1, 
                    ,   1, 
                KCP2,   1
            }

            Method (RDEC, 1, Serialized)
            {
                If (ECON)
                {
                    OperationRegion (ECRM, EmbeddedControl, Arg0, One)
                    Field (ECRM, ByteAcc, Lock, Preserve)
                    {
                        ECRB,   8
                    }

                    Return (ECRB) /* \_SB_.PCI0.LPCB.EC0_.RDEC.ECRB */
                }
                Else
                {
                    Return (RBEC (Arg0))
                }
            }

            Field (RAM, ByteAcc, Lock, Preserve)
            {
                Offset (0x01), 
                TIID,   8, 
                Offset (0x17), 
                SMCS,   8, 
                SMPR,   8, 
                SMST,   8, 
                SMAR,   8, 
                SMCM,   8, 
