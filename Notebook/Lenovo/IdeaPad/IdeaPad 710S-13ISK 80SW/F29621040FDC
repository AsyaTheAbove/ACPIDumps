    Scope (_SB)
    {
        Device (AMW1)
        {
            Name (_HID, EisaId ("PNP0C14") /* Windows Management Instrumentation Device */)  // _HID: Hardware ID
            Name (_UID, One)  // _UID: Unique ID
            Name (WLMP, Zero)
            Name (WMID, Zero)
            Name (B0ED, Buffer (0x04)
            {
                 0x00, 0x00, 0x00, 0x00                           // ....
            })
            CreateDWordField (B0ED, Zero, WLID)
            Name (_WDG, Buffer (0x50)
            {
                /* 0000 */  0x6F, 0x0F, 0xBC, 0xAB, 0xA1, 0x8E, 0xD1, 0x11,  // o.......
                /* 0008 */  0x00, 0xA0, 0xC9, 0x06, 0x29, 0x10, 0x00, 0x00,  // ....)...
                /* 0010 */  0x41, 0x42, 0x01, 0x02, 0x72, 0x0F, 0xBC, 0xAB,  // AB..r...
                /* 0018 */  0xA1, 0x8E, 0xD1, 0x11, 0x00, 0xA0, 0xC9, 0x06,  // ........
                /* 0020 */  0x29, 0x10, 0x00, 0x00, 0xB0, 0x00, 0x01, 0x08,  // ).......
                /* 0028 */  0x0B, 0x00, 0x2C, 0x23, 0x87, 0xE7, 0x21, 0x46,  // ..,#..!F
                /* 0030 */  0x93, 0xB1, 0x02, 0xCD, 0xA9, 0x15, 0x5D, 0x1E,  // ......].
                /* 0038 */  0xB1, 0x00, 0x01, 0x08, 0x21, 0x12, 0x90, 0x05,  // ....!...
                /* 0040 */  0x66, 0xD5, 0xD1, 0x11, 0xB2, 0xF0, 0x00, 0xA0,  // f.......
                /* 0048 */  0xC9, 0x06, 0x29, 0x10, 0x44, 0x44, 0x01, 0x00   // ..).DD..
            })
            Method (_WED, 1, NotSerialized)  // _Wxx: Wake Event, xx=0x00-0xFF
            {
                P80H = Arg0
                If ((Arg0 == 0xB0))
                {
                    Return (B0ED) /* \_SB_.AMW1.B0ED */
                }
            }

            Method (WMAB, 3, NotSerialized)
            {
                P80H = 0xAB
                If ((Arg1 == One))
                {
                    Return (^^PCI0.LPCB.EC0.TRMD) /* \_SB_.PCI0.LPCB.EC0_.TRMD */
                }

                If ((Arg1 == 0x02))
                {
                    Return (^^PCI0.LPCB.EC0.XVAL) /* \_SB_.PCI0.LPCB.EC0_.XVAL */
                }

                If ((Arg1 == 0x03))
                {
                    Return (^^PCI0.LPCB.EC0.YVAL) /* \_SB_.PCI0.LPCB.EC0_.YVAL */
                }

                If ((Arg1 == 0x04))
                {
                    Return (^^PCI0.LPCB.EC0.ZVAL) /* \_SB_.PCI0.LPCB.EC0_.ZVAL */
                }

                If ((Arg1 == 0x05))
                {
                    Return (^^PCI0.LPCB.EC0.A4AL) /* \_SB_.PCI0.LPCB.EC0_.A4AL */
                }

                If ((Arg1 == 0x06))
                {
                    Return (^^PCI0.LPCB.EC0.A5AL) /* \_SB_.PCI0.LPCB.EC0_.A5AL */
                }

                If ((Arg1 == 0x07))
                {
                    Return (^^PCI0.LPCB.EC0.A6AL) /* \_SB_.PCI0.LPCB.EC0_.A6AL */
                }
            }
        }
    }

