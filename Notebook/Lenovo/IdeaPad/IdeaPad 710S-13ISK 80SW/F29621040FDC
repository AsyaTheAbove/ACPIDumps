    0000: 55 45 46 49 42 00 00 00 01 94 4C 45 4E 4F 56 4F  // UEFIB.....LENOVO
    0010: 43 42 2D 30 31 20 20 20 02 00 00 00 4C 45 4E 4F  // CB-01   ....LENO
    0020: 01 00 00 00 E2 D8 8E C6 C6 9D BD 4C 9D 94 DB 65  // ...........L...e
    0030: AC C5 C3 32 36 00 02 00 00 00 00 00 00 00 00 00  // ...26...........
    0040: 00 00                                            // ..


UEFI2
-----
[000h 0000 004h]                   Signature : "UEFI"    [UEFI Boot Optimization Table]
[004h 0004 004h]                Table Length : 0000012A
[008h 0008 001h]                    Revision : 01
[009h 0009 001h]                    Checksum : 24
[00Ah 0010 006h]                      Oem ID : "LENOVO"
[010h 0016 008h]                Oem Table ID : "CB-01   "
[018h 0024 004h]                Oem Revision : 00000001
[01Ch 0028 004h]             Asl Compiler ID : "LENO"
[020h 0032 004h]       Asl Compiler Revision : 00000001

[024h 0036 010h]             UUID Identifier : 0D9FB197-CEFC-4E91-ACB1-2535D9E5A844
[034h 0052 002h]                 Data Offset : 0036

