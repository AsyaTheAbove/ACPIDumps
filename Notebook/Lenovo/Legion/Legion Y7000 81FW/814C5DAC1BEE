            Method (CHKS, 0, NotSerialized)
            {
                Local0 = 0x07D0
                While (HMPR)
                {
                    Sleep (One)
                    Local0--
                    If (!Local0)
                    {
                        Return (0x8080)
                    }
                }

                If ((SMST & 0x80))
                {
                    Return (Zero)
                }

                Return (0x8081)
            }

