                    Method (WMAA, 3, NotSerialized)
                    {
                        If ((Arg1 == One))
                        {
                            If (^^PCI0.LPCB.EC0.ECAV)
                            {
                                If ((Acquire (^^PCI0.LPCB.EC0.LFCM, 0xA000) == Zero))
                                {
                                    Local0 = ^^PCI0.LPCB.EC0.CPUT /* \_SB_.PCI0.LPCB.EC0_.CPUT */
                                    Release (^^PCI0.LPCB.EC0.LFCM)
                                    Return (Local0)
                                }
                            }
