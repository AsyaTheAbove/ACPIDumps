                                    Release (^^PCI0.LPCB.EC0.LFCM)
                                    Return (Local0)
                                }
                            }
                        }

                        If ((Arg1 == 0x04))
                        {
                            Return (Zero)
                        }

                        If ((Arg1 == 0x05))
                        {
                            If (^^PCI0.LPCB.EC0.ECAV)
                            {
                                If ((Acquire (^^PCI0.LPCB.EC0.LFCM, 0xA000) == Zero))
                                {
                                    If (^^PCI0.LPCB.EC0.ADPT)
                                    {
