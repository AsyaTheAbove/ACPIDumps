                    Method (_WED, 1, NotSerialized)  // _Wxx: Wake Event, xx=0x00-0xFF
                    {
                        If ((Arg0 == 0xD0))
                        {
                            If (^^PCI0.LPCB.EC0.ECAV)
                            {
                                If ((Acquire (^^PCI0.LPCB.EC0.LFCM, 0xA000) == Zero))
                                {
                                    Local1 = ^^PCI0.LPCB.EC0.GUST /* \_SB_.PCI0.LPCB.EC0_.GUST */
                                    ^^PCI0.LPCB.EC0.GUST = Zero
                                    Release (^^PCI0.LPCB.EC0.LFCM)
                                    Return (Local1)
                                }
                            }
                        }

                        If ((Arg0 == 0xD1))
                        {
                            Return (0xFF)
                        }

                        If ((Arg0 == 0xE0))
                        {
                            Return (0xFF)
                        }

                        If ((Arg0 == 0xE1))
                        {
                            If (^^PCI0.LPCB.EC0.ECAV)
                            {
                                If ((Acquire (^^PCI0.LPCB.EC0.LFCM, 0xA000) == Zero))
                                {
                                    If ((^^PCI0.LPCB.EC0.FCST & One))
                                    {
                                        Release (^^PCI0.LPCB.EC0.LFCM)
                                        Return (Zero)
                                    }
                                    Else
                                    {
                                        Release (^^PCI0.LPCB.EC0.LFCM)
                                        Return (One)
                                    }
                                }
                            }
                        }

                        If ((Arg0 == 0xE2))
                        {
                            If (^^PCI0.LPCB.EC0.ECAV)
                            {
                                If ((Acquire (^^PCI0.LPCB.EC0.LFCM, 0xA000) == Zero))
                                {
                                    Local2 = Zero
                                    If (((^^PCI0.LPCB.EC0.GDST & 0x02) == 0x02))
                                    {
                                        Local2 |= One
                                    }

                                    If (((^^PCI0.LPCB.EC0.GDST & 0x04) == 0x04))
                                    {
                                        Local2 |= 0x02
                                    }

                                    Release (^^PCI0.LPCB.EC0.LFCM)
                                    Return (Local2)
                                }
                            }
                        }
                    }

                    Name (WQDD, Buffer (0x140F)
