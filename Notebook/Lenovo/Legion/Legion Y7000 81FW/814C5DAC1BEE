                                        {
                                            Release (^^PCI0.LPCB.EC0.LFCM)
                                            Return (One)
                                        }
                                    }

                                    Release (^^PCI0.LPCB.EC0.LFCM)
                                    Return (Zero)
                                }
                            }
                        }

                        If ((Arg1 == 0x06))
                        {
                            If (^^PCI0.LPCB.EC0.ECAV)
                            {
