    External (_SB_.PCI0.LPC_.EC__.BRNS, MethodObj)    // Warning: Unknown method, guessing 3 arguments
    External (_SB_.PCI0.LPC_.EC__.BRTO, MethodObj)    // Warning: Unknown method, guessing 5 arguments
    External (_SB_.PCI0.LPC_.EC__.BRTT, MethodObj)    // Warning: Unknown method, guessing 0 arguments
    External (_SB_.PCI0.VID_, DeviceObj)
