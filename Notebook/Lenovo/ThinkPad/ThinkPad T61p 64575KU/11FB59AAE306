        {
            Local0 = Match (BCLP, MEQ, Arg0, MTR, 0x00, 0x00)
            If ((Local0 != Ones))
            {
                \BRLV = DerefOf (BCLL [Local0])
                \_SB.PCI0.LPC.EC.BRTO (0x0A, \_SB.PCI0.LPC.EC.BRNS (\UCMS (0x16, \_SB.PCI0.LPC.EC.BRTT ())))
            }
        }

        Method (_BQC, 0, NotSerialized)  // _BQC: Brightness Query Current
        {
            Local0 = \BRLV /* External reference */
            If ((Local0 <= 0x0F))
            {
                Return (DerefOf (BCLP [Local0]))
            }
            Else
            {
                Return (Zero)
            }
        }
    }

    Scope (\_SB.PCI0.AGP.VID.LCD0)
    {
        Method (_BCL, 0, NotSerialized)  // _BCL: Brightness Control Levels
        {
            NBCF = 0x01
