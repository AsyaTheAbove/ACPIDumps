    {
        Method (KOU2, 0, NotSerialized)
        {
            Stall (0x64)
        }
    }

    Scope (\_SB.PCI0.VID)
    {
    }

    Scope (\_SB.PCI0.VID.LCD0)
    {
        Method (_BCL, 0, NotSerialized)  // _BCL: Brightness Control Levels
        {
            \NBCF = 0x01
            Return (Package (0x12)
            {
                0x64, 
                0x64, 
                0x14, 
                0x19, 
                0x1E, 
