                                {}
                        })
                        CreateWordField (EPBF, \_SB.PCI0.LPC.DECP._Y25._MIN, ECN0)  // _MIN: Minimum Base Address
                        CreateWordField (EPBF, \_SB.PCI0.LPC.DECP._Y25._MAX, ECX0)  // _MAX: Maximum Base Address
                        CreateByteField (EPBF, \_SB.PCI0.LPC.DECP._Y25._LEN, ECL0)  // _LEN: Length
                        CreateWordField (EPBF, \_SB.PCI0.LPC.DECP._Y26._MIN, ECN1)  // _MIN: Minimum Base Address
                        CreateWordField (EPBF, \_SB.PCI0.LPC.DECP._Y26._MAX, ECX1)  // _MAX: Maximum Base Address
