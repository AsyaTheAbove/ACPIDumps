                            /* 1050 */  0x10, 0x94, 0x04, 0x05, 0xA1, 0xE1, 0x9E, 0x1B,  // ........
                            /* 1058 */  0x1A, 0x38, 0x71, 0x09, 0xC2, 0x62, 0x82, 0xD0,  // .8q..b..
                            /* 1060 */  0x50, 0x8F, 0x39, 0x81, 0xF8, 0xFF, 0x0F         // P.9....
                        })
                        Method (WMAB, 3, NotSerialized)
                        {
                            If ((SizeOf (Arg2) >= 0x04))
                            {
                                CreateDWordField (Arg2, 0x00, FUNC)
                                If ((SizeOf (Arg2) >= 0x08))
                                {
                                    CreateDWordField (Arg2, 0x04, SUBF)
                                    Local3 = SUBF /* \_SB_.WMI0.WMAB.SUBF */
                                }
                                Else
                                {
                                    Local3 = 0x00
                                }

                                Local0 = (SizeOf (Arg2) - 0x08)
                                If ((Local0 > 0x00))
                                {
                                    Local2 = Buffer (Local0){}
                                    While (Local0)
                                    {
                                        Local0--
                                        Local2 [Local0] = DerefOf (Arg2 [(Local0 + 0x08)]
                                            )
                                    }
                                }
                                Else
                                {
                                    Local2 = Buffer (0x04)
                                        {
                                             0x00, 0x00, 0x00, 0x00                           // ....
                                        }
                                }

                                Return (\_SB.PCI0.AGP.VID.NVIF (FUNC, Local3, Local2))
                            }
                        }
                    }
                }

                OperationRegion (PEGC, PCI_Config, 0x00, 0x0100)
                Field (PEGC, DWordAcc, NoLock, Preserve)
                {
                    Offset (0xEC), 
                    GMGP,   1, 
                    HPGP,   1, 
                    PMGP,   1
                }
            }

            Device (IGBE)
