                }
            }

            Method (_TMP, 0, Serialized)  // _TMP: Temperature
            {
                If (CondRefOf (\_SB.PCI0.LPCB.EC.GSTM))
                {
                    Local0 = \_SB.PCI0.LPCB.EC.GSTM (0x02)
                }
                Else
                {
                    Local0 = \GTST (0x02)
                    If ((Local0 == 0x80))
                    {
                        Local0 = One
                    }
                }

                Local1 = \_TZ._C2K (Local0)
                Return (Local1)
            }

            Name (PATC, Zero)
            Name (GTSH, 0x14)
