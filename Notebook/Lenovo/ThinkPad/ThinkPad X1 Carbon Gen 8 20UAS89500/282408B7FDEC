                                DerefOf (DEVY [0x43]) [One] = One
                            }

                            If ((TCNT >= 0x14))
                            {
                                DerefOf (DEVY [0x44]) [One] = One
                            }

                            If (((PEPC & 0x0400) == Zero))
                            {
                                DerefOf (DEVY [Zero]) [One] = Zero
                                DerefOf (DEVY [One]) [One] = Zero
                                DerefOf (DEVY [0x02]) [One] = Zero
                                DerefOf (DEVY [0x03]) [One] = Zero
