                    /* 06B0 */  0xEF, 0xAD, 0x9B, 0xEF, 0x12, 0xDD, 0x76, 0xE6,  // ......v.
                    /* 06B8 */  0xA3, 0x66, 0x00, 0x2A, 0x00, 0x6A, 0x18, 0x15,  // .f.*.j..
                    /* 06C0 */  0x5C, 0x02, 0xCE, 0x4C, 0xDD, 0x3A, 0xF4, 0x4A,  // \..L.:.J
