            Name (YSEM, Zero)
            Name (DPTE, Zero)
            Method (_OSC, 4, Serialized)  // _OSC: Operating System Capabilities
            {
                CreateDWordField (Arg3, Zero, STS1)
                CreateDWordField (Arg3, 0x04, CAP1)
                If ((CAP1 & One))
                {
                    If ((DPTE == Zero))
                    {
                        DPTE = One
                        \_SB.PCI0.LPCB.EC.HKEY.DYTC (0x800F0001)
                        Local2 = (\STDV << 0x10)
                        If (\_SB.PCI0.LPCB.EC.HKEY.DHKC) {}
                    }
                }
