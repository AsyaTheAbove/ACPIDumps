                    /* 06D8 */  0xB1, 0x8B, 0x7C, 0x75, 0xBC, 0xFA, 0x29, 0x35,  // ..|u..)5
                    /* 06E0 */  0xFA, 0xF2, 0xEB, 0x24, 0x48, 0x9C, 0x1C, 0x29,  // ...$H..)
                    /* 06E8 */  0xA8, 0x03, 0x4A, 0xE4, 0xE5, 0xB9, 0x90, 0xD5,  // ..J.....
                    /* 06F0 */  0x64, 0x1B, 0xBB, 0x6E, 0xF5, 0xF2, 0x75, 0x60,  // d..n..u`
                    /* 06F8 */  0xE0, 0x0B, 0x8D, 0xFC, 0xF1, 0xD4, 0x21, 0x53,  // ......!S
                    /* 0700 */  0xBA, 0x73, 0x46, 0xE6, 0x04, 0xED, 0xF8, 0xE1,  // .sF.....
                    /* 0708 */  0xBF, 0x79, 0xA5, 0x59, 0x22, 0x31, 0xB9, 0x8D,  // .y.Y"1..
                    /* 0710 */  0x42, 0x08, 0x28, 0xC1, 0x8E, 0x68               // B.(..h
                }
            })
            Method (GBDV, 0, Serialized)
            {
                Switch (\MPID)
                {
                    Case (Zero)
                    {
                        ADBG ("Load DV File0")
                        Return (DBD0) /* \_SB_.IETM.DBD0 */
                    }
                    Case (One)
                    {
                        ADBG ("Load DV File1")
                        Return (DBD1) /* \_SB_.IETM.DBD1 */
                    }
                    Default
                    {
                        ADBG ("ERROR to load the DV")
                    }

                }
            }
        }
    }
}



