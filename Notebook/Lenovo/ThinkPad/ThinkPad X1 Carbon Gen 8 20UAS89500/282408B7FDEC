                                        DerefOf (DEVY [0x1D]) [One] = One
                                    }
                                }

                                If (CondRefOf (\_SB.PCI0.RP08.PXSX.WIST))
                                {
                                    If (^^PCI0.RP08.PXSX.WIST ())
                                    {
                                        DerefOf (DerefOf (DerefOf (DEVY [0x1E]) [0x02]
                                            ) [One]) [One] = 0x03
                                        DerefOf (DerefOf (DerefOf (DEVY [0x1E]) [0x02]
