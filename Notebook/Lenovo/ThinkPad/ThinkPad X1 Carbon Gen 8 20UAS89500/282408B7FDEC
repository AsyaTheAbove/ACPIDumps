                                            ) [One]) [One] = 0x03
                                        DerefOf (DerefOf (DerefOf (DEVY [0x19]) [0x02]
                                            ) [One]) [0x02] = Zero
                                        DerefOf (DEVY [0x19]) [One] = One
                                    }
                                }

                                If (CondRefOf (\_SB.PCI0.RP04.PXSX.WIST))
                                {
                                    If (^^PCI0.RP04.PXSX.WIST ())
                                    {
                                        DerefOf (DerefOf (DerefOf (DEVY [0x1A]) [0x02]
                                            ) [One]) [One] = 0x03
                                        DerefOf (DerefOf (DerefOf (DEVY [0x1A]) [0x02]
