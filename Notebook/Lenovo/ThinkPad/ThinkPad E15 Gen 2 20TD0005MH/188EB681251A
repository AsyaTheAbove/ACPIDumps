    External (_SB_.DDRF, UnknownObj)
    External (_SB_.PC00.LPCB.H_EC.CFSP, UnknownObj)
    External (_SB_.PC00.LPCB.H_EC.DIM0, UnknownObj)
    External (_SB_.PC00.LPCB.H_EC.DIM1, UnknownObj)
