                        Local0++
                    }
                }

                Method (VLOF, 2, Serialized)
                {
                    Name (DRVA, Zero)
                    Local1 = Arg0
                    Local0 = Zero
                    While ((Local1 > Local0))
                    {
                        DRVA = ToInteger (DerefOf (Arg1 [Local0]))
                        Switch (ToInteger (DRVA))
                        {
                            Case (0x80E0FFFF)
                            {
                                If ((HBSL & One))
                                {
                                    If (D3CV (^RP01.RSPT, ^RP01.RSPI))
                                    {
                                        ^RP03.RL23 ()
                                        PSD3 (^RP03.RSPI)
                                        HBCM (^RP03.RSPI, Zero)
                                    }
                                }

                                ^RP01.VPOF ()
                            }
                            Case (0x80E1FFFF)
                            {
                                ^RP02.VPOF ()
                            }
                            Case (0x80E2FFFF)
                            {
                                If ((HBSL & One)){}
                                Else
                                {
                                    ^RP03.VPOF ()
                                }
                            }
                            Case (0x80E3FFFF)
                            {
                                ^RP04.VPOF ()
                            }
                            Case (0x80E4FFFF)
                            {
                                If ((HBSL & 0x02))
