                                    }
                                }

                                ^RP05.VPOF ()
                            }
                            Case (0x80E5FFFF)
                            {
                                ^RP06.VPOF ()
                            }
                            Case (0x80E6FFFF)
                            {
                                If ((HBSL & 0x02)){}
                                Else
                                {
                                    ^RP07.VPOF ()
                                }
                            }
                            Case (0x80E7FFFF)
                            {
                                ^RP08.VPOF ()
                            }
                            Case (0x80E8FFFF)
                            {
                                If ((HBSL & 0x04))
                                {
