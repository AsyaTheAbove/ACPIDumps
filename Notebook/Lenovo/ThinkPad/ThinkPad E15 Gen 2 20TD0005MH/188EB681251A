                                    {
                                        ^RP23.RLA ()
                                        Sleep (0x64)
                                    }
                                }
                            }
                            Case (0x000000080DDAFFFF)
                            {
                                ^RP22.VPON ()
                            }
                            Case (0x80DEFFFF)
                            {
                                If ((HBSL & 0x20)){}
                                Else
                                {
                                    ^RP23.VPON ()
                                }
                            }
                            Case (0x80DFFFFF)
                            {
                                ^RP24.VPON ()
                            }
                            Case (0x8030FFFF)
                            {
                                ^PEG0.VPON ()
                            }
                            Case (0x8008FFFF)
                            {
                                ^PEG1.VPON ()
                            }
                            Case (0x8009FFFF)
                            {
                                ^PEG2.VPON ()
                            }
                            Case (0x800AFFFF)
