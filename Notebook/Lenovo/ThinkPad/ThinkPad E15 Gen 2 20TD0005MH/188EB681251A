        Method (_Q26, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            Sleep (0x01F4)
            Notify (AC, 0x80) // Status Change
            If (VIGD)
            {
                If (WVIS)
                {
                    VBTD ()
                }

                BRNS ()
            }
            Else
            {
                SCMS (0x12)
            }

            If (WXPF)
            {
                Acquire (MCPU, 0xFFFF)
            }

            PWRS = One
            If ((CFGD & One))
            {
                If ((OSPX || CPPX))
                {
                    PNTF (0x80)
                }
            }

            If (WXPF)
            {
                Sleep (0x64)
            }

            If (OSC4)
            {
                PNTF (0x81)
            }

            If (WXPF)
            {
                Release (MCPU)
            }

            ^HKEY.MHKQ (0x6040)
        }

        Method (_Q27, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
