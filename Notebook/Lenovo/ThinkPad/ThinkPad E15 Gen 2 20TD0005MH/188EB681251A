            {
                D0VI,   16, 
                Offset (0x2C), 
                S0VI,   16, 
                S0DI,   16
            }

            OperationRegion (CAP0, SystemMemory, ((XBAS + (SBN0 << 0x14)) + EECP), 0x14)
