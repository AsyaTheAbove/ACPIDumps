                    One)) && ((FDRP & 0x80) == 0x80)))
                {
                    Return (HETF) /* \_SB_.PC00.SAT0.PRT1.HETF */
                }
                Else
                {
                    Return (HDTF) /* \_SB_.PC00.SAT0.PRT1.HDTF */
                }
            }
