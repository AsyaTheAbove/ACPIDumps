                            \_SB.SHPO (P2WK, Zero)
                        }
                    }
                }
            }
        }

        Scope (\_SB.PC00.PEG3)
        {
            Name (WKEN, Zero)
            PowerResource (PG03, 0x00, 0x0000)
            {
                Name (_STA, One)  // _STA: Status
                Method (_ON, 0, Serialized)  // _ON_: Power On
                {
                    If ((OSYS > 0x07D9))
                    {
                        PGON (0x03)
