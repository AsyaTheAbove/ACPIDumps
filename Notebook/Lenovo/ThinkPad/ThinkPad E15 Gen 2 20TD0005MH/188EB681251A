            {
                Offset (0x0C), 
                LCP0,   32, 
                LCT0,   16
            }

            OperationRegion (OPG1, SystemMemory, (XBAS + 0x8000), 0x1000)
            Field (OPG1, AnyAcc, NoLock, Preserve)
            {
                P1VI,   16, 
                P1DI,   16, 
                Offset (0x06), 
                DSO1,   16, 
                Offset (0x34), 
                CPO1,   8, 
                Offset (0xB0), 
