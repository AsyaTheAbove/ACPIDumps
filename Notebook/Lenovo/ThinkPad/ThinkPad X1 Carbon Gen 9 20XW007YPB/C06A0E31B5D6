                LCP1,   32, 
                LCT1,   16
            }

            OperationRegion (OPG2, SystemMemory, (XBAS + 0x9000), 0x1000)
            Field (OPG2, AnyAcc, NoLock, Preserve)
            {
                P2VI,   16, 
                P2DI,   16, 
                Offset (0x06), 
                DSO2,   16, 
                Offset (0x34), 
                CPO2,   8, 
                Offset (0xB0), 
                    ,   4, 
