                    If ((RTD3 != Zero))
                    {
                        PowerResource (V3PR, 0x04, 0x0000)
                        {
                            Name (_STA, One)  // _STA: Status
                            Method (_ON, 0, NotSerialized)  // _ON_: Power On
                            {
                                If (VD3A)
                                {
                                    VLON (VV3S, VV3D)
                                    _STA = One
                                }
                                Else
                                {
