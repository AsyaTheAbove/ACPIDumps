        Scope (\_SB.PC00)
        {
            Name (IVID, 0xFFFF)
            Name (PEBA, Zero)
            Name (PION, Zero)
            Name (PIOF, Zero)
            Name (PBUS, Zero)
            Name (PDEV, Zero)
            Name (PFUN, Zero)
            Name (EBUS, Zero)
            Name (EDEV, Zero)
            Name (EFN0, Zero)
            Name (EFN1, One)
            Name (LTRS, Zero)
            Name (OBFS, Zero)
            Name (DSOF, 0x06)
            Name (CPOF, 0x34)
            Name (SBOF, 0x19)
            Name (ELC0, Zero)
            Name (ECP0, 0xFFFFFFFF)
            Name (H0VI, Zero)
            Name (H0DI, Zero)
            Name (ELC1, Zero)
            Name (ECP1, 0xFFFFFFFF)
            Name (H1VI, Zero)
            Name (H1DI, Zero)
            Name (ELC2, Zero)
            Name (ECP2, 0xFFFFFFFF)
            Name (H2VI, Zero)
            Name (H2DI, Zero)
            Name (ELC3, Zero)
            Name (ECP3, 0xFFFFFFFF)
            Name (H3VI, Zero)
            Name (H3DI, Zero)
            Name (AFL0, Zero)
            Name (AFL1, Zero)
            Name (AFL2, Zero)
            Name (AFL3, Zero)
            Name (AFL4, Zero)
            Name (AFL5, Zero)
            Name (AFL6, Zero)
            Name (AFL7, Zero)
            Name (AFL8, Zero)
            Name (AFL9, Zero)
            Name (AFLA, Zero)
            Name (AFLB, Zero)
            Name (AFLC, Zero)
            Name (AFLD, Zero)
            Name (AFLE, Zero)
            Name (AFLF, Zero)
            OperationRegion (OPG0, SystemMemory, (XBAS + 0x00030000), 0x1000)
            Field (OPG0, AnyAcc, NoLock, Preserve)
            {
                P0VI,   16, 
                P0DI,   16, 
