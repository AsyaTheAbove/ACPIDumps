    Scope (\_SB.IETM)
    {
        Method (GDDV, 0, Serialized)
        {
            If ((\MPID == Zero))
            {
                Return (Package (0x01)
                {
                    Buffer (0x08F6)
                    {
                        /* 0000 */  0xE5, 0x1F, 0x94, 0x00, 0x00, 0x00, 0x00, 0x02,  // ........
