            Name (WQBA, Buffer (0x0EAC)
            {
                /* 0000 */  0x46, 0x4F, 0x4D, 0x42, 0x01, 0x00, 0x00, 0x00,  // FOMB....
                /* 0008 */  0x9C, 0x0E, 0x00, 0x00, 0xFA, 0x72, 0x00, 0x00,  // .....r..
