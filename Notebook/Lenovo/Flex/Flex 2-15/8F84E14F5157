                        \_SB.PCI0.RP01.PEGP.DGPW = One
                        While ((\_SB.PCI0.RP01.PEGP.DGPG != Zero))
                        {
                            Sleep (One)
                        }

                        Sleep (0x32)
                        _STA = Zero
                        MSD3 = 0x03
                    }
                }
            }

            PowerResource (NVP2, 0x00, 0x0000)
            {
