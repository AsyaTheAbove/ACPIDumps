            {
                \_SB.AMW0.WLID = Arg0
                Notify (\_SB.AMW0, 0xB0) // Device-Specific
            }
            Else
            {
                TRPS (0x13, Arg0)
            }
        }

        Method (RBEC, 1, NotSerialized)
        {
