    External (_SB_.PCI0.RP01.PEGP, DeviceObj)
    External (_SB_.PCI0.RP01.PEGP._ADR, UnknownObj)
    External (P8XH, MethodObj)    // Warning: Unknown method, guessing 2 arguments
    External (SGMD, UnknownObj)

    If ((SGMD == 0x02))
    {
        Name (RPA0, 0x001C0000)
        Scope (\_SB.PCI0.RP01)
        {
            OperationRegion (RPCX, PCI_Config, Zero, 0x0340)
            Field (RPCX, DWordAcc, NoLock, Preserve)
            {
                PVID,   16, 
                PDID,   16
            }

            OperationRegion (RPCI, SystemMemory, 0xE00E0000, 0x1000)
