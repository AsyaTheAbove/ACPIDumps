    Scope (_SB.PCI0.LPCB.H_EC)
    {
        Name (NDLC, Zero)
        Method (SNDL, 1, NotSerialized)
        {
            NDLC = Zero
            NADL = Zero
            NDL2 = Zero
            NDL3 = Zero
            NDL4 = Zero
            NDL5 = Zero
            If ((Arg0 & 0x08))
            {
                S2ND (0x0400)
            }

            If ((Arg0 & One))
            {
                S2ND (0x0100)
            }

            If ((Arg0 & 0x02))
            {
                S2ND (0x0200)
            }

            If ((Arg0 & 0x04))
            {
                S2ND (0x0300)
            }
        }

        Method (S2ND, 1, NotSerialized)
        {
            NDLC++
            If ((NDLC == One))
            {
                NADL = Arg0
            }

            If ((NDLC == 0x02))
            {
                NDL2 = Arg0
            }

            If ((NDLC == 0x03))
            {
                NDL3 = Arg0
            }

            If ((NDLC == 0x04))
            {
                NDL4 = Arg0
            }

            If ((NDLC == 0x05))
            {
                NDL5 = Arg0
            }
        }

        Method (SPBR, 2, NotSerialized)
        {
            If (Arg1)
            {
                SBLX (Arg0)
            }
            Else
            {
                Local0 = DMOD /* External reference */
                If ((Local0 == Zero))
                {
                    HKEY (0x1C)
                }
                ElseIf ((Local0 == One))
                {
                    SBLX (Arg0)
                }
            }
        }

        Method (SBLX, 1, NotSerialized)
        {
            ^^^GFX0.BCLP = Arg0
            ^^^GFX0.BCLP |= 0x80000000
            ASLC = 0x02
            ASLE = One
        }
    }

    Scope (_SB.PCI0)
    {
        Method (GOST, 0, NotSerialized)
        {
            Local0 = Zero
            TYOS = Zero
            If (((OSYS > 0x07D0) && (OSYS < 0x07D6)))
            {
                Local0 = One
            }

            If ((OSYS == 0x07D6))
            {
                Local0 = 0x02
            }

            If ((OSYS == 0x07D9))
            {
                Local0 = 0x03
                TYOS = 0x37
            }

            If ((OSYS == 0x07DC))
            {
                Local0 = 0x04
            }

            If ((OSYS == 0x07DD))
            {
                Local0 = 0x04
            }

            If (_OSI ("Linux"))
            {
                Local0 = 0x40
            }

            Return (Local0)
        }
    }

    Scope (_SB.PCI0.LPCB.H_EC)
    {
        Method (LVKI, 0, NotSerialized)
        {
        }

        Method (_Q5C, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x5C
            Notify (ADP1, 0x80) // Status Change
            VP1D |= 0x04
            Notify (VPC0, 0x80) // Status Change
        }

        Method (_Q28, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x28
            If (One)
            {
                VP2D |= 0x20
                Notify (VPC0, 0x80) // Status Change
            }
            Else
            {
            }
        }

        Method (_Q14, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x14
            If ((Zero == OPST))
            {
                OPST = One
            }
            Else
            {
                OPST = Zero
            }
        }

        Method (_Q22, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x22
            VP1D |= 0x20
            Notify (VPC0, 0x80) // Status Change
        }

        Method (_Q2B, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x2B
            If ((GWSS == Zero)){}
            Else
            {
                If (WLPS)
                {
                    WLEN = WRST /* \WRST */
                }

                If (BTPS)
                {
                    BTEN = BTSU /* \BTSU */
                }

                If (WNPS)
                {
                    WWAN = WNST /* \WNST */
                }
            }

            VP2D |= 0x02
            Notify (VPC0, 0x80) // Status Change
        }

        Method (_Q2E, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            OSTP = GOST ()
            If ((OSTP != 0x04))
            {
                P80H = 0x2E
                If (CAMC)
                {
                    CAMC = Zero
                }
                Else
                {
                    CAMC = One
                }

                VP1D |= 0x80
                Notify (VPC0, 0x80) // Status Change
            }
        }

        Method (_Q70, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x70
            LAMN (0x7A)
        }

        Method (_Q72, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x72
            LAMN (0x7C)
        }

        Method (_Q73, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x73
            LAMN (0x7D)
        }

        Method (_Q74, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x74
            LAMN (0x7E)
        }

        Method (_Q75, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x75
            LAMN (0x7F)
        }

        Method (_Q27, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x27
        }
    }

    Scope (_SB.PCI0.LPCB.H_EC)
    {
        Name (VCMD, Zero)
        Name (VDAT, Zero)
        Name (NOVS, Zero)
        Name (VP1D, Zero)
        Name (VP2D, Zero)
        Name (GSPS, Zero)
        Name (APDT, Zero)
        Name (OKAD, Zero)
        Name (ZPOS, Zero)
        Name (AMSV, Zero)
        Name (SPKP, Zero)
        Name (SMKP, Zero)
        Name (BRLV, Buffer (0x0B)
        {
             0x00                                             // .
        })
        Name (RLV2, Package (0x0B)
        {
            0x64, 
            0x5A, 
            0x50, 
            0x46, 
            0x3C, 
            0x32, 
            0x28, 
            0x1E, 
            0x14, 
            0x0A, 
            Zero
        })
        Name (BPWM, Buffer (0x0B)
        {
             0x00                                             // .
        })
        Name (PWM2, Buffer (0x0B)
        {
            /* 0000 */  0x10, 0x28, 0x3D, 0x50, 0x69, 0x82, 0x9B, 0xB4,  // .(=Pi...
            /* 0008 */  0xCD, 0xE6, 0xFF                                 // ...
        })
        Name (BBLM, Buffer (0x16)
        {
             0x00                                             // .
        })
        Device (VPC0)
        {
            Name (_HID, "VPC2004")  // _HID: Hardware ID
            Name (_UID, Zero)  // _UID: Unique ID
            Name (VCFG, 0x000D0440)
            Name (VPCD, Zero)
            Method (_STA, 0, NotSerialized)  // _STA: Status
            {
                Return (0x0F)
            }

            Method (_CFG, 0, NotSerialized)
            {
                Local0 = 0x00080000
                If ((CENS == One))
                {
                    Local0 |= 0x00800000
                }

                Local0 |= 0x00700000
                If ((WLPS == One))
                {
                    Local0 |= 0x00040000
                }

                If ((BTPS == One))
                {
                    Local0 |= 0x00010000
                }

                If ((WNPS == One))
                {
                    Local0 |= 0x00020000
                }

                If (SGEN)
                {
                    If ((SKUT == Zero))
                    {
                        Local0 |= 0x0100
                    }
                    Else
                    {
                        Local0 |= 0x0500
                        If ((^^^^GFX0.CGPU == One))
                        {
                            Local0 &= 0xFFFFEFFF
                        }
                        Else
                        {
                            Local0 |= 0x1000
                        }
                    }
                }

                Local0 |= 0x02
                If (SVCU)
                {
                    Local0 |= 0x04
                }

                Local0 |= 0x01000000
                Local0 |= 0x02000000
                If ((((PJID == One) || (PJID == 0x04)) || (PJID == 0x05)))
                {
                    Local0 |= 0x08000000
                }

                Local0 |= 0x10000000
                Local0 |= 0x40000000
                Return (Local0)
            }

            Method (APPC, 1, Serialized)
            {
                Return (One)
            }

            Method (DBSL, 0, NotSerialized)
            {
                If ((OSYS <= 0x07D2))
                {
                    Return (RLV2) /* \_SB_.PCI0.LPCB.H_EC.RLV2 */
                }
                Else
                {
                    Return (BRLV) /* \_SB_.PCI0.LPCB.H_EC.BRLV */
                }
            }

            Method (SBSL, 0, NotSerialized)
            {
            }

            Method (STHT, 1, Serialized)
            {
                If (Arg0){}
                Else
                {
                }
            }

            Method (SHDC, 1, Serialized)
            {
                If (Arg0){}
                Else
                {
                }
            }

            Method (SVCR, 1, Serialized)
            {
                If ((Arg0 == 0x02))
                {
                    AMCB = Zero
                }
                ElseIf ((Arg0 == 0x03))
                {
                    AMCB = One
                }

                If ((Arg0 == 0x04)){}
            }

            Method (VPCR, 1, Serialized)
            {
                If ((Arg0 == One))
                {
                    VPCD = VCMD /* \_SB_.PCI0.LPCB.H_EC.VCMD */
                }
                ElseIf ((Arg0 == Zero))
                {
                    VPCD = VDAT /* \_SB_.PCI0.LPCB.H_EC.VDAT */
                }
                Else
                {
                    VPCD = Zero
                }

                Return (VPCD) /* \_SB_.PCI0.LPCB.H_EC.VPC0.VPCD */
            }

            Method (VPCW, 2, Serialized)
            {
                VCMD = One
                If ((Arg0 == One))
                {
                    XCMD (Arg1)
                }
                Else
                {
                    VDAT = Arg1
                }

                VCMD = Zero
                Return (Zero)
            }

            Method (XCMD, 1, Serialized)
            {
                Switch (Arg0)
                {
                    Case (0x10)
                    {
                        VDAT = VP1D /* \_SB_.PCI0.LPCB.H_EC.VP1D */
                        VP1D = Zero
                    }
                    Case (0x1A)
                    {
                        VDAT = VP2D /* \_SB_.PCI0.LPCB.H_EC.VP2D */
                        VP2D = Zero
                    }
                    Case (0x11)
                    {
                        If ((OSYS <= 0x07D2))
                        {
                            VDAT = SizeOf (RLV2)
                        }
                        Else
                        {
                            VDAT = SizeOf (BRLV)
                        }
                    }
                    Case (0x12)
                    {
                        Local0 = (^^^^GFX0.CBLV & 0xFF)
                        If ((OSYS > 0x07D2))
                        {
                            ToBuffer (^^^^GFX0.BCLM, BBLM) /* \_SB_.PCI0.LPCB.H_EC.BBLM */
                            Local2 = Zero
                            While ((Local2 < SizeOf (BBLM)))
                            {
                                Local1 = DerefOf (BBLM [Local2])
                                Divide (Local2, 0x02, Local5, Local6)
                                If ((Zero == Local5))
                                {
                                    BPWM [Local6] = Local1
                                }
                                Else
                                {
                                    BRLV [Local6] = (Local1 & 0x7F)
                                }

                                Local2++
                            }
                        }

                        Local2 = Zero
                        If ((OSYS <= 0x07D2))
                        {
                            Local3 = 0x0A
                        }
                        Else
                        {
                            Local3 = SizeOf (BPWM)
                        }

                        While ((Local2 <= Local3))
                        {
                            If ((OSYS <= 0x07D2))
                            {
                                Local1 = DerefOf (PWM2 [Local2])
                            }
                            Else
                            {
                                Local1 = DerefOf (BPWM [Local2])
                            }

                            Local4 = (Local1 * 0x64)
                            Divide (Local4, 0xFF, Local5, Local6)
                            If ((Local0 == Local6))
                            {
                                VDAT = Local2
                                Break
                            }

                            Local2++
                        }
                    }
                    Case (0x13)
                    {
                    }
                    Case (0x14)
                    {
                        VDAT = WLEN /* \_SB_.PCI0.LPCB.H_EC.WLEN */
                        WRST = VDAT /* \_SB_.PCI0.LPCB.H_EC.VDAT */
                        W7WL = VDAT /* \_SB_.PCI0.LPCB.H_EC.VDAT */
                    }
                    Case (0x15)
                    {
                        WLEN = VDAT /* \_SB_.PCI0.LPCB.H_EC.VDAT */
                        WRST = VDAT /* \_SB_.PCI0.LPCB.H_EC.VDAT */
                        W7WL = VDAT /* \_SB_.PCI0.LPCB.H_EC.VDAT */
                    }
                    Case (0x16)
                    {
                        VDAT = BTEN /* \_SB_.PCI0.LPCB.H_EC.BTEN */
                        BTSU = VDAT /* \_SB_.PCI0.LPCB.H_EC.VDAT */
                        W7BT = VDAT /* \_SB_.PCI0.LPCB.H_EC.VDAT */
                    }
                    Case (0x17)
                    {
                        BTEN = VDAT /* \_SB_.PCI0.LPCB.H_EC.VDAT */
                        BTSU = VDAT /* \_SB_.PCI0.LPCB.H_EC.VDAT */
                        W7BT = VDAT /* \_SB_.PCI0.LPCB.H_EC.VDAT */
                    }
                    Case (0x18)
                    {
                        VDAT = BLST /* \_SB_.PCI0.LPCB.H_EC.BLST */
                    }
                    Case (0x33)
                    {
                        SWBL = VDAT /* \_SB_.PCI0.LPCB.H_EC.VDAT */
                    }
                    Case (0x19)
                    {
                        VDAT = NOVS /* \_SB_.PCI0.LPCB.H_EC.NOVS */
                    }
                    Case (0x1B)
                    {
                        VDAT = TPEN /* \_SB_.PCI0.LPCB.H_EC.TPEN */
                        If ((TPEN == One)){}
                        Else
                        {
                        }
                    }
                    Case (0x1C)
                    {
                    }
                    Case (0x1D)
                    {
                        VDAT = CAMC /* \_SB_.PCI0.LPCB.H_EC.CAMC */
                    }
                    Case (0x1E)
                    {
                        CAMC = VDAT /* \_SB_.PCI0.LPCB.H_EC.VDAT */
                    }
                    Case (0x1F)
                    {
                        VDAT = WWAN /* \_SB_.PCI0.LPCB.H_EC.WWAN */
                        WNST = VDAT /* \_SB_.PCI0.LPCB.H_EC.VDAT */
                    }
                    Case (0x20)
                    {
                        WWAN = VDAT /* \_SB_.PCI0.LPCB.H_EC.VDAT */
                        WNST = VDAT /* \_SB_.PCI0.LPCB.H_EC.VDAT */
                    }
                    Case (0x21)
                    {
                        VDAT = ZPOS /* \_SB_.PCI0.LPCB.H_EC.ZPOS */
                        ZPOS = Zero
                    }
                    Case (0x22)
                    {
                        DUST = VDAT /* \_SB_.PCI0.LPCB.H_EC.VDAT */
                    }
                    Case (0x23)
                    {
                        VDAT = GWSS /* \_SB_.PCI0.LPCB.H_EC.GWSS */
                    }
                    Case (0x24)
                    {
                        BTEN = VDAT /* \_SB_.PCI0.LPCB.H_EC.VDAT */
                        BTSU = VDAT /* \_SB_.PCI0.LPCB.H_EC.VDAT */
                        W7BT = VDAT /* \_SB_.PCI0.LPCB.H_EC.VDAT */
                        WWAN = VDAT /* \_SB_.PCI0.LPCB.H_EC.VDAT */
                        WNST = VDAT /* \_SB_.PCI0.LPCB.H_EC.VDAT */
                        WLEN = VDAT /* \_SB_.PCI0.LPCB.H_EC.VDAT */
                        WRST = VDAT /* \_SB_.PCI0.LPCB.H_EC.VDAT */
                        W7WL = VDAT /* \_SB_.PCI0.LPCB.H_EC.VDAT */
                    }
                    Case (0x25)
                    {
                        VDAT = One
                    }
                    Case (0x27)
                    {
                        VDAT = CBST /* \_SB_.PCI0.LPCB.H_EC.CBST */
                    }
                    Case (0x29)
                    {
                        VDAT = GSPS /* \_SB_.PCI0.LPCB.H_EC.GSPS */
                    }
                    Case (0x2B)
                    {
                        VDAT = One
                        If ((DUST == One))
                        {
                            VDAT |= 0x02 /* \_SB_.PCI0.LPCB.H_EC.VDAT */
                        }
                        ElseIf (((DUS1 == One) && (DUS2 == Zero)))
                        {
                            VDAT |= 0x84 /* \_SB_.PCI0.LPCB.H_EC.VDAT */
                        }
                        Else
                        {
                            VDAT |= 0x04 /* \_SB_.PCI0.LPCB.H_EC.VDAT */
                        }
                    }
                    Case (0x2F)
                    {
                        Local0 = Zero
                        If ((SPKP == One))
                        {
                            Local0 |= 0x10
                            SPKP = Zero
                        }

                        If ((SMKP == One))
                        {
                            Local0 |= 0x20
                            SMKP = Zero
                        }

                        If ((AMSV == One))
                        {
                            If ((RSBM == Zero))
                            {
                                Local0 |= 0x04
                            }

                            AMSV = Zero
                        }
                        ElseIf ((AMSV == 0x02))
                        {
                            Local0 |= 0x02
                        }

                        VDAT = Local0
                    }
                    Case (0x31)
                    {
                        VDAT = OKAD /* \_SB_.PCI0.LPCB.H_EC.OKAD */
                        OKAD = Zero
                    }
                    Default
                    {
                        Noop
                    }

                }
            }
        }

        Method (_Q29, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x29
            NOVS = One
            VP1D |= 0x08
            Notify (VPC0, 0x80) // Status Change
        }

        Method (_Q76, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x76
            NOVS = Zero
            VP1D |= 0x08
            Notify (VPC0, 0x80) // Status Change
        }

        Method (MNOV, 0, NotSerialized)
        {
            LAMN (0x76)
        }

        Method (_Q2F, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x2F
            VP2D |= 0x04
            Notify (VPC0, 0x80) // Status Change
        }

        Method (_Q77, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            OSTP = GOST ()
            If ((OSTP != 0x04))
            {
                P80H = 0x77
                VP2D |= 0x08
                Notify (VPC0, 0x80) // Status Change
            }
        }

        Method (_Q19, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x19
            If ((OSYS <= 0x07D2))
            {
                If (IGDS)
                {
                    ^^^GFX0.GHDS (Zero)
                }
            }
            Else
            {
                VP1D |= 0x40
                Notify (VPC0, 0x80) // Status Change
            }
        }

        Method (_Q66, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x66
            OKAD |= 0x10
            VP1D |= One
            Notify (VPC0, 0x80) // Status Change
        }

        Method (_Q67, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x67
            AMSV = One
            VP1D |= 0x02
            Notify (VPC0, 0x80) // Status Change
        }

        Method (_Q69, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x69
            OKAD |= 0x08
            VP1D |= One
            Notify (VPC0, 0x80) // Status Change
        }

        Method (_Q6A, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x6A
            OKAD |= 0x02
            VP1D |= One
            Notify (VPC0, 0x80) // Status Change
        }

        Method (_Q6C, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x6C
            Notify (VPC0, 0x80) // Status Change
        }

        Method (_Q6F, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x6F
            AMSV = 0x02
            VP1D |= 0x02
            Notify (VPC0, 0x80) // Status Change
        }

        Method (_Q71, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x71
            AMSV = Zero
            VP1D |= 0x02
            Notify (VPC0, 0x80) // Status Change
        }

        Method (_Q16, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x16
            VP2D |= One
            Notify (VPC0, 0x80) // Status Change
        }

        Method (_Q24, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x24
            SPKP = One
            VP1D |= 0x02
            Notify (VPC0, 0x80) // Status Change
        }

        Method (_Q25, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
            P80H = 0x25
            SMKP = One
            VP1D |= 0x02
            Notify (VPC0, 0x80) // Status Change
        }
    }

    Scope (_SB.PCI0.LPCB.H_EC.VPC0)
    {
        Name (KSBM, Zero)
        Name (POSB, Zero)
        Method (GBMD, 0, NotSerialized)
        {
            Local0 = 0x10000000
            If ((RSBM == One))
            {
                Local0 |= One
            }

            If ((SMBM == One))
            {
                Local0 |= 0x20
            }

            If ((MBNG == One))
            {
                Local0 |= 0x08
            }

            If ((SBNG == One))
            {
                Local0 |= 0x10
            }

            If ((LBBM == Zero))
            {
                Local0 |= 0x80
            }

            If ((BNBM == Zero))
            {
                Local0 |= 0x0100
            }

            If ((CSBM == One))
            {
                Local0 |= 0x0200
            }

            If ((OPBM == One))
            {
                Local0 |= 0x0400
            }

            If ((ROBM == One))
            {
                Local0 |= 0x0800
            }

            If ((PJID >= 0x02))
            {
                If ((ADTP == 0x2D))
                {
                    Local0 |= 0x00010000
                    Local0 &= 0xFFFF7FFF
                }
            }

            Local0 &= 0xFFFFFBFF
            Local0 |= 0x00040000
            Return (Local0)
        }

        Method (SBMC, 1, NotSerialized)
        {
            If ((Arg0 == Zero))
            {
                RSBM = Zero
            }

            If ((Arg0 == One))
            {
                RSBM = One
            }

            If ((Arg0 == 0x02)){}
            If ((Arg0 == 0x03))
            {
                SMBM = One
            }

            If ((Arg0 == 0x04)){}
            If ((Arg0 == 0x05))
            {
                SMBM = Zero
                RSBM = Zero
            }

            If ((Arg0 == 0x06)){}
            If ((Arg0 == 0x09))
            {
                SHIP = One
            }
        }

        Method (MHCF, 1, NotSerialized)
        {
            If ((POSB == Zero))
            {
                Local0 = Arg0
                Local0 &= 0x20
                If ((Local0 == Zero))
                {
                    BFUF = One
                }
                Else
                {
                    BFUR = One
                }
            }
            Else
            {
                Local0 = Arg0
                Local0 &= 0x40
                If ((Local0 == Zero))
                {
                    BFUF = One
                }
                Else
                {
                    BFUR = One
                }
            }

            While ((BFUP != One))
            {
                Stall (One)
            }

            Return (Arg0)
        }

        Method (MHPF, 1, NotSerialized)
        {
            Name (TEMP, Buffer (0x25){})
            TEMP = Arg0
            If ((POSB == Zero))
            {
                SMCS = Zero
            }
            Else
            {
                SMCS = One
            }

            SMAR = DerefOf (TEMP [0x02])
            SMCM = DerefOf (TEMP [0x03])
            SMBC = DerefOf (TEMP [0x24])
            SD00 = DerefOf (TEMP [0x04])
            SD01 = DerefOf (TEMP [0x05])
            SD02 = DerefOf (TEMP [0x06])
            SD03 = DerefOf (TEMP [0x07])
            SD04 = DerefOf (TEMP [0x08])
            SD05 = DerefOf (TEMP [0x09])
            SD06 = DerefOf (TEMP [0x0A])
            SD07 = DerefOf (TEMP [0x0B])
            SD08 = DerefOf (TEMP [0x0C])
            SD09 = DerefOf (TEMP [0x0D])
            SD10 = DerefOf (TEMP [0x0E])
            SD11 = DerefOf (TEMP [0x0F])
            SD12 = DerefOf (TEMP [0x10])
            SD13 = DerefOf (TEMP [0x11])
            SD14 = DerefOf (TEMP [0x12])
            SD15 = DerefOf (TEMP [0x13])
            SD16 = DerefOf (TEMP [0x14])
            SD17 = DerefOf (TEMP [0x15])
            SD18 = DerefOf (TEMP [0x16])
            SD19 = DerefOf (TEMP [0x17])
            SD20 = DerefOf (TEMP [0x18])
            SD21 = DerefOf (TEMP [0x19])
            SD22 = DerefOf (TEMP [0x1A])
            SD23 = DerefOf (TEMP [0x1B])
            SD24 = DerefOf (TEMP [0x1C])
            SD25 = DerefOf (TEMP [0x1D])
            SD26 = DerefOf (TEMP [0x1E])
            SD27 = DerefOf (TEMP [0x1F])
            SD28 = DerefOf (TEMP [0x20])
            SD29 = DerefOf (TEMP [0x21])
            SD30 = DerefOf (TEMP [0x22])
            SD31 = DerefOf (TEMP [0x23])
            SMPR = DerefOf (TEMP [Zero])
            While ((SMPR != Zero))
            {
                Stall (0x64)
            }

            Name (BUPD, Buffer (0x25)
            {
                /* 0000 */  0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00,  // ........
                /* 0008 */  0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00,  // ........
                /* 0010 */  0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00,  // ........
                /* 0018 */  0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00,  // ........
                /* 0020 */  0x00, 0x00, 0x00, 0x00, 0x00                     // .....
            })
            BUPD [Zero] = SMPR /* \_SB_.PCI0.LPCB.H_EC.SMPR */
            BUPD [One] = SMST /* \_SB_.PCI0.LPCB.H_EC.SMST */
            BUPD [0x02] = SMAR /* \_SB_.PCI0.LPCB.H_EC.SMAR */
            BUPD [0x03] = SMCM /* \_SB_.PCI0.LPCB.H_EC.SMCM */
            BUPD [0x04] = SD00 /* \_SB_.PCI0.LPCB.H_EC.SD00 */
            BUPD [0x05] = SD01 /* \_SB_.PCI0.LPCB.H_EC.SD01 */
            BUPD [0x06] = SD02 /* \_SB_.PCI0.LPCB.H_EC.SD02 */
            BUPD [0x07] = SD03 /* \_SB_.PCI0.LPCB.H_EC.SD03 */
            BUPD [0x08] = SD04 /* \_SB_.PCI0.LPCB.H_EC.SD04 */
            BUPD [0x09] = SD05 /* \_SB_.PCI0.LPCB.H_EC.SD05 */
            BUPD [0x0A] = SD06 /* \_SB_.PCI0.LPCB.H_EC.SD06 */
            BUPD [0x0B] = SD07 /* \_SB_.PCI0.LPCB.H_EC.SD07 */
            BUPD [0x0C] = SD08 /* \_SB_.PCI0.LPCB.H_EC.SD08 */
            BUPD [0x0D] = SD09 /* \_SB_.PCI0.LPCB.H_EC.SD09 */
            BUPD [0x0E] = SD10 /* \_SB_.PCI0.LPCB.H_EC.SD10 */
            BUPD [0x0F] = SD11 /* \_SB_.PCI0.LPCB.H_EC.SD11 */
            BUPD [0x10] = SD12 /* \_SB_.PCI0.LPCB.H_EC.SD12 */
            BUPD [0x11] = SD13 /* \_SB_.PCI0.LPCB.H_EC.SD13 */
            BUPD [0x12] = SD14 /* \_SB_.PCI0.LPCB.H_EC.SD14 */
            BUPD [0x13] = SD15 /* \_SB_.PCI0.LPCB.H_EC.SD15 */
            BUPD [0x14] = SD16 /* \_SB_.PCI0.LPCB.H_EC.SD16 */
            BUPD [0x15] = SD17 /* \_SB_.PCI0.LPCB.H_EC.SD17 */
            BUPD [0x16] = SD18 /* \_SB_.PCI0.LPCB.H_EC.SD18 */
            BUPD [0x17] = SD19 /* \_SB_.PCI0.LPCB.H_EC.SD19 */
            BUPD [0x18] = SD20 /* \_SB_.PCI0.LPCB.H_EC.SD20 */
            BUPD [0x19] = SD21 /* \_SB_.PCI0.LPCB.H_EC.SD21 */
            BUPD [0x1A] = SD22 /* \_SB_.PCI0.LPCB.H_EC.SD22 */
            BUPD [0x1B] = SD23 /* \_SB_.PCI0.LPCB.H_EC.SD23 */
            BUPD [0x1C] = SD24 /* \_SB_.PCI0.LPCB.H_EC.SD24 */
            BUPD [0x1D] = SD25 /* \_SB_.PCI0.LPCB.H_EC.SD25 */
            BUPD [0x1E] = SD26 /* \_SB_.PCI0.LPCB.H_EC.SD26 */
            BUPD [0x1F] = SD27 /* \_SB_.PCI0.LPCB.H_EC.SD27 */
            BUPD [0x20] = SD28 /* \_SB_.PCI0.LPCB.H_EC.SD28 */
            BUPD [0x21] = SD29 /* \_SB_.PCI0.LPCB.H_EC.SD29 */
            BUPD [0x22] = SD30 /* \_SB_.PCI0.LPCB.H_EC.SD30 */
            BUPD [0x23] = SD31 /* \_SB_.PCI0.LPCB.H_EC.SD31 */
            BUPD [0x24] = SMBC /* \_SB_.PCI0.LPCB.H_EC.SMBC */
            Return (BUPD) /* \_SB_.PCI0.LPCB.H_EC.VPC0.MHPF.BUPD */
        }

        Method (MHIF, 1, NotSerialized)
        {
            Name (BFIF, Buffer (0x0A)
            {
                /* 0000 */  0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00,  // ........
                /* 0008 */  0x00, 0x00                                       // ..
            })
            BFUS = One
            Local0 = Arg0
            Local0 &= One
            If ((Local0 == Zero))
            {
                POSB = Zero
                BFIF [Zero] = 0x80
                BFIF [One] = Zero
                BFIF [0x02] = BAI0 /* \_SB_.PCI0.LPCB.H_EC.BAI0 */
                BFIF [0x03] = BAI1 /* \_SB_.PCI0.LPCB.H_EC.BAI1 */
                BFIF [0x04] = BAI2 /* \_SB_.PCI0.LPCB.H_EC.BAI2 */
                BFIF [0x05] = BAI3 /* \_SB_.PCI0.LPCB.H_EC.BAI3 */
                BFIF [0x06] = BAI4 /* \_SB_.PCI0.LPCB.H_EC.BAI4 */
                BFIF [0x07] = BAI5 /* \_SB_.PCI0.LPCB.H_EC.BAI5 */
                BFIF [0x08] = BAI6 /* \_SB_.PCI0.LPCB.H_EC.BAI6 */
                BFIF [0x09] = BAI7 /* \_SB_.PCI0.LPCB.H_EC.BAI7 */
                Return (BFIF) /* \_SB_.PCI0.LPCB.H_EC.VPC0.MHIF.BFIF */
            }
            Else
            {
                POSB = One
                Return (BFIF) /* \_SB_.PCI0.LPCB.H_EC.VPC0.MHIF.BFIF */
            }
        }

        Method (GBID, 0, NotSerialized)
        {
            Name (BCFV, Package (0x04)
            {
                Buffer (0x02)
                {
                     0x00, 0x00                                       // ..
                }, 

                Buffer (0x02)
                {
                     0x00, 0x00                                       // ..
                }, 

                Buffer (0x08)
                {
                     0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00   // ........
                }, 

                Buffer (0x08)
                {
                     0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00   // ........
                }
            })
            If ((POSB == Zero))
            {
                DerefOf (BCFV [0x02]) [Zero] = BAI0 /* \_SB_.PCI0.LPCB.H_EC.BAI0 */
                DerefOf (BCFV [0x02]) [One] = BAI1 /* \_SB_.PCI0.LPCB.H_EC.BAI1 */
                DerefOf (BCFV [0x02]) [0x02] = BAI2 /* \_SB_.PCI0.LPCB.H_EC.BAI2 */
                DerefOf (BCFV [0x02]) [0x03] = BAI3 /* \_SB_.PCI0.LPCB.H_EC.BAI3 */
                DerefOf (BCFV [0x02]) [0x04] = BAI4 /* \_SB_.PCI0.LPCB.H_EC.BAI4 */
                DerefOf (BCFV [0x02]) [0x05] = BAI5 /* \_SB_.PCI0.LPCB.H_EC.BAI5 */
                DerefOf (BCFV [0x02]) [0x06] = BAI6 /* \_SB_.PCI0.LPCB.H_EC.BAI6 */
                DerefOf (BCFV [0x02]) [0x07] = BAI7 /* \_SB_.PCI0.LPCB.H_EC.BAI7 */
                Return (BCFV) /* \_SB_.PCI0.LPCB.H_EC.VPC0.GBID.BCFV */
            }
            Else
            {
                DerefOf (BCFV [0x03]) [Zero] = BAI0 /* \_SB_.PCI0.LPCB.H_EC.BAI0 */
                DerefOf (BCFV [0x03]) [One] = BAI1 /* \_SB_.PCI0.LPCB.H_EC.BAI1 */
                DerefOf (BCFV [0x03]) [0x02] = BAI2 /* \_SB_.PCI0.LPCB.H_EC.BAI2 */
                DerefOf (BCFV [0x03]) [0x03] = BAI3 /* \_SB_.PCI0.LPCB.H_EC.BAI3 */
                DerefOf (BCFV [0x03]) [0x04] = BAI4 /* \_SB_.PCI0.LPCB.H_EC.BAI4 */
                DerefOf (BCFV [0x03]) [0x05] = BAI5 /* \_SB_.PCI0.LPCB.H_EC.BAI5 */
                DerefOf (BCFV [0x03]) [0x06] = BAI6 /* \_SB_.PCI0.LPCB.H_EC.BAI6 */
                DerefOf (BCFV [0x03]) [0x07] = BAI7 /* \_SB_.PCI0.LPCB.H_EC.BAI7 */
                Return (BCFV) /* \_SB_.PCI0.LPCB.H_EC.VPC0.GBID.BCFV */
            }
        }
    }

    Name (_S0, Package (0x04)  // _S0_: S0 System State
    {
        Zero, 
        Zero, 
        Zero, 
        Zero
    })
    Name (_S3, Package (0x04)  // _S3_: S3 System State
    {
        0x05, 
        Zero, 
        Zero, 
        Zero
    })
    Name (_S4, Package (0x04)  // _S4_: S4 System State
    {
        0x06, 
        Zero, 
        Zero, 
        Zero
    })
    Name (_S5, Package (0x04)  // _S5_: S5 System State
    {
        0x07, 
        Zero, 
        Zero, 
        Zero
    })
    Method (PTS, 1, NotSerialized)
    {
        If (Arg0){}
    }

    Method (WAK, 1, NotSerialized)
    {
    }
}



