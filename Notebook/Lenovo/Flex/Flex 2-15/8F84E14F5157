        }

        Method (TRPS, 2, NotSerialized)
        {
            Acquire (MSMI, 0xFFFF)
            SPR0 = Arg1
            SFUN = Arg0
            SSMP = TSMI /* \TSMI */
            Local0 = SPR0 /* \SPR0 */
            Release (MSMI)
            Return (Local0)
        }
    }

    Scope (\)
    {
        Mutex (MSMI, 0x00)
        Method (WSMI, 1, NotSerialized)
        {
            SFUN = Arg0
            SSMP = TSMI /* \TSMI */
        }

        Method (HKEY, 1, NotSerialized)
        {
            TRPS (0x14, Arg0)
        }

        Method (LAMN, 1, NotSerialized)
