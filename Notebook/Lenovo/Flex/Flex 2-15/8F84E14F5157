                        _STA = One
                        \_SB.PCI0.RP01.PEGP.DGRS = Zero
                        \_SB.PCI0.RP01.PEGP.DGPW = Zero
                        While ((\_SB.PCI0.RP01.PEGP.DGPG != One))
                        {
                            Sleep (One)
                        }

                        Stall (0x64)
                        \_SB.PCI0.RP01.PEGP.DGRS = One
                        \_SB.PCI0.RP01.LNKD = Zero
                        While ((\_SB.PCI0.RP01.LNKS < 0x07))
                        {
                            Sleep (One)
