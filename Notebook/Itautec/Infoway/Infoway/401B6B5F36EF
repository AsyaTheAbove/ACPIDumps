            Notify (\_SB.PCI0.USB1, 0x02) // Device Wake
            Notify (\_SB.PWRB, 0x02) // Device Wake
        }

        Method (_L04, 0, NotSerialized)  // _Lxx: Level-Triggered GPE, xx=0x00-0xFF
        {
            Notify (\_SB.PCI0.USB2, 0x02) // Device Wake
            Notify (\_SB.PWRB, 0x02) // Device Wake
        }

        Method (_L05, 0, NotSerialized)  // _Lxx: Level-Triggered GPE, xx=0x00-0xFF
        {
            Notify (\_SB.PCI0.USB5, 0x02) // Device Wake
            Notify (\_SB.PWRB, 0x02) // Device Wake
        }

        Method (_L06, 0, NotSerialized)  // _Lxx: Level-Triggered GPE, xx=0x00-0xFF
        {
            If (\_SB.PCI0.GFX0.GSSE)
            {
                \_SB.PCI0.GFX0.GSCI ()
            }
            Else
            {
                SCIS = One
            }
        }

        Method (_L07, 0, NotSerialized)  // _Lxx: Level-Triggered GPE, xx=0x00-0xFF
        {
            \_SB.PCI0.SBUS.HSTS = 0x20
        }

        Method (_L08, 0, NotSerialized)  // _Lxx: Level-Triggered GPE, xx=0x00-0xFF
        {
            Acquire (GLCK, 0xFFFF)
            Local0 = \_SB.PCI0.LPCB.PMRD (0xA8)
            Local1 = \_SB.PCI0.LPCB.PMRD (0xA9)
            Local2 = \_SB.PCI0.LPCB.PMRD (0xAA)
            If (Local3 = (Local0 & One))
            {
                \_SB.LID.LIDF = One
                Notify (\_SB.LID, 0x80) // Status Change
                Local4 = \_SB.PCI0.LPCB.PMRD (0xB8)
                Local5 = \_SB.PCI0.LPCB.PMRD (0xB7)
                Local4 &= 0xFE
                \_SB.PCI0.LPCB.PMWT (0xB8, Local4)
                If ((\_SB.PCI0.LPCB.PMRD (0xB5) & One))
                {
                    Local7 = \_SB.PCI0.LPCB.PMRD (0xB7)
                    Local7 &= 0xFE
                    \_SB.PCI0.LPCB.PMWT (0xB7, Local7)
                    \_SB.PCI0.LPCB.PMWT (0xF2, Zero)
                    \_SB.LID.LIDF = One
                }
                Else
                {
                    Local7 = \_SB.PCI0.LPCB.PMRD (0xB7)
                    Local7 |= One
                    \_SB.PCI0.LPCB.PMWT (0xB7, Local7)
                    \_SB.PCI0.LPCB.PMWT (0xF2, One)
                    \_SB.LID.LIDF = Zero
                }

                Local6 = \_SB.PCI0.LPCB.PMRD (0xA8)
                Local6 &= 0xFE
                \_SB.PCI0.LPCB.PMWT (0xA8, Local6)
                Local4 |= One
                \_SB.PCI0.LPCB.PMWT (0xB8, Local4)
                Notify (\_SB.LID, 0x80) // Status Change
            }

            Local7 = (\_SB.PCI0.LPCB.PMRD (0xAC) & 0xEF)
            \_SB.PCI0.LPCB.PMWT (0xAC, Local7)
            Release (GLCK)
        }

        Method (_L09, 0, NotSerialized)  // _Lxx: Level-Triggered GPE, xx=0x00-0xFF
        {
            If (\_SB.PCI0.RP01.PSPX)
            {
                \_SB.PCI0.RP01.PSPX = One
                \_SB.PCI0.RP01.PMSX = One
                Notify (\_SB.PCI0.RP01, 0x02) // Device Wake
            }

            If (\_SB.PCI0.RP02.PSPX)
            {
                \_SB.PCI0.RP02.PSPX = One
                \_SB.PCI0.RP02.PMSX = One
                Notify (\_SB.PCI0.RP02, 0x02) // Device Wake
            }

            If (\_SB.PCI0.RP03.PSPX)
            {
                \_SB.PCI0.RP03.PSPX = One
                \_SB.PCI0.RP03.PMSX = One
                Notify (\_SB.PCI0.RP03, 0x02) // Device Wake
            }

            If (\_SB.PCI0.RP04.PSPX)
            {
                \_SB.PCI0.RP04.PSPX = One
                \_SB.PCI0.RP04.PMSX = One
                Notify (\_SB.PCI0.RP04, 0x02) // Device Wake
            }

            If (\_SB.PCI0.RP05.PSPX)
            {
                \_SB.PCI0.RP05.PSPX = One
                \_SB.PCI0.RP05.PMSX = One
                Notify (\_SB.PCI0.RP05, 0x02) // Device Wake
            }

            If (\_SB.PCI0.RP06.PSPX)
            {
                \_SB.PCI0.RP06.PSPX = One
                \_SB.PCI0.RP06.PMSX = One
                Notify (\_SB.PCI0.RP06, 0x02) // Device Wake
            }

            Notify (\_SB.PWRB, 0x02) // Device Wake
        }

        Method (_L0B, 0, NotSerialized)  // _Lxx: Level-Triggered GPE, xx=0x00-0xFF
        {
            Notify (\_SB.PCI0.PCIB, 0x02) // Device Wake
            Notify (\_SB.PWRB, 0x02) // Device Wake
        }

        Method (_L0C, 0, NotSerialized)  // _Lxx: Level-Triggered GPE, xx=0x00-0xFF
        {
            Notify (\_SB.PCI0.USB3, 0x02) // Device Wake
            Notify (\_SB.PWRB, 0x02) // Device Wake
        }

        Method (_L0D, 0, NotSerialized)  // _Lxx: Level-Triggered GPE, xx=0x00-0xFF
        {
            If (\_SB.PCI0.EHC1.PMES)
            {
                \_SB.PCI0.EHC1.PMES = One
                Notify (\_SB.PCI0.EHC1, 0x02) // Device Wake
            }

            If (\_SB.PCI0.EHC2.PMES)
            {
                \_SB.PCI0.EHC2.PMES = One
                Notify (\_SB.PCI0.EHC2, 0x02) // Device Wake
            }

            If (\_SB.PCI0.HDEF.PMES)
            {
                \_SB.PCI0.HDEF.PMES = One
                Notify (\_SB.PCI0.HDEF, 0x02) // Device Wake
            }
        }

        Method (_L0E, 0, NotSerialized)  // _Lxx: Level-Triggered GPE, xx=0x00-0xFF
        {
            Notify (\_SB.PCI0.USB4, 0x02) // Device Wake
            Notify (\_SB.PWRB, 0x02) // Device Wake
        }

        Method (_L18, 0, NotSerialized)  // _Lxx: Level-Triggered GPE, xx=0x00-0xFF
        {
            Acquire (GLCK, 0xFFFF)
            Local0 = \_SB.PCI0.LPCB.PMRD (0xAB)
            Local7 = \_SB.PCI0.LPCB.PMRD (0xAF)
            Local3 = (Local7 & 0x04)
            If ((Local3 != Zero))
            {
                \_SB.PCI0.LPCB.PMMW (0x01E8, Zero)
            }

            If (Local3 = (Local7 & One))
            {
                Local5 = (\_SB.PCI0.LPCB.PMRD (0xAD) & 0xFE)
                \_SB.PCI0.LPCB.PMWT (0xAD, Local5)
                Local5 = (\_SB.PCI0.LPCB.PMRD (0xAE) & 0xFE)
                \_SB.PCI0.LPCB.PMWT (0xAE, Local5)
                Local5 = (\_SB.PCI0.LPCB.PMRD (0xAF) & 0xFC)
                \_SB.PCI0.LPCB.PMWT (0xAF, Local5)
                Notify (\_TZ.TZ00, 0x80) // Thermal Status Change
            }

            If (Local3 = (Local0 & One))
            {
                Local2 = (\_SB.PCI0.LPCB.PMRD (0xAB) & 0xFE)
                \_SB.PCI0.LPCB.PMWT (0xAB, Local2)
                PWRS = (\_SB.PCI0.LPCB.PMRD (0xA0) & One)
                TRAP (0x5A)
                Notify (\_SB.ACAD, 0x80) // Status Change
                TRAP (0x2B)
                PNOT ()
            }

            If (Local3 = (Local0 & 0x02))
            {
                Notify (\_SB.ACAD, 0x80) // Status Change
                Local1 = \_SB.PCI0.LPCB.PMRD (0xA1)
                Local4 = \_SB.PCI0.LPCB.PMRD (0xF1)
                If (((Local3 = (Local1 & One)) != (Local5 = (Local4 & One))))
                {
                    Local6 = \_SB.PCI0.LPCB.PMRD (0xA1)
                    \_SB.PCI0.LPCB.PMWT (0xF1, Local6)
                    Notify (\_SB.BAT0, 0x81) // Information Change
                }

                If (((Local3 = (Local1 & 0x02)) != (Local5 = (Local4 & 0x02))))
                {
                    Local6 = \_SB.PCI0.LPCB.PMRD (0xA1)
                    \_SB.PCI0.LPCB.PMWT (0xF1, Local6)
                    Notify (\_SB.BAT0, 0x80) // Status Change
                }

                If (((Local3 = (Local1 & 0x04)) != (Local5 = (Local4 & 0x04))))
                {
                    Local6 = \_SB.PCI0.LPCB.PMRD (0xA1)
                    \_SB.PCI0.LPCB.PMWT (0xF1, Local6)
                    Notify (\_SB.BAT0, 0x80) // Status Change
                }

                If (((Local3 = (Local1 & 0x08)) != (Local5 = (Local4 & 0x08))))
                {
                    Local6 = \_SB.PCI0.LPCB.PMRD (0xA1)
                    \_SB.PCI0.LPCB.PMWT (0xF1, Local6)
                    Notify (\_SB.BAT0, 0x80) // Status Change
                }

                If (((Local3 = (Local1 & 0x10)) != (Local5 = (Local4 & 0x10))))
                {
                    Local6 = \_SB.PCI0.LPCB.PMRD (0xA1)
                    \_SB.PCI0.LPCB.PMWT (0xF1, Local6)
                    Notify (\_SB.BAT0, 0x80) // Status Change
                }

                If (((Local3 = (Local1 & 0x20)) != (Local5 = (Local4 & 0x20))))
                {
                    Local6 = \_SB.PCI0.LPCB.PMRD (0xA1)
                    \_SB.PCI0.LPCB.PMWT (0xF1, Local6)
                    Notify (\_SB.BAT0, 0x80) // Status Change
                }

                If (((Local3 = (Local1 & 0x40)) != (Local5 = (Local4 & 0x40))))
                {
                    Local6 = \_SB.PCI0.LPCB.PMRD (0xA1)
                    \_SB.PCI0.LPCB.PMWT (0xF1, Local6)
                    Notify (\_SB.BAT0, 0x80) // Status Change
                }

                If (((Local3 = (Local1 & 0x80)) != (Local5 = (Local4 & 0x80))))
                {
                    Local6 = \_SB.PCI0.LPCB.PMRD (0xA1)
                    \_SB.PCI0.LPCB.PMWT (0xF1, Local6)
                    Notify (\_SB.BAT0, 0x80) // Status Change
                }

                Local2 = (\_SB.PCI0.LPCB.PMRD (0xAB) & 0xFD)
                \_SB.PCI0.LPCB.PMWT (0xAB, Local2)
            }

            Release (GLCK)
        }
    }

    Scope (_PR)
    {
        Processor (CPU0, 0x00, 0x00001010, 0x06) {}
        Processor (CPU1, 0x01, 0x00001010, 0x06) {}
        Method (RPPC, 0, NotSerialized)
        {
            If ((OSYS == 0x07D2))
            {
                If ((CFGD & One))
                {
                    If ((^CPU0._PPC > Zero))
                    {
                        ^CPU0._PPC -= One
                        PNOT ()
                        ^CPU0._PPC += One
                        PNOT ()
                    }
                    Else
                    {
                        ^CPU0._PPC += One
                        PNOT ()
                        ^CPU0._PPC -= One
                        PNOT ()
                    }
                }
            }
        }
    }

    Scope (_TZ)
    {
        PowerResource (FN00, 0x00, 0x0000)
        {
            Method (_STA, 0, Serialized)  // _STA: Status
            {
                Return (VFN0) /* \VFN0 */
            }

            Method (_ON, 0, Serialized)  // _ON_: Power On
            {
                VFN0 = One
            }

            Method (_OFF, 0, Serialized)  // _OFF: Power Off
            {
                VFN0 = Zero
            }
        }

        Device (FAN0)
        {
            Name (_HID, EisaId ("PNP0C0B") /* Fan (Thermal Solution) */)  // _HID: Hardware ID
            Name (_UID, Zero)  // _UID: Unique ID
            Name (_PR0, Package (0x01)  // _PR0: Power Resources for D0
            {
                FN00
            })
        }

        PowerResource (FN01, 0x00, 0x0000)
        {
            Method (_STA, 0, Serialized)  // _STA: Status
            {
                Return (VFN1) /* \VFN1 */
            }

            Method (_ON, 0, Serialized)  // _ON_: Power On
            {
                VFN1 = One
            }

            Method (_OFF, 0, Serialized)  // _OFF: Power Off
            {
                VFN1 = Zero
            }
        }

        Device (FAN1)
        {
            Name (_HID, EisaId ("PNP0C0B") /* Fan (Thermal Solution) */)  // _HID: Hardware ID
            Name (_UID, One)  // _UID: Unique ID
            Name (_PR0, Package (0x01)  // _PR0: Power Resources for D0
            {
                FN01
            })
        }

        ThermalZone (TZ00)
        {
            Method (_CRT, 0, Serialized)  // _CRT: Critical Temperature
            {
                Return ((0x0AAC + (CRTT * 0x0A)))
            }

            Method (_TMP, 0, Serialized)  // _TMP: Temperature
            {
                Local0 = \_SB.PCI0.LPCB.PMRD (0xE7)
                Local1 = 0x03
                While (Local1)
                {
                    If (((Local0 <= Zero) || (Local0 >= CRTT)))
                    {
                        Local0 = \_SB.PCI0.LPCB.PMRD (0xE7)
                    }

                    Local1--
                }

                Return ((0x0AAC + (Local0 * 0x0A)))
            }

            Method (_PSL, 0, Serialized)  // _PSL: Passive List
            {
                If (MPEN)
                {
                    Return (Package (0x02)
                    {
                        \_PR.CPU0, 
                        \_PR.CPU1
                    })
                }

                Return (Package (0x01)
                {
                    \_PR.CPU0
                })
            }

            Method (_PSV, 0, Serialized)  // _PSV: Passive Temperature
            {
                Return ((0x0AAC + (PSVT * 0x0A)))
            }

            Method (_TC1, 0, Serialized)  // _TC1: Thermal Constant 1
            {
                Return (Zero)
            }

            Method (_TC2, 0, Serialized)  // _TC2: Thermal Constant 2
            {
                Return (0x0A)
            }

            Method (_TSP, 0, Serialized)  // _TSP: Thermal Sampling Period
            {
                Return (0x1E)
            }
        }

        ThermalZone (TZ01)
        {
            Method (_AC0, 0, Serialized)  // _ACx: Active Cooling, x=0-9
            {
                Return ((0x0AAC + (ACTT * 0x0A)))
            }

            Method (_AC1, 0, Serialized)  // _ACx: Active Cooling, x=0-9
            {
                Return ((0x0AAC + (ACT1 * 0x0A)))
            }

            Name (_AL0, Package (0x01)  // _ALx: Active List, x=0-9
            {
                FAN1
            })
            Name (_AL1, Package (0x01)  // _ALx: Active List, x=0-9
            {
                FAN0
            })
            Method (_CRT, 0, Serialized)  // _CRT: Critical Temperature
            {
                Return ((0x0AAC + (CRTT * 0x0A)))
            }

            Method (_SCP, 1, Serialized)  // _SCP: Set Cooling Policy
            {
                CTYP = Arg0
            }

            Method (_TMP, 0, Serialized)  // _TMP: Temperature
            {
                If (DTSE)
                {
                    If ((DTS1 >= DTS2))
                    {
                        Return ((0x0AAC + (DTS1 * 0x0A)))
                    }

                    Return ((0x0AAC + (DTS2 * 0x0A)))
                }

                Return (0x0BB8)
            }

            Method (_PSL, 0, Serialized)  // _PSL: Passive List
            {
                If (MPEN)
                {
                    Return (Package (0x02)
                    {
                        \_PR.CPU0, 
                        \_PR.CPU1
                    })
                }

                Return (Package (0x01)
                {
                    \_PR.CPU0
                })
            }

            Method (_PSV, 0, Serialized)  // _PSV: Passive Temperature
            {
                Return ((0x0AAC + (PSVT * 0x0A)))
            }

            Method (_TC1, 0, Serialized)  // _TC1: Thermal Constant 1
            {
                Return (TC1V) /* \TC1V */
            }

            Method (_TC2, 0, Serialized)  // _TC2: Thermal Constant 2
            {
                Return (TC2V) /* \TC2V */
            }

            Method (_TSP, 0, Serialized)  // _TSP: Thermal Sampling Period
            {
                Return (TSPV) /* \TSPV */
            }
        }
    }

    Scope (_SB)
    {
        Mutex (PSMX, 0x00)
        Device (ACAD)
        {
            Name (_HID, "ACPI0003" /* Power Source Device */)  // _HID: Hardware ID
            Name (_PCL, Package (0x01)  // _PCL: Power Consumer List
            {
                _SB
            })
            Name (ACFG, Zero)
            Method (_PSR, 0, NotSerialized)  // _PSR: Power Source
            {
                Acquire (PSMX, 0xFFFF)
                Local0 = (One & ^^PCI0.LPCB.PMRD (0xA0))
                ACFG = Local0
                Release (PSMX)
                Return (ACFG) /* \_SB_.ACAD.ACFG */
            }

            Method (_STA, 0, NotSerialized)  // _STA: Status
            {
                Return (0x0F)
            }
        }

        Device (BAT0)
        {
            Name (_HID, EisaId ("PNP0C0A") /* Control Method Battery */)  // _HID: Hardware ID
            Name (_UID, One)  // _UID: Unique ID
            Method (_STA, 0, NotSerialized)  // _STA: Status
            {
                Acquire (PSMX, 0xFFFF)
                Local0 = (^^PCI0.LPCB.PMRD (0xA1) & One)
                FSTA = Local0
                Release (PSMX)
                If ((Local0 == Zero))
                {
                    Return (0x0F)
                }
                Else
                {
                    Return (0x1F)
                }
            }

            Method (_BIF, 0, NotSerialized)  // _BIF: Battery Information
            {
                Local0 = (^^PCI0.LPCB.PMRD (0xA1) & One)
                If (Local0)
                {
                    UPBI ()
                }
                Else
                {
                    IVBI ()
                    IVBS ()
                }

                FBIF = Zero
                Return (PBIF) /* \_SB_.BAT0.PBIF */
            }

            Method (_BST, 0, NotSerialized)  // _BST: Battery Status
            {
                UPBS ()
                Return (PBST) /* \_SB_.BAT0.PBST */
            }

            Method (IVBI, 0, NotSerialized)
            {
                PBIF [One] = 0xFFFFFFFF
                PBIF [0x02] = 0xFFFFFFFF
                PBIF [0x04] = 0xFFFFFFFF
                PBIF [0x09] = "Bad"
                PBIF [0x0A] = "Bad"
                PBIF [0x0B] = "Bad"
                PBIF [0x0C] = "Bad"
                FBIF = One
            }

            Method (IVBS, 0, NotSerialized)
            {
                PBST [Zero] = Zero
                PBST [One] = 0xFFFFFFFF
                PBST [0x02] = 0xFFFFFFFF
                PBST [0x03] = 0xFFFFFFFF
                FSTA = One
            }

            Method (UPBI, 0, NotSerialized)
            {
                Acquire (PSMX, 0xFFFF)
                Local0 = ^^PCI0.LPCB.PMRD (Zero)
                Local1 = ^^PCI0.LPCB.PMRD (One)
                Local2 = (Local1 * 0x0100)
                Local3 = (Local0 | Local2)
                PBIF [Zero] = Local3
                Local0 = ^^PCI0.LPCB.PMRD (0x02)
                Local1 = ^^PCI0.LPCB.PMRD (0x03)
                Local2 = ^^PCI0.LPCB.PMRD (0x49)
                Local3 = (Local1 * 0x0100)
                Local4 = (Local2 * 0x00010000)
                Local5 = (Local3 | Local0)
                Local6 = (Local4 | Local5)
                PBIF [One] = Local6
                Local0 = ^^PCI0.LPCB.PMRD (0x04)
                Local1 = ^^PCI0.LPCB.PMRD (0x05)
                Local2 = ^^PCI0.LPCB.PMRD (0x4A)
                Local3 = (Local1 * 0x0100)
                Local4 = (Local2 * 0x00010000)
                Local5 = (Local3 | Local0)
                Local6 = (Local4 | Local5)
                PBIF [0x02] = Local6
                Local0 = ^^PCI0.LPCB.PMRD (0x06)
                Local1 = ^^PCI0.LPCB.PMRD (0x07)
                Local2 = (Local1 * 0x0100)
                Local3 = (Local2 | Local0)
                PBIF [0x03] = Local3
                Local0 = ^^PCI0.LPCB.PMRD (0x08)
                Local1 = ^^PCI0.LPCB.PMRD (0x09)
                Local2 = (Local1 * 0x0100)
                Local3 = (Local2 | Local0)
                PBIF [0x04] = Local3
                Local0 = ^^PCI0.LPCB.PMRD (0x0A)
                Local1 = ^^PCI0.LPCB.PMRD (0x0B)
                Local2 = (Local1 * 0x0100)
                Local3 = (Local2 | Local0)
                PBIF [0x05] = Local3
                PBIF [0x06] = Zero
                Local0 = ^^PCI0.LPCB.PMRD (0x0E)
                Local1 = ^^PCI0.LPCB.PMRD (0x0F)
                Local2 = (Local1 * 0x0100)
                Local3 = (Local2 | Local0)
                PBIF [0x07] = Local3
                Local0 = ^^PCI0.LPCB.PMRD (0x10)
                Local1 = ^^PCI0.LPCB.PMRD (0x11)
                Local2 = (Local1 * 0x0100)
                Local3 = (Local2 | Local0)
                PBIF [0x08] = Local3
                PBIF [0x09] = ""
                PBIF [0x0A] = ""
                PBIF [0x0B] = "Li-ion"
                Local0 = ^^PCI0.LPCB.PMRD (0x18)
                If ((Local0 == Zero))
                {
                    PBIF [0x0A] = "Moli"
                }
                ElseIf ((Local0 == One))
                {
                    PBIF [0x0A] = "Panasonic"
                }
                ElseIf ((Local0 == 0x02))
                {
                    PBIF [0x0A] = "Sanyo"
                }
                ElseIf ((Local0 == 0x03))
                {
                    PBIF [0x0A] = "Simplo"
                }
                ElseIf ((Local0 == 0x05))
                {
                    PBIF [0x0A] = "Sony"
                }
                ElseIf ((Local0 == 0x06))
                {
                    PBIF [0x0A] = "Sole"
                }
                ElseIf ((Local0 == 0x07))
                {
                    PBIF [0x0A] = "Celxpert"
                }
                ElseIf ((Local0 == 0x08))
                {
                    PBIF [0x0A] = "Dynapack"
                }
                ElseIf ((Local0 == 0x09))
                {
                    PBIF [0x0A] = "Gallopwire"
                }
                Else
                {
                    PBIF [0x0C] = "FIC "
                }

                Release (PSMX)
                Return (PBIF) /* \_SB_.BAT0.PBIF */
            }

            Method (UPBS, 0, NotSerialized)
            {
                Acquire (PSMX, 0xFFFF)
                Local0 = ^^PCI0.LPCB.PMRD (0x1A)
                Local1 = ^^PCI0.LPCB.PMRD (0x1B)
                Local2 = (Local1 * 0x0100)
                Local3 = (Local2 | Local0)
                PBST [Zero] = Local3
                Local0 = ^^PCI0.LPCB.PMRD (0x1C)
                Local1 = ^^PCI0.LPCB.PMRD (0x1D)
                Local2 = (Local1 * 0x0100)
                Local3 = (Local2 | Local0)
                PBST [One] = Local3
                Local0 = ^^PCI0.LPCB.PMRD (0x1E)
                Local1 = ^^PCI0.LPCB.PMRD (0x1F)
                Local2 = ^^PCI0.LPCB.PMRD (0x4B)
                Local3 = (Local1 * 0x0100)
                Local4 = (Local2 * 0x00010000)
                Local5 = (Local3 | Local0)
                Local6 = (Local4 | Local5)
                PBST [0x02] = Local6
                Local0 = ^^PCI0.LPCB.PMRD (0x20)
                Local1 = ^^PCI0.LPCB.PMRD (0x21)
                Local2 = (Local1 * 0x0100)
                Local3 = (Local2 | Local0)
                PBST [0x03] = Local3
                Release (PSMX)
                Return (PBST) /* \_SB_.BAT0.PBST */
            }

            Method (_BTP, 1, NotSerialized)  // _BTP: Battery Trip Point
            {
                Acquire (PSMX, 0xFFFF)
                Local0 = ^^PCI0.LPCB.PMRD (0xB2)
                Local0 &= 0x7F
                ^^PCI0.LPCB.PMWT (0xB2, Local0)
                Local0 = Arg0
                Local6 = Local0
                Local1 = (Local0 & 0xFF)
                ^^PCI0.LPCB.PMWT (0x22, Local1)
                Local0 = Local6
                Local1 = (Local0 & 0xFF00)
                Local2 = (Local1 >> 0x08)
                ^^PCI0.LPCB.PMWT (0x23, Local2)
                Local0 = Local6
                Local1 = (Local0 & 0x00FF0000)
                Local2 = (Local1 >> 0x10)
                ^^PCI0.LPCB.PMWT (0x4C, Local2)
                Local3 = ^^PCI0.LPCB.PMRD (0xF1)
                If (Local3 &= 0x80)
                {
                    Local4 = ^^PCI0.LPCB.PMRD (0xA1)
                    ^^PCI0.LPCB.PMWT (0xF1, Local4)
                }

                Local0 = ^^PCI0.LPCB.PMRD (0xB2)
                Local0 |= 0x80
                ^^PCI0.LPCB.PMWT (0xB2, Local0)
                Release (PSMX)
            }

