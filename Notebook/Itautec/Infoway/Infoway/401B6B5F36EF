            Device (LPCB)
            {
                Name (_ADR, 0x001F0000)  // _ADR: Address
                OperationRegion (LPC0, PCI_Config, 0x40, 0xC0)
                Field (LPC0, AnyAcc, NoLock, Preserve)
                {
                    Offset (0x20), 
                    PARC,   8, 
                    PBRC,   8, 
                    PCRC,   8, 
                    PDRC,   8, 
                    Offset (0x28), 
                    PERC,   8, 
                    PFRC,   8, 
                    PGRC,   8, 
                    PHRC,   8, 
                    Offset (0x40), 
                    IOD0,   8, 
                    IOD1,   8, 
                    Offset (0xB0), 
                    RAEN,   1, 
                        ,   13, 
                    RCBA,   18
                }

                OperationRegion (PMIF, SystemIO, 0x68, 0x08)
                Field (PMIF, ByteAcc, NoLock, Preserve)
                {
                    PMUD,   8, 
                    Offset (0x04), 
                    PMUC,   8, 
                    Offset (0x08)
                }

                Mutex (ELCK, 0x00)
                Method (WIBF, 0, NotSerialized)
                {
                    Local6 = 0x0009FFFF
                    Noop
                    Noop
                    Local2 = PMUC /* \_SB_.PCI0.LPCB.PMUC */
                    Local2 &= 0x22
                    If (Local3 = (Local2 & 0x20))
                    {
                        Return (One)
                    }

                    Local2 &= 0x02
                    While (((Local2 != Zero) && (Local6 != Zero)))
                    {
                        Noop
                        Noop
                        Local2 = PMUC /* \_SB_.PCI0.LPCB.PMUC */
                        Local6--
                        If (Local3 = (Local2 & 0x20))
                        {
                            Return (One)
                        }

                        Local2 &= 0x02
                    }

                    Return (Zero)
                }

                Method (WOBF, 0, NotSerialized)
                {
                    Local6 = 0x009FFFFF
                    Noop
                    Noop
                    Local3 = PMUC /* \_SB_.PCI0.LPCB.PMUC */
                    Local3 &= 0x21
                    If (Local2 = (Local3 & 0x20))
                    {
                        Return (One)
                    }

                    While (((Local3 == Zero) && (Local6 != Zero)))
                    {
                        Noop
                        Noop
                        Local3 = PMUC /* \_SB_.PCI0.LPCB.PMUC */
                        Local3 &= 0x21
                        If (Local2 = (Local3 & 0x20))
                        {
                            Return (One)
                        }

                        Local6--
                    }

                    Return (Zero)
                }

                Method (RCMD, 2, NotSerialized)
                {
                    Local0 = Arg0
                    Local1 = Arg1
                    PMUC = Local0
                    If (WIBF ())
                    {
                        Return (One)
                    }

                    PMUD = Local1
                    If (WOBF ())
                    {
                        Return (One)
                    }

                    Return (Zero)
                }

                Method (WCMD, 2, NotSerialized)
                {
                    Local0 = Arg0
                    Local1 = Arg1
                    PMUC = Local0
                    If (WIBF ())
                    {
                        Return (One)
                    }

                    PMUD = Local1
                    If (WIBF ())
                    {
                        Return (One)
                    }

                    Return (Zero)
                }

                Method (PMRD, 1, NotSerialized)
                {
                    Acquire (ELCK, 0xFFFF)
                    EDFG = Zero
                    RECT = 0x14
                    While (((EDFG == Zero) && (RECT != Zero)))
                    {
                        Local0 = Arg0
                        Local6 = 0x03E8
                        Local1 = PMUC /* \_SB_.PCI0.LPCB.PMUC */
                        Local1 &= 0x13
                        While (((Local1 != Zero) && (Local6 != Zero)))
                        {
                            Sleep (One)
                            Local1 = PMUC /* \_SB_.PCI0.LPCB.PMUC */
                            Local1 &= 0x13
                            If (Local2 = (Local1 & One))
                            {
                                Local3 = PMUD /* \_SB_.PCI0.LPCB.PMUD */
                            }

                            Local6--
                        }

                        While (RCMD (0x80, Local0))
                        {
                            Noop
                            Noop
                            Local5 = PMUC /* \_SB_.PCI0.LPCB.PMUC */
                            If (Local2 = (Local5 & One))
                            {
                                Local5 = PMUD /* \_SB_.PCI0.LPCB.PMUD */
                            }
                        }

                        Local6 = 0x07D0
                        Local1 = PMUC /* \_SB_.PCI0.LPCB.PMUC */
                        Local1 &= 0x10
                        While (((Local1 != Zero) && (Local6 != Zero)))
                        {
                            Noop
                            Noop
                            Local1 = PMUC /* \_SB_.PCI0.LPCB.PMUC */
                            Local1 &= 0x10
                            Local6--
                        }

                        Local1 = PMUC /* \_SB_.PCI0.LPCB.PMUC */
                        Local1 &= 0x20
                        If ((Local1 == Zero))
                        {
                            EDFG = One
                        }

                        RECT--
                    }

                    Local4 = PMUD /* \_SB_.PCI0.LPCB.PMUD */
                    Release (ELCK)
                    Return (Local4)
                }

                Method (PMWT, 2, NotSerialized)
                {
                    Acquire (ELCK, 0xFFFF)
                    EDFG = Zero
                    RECT = 0x14
                    While (((EDFG == Zero) && (RECT != Zero)))
                    {
                        Local0 = Arg0
                        Local6 = 0x03E8
                        Local1 = PMUC /* \_SB_.PCI0.LPCB.PMUC */
                        Local1 &= 0x13
                        While (((Local1 != Zero) && (Local6 != Zero)))
                        {
                            Sleep (One)
                            Local1 = PMUC /* \_SB_.PCI0.LPCB.PMUC */
                            Local1 &= 0x13
                            Local6--
                        }

                        While (WCMD (0x81, Local0))
                        {
                            Noop
                            Noop
                            Local5 = PMUC /* \_SB_.PCI0.LPCB.PMUC */
                            If (Local2 = (Local5 & One))
                            {
                                Local5 = PMUD /* \_SB_.PCI0.LPCB.PMUD */
                            }
                        }

                        PMUD = Arg1
                        Local6 = 0x07D0
                        Local1 = PMUC /* \_SB_.PCI0.LPCB.PMUC */
                        Local1 &= 0x12
                        While (((Local1 != Zero) && (Local6 != Zero)))
                        {
                            ED = Zero
                            Noop
                            Noop
                            ED = Zero
                            Local1 = PMUC /* \_SB_.PCI0.LPCB.PMUC */
                            Local1 &= 0x12
                            Local6--
                        }

                        Local1 = PMUC /* \_SB_.PCI0.LPCB.PMUC */
                        Local1 &= 0x20
                        If ((Local1 == Zero))
                        {
                            EDFG = One
                        }

                        RECT--
                    }

                    Release (ELCK)
                }

                Method (PMMW, 2, NotSerialized)
                {
                    Local0 = Arg0
                    Local1 = Arg1
                    Local2 = (Local0 & 0xFF)
                    Local3 = (Local0 & 0xFF00)
                    Local3 >>= 0x08
                    Local4 = (Local2 + Local3)
                    Local4 += 0xC1
                    Local7 = (Local4 + Local1)
                    Local7 &= 0xFF
                    PMWR (0x043F, Local7)
                    PMWR (Local0, Local1)
                }

                Method (PMWR, 2, NotSerialized)
                {
                    Acquire (ELCK, 0xFFFF)
                    EDFG = Zero
                    RECT = 0x14
                    While (((EDFG == Zero) && (RECT != Zero)))
                    {
                        Local6 = 0x03E8
                        Local1 = PMUC /* \_SB_.PCI0.LPCB.PMUC */
                        Local1 &= 0x13
                        While (((Local1 != Zero) && (Local6 != Zero)))
                        {
                            Sleep (One)
                            Local1 = PMUC /* \_SB_.PCI0.LPCB.PMUC */
                            Local1 &= 0x13
                            Local6--
                        }

                        Local0 = Arg0
                        While (WCM2 (0xC1, Local0))
                        {
                            ED = Zero
                            Noop
                            Noop
                            ED = Zero
                            Local5 = PMUC /* \_SB_.PCI0.LPCB.PMUC */
                            If (Local2 = (Local5 & One))
                            {
                                Local5 = PMUD /* \_SB_.PCI0.LPCB.PMUD */
                            }
                        }

                        PMUD = Arg1
                        Local6 = 0x07D0
                        Local1 = PMUC /* \_SB_.PCI0.LPCB.PMUC */
                        Local1 &= 0x12
                        While (((Local1 != Zero) && (Local6 != Zero)))
                        {
                            ED = Zero
                            Noop
                            Noop
                            ED = Zero
                            Local1 = PMUC /* \_SB_.PCI0.LPCB.PMUC */
                            Local1 &= 0x12
                            Local6--
                        }

                        Local1 = PMUC /* \_SB_.PCI0.LPCB.PMUC */
                        Local1 &= 0x20
                        If ((Local1 == Zero))
                        {
                            EDFG = One
                        }

                        RECT--
                    }

                    Release (ELCK)
                }

                Method (WCM2, 2, NotSerialized)
                {
                    Local0 = Arg0
                    Local1 = Arg1
                    Local2 = Arg1
                    Local1 &= 0xFF
                    Local2 &= 0xFF00
                    Local2 >>= 0x08
                    PMUC = Local0
                    If (WIBF ())
                    {
                        Return (One)
                    }

                    PMUD = Local2
                    If (WIBF ())
                    {
                        Return (One)
                    }

                    PMUD = Local1
                    If (WIBF ())
                    {
                        Return (One)
                    }

                    Return (Zero)
                }

                Name (EDFG, Zero)
                Name (RECT, Zero)
                OperationRegion (DELY, SystemIO, 0xED, One)
                Field (DELY, ByteAcc, NoLock, Preserve)
                {
                    ED,     8
                }

                Method (SMST, 0, NotSerialized)
                {
                    Local6 = 0x10
                    Local0 = PMRD (0x70)
                    While (((Local0 != Zero) && (Local6 != Zero)))
                    {
                        Local0 = PMRD (0x70)
                        Local6--
                    }

                    Local6 = 0x10
                    Local1 = PMRD (0x71)
                    While (((Local1 != 0x80) && (Local6 != Zero)))
                    {
                        Local1 = PMRD (0x71)
                        Local6--
                    }
                }

                Method (SMWT, 3, NotSerialized)
                {
                    Acquire (ELCK, 0xFFFF)
                    Local6 = 0x06
                    Local0 = Arg0
                    Local1 = Arg1
                    Local2 = Arg2
                    While ((Local6 != Zero))
                    {
                        PMWT (0x71, Zero)
                        PMWT (0x72, Local0)
                        PMWT (0x73, Local1)
                        PMWT (0x74, Local2)
                        PMWT (0x70, 0x06)
                        SMST ()
                        Local6--
                    }

                    Release (ELCK)
                }

                Method (SMWD, 4, NotSerialized)
                {
                    Acquire (ELCK, 0xFFFF)
                    Local6 = 0x06
                    Local0 = Arg0
                    Local1 = Arg1
                    Local2 = Arg2
                    Local3 = Arg3
                    While ((Local6 != Zero))
                    {
                        PMWT (0x71, Zero)
                        PMWT (0x72, Local0)
                        PMWT (0x73, Local1)
                        PMWT (0x74, Local2)
                        PMWT (0x75, Local3)
                        PMWT (0x70, 0x08)
                        SMST ()
                        Local6--
                    }

                    Release (ELCK)
                }

                Device (LNKA)
                {
                    Name (_HID, EisaId ("PNP0C0F") /* PCI Interrupt Link Device */)  // _HID: Hardware ID
                    Name (_UID, One)  // _UID: Unique ID
                    Method (_DIS, 0, Serialized)  // _DIS: Disable Device
                    {
                        PARC = 0x80
                    }

                    Name (_PRS, ResourceTemplate ()  // _PRS: Possible Resource Settings
                    {
                        IRQ (Level, ActiveLow, Shared, )
                            {1,3,4,5,6,7,10,12,14,15}
                    })
                    Method (_CRS, 0, Serialized)  // _CRS: Current Resource Settings
                    {
                        Name (RTLA, ResourceTemplate ()
                        {
                            IRQ (Level, ActiveLow, Shared, )
                                {}
                        })
                        CreateWordField (RTLA, One, IRQ0)
                        IRQ0 = Zero
                        IRQ0 = (One << (PARC & 0x0F))
                        Return (RTLA) /* \_SB_.PCI0.LPCB.LNKA._CRS.RTLA */
                    }

                    Method (_SRS, 1, Serialized)  // _SRS: Set Resource Settings
                    {
                        CreateWordField (Arg0, One, IRQ0)
                        FindSetRightBit (IRQ0, Local0)
                        Local0--
                        PARC = Local0
                    }

                    Method (_STA, 0, Serialized)  // _STA: Status
                    {
                        If ((PARC & 0x80))
                        {
                            Return (0x09)
                        }
                        Else
                        {
                            Return (0x0B)
                        }
                    }
                }

