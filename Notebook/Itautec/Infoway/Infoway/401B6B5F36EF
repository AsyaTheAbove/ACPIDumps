            Name (GUID, ToUUID ("33db4d5b-1ff7-401c-9657-7441c03dd766") /* PCI Host Bridge Device */)
            OSCS = Zero
            OSCC = Zero
            Method (_OSC, 4, Serialized)  // _OSC: Operating System Capabilities
            {
                If (((Arg0 == GUID) && NEXP))
                {
                    Local0 = Arg3
                    CreateDWordField (Local0, Zero, CDW1)
                    CreateDWordField (Local0, 0x04, CDW2)
                    CreateDWordField (Local0, 0x08, CDW3)
                    OSCS = CDW2 /* \_SB_.PCI0._OSC.CDW2 */
                    OSCC = CDW3 /* \_SB_.PCI0._OSC.CDW3 */
                    If (~(CDW1 & One))
                    {
                        If ((OSCC & 0x02))
                        {
                            NHPG ()
                        }

                        If ((OSCC & 0x04))
                        {
                            NPME ()
                        }
                    }

                    If ((Arg1 != One))
                    {
                        CDW1 |= 0x08
                    }

                    If ((CDW3 != OSCC))
                    {
                        CDW1 |= 0x10
                    }

                    CDW3 = OSCC /* \OSCC */
                    Return (Local0)
                }
                Else
                {
                    CDW1 |= 0x04
                    Return (Local0)
                }
            }

