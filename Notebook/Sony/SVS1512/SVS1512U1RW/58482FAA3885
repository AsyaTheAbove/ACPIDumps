                    Device (BAT1)
                    {
                        Name (_HID, EisaId ("PNP0C0A") /* Control Method Battery */)  // _HID: Hardware ID
                        Name (_UID, One)  // _UID: Unique ID
                        Method (_STA, 0, NotSerialized)  // _STA: Status
                        {
                            If ((BNUM & One))
                            {
                                Return (0x1F)
                            }

                            Return (0x0B)
                        }

                        Method (_BIF, 0, NotSerialized)  // _BIF: Battery Information
                        {
                            Name (BPKG, Package (0x0D)
                            {
                                Zero, 
                                Ones, 
                                Ones, 
                                One, 
                                Ones, 
                                Ones, 
                                Zero, 
                                0x64, 
                                Zero, 
                                "", 
                                "", 
                                "Lion", 
                                "Sony Corporation"
                            })
                            If ((B1ST & 0x10))
                            {
                                Return (BPKG) /* \_SB_.PCI0.LPCB.H_EC.BAT1._BIF.BPKG */
                            }

                            If ((B1DR != 0x3F))
                            {
                                Return (BPKG) /* \_SB_.PCI0.LPCB.H_EC.BAT1._BIF.BPKG */
                            }

                            BPKG [One] = (B1DC * 0x0A)
                            BPKG [0x02] = (B1FC * 0x0A)
                            BPKG [0x04] = B1DV /* \_SB_.PCI0.LPCB.H_EC.B1DV */
                            If (B1FC)
                            {
                                BPKG [0x05] = B1FC /* \_SB_.PCI0.LPCB.H_EC.B1FC */
                                BPKG [0x06] = One
                                BPKG [0x07] = ((B1DC * 0x0A) / 0x01F4)
                                BPKG [0x08] = ((B1DC * 0x0A) / 0x01F4)
                            }

                            Return (BPKG) /* \_SB_.PCI0.LPCB.H_EC.BAT1._BIF.BPKG */
                        }

                        Method (_BST, 0, NotSerialized)  // _BST: Battery Status
                        {
                            Name (PKG1, Package (0x04)
                            {
                                Ones, 
                                Ones, 
                                Ones, 
                                Ones
                            })
                            PKG1 [Zero] = (B1ST & 0x07)
                            If ((B1ST & 0x10))
                            {
                                PKG1 [One] = Zero
                                PKG1 [0x02] = Zero
                                PKG1 [0x03] = Zero
                                Return (PKG1) /* \_SB_.PCI0.LPCB.H_EC.BAT1._BST.PKG1 */
                            }

                            If ((B1DR != 0x3F))
                            {
                                Return (PKG1) /* \_SB_.PCI0.LPCB.H_EC.BAT1._BST.PKG1 */
                            }

                            Local0 = B1AI /* \_SB_.PCI0.LPCB.H_EC.B1AI */
                            If ((Local0 & 0x8000))
                            {
                                Local0 ^= 0xFFFF
                                Local0++
                            }

                            Local0 = (Local0 * B1FV)
                            Local0 = (Local0 / 0x03E8)
                            PKG1 [One] = Local0
                            PKG1 [0x02] = (B1RC * 0x0A)
                            PKG1 [0x03] = B1FV /* \_SB_.PCI0.LPCB.H_EC.B1FV */
                            Return (PKG1) /* \_SB_.PCI0.LPCB.H_EC.BAT1._BST.PKG1 */
                        }

                        Method (_PCL, 0, NotSerialized)  // _PCL: Power Consumer List
                        {
                            Return (_SB) /* \_SB_ */
                        }
                    }

                    Scope (\)
                    {
                        Field (GNVS, AnyAcc, Lock, Preserve)
                        {
                            Offset (0x21), 
                            B2SC,   8, 
                            Offset (0x24), 
                            B2SS,   8
                        }
                    }

                    Device (BAT2)
                    {
                        Name (_HID, EisaId ("PNP0C0A") /* Control Method Battery */)  // _HID: Hardware ID
                        Name (_UID, 0x02)  // _UID: Unique ID
                        Method (_STA, 0, NotSerialized)  // _STA: Status
                        {
                            If ((BNUM & 0x02))
                            {
                                Return (0x1F)
                            }

                            Return (0x0B)
                        }

                        Method (_BIF, 0, NotSerialized)  // _BIF: Battery Information
                        {
                            Name (BPK2, Package (0x0D)
                            {
                                Zero, 
                                Ones, 
                                Ones, 
                                One, 
                                Ones, 
                                Ones, 
                                Zero, 
                                0x64, 
                                Zero, 
                                "", 
                                "", 
                                "Lion", 
                                "Sony Corporation"
                            })
                            If ((B2ST & 0x10))
                            {
                                Return (BPK2) /* \_SB_.PCI0.LPCB.H_EC.BAT2._BIF.BPK2 */
                            }

                            If ((B2DR != 0x3F))
                            {
                                Return (BPK2) /* \_SB_.PCI0.LPCB.H_EC.BAT2._BIF.BPK2 */
                            }

                            BPK2 [One] = (B2DC * 0x0A)
                            BPK2 [0x02] = (B2FC * 0x0A)
                            BPK2 [0x04] = B2DV /* \_SB_.PCI0.LPCB.H_EC.B2DV */
                            If (B2FC)
                            {
                                BPK2 [0x05] = B2FC /* \_SB_.PCI0.LPCB.H_EC.B2FC */
                                BPK2 [0x06] = One
                                BPK2 [0x07] = ((B2DC * 0x0A) / 0x01F4)
                                BPK2 [0x08] = ((B2DC * 0x0A) / 0x01F4)
                            }

                            Return (BPK2) /* \_SB_.PCI0.LPCB.H_EC.BAT2._BIF.BPK2 */
                        }

                        Method (_BST, 0, NotSerialized)  // _BST: Battery Status
                        {
                            Name (PKG2, Package (0x04)
                            {
                                Ones, 
                                Ones, 
                                Ones, 
                                Ones
                            })
                            PKG2 [Zero] = (B2ST & 0x07)
                            If ((B2ST & 0x10))
                            {
                                PKG2 [One] = Zero
                                PKG2 [0x02] = Zero
                                PKG2 [0x03] = Zero
                                Return (PKG2) /* \_SB_.PCI0.LPCB.H_EC.BAT2._BST.PKG2 */
                            }

                            If ((B2DR != 0x3F))
                            {
                                Return (PKG2) /* \_SB_.PCI0.LPCB.H_EC.BAT2._BST.PKG2 */
                            }

                            Local0 = B2AI /* \_SB_.PCI0.LPCB.H_EC.B2AI */
                            If ((Local0 & 0x8000))
                            {
                                Local0 ^= 0xFFFF
                                Local0++
                            }

                            Local0 = (Local0 * B2FV)
                            Local0 = (Local0 / 0x03E8)
                            PKG2 [One] = Local0
                            PKG2 [0x02] = (B2RC * 0x0A)
                            PKG2 [0x03] = B2FV /* \_SB_.PCI0.LPCB.H_EC.B2FV */
                            Return (PKG2) /* \_SB_.PCI0.LPCB.H_EC.BAT2._BST.PKG2 */
                        }

                        Method (_PCL, 0, NotSerialized)  // _PCL: Power Consumer List
                        {
                            Return (_SB) /* \_SB_ */
                        }
                    }

                    Method (_REG, 2, NotSerialized)  // _REG: Region Availability
                    {
                        If (((Arg0 == 0x03) && (Arg1 == One)))
                        {
                            PSVT = PCOL /* \_SB_.PCI0.LPCB.H_EC.PCOL */
                            CRTT = CSHD /* \_SB_.PCI0.LPCB.H_EC.CSHD */
                            Notify (\_TZ.TZ01, 0x81) // Thermal Trip Point Change
                            If ((LSTE == Zero))
                            {
                                ^^^GFX0.CLID = Zero
                            }

                            If ((LSTE == One))
                            {
                                ^^^GFX0.CLID = 0x03
                            }

                            LIDS = LSTE /* \_SB_.PCI0.LPCB.H_EC.LSTE */
                            Notify (LID0, 0x80) // Status Change
                            PWRS = ADTS /* \_SB_.PCI0.LPCB.H_EC.ADTS */
                            Notify (ADP1, 0x80) // Status Change
                            BNUD ()
                            PNOT ()
                            If ((OSYS >= 0x07DC))
                            {
                                OSVR = One
                            }
                            Else
                            {
                                OSVR = Zero
                            }
                        }
                    }

                    Name (_GPE, 0x17)  // _GPE: General Purpose Events
                    Method (_Q01, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        P80H = One
                        BNUD ()
                        PNOT ()
                    }

                    Method (_Q02, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        P80H = 0x02
                        BNUD ()
                        PNOT ()
                    }

                    Method (_Q03, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        P80H = 0x03
                    }

                    Method (_Q04, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        P80H = 0x04
                        PWRS = One
                        Notify (ADP1, 0x80) // Status Change
                        PNOT ()
                    }

                    Method (_Q05, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        P80H = 0x05
                        PWRS = Zero
                        Notify (ADP1, 0x80) // Status Change
                        PNOT ()
                    }

                    Method (_Q06, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        P80H = 0x06
                    }

                    Method (_Q07, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        P80H = 0x07
                    }

                    Method (_Q08, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        P80H = 0x08
                        LIDS = Zero
                        Notify (LID0, 0x80) // Status Change
                    }

                    Method (_Q09, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        P80H = 0x09
                        LIDS = One
                        ^^^GFX0.GLID (LIDS)
                        Notify (LID0, 0x80) // Status Change
                    }

                    Method (_Q0A, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        P80H = 0x0A
                        If (HKYS)
                        {
                            ^^SNC.ECR = EVTC /* \_SB_.PCI0.LPCB.H_EC.EVTC */
                            ^^SNC.SNNE (Zero)
                        }
                    }

                    Method (_Q0B, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        P80H = 0x0B
                        If (HKYS)
                        {
                            ^^SNC.ECR = EVTC /* \_SB_.PCI0.LPCB.H_EC.EVTC */
                            ^^SNC.SNNE (Zero)
                        }
                    }

                    Method (_Q0C, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        P80H = 0x0C
                    }

                    Method (_Q0D, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        P80H = 0x0D
                        Notify (PWRB, 0x80) // Status Change
                    }

                    Method (_Q0E, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        P80H = 0x0E
                        PSVT = PCOL /* \_SB_.PCI0.LPCB.H_EC.PCOL */
                        CRTT = CSHD /* \_SB_.PCI0.LPCB.H_EC.CSHD */
                        Notify (\_TZ.TZ01, 0x81) // Thermal Trip Point Change
                    }

                    Method (_Q0F, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        P80H = 0x0F
                    }

                    Method (_Q10, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        P80H = 0x10
                    }

                    Method (_Q11, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        P80H = 0x11
                    }

                    Method (_Q12, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        P80H = 0x12
                        Notify (\_TZ.TZ01, 0x80) // Thermal Status Change
                    }

                    Method (_Q13, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        P80H = 0x13
                    }

                    Method (_Q14, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        P80H = 0x14
                    }

                    Method (_Q15, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        P80H = 0x15
                        ^^SNC.SCNG = 0x02
                        ^^SNC.SNNE (0x0B)
                    }

                    Method (_Q16, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        P80H = 0x16
                        ^^SNC.SCNG = 0x02
                        ^^SNC.SNNE (0x0B)
                    }

                    Method (_Q17, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        P80H = 0x17
                        \_PR.CPU0._PPC = One
                        PNOT ()
                    }

                    Method (_Q18, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        P80H = 0x18
                        \_PR.CPU0._PPC = Zero
                        PNOT ()
                    }

                    Method (_Q19, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        P80H = 0x19
                        If ((One && ^^SNC.P147))
                        {
                            ^^SNC.SNNE (0x0A)
                        }
                    }

                    Method (_Q1A, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        P80H = 0x1A
                        GL08 |= 0x10
                        EJRQ = One
                        Notify (AHCI, 0x84) // Reserved
                    }

                    Method (_Q1B, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        P80H = 0x1B
                        If (^^SNC.ASCS)
                        {
                            ^^SNC.INTS = 0x02
                            ^^SNC.SNNE (0x04)
                        }
                    }

                    Method (_Q1C, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        P80H = 0x1C
                        Notify (^^^PEG0.PEGP, 0xDF) // Hardware-Specific
                        ^^SNC.SNNE (0x05)
                    }

                    Method (_Q1E, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        P80H = 0x1E
                        Notify (^^^GFX0.DD02, 0x86) // Device-Specific
                    }

                    Method (_Q1F, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
                    {
                        P80H = 0x1F
                        Notify (^^^GFX0.DD02, 0x87) // Device-Specific
                    }
                }

                Scope (\_SB)
