                Method (_BCL, 0, NotSerialized)  // _BCL: Brightness Control Levels
                {
                    If ((OSYS >= 0x07DC))
                    {
                        BLT8 = BLP8 /* \BLP8 */
                        Return (VBL8) /* \_SB_.PCI0.GFX0.DD02.VBL8 */
                    }

                    BLT7 = BLP7 /* \BLP7 */
                    Return (VBL7) /* \_SB_.PCI0.GFX0.DD02.VBL7 */
                }

                Method (_BCM, 1, NotSerialized)  // _BCM: Brightness Control Method
                {
                    If (^^^LPCB.SNC.ASCS)
                    {
                        BRTL = Arg0
                        ^^^LPCB.SNC.INTS = One
                        ^^^LPCB.SNC.SNNE (0x04)
                        Return (Zero)
                    }

                    If (((Arg0 >= Zero) && (Arg0 <= 0x64)))
                    {
                        If ((OSYS >= 0x07DC))
                        {
                            Local0 = Match (VBL8, MEQ, Arg0, MTR, Zero, 0x02)
                        }
                        Else
                        {
                            Local0 = Match (VBL7, MEQ, Arg0, MTR, Zero, 0x02)
                        }

                        If ((Local0 == Ones))
                        {
                            Return (Zero)
                        }

                        Local0 -= 0x02
                        If ((OSYS >= 0x07DC))
                        {
                            Local0 = DerefOf (BLT8 [Local0])
                        }
                        Else
                        {
                            Local0 = DerefOf (BLT7 [Local0])
                        }

                        AINT (One, Local0)
                        BRTL = Arg0
                    }
                }

                Method (_BQC, 0, NotSerialized)  // _BQC: Brightness Query Current
                {
                    Return (BRTL) /* \BRTL */
                }
            }

