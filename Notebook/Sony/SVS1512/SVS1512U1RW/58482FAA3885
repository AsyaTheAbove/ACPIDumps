            Name (SUCC, One)
            Name (NVLD, 0x02)
            Name (CRIT, 0x04)
            Name (NCRT, 0x06)
            Method (GSCI, 0, Serialized)
            {
                Method (GBDA, 0, Serialized)
                {
                    If ((GESF == Zero))
                    {
                        PARM = 0x0679
                        GESF = Zero
                        Return (SUCC) /* \_SB_.PCI0.GFX0.SUCC */
                    }

                    If ((GESF == One))
                    {
                        PARM = 0x0240
                        GESF = Zero
                        Return (SUCC) /* \_SB_.PCI0.GFX0.SUCC */
                    }

                    If ((GESF == 0x04))
                    {
                        PARM &= 0xEFFF0000
                        PARM &= (DerefOf (DBTB [IBTT]) << 0x10)
                        PARM |= IBTT /* \_SB_.PCI0.GFX0.PARM */
                        GESF = Zero
                        Return (SUCC) /* \_SB_.PCI0.GFX0.SUCC */
                    }

                    If ((GESF == 0x05))
                    {
                        PARM = IPSC /* \IPSC */
                        PARM |= (IPAT << 0x08)
                        PARM += 0x0100
                        PARM |= (LIDS << 0x10)
                        PARM += 0x00010000
                        PARM |= (IBIA << 0x14)
                        GESF = Zero
                        Return (SUCC) /* \_SB_.PCI0.GFX0.SUCC */
                    }

                    If ((GESF == 0x06))
                    {
                        PARM = ITVF /* \ITVF */
                        PARM |= (ITVM << 0x04)
                        GESF = Zero
                        Return (SUCC) /* \_SB_.PCI0.GFX0.SUCC */
                    }

                    If ((GESF == 0x07))
                    {
                        PARM = GIVD /* \_SB_.PCI0.GFX0.GIVD */
                        PARM ^= One
                        PARM |= (GMFN << One)
                        PARM |= 0x1800
                        PARM |= (IDMS << 0x11)
                        PARM |= (DerefOf (DerefOf (CDCT [HVCO]) [CDVL]) << 
                            0x15) /* \_SB_.PCI0.GFX0.PARM */
                        GESF = One
                        Return (SUCC) /* \_SB_.PCI0.GFX0.SUCC */
                    }

                    If ((GESF == 0x0A))
                    {
                        PARM = Zero
                        If (ISSC)
                        {
                            PARM |= 0x03
                        }

                        GESF = Zero
                        Return (SUCC) /* \_SB_.PCI0.GFX0.SUCC */
                    }

                    If ((GESF == 0x0B))
                    {
                        PARM = KSV0 /* \KSV0 */
                        GESF = KSV1 /* \KSV1 */
                        Return (SUCC) /* \_SB_.PCI0.GFX0.SUCC */
                    }

                    GESF = Zero
                    Return (CRIT) /* \_SB_.PCI0.GFX0.CRIT */
                }

                Method (SBCB, 0, Serialized)
                {
                    If ((GESF == Zero))
                    {
                        PARM = Zero
                        PARM = 0x000F87FD
                        GESF = Zero
                        Return (SUCC) /* \_SB_.PCI0.GFX0.SUCC */
                    }

                    If ((GESF == One))
                    {
                        GESF = Zero
                        PARM = Zero
                        Return (SUCC) /* \_SB_.PCI0.GFX0.SUCC */
                    }

                    If ((GESF == 0x03))
                    {
                        GESF = Zero
                        PARM = Zero
                        Return (SUCC) /* \_SB_.PCI0.GFX0.SUCC */
                    }

                    If ((GESF == 0x04))
                    {
                        GESF = Zero
                        PARM = Zero
                        Return (SUCC) /* \_SB_.PCI0.GFX0.SUCC */
                    }

                    If ((GESF == 0x05))
                    {
                        GESF = Zero
                        PARM = Zero
                        Return (SUCC) /* \_SB_.PCI0.GFX0.SUCC */
                    }

                    If ((GESF == 0x06))
                    {
                        ITVF = (PARM & 0x0F)
                        ITVM = ((PARM & 0xF0) >> 0x04)
                        GESF = Zero
                        PARM = Zero
                        Return (SUCC) /* \_SB_.PCI0.GFX0.SUCC */
                    }

                    If ((GESF == 0x07))
                    {
                        If ((PARM == Zero))
                        {
                            Local0 = CLID /* \_SB_.PCI0.GFX0.CLID */
                            If ((0x80000000 & Local0))
                            {
                                CLID &= 0x0F
                                GLID (CLID)
                            }
                        }

                        GESF = Zero
                        PARM = Zero
                        Return (SUCC) /* \_SB_.PCI0.GFX0.SUCC */
                    }

                    If ((GESF == 0x08))
                    {
                        GESF = Zero
                        PARM = Zero
                        Return (SUCC) /* \_SB_.PCI0.GFX0.SUCC */
                    }

                    If ((GESF == 0x09))
                    {
                        IBTT = (PARM & 0xFF)
                        GESF = Zero
                        PARM = Zero
                        Return (SUCC) /* \_SB_.PCI0.GFX0.SUCC */
                    }

                    If ((GESF == 0x0A))
                    {
                        IPSC = (PARM & 0xFF)
                        If (((PARM >> 0x08) & 0xFF))
                        {
                            IPAT = ((PARM >> 0x08) & 0xFF)
                            IPAT--
                        }

                        IBIA = ((PARM >> 0x14) & 0x07)
                        GESF = Zero
                        PARM = Zero
                        Return (SUCC) /* \_SB_.PCI0.GFX0.SUCC */
                    }

                    If ((GESF == 0x0B))
                    {
                        IF1E = ((PARM >> One) & One)
                        If ((PARM & 0x0001E000))
                        {
                            IDMS = ((PARM >> 0x0D) & 0x0F)
                        }
                        Else
                        {
                            IDMS = ((PARM >> 0x11) & 0x0F)
                        }

                        GESF = Zero
                        PARM = Zero
                        Return (SUCC) /* \_SB_.PCI0.GFX0.SUCC */
                    }

                    If ((GESF == 0x10))
                    {
                        GESF = Zero
                        PARM = Zero
                        Return (SUCC) /* \_SB_.PCI0.GFX0.SUCC */
                    }

                    If ((GESF == 0x11))
                    {
                        PARM = (LIDS << 0x08)
                        PARM += 0x0100
                        GESF = Zero
                        Return (SUCC) /* \_SB_.PCI0.GFX0.SUCC */
                    }

                    If ((GESF == 0x12))
                    {
                        If ((PARM & One))
                        {
                            If (((PARM >> One) == One))
                            {
                                ISSC = One
                            }
                            Else
                            {
                                GESF = Zero
                                Return (CRIT) /* \_SB_.PCI0.GFX0.CRIT */
                            }
                        }
                        Else
                        {
                            ISSC = Zero
                        }

                        GESF = Zero
                        PARM = Zero
                        Return (SUCC) /* \_SB_.PCI0.GFX0.SUCC */
                    }

                    If ((GESF == 0x13))
                    {
                        GESF = Zero
                        PARM = Zero
                        Return (SUCC) /* \_SB_.PCI0.GFX0.SUCC */
                    }

                    If ((GESF == 0x14))
                    {
                        PAVP = (PARM & 0x0F)
                        GESF = Zero
                        PARM = Zero
                        Return (SUCC) /* \_SB_.PCI0.GFX0.SUCC */
                    }

                    GESF = Zero
                    Return (SUCC) /* \_SB_.PCI0.GFX0.SUCC */
                }

                If ((GEFC == 0x04))
                {
                    GXFC = GBDA ()
                }

                If ((GEFC == 0x06))
                {
                    GXFC = SBCB ()
                }

                GEFC = Zero
                SCIS = One
                GSSE = Zero
                SCIE = Zero
                Return (Zero)
            }

            Method (PDRD, 0, NotSerialized)
            {
                If (!DRDY)
                {
                    Sleep (ASLP)
                }

                Return (!DRDY)
            }

            Method (PSTS, 0, NotSerialized)
            {
                If ((CSTS > 0x02))
                {
                    Sleep (ASLP)
                }

                Return ((CSTS == 0x03))
            }

            Method (GNOT, 2, NotSerialized)
            {
                If (PDRD ())
                {
                    Return (One)
                }

                CEVT = Arg0
                CSTS = 0x03
                If (((CHPD == Zero) && (Arg1 == Zero)))
                {
                    If (((OSYS > 0x07D0) || (OSYS < 0x07D6)))
                    {
                        Notify (PCI0, Arg1)
                    }
                    Else
                    {
                        Notify (GFX0, Arg1)
                    }
                }

                If (CondRefOf (HNOT))
                {
                    HNOT (Arg0)
                }
                Else
                {
                    Notify (GFX0, 0x80) // Status Change
                }

                Return (Zero)
            }

            Method (GHDS, 1, NotSerialized)
            {
                TIDX = Arg0
                Return (GNOT (One, Zero))
            }

            Method (GLID, 1, NotSerialized)
            {
                If ((Arg0 == One))
                {
                    CLID = 0x03
                }
                Else
                {
                    CLID = Arg0
                }

                Return (GNOT (0x02, Zero))
            }

            Method (GDCK, 1, NotSerialized)
            {
                CDCK = Arg0
                Return (GNOT (0x04, Zero))
            }

            Method (PARD, 0, NotSerialized)
            {
                If (!ARDY)
                {
                    Sleep (ASLP)
                }

                Return (!ARDY)
            }

            Method (AINT, 2, NotSerialized)
            {
                If (!(TCHE & (One << Arg0)))
                {
                    Return (One)
                }

                If (PARD ())
                {
                    Return (One)
                }

                If ((Arg0 == 0x02))
                {
                    If (CPFM)
                    {
                        Local0 = (CPFM & 0x0F)
                        Local1 = (EPFM & 0x0F)
                        If ((Local0 == One))
                        {
                            If ((Local1 & 0x06))
                            {
                                PFIT = 0x06
                            }
                            ElseIf ((Local1 & 0x08))
                            {
                                PFIT = 0x08
                            }
                            Else
                            {
                                PFIT = One
                            }
                        }

                        If ((Local0 == 0x06))
                        {
                            If ((Local1 & 0x08))
                            {
                                PFIT = 0x08
                            }
                            ElseIf ((Local1 & One))
                            {
                                PFIT = One
                            }
                            Else
                            {
                                PFIT = 0x06
                            }
                        }

                        If ((Local0 == 0x08))
                        {
                            If ((Local1 & One))
                            {
                                PFIT = One
                            }
                            ElseIf ((Local1 & 0x06))
                            {
                                PFIT = 0x06
                            }
                            Else
                            {
                                PFIT = 0x08
                            }
                        }
                    }
                    Else
                    {
                        PFIT ^= 0x07
                    }

                    PFIT |= 0x80000000
                    ASLC = 0x04
                }
                ElseIf ((Arg0 == One))
                {
                    BCLP = Arg1
                    BCLP |= 0x80000000
                    ASLC = 0x02
                }
                ElseIf ((Arg0 == Zero))
                {
                    ALSI = Arg1
                    ASLC = One
                }
                Else
                {
                    Return (One)
                }

                ASLE = One
                Return (Zero)
            }

            Method (SCIP, 0, NotSerialized)
            {
                If ((OVER != Zero))
                {
                    Return (!GSMI)
                }

                Return (Zero)
            }

            Device (^^MEM2)
            {
                Name (_HID, EisaId ("PNP0C01") /* System Board */)  // _HID: Hardware ID
                Name (_UID, 0x02)  // _UID: Unique ID
                Name (CRS1, ResourceTemplate ()
                {
                    Memory32Fixed (ReadWrite,
                        0x20000000,         // Address Base
                        0x00200000,         // Address Length
                        )
                    Memory32Fixed (ReadWrite,
                        0x40000000,         // Address Base
                        0x00200000,         // Address Length
                        )
                })
                Name (CRS2, ResourceTemplate ()
                {
                    Memory32Fixed (ReadWrite,
                        0x20000000,         // Address Base
                        0x00200000,         // Address Length
                        )
                    Memory32Fixed (ReadWrite,
                        0x40004000,         // Address Base
                        0x00001000,         // Address Length
                        )
                })
                Method (_STA, 0, NotSerialized)  // _STA: Status
                {
                    If (IGDS)
                    {
                        Return (0x0F)
                    }

                    Return (Zero)
                }

                Method (_CRS, 0, NotSerialized)  // _CRS: Current Resource Settings
                {
                    If ((((PNHM & 0x0FFF0FF0) == 0x000206A0) || ((PNHM & 0x0FFF0FFF
                        ) == 0x000306A0)))
                    {
                        Return (CRS1) /* \_SB_.MEM2.CRS1 */
                    }

                    Return (CRS2) /* \_SB_.MEM2.CRS2 */
                }
            }

            Method (_DSM, 4, Serialized)  // _DSM: Device-Specific Method
            {
                Name (DRET, Buffer (0x04)
                {
                     0x00                                             // .
                })
                If ((Arg0 == ToUUID ("6f3975e1-7a82-4f67-8b97-15bee060bedf") /* Unknown UUID */))
                {
                    If ((Arg2 == Zero))
                    {
                        CreateWordField (DRET, Zero, F0SS)
                        F0SS = 0x02
                        Return (DRET) /* \_SB_.PCI0.GFX0._DSM.DRET */
                    }

                    If ((Arg2 == One))
                    {
                        If ((^^PEG0.PEGP.LNKV == 0x03))
                        {
                            Return (Zero)
                        }

                        Return (One)
                    }
                }

                Return (DRET) /* \_SB_.PCI0.GFX0._DSM.DRET */
            }
        }
    }

    Scope (\)
    {
        OperationRegion (APMT, SystemMemory, 0x9AFB8F18, 0x0000007E)
        Field (APMT, AnyAcc, NoLock, Preserve)
        {
            BLP7,   128, 
            PPMB,   72, 
            BLP8,   808
        }

        Method (SPTS, 1, NotSerialized)
        {
            If ((Arg0 == 0x03)) {}
            If ((Arg0 == 0x04))
            {
                \_SB.PCI0.LPCB.H_EC.GOS4 = One
            }

            If ((Arg0 == 0x05)) {}
            If ((Arg0 <= 0x04))
            {
                ASTF = Zero
                \_SB.PCI0.LPCB.SNC.ASCS = Zero
            }

            \_SB.PCI0.LPCB.SNC.I147 ()
            Return (Zero)
        }

        Method (SWAK, 1, NotSerialized)
        {
            If ((Arg0 == 0x03))
            {
                \_SB.PCI0.LPCB.SNC.WKRS = \_SB.PCI0.LPCB.H_EC.WKRR
            }

            If ((Arg0 == 0x04))
            {
                \_SB.PCI0.LPCB.SNC.WKRS = Zero
            }

            \_SB.PCI0.LPCB.SNC.ENCR = Zero
            \_SB.PCI0.LPCB.SNC.ESR = Zero
            NXSL = Zero
            \_SB.PCI0.LPCB.SNC.ECR = Zero
            \_SB.PCI0.LPCB.H_EC.HKYS = Zero
            Return (Zero)
        }
    }

    Scope (_SB.PCI0)
    {
        Method (SINI, 0, NotSerialized)
        {
            Return (Zero)
        }
    }

    Scope (_SB.PCI0.LPCB)
    {
        Device (SNC)
        {
            Name (_HID, EisaId ("SNY5001"))  // _HID: Hardware ID
            Method (_INI, 0, NotSerialized)  // _INI: Initialize
            {
                Return (Zero)
            }

            Method (PWAK, 0, NotSerialized)
            {
                Return (Zero)
            }

            Name (RSLC, Buffer (0x04)
            {
                 0x00                                             // .
            })
            CreateByteField (RSLC, Zero, SWKR)
            Method (WAKR, 0, NotSerialized)
            {
                SWKR = ^^H_EC.WEBR /* \_SB_.PCI0.LPCB.H_EC.WEBR */
                Return (RSLC) /* \_SB_.PCI0.LPCB.SNC_.RSLC */
            }

            Method (EAWK, 1, NotSerialized)
            {
                SEAW (0x14)
                Return (~Arg0)
            }

            Name (CFGI, Package (0x30)
            {
                0x53636E53, 
                0x6F707075, 
                0x64657472, 
                0x0100, 
                0x38430000, 
                Zero, 
                Zero, 
                Zero, 
                Zero, 
                Zero, 
                Zero, 
                Zero, 
                Zero, 
                Zero, 
                Zero, 
                Zero, 
                0xFFFF, 
                Zero, 
                Zero, 
                Zero, 
                Zero, 
                Zero, 
                Zero, 
                Zero, 
                Zero, 
                Zero, 
                Zero, 
                Zero, 
                Zero, 
                Zero, 
                Zero, 
                Zero, 
                0x0100, 
                0x0122, 
                0x0148, 
                0x011D, 
                0x0163, 
                0x015B, 
                0x0149, 
                0x0157, 
                0x0114, 
                0x013F, 
                0x0147, 
                0x0135, 
                0x0155, 
                0x015D, 
                0x013A, 
                0x0156
            })
            Name (ENCR, Zero)
            Name (ESR, Zero)
            Mutex (LENR, 0x00)
            Name (BUFF, Buffer (0x0410) {})
            CreateByteField (BUFF, Zero, CHN)
            CreateByteField (BUFF, One, CMD)
            CreateField (BUFF, 0x10, 0x2070, CP)
            Alias (BUFF, RC)
            CreateDWordField (BUFF, 0x02, CPD0)
            CreateDWordField (BUFF, 0x06, CPD1)
            CreateWordField (BUFF, 0x02, CPW0)
            CreateWordField (BUFF, 0x04, CPW1)
            CreateWordField (BUFF, 0x06, CPW2)
            CreateWordField (BUFF, 0x08, CPW3)
            CreateByteField (BUFF, 0x02, CPB0)
            CreateByteField (BUFF, 0x03, CPB1)
            CreateByteField (BUFF, 0x04, CPB2)
            CreateByteField (BUFF, 0x05, CPB3)
            CreateByteField (BUFF, 0x06, CPB4)
            CreateByteField (BUFF, 0x07, CPB5)
            CreateDWordField (BUFF, Zero, RCD0)
            CreateDWordField (BUFF, 0x04, RCD1)
            CreateDWordField (BUFF, 0x08, RCD2)
            CreateDWordField (BUFF, 0x0C, RCD3)
            CreateWordField (BUFF, Zero, RCW0)
            CreateWordField (BUFF, 0x02, RCW1)
            CreateWordField (BUFF, 0x04, RCW2)
            CreateWordField (BUFF, 0x06, RCW3)
            CreateByteField (BUFF, Zero, RCB0)
            CreateByteField (BUFF, One, RCB1)
            CreateByteField (BUFF, 0x02, RCB2)
            CreateByteField (BUFF, 0x03, RCB3)
            CreateByteField (BUFF, 0x04, RCB4)
            CreateByteField (BUFF, 0x05, RCB5)
            CreateByteField (BUFF, 0x06, RCB6)
            CreateByteField (BUFF, 0x07, RCB7)
            Method (SN00, 1, NotSerialized)
            {
                If (((Arg0 >= Zero) && (Arg0 < 0x30)))
                {
                    Return (DerefOf (CFGI [Arg0]))
                }

                Return (Zero)
            }

            Method (SN01, 0, NotSerialized)
            {
                Return (ENCR) /* \_SB_.PCI0.LPCB.SNC_.ENCR */
            }

            Method (SN02, 1, NotSerialized)
            {
                Local0 = (Arg0 & 0xFFFF)
                Acquire (LENR, 0xFFFF)
                ENCR |= Local0
                Release (LENR)
            }

            Method (SN03, 1, NotSerialized)
            {
                Local0 = ~Arg0
                Local0 &= 0xFFFF
                Acquire (LENR, 0xFFFF)
                ENCR &= Local0
                Release (LENR)
            }

            Method (SN04, 0, NotSerialized)
            {
                Return (ESR) /* \_SB_.PCI0.LPCB.SNC_.ESR_ */
            }

            Method (SN05, 1, NotSerialized)
            {
                If (HS05 (Arg0))
                {
                    Local0 = ~Arg0
                    Local0 &= 0xFFFF
                    ESR &= Local0
                }
            }

            Method (SN06, 1, NotSerialized)
            {
                Return (SNCF (Arg0, Zero))
            }

            Method (SN07, 1, NotSerialized)
            {
                Return (SNCF (Arg0, One))
            }

            Method (SNCF, 2, Serialized)
            {
                BUFF = Arg0
                Local0 = (CHN & 0x0F)
                If (((One << Local0) & DerefOf (CFGI [0x10])))
                {
                    If ((Local0 > 0x10)) {}
                    ElseIf ((Local0 == Zero))
                    {
                        F100 ()
                    }
                    ElseIf ((Local0 == One))
                    {
                        F122 ()
                    }
                    ElseIf ((Local0 == 0x02))
                    {
                        F148 ()
                    }
                    ElseIf ((Local0 == 0x03))
                    {
                        F11D ()
                    }
                    ElseIf ((Local0 == 0x04))
                    {
                        F163 ()
                    }
                    ElseIf ((Local0 == 0x05))
                    {
                        F15B ()
                    }
                    ElseIf ((Local0 == 0x06))
                    {
                        F149 ()
                    }
                    ElseIf ((Local0 == 0x07))
                    {
                        F157 ()
                    }
                    ElseIf ((Local0 == 0x08))
                    {
                        F114 ()
                    }
                    ElseIf ((Local0 == 0x09))
                    {
                        F13F ()
                    }
                    ElseIf ((Local0 == 0x0A))
                    {
                        F147 ()
                    }
                    ElseIf ((Local0 == 0x0B))
                    {
                        F135 ()
                    }
                    ElseIf ((Local0 == 0x0C))
                    {
                        F155 ()
                    }
                    ElseIf ((Local0 == 0x0D))
                    {
                        F15D ()
                    }
                    ElseIf ((Local0 == 0x0E))
                    {
                        F13A ()
                    }
                    ElseIf ((Local0 == 0x0F))
                    {
                        F156 ()
                    }
                }

                If ((Arg1 == One))
                {
                    Return (RCD0) /* \_SB_.PCI0.LPCB.SNC_.RCD0 */
                }

                Return (RC) /* \_SB_.PCI0.LPCB.SNC_.RC__ */
            }

            Method (SNNE, 1, Serialized)
            {
                Local0 = (One << Arg0)
                If ((Local0 & DerefOf (CFGI [0x10])))
                {
                    ESR |= Local0
                    If ((ENCR & Local0))
                    {
                        Notify (SNC, (0x90 + Arg0))
                    }
                }
            }

            Name (ECR, Zero)
            Method (F100, 0, NotSerialized)
            {
                If ((CMD == Zero))
                {
                    ^^H_EC.HKYS = One
                }
                ElseIf ((CMD == One))
                {
                    ^^H_EC.HKYS = Zero
                }
                ElseIf ((CMD == 0x02))
                {
                    RCB0 = ECR /* \_SB_.PCI0.LPCB.SNC_.ECR_ */
                }
                Else
                {
                    Noop
                }

                Return (Zero)
            }

            Method (F122, 0, NotSerialized)
            {
                If ((CMD == Zero))
                {
                    RCB0 = 0x03
                }
                ElseIf ((CMD == One))
                {
                    RCB0 = (^^H_EC.TMLP & 0x03)
                }
                ElseIf ((CMD == 0x02))
                {
                    ^^H_EC.TMLP = (CPB0 & 0x03)
                    ETCP (CPB0)
                }
            }

            Method (ETCP, 1, NotSerialized)
            {
                If ((Arg0 == One))
                {
                    Name (TPPS, Zero)
                    Name (TPCF, Zero)
                    Name (SMCL, Zero)
                    TPPS = SizeOf (\_PR.CPU0._PSS)
                    TPPS--
                    If ((^^H_EC.CPUT == Zero))
                    {
                        SMCL = 0x0640
                    }
                    Else
                    {
                        SMCL = 0x0640
                    }

                    While (One)
                    {
                        TPCF = DerefOf (DerefOf (\_PR.CPU0._PSS [TPPS]) [Zero])
                        If ((TPCF >= SMCL))
                        {
                            Break
                        }
                        ElseIf ((TPPS <= Zero))
                        {
                            TPPS = Zero
                            Break
                        }

                        TPPS--
                    }

                    \_PR.CPU0._PPC = TPPS /* \_SB_.PCI0.LPCB.SNC_.ETCP.TPPS */
                }
                Else
                {
                    \_PR.CPU0._PPC = Zero
                }

                PNOT ()
                Return (Zero)
            }

            Method (F148, 0, NotSerialized)
            {
                If ((CMD == Zero))
                {
                    RCB0 = ^^H_EC.PTDS /* \_SB_.PCI0.LPCB.H_EC.PTDS */
                }
                ElseIf ((CMD == One))
                {
                    ^^H_EC.PTDS = CPB0 /* \_SB_.PCI0.LPCB.SNC_.CPB0 */
                }
                Else
                {
                    Noop
                }

                Return (Zero)
            }

            Method (F11D, 0, NotSerialized)
            {
                If ((CMD == Zero))
                {
                    RCB0 = DCID /* \DCID */
                    RCB1 = Zero
                }
                Else
                {
                    Noop
                }

                Return (Zero)
            }

            Name (ASCS, Zero)
            Name (INTS, Zero)
            Method (F163, 0, NotSerialized)
            {
                If ((CMD == Zero))
                {
                    RC = BLP7 /* \BLP7 */
                    CreateField (RC, 0x80, 0x48, RC16)
                    RC16 = PPMB /* \PPMB */
                    CreateField (RC, 0xC8, 0x0328, RC25)
                    RC25 = BLP8 /* \BLP8 */
                }
                ElseIf ((CMD == One))
                {
                    Local0 = 0x02
                    If ((^^H_EC.KBLP && One))
                    {
                        Local0 |= One
                    }

                    RCB0 = Local0
                }
                ElseIf ((CMD == 0x10))
                {
                    RCB0 = ^^H_EC.ILML /* \_SB_.PCI0.LPCB.H_EC.ILML */
                    RCB1 = ^^H_EC.ILMH /* \_SB_.PCI0.LPCB.H_EC.ILMH */
                    ^^H_EC.ADFS = One
                    RCB2 = One
                }
                ElseIf ((CMD == 0x20))
                {
                    RCB0 = INTS /* \_SB_.PCI0.LPCB.SNC_.INTS */
                }
                ElseIf ((CMD == 0x21))
                {
                    RCB0 = ASCS /* \_SB_.PCI0.LPCB.SNC_.ASCS */
                }
                ElseIf ((CMD == 0x22))
                {
                    ASCS = (CPB0 & One)
                }
                ElseIf ((CMD == 0x30))
                {
                    ^^^GFX0.AINT (One, CPB0)
                }
                ElseIf ((CMD == 0x31))
                {
                    RCB0 = ^^^GFX0.BCLP /* \_SB_.PCI0.GFX0.BCLP */
                }
                ElseIf ((CMD == 0x40))
                {
                    ^^H_EC.KLPC = (CPB0 & 0x03)
                }
                ElseIf ((CMD == 0x41))
                {
                    ^^H_EC.KLST = (CPB0 & One)
                }
                ElseIf ((CMD == 0x42))
                {
                    Name (SKBT, Package (0x04)
                    {
                        Zero, 
                        One, 
                        0x02, 
                        0x03
                    })
                    If ((Match (SKBT, MEQ, CPB0, MTR, Zero, Zero) != Ones))
                    {
                        ^^H_EC.KLTM = CPB0 /* \_SB_.PCI0.LPCB.SNC_.CPB0 */
                    }
                    Else
                    {
                        Noop
                    }
                }
                Else
                {
                    Noop
                }
            }

            Method (F15B, 0, NotSerialized)
            {
                If ((CMD == Zero))
                {
                    RCB0 = ^^H_EC.PFSP /* \_SB_.PCI0.LPCB.H_EC.PFSP */
                }
            }

            Name (FDP1, Buffer (0x02)
            {
                 0x01, 0x46                                       // .F
            })
            Name (FDP2, Buffer (0x02)
            {
                 0x01, 0x39                                       // .9
            })
            Method (F149, 0, NotSerialized)
            {
                If ((CMD == Zero))
                {
                    If (((GL00 & 0x04) == 0x04))
                    {
                        RC = FDP2 /* \_SB_.PCI0.LPCB.SNC_.FDP2 */
                    }
                    Else
                    {
                        RC = FDP1 /* \_SB_.PCI0.LPCB.SNC_.FDP1 */
                    }
                }
                ElseIf ((CMD == One))
                {
                    RCB0 = ^^H_EC.FCMD /* \_SB_.PCI0.LPCB.H_EC.FCMD */
                }
                ElseIf ((CMD == 0x02))
                {
                    ^^H_EC.FCMD = CPB0 /* \_SB_.PCI0.LPCB.SNC_.CPB0 */
                }
                ElseIf ((CMD == 0x03))
                {
                    RCB0 = (^^H_EC.FRPM / 0x64)
                }
                Else
                {
                    Noop
                }

                Return (Zero)
            }

            Method (F157, 0, NotSerialized)
            {
                If ((CMD == Zero))
                {
                    If ((CPB0 == Zero))
                    {
                        NXSL = Zero
                        RCB0 = Zero
                    }
                    ElseIf ((CPB0 == One))
                    {
                        NXSL = One
                        RCB0 = Zero
                    }
                    Else
                    {
                        NXSL = Zero
                        RCB0 = One
                    }
                }
                Else
                {
                    Noop
                }
            }

            Method (F114, 0, NotSerialized)
            {
                If ((CMD == Zero))
                {
                    RCB0 = ^^H_EC.BATM /* \_SB_.PCI0.LPCB.H_EC.BATM */
                }
                Else
                {
                    Noop
                }

                Return (Zero)
            }

            Method (F13F, 0, NotSerialized)
            {
                If ((CMD == Zero))
                {
                    RCB0 = (^^H_EC.BCCS & 0x31)
                }
                ElseIf ((CMD == One))
                {
                    ^^H_EC.BCCS = (CPB0 & 0x31)
                }
                ElseIf ((CMD == 0x02))
                {
                    RCB0 = ^^H_EC.BCRC /* \_SB_.PCI0.LPCB.H_EC.BCRC */
                }
                Else
                {
                    Noop
                }
            }

            Name (P147, Zero)
            Name (ULDC, Zero)
            Name (HDDF, Zero)
            Name (DTMD, One)
            Method (F147, 0, NotSerialized)
            {
                Local0 = Zero
                Local1 = Zero
                If ((CMD == Zero))
                {
                    P147 = (One & CPB0)
                }
                ElseIf ((CMD == One))
                {
                    RCB0 = (One & P147)
                }
                ElseIf ((CMD == 0x02))
                {
                    Local1 |= One
                    If (((One && ULDC) && (One && HDDF)))
                    {
                        Local1 |= 0x02
                    }
                    Else
                    {
                        Noop
                    }

                    RCB0 = Local1
                }
                ElseIf ((CMD == 0x03))
                {
                    ^^H_EC.ULCB = (One & CPB0)
                }
                ElseIf ((CMD == 0x04))
                {
                    RCB0 = (One & ^^H_EC.ULCB)
                }
                Else
                {
                    Noop
                }

                Return (Zero)
            }

            Method (CHHD, 1, NotSerialized)
            {
                CreateWordField (Arg0, 0x012C, ULDP)
                CreateWordField (Arg0, 0x01B2, NMMR)
                ULDC = ((ULDP & 0x4000) >> 0x0E)
                If ((NMMR == One))
                {
                    HDDF = Zero
                }
                Else
                {
                    HDDF = One
                }

                Return (Zero)
            }

            Method (I147, 0, NotSerialized)
            {
                P147 = Zero
                Return (Zero)
            }

            Name (WCNG, Zero)
            Name (SCNG, Zero)
            Name (WLAP, Zero)
            Name (BLTP, Zero)
            Name (WWAP, Zero)
            Name (SIMP, Zero)
            Name (WMXP, Zero)
            Name (GPSP, Zero)
            Name (WWAD, Zero)
            Name (WLRF, Zero)
            Name (WMRF, Zero)
            Name (FSMC, One)
            Method (F135, 0, NotSerialized)
            {
                Name (TWSL, Zero)
                Name (TWRF, Zero)
                Name (TECS, Zero)
                If (FSMC)
                {
                    WLAP = Zero
                    WWAP = Zero
                    SIMP = Zero
                    GPSP = Zero
                    WMXP = Zero
                    If (WLDV)
                    {
                        If ((^^^RP01.WLAN.VID != 0xFFFF))
                        {
                            WLAP = One
                        }

                        If (^^H_EC.BTDP)
                        {
                            BLTP = Zero
                        }
                        Else
                        {
                            BLTP = One
                        }

                        Local0 = ^^H_EC.WWDP /* \_SB_.PCI0.LPCB.H_EC.WWDP */
                        If ((Local0 == Zero))
                        {
                            WWAD = 0x51
                            WWAP = One
                            GPSP = One
                            SIMP = One
                        }
                        ElseIf ((Local0 == One))
                        {
                            WWAD = 0x50
                            WWAP = One
                            GPSP = Zero
                            SIMP = One
                        }
                        ElseIf ((Local0 == 0x02))
                        {
                            WWAD = 0x29
                            WWAP = One
                            GPSP = One
                            SIMP = One
                        }
                        ElseIf ((Local0 == 0x03))
                        {
                            WWAD = 0x28
                            WWAP = One
                            GPSP = Zero
                            SIMP = One
                        }
                        Else
                        {
                            WWAD = Zero
                            WWAP = Zero
                            GPSP = Zero
                            SIMP = Zero
                        }

                        If (WLAP)
                        {
                            If ((^^^RP01.WLAN.VID == 0x8086))
                            {
                                Local0 = ^^^RP01.WLAN.DID /* \_SB_.PCI0.RP01.WLAN.DID_ */
                                If ((Local0 == 0x87))
                                {
                                    WMXP = One
                                }
                                ElseIf ((Local0 == 0x0885))
                                {
                                    WMXP = One
                                }
                            }

                            If ((One != WMXP))
                            {
                                Local0 = ^^H_EC.WLNS /* \_SB_.PCI0.LPCB.H_EC.WLNS */
                                Local0 &= 0xFFFFFFFB
                                ^^H_EC.WLNS = Local0
                            }
                        }

                        Local0 = ^^H_EC.WLNS /* \_SB_.PCI0.LPCB.H_EC.WLNS */
                        If (WLAP)
                        {
                            If ((Local0 & 0x02))
                            {
                                WLRF = One
                            }
                            Else
                            {
                                WLRF = Zero
                            }
                        }

                        If (WMXP)
                        {
                            If ((Local0 & 0x04))
                            {
                                WMRF = One
                            }
                            Else
                            {
                                WMRF = Zero
                            }
                        }
                    }

                    FSMC = Zero
                }

                If ((CMD == Zero))
                {
                    Local0 = Zero
                    If (WLAP)
                    {
                        RC [Local0] = Zero
                        Local0++
                    }

                    If (BLTP)
                    {
                        RC [Local0] = 0x10
                        Local0++
                    }

                    If (WWAP)
                    {
                        If ((WWAD == Zero))
                        {
                            Noop
                        }
                        Else
                        {
                            RC [Local0] = WWAD /* \_SB_.PCI0.LPCB.SNC_.WWAD */
                            Local0++
                        }
                    }

                    If (WMXP)
                    {
                        RC [Local0] = 0x30
                        Local0++
                    }

                    If (!SIMP)
                    {
                        RC [Local0] = 0x70
                        Local0++
                    }
                    Else
                    {
                        RC [Local0] = 0x71
                        Local0++
                    }

                    RC [Local0] = 0x7E
                    Local0++
                    RC [Local0] = 0x7F
                    Local0++
                    RC [Local0] = 0xFF
                }
                ElseIf ((CMD == One))
                {
                    RCB0 = (WCNG | SCNG) /* \_SB_.PCI0.LPCB.SNC_.SCNG */
                }
                ElseIf ((CMD == 0x02))
                {
                    RCB0 = (^^H_EC.WLSS & 0x03)
                }
                ElseIf ((CMD == 0x03))
                {
                    If (WLAP)
                    {
                        RCB0 = Zero
                        TECS = ^^H_EC.WLNS /* \_SB_.PCI0.LPCB.H_EC.WLNS */
                        TWSL = (TECS & 0x02)
                        TWRF = (TECS & One)
                        RCB0 |= TWSL /* \_SB_.PCI0.LPCB.SNC_.RCB0 */
                        RCB0 |= (TWRF & WLRF) /* \_SB_.PCI0.LPCB.SNC_.RCB0 */
                    }
                }
                ElseIf ((CMD == 0x04))
                {
                    If (WLAP)
                    {
                        TWSL = (CPB0 & 0x02)
                        TWRF = (CPB0 & One)
                        TECS = ^^H_EC.WLNS /* \_SB_.PCI0.LPCB.H_EC.WLNS */
                        TECS &= 0xFFFFFFFC
                        TECS |= TWSL /* \_SB_.PCI0.LPCB.SNC_.F135.TECS */
                        WLRF = TWRF /* \_SB_.PCI0.LPCB.SNC_.F135.TWRF */
                        TECS |= (WLRF | WMRF) /* \_SB_.PCI0.LPCB.SNC_.F135.TECS */
                        ^^H_EC.WLNS = TECS /* \_SB_.PCI0.LPCB.SNC_.F135.TECS */
                    }
                }
                ElseIf ((CMD == 0x05))
                {
                    If (BLTP)
                    {
                        RCB0 = (^^H_EC.BTDS & 0x03)
                    }
                }
                ElseIf ((CMD == 0x06))
                {
                    If (BLTP)
                    {
                        ^^H_EC.BTDS = (CPB0 & 0x03)
                    }
                }
                ElseIf ((CMD == 0x07))
                {
                    If (WWAP)
                    {
                        RCB0 = (^^H_EC.WWNS & 0x07)
                    }
                }
                ElseIf ((CMD == 0x08))
                {
                    If (WWAP)
                    {
                        ^^H_EC.WWNS = (CPB0 & 0x07)
                    }
                }
                ElseIf ((CMD == 0x09))
                {
                    If (WMXP)
                    {
                        RCB0 = Zero
                        TECS = ^^H_EC.WLNS /* \_SB_.PCI0.LPCB.H_EC.WLNS */
                        TWSL = ((TECS & 0x04) >> One)
                        TWRF = (TECS & One)
                        RCB0 |= TWSL /* \_SB_.PCI0.LPCB.SNC_.RCB0 */
                        RCB0 |= (TWRF & WMRF) /* \_SB_.PCI0.LPCB.SNC_.RCB0 */
                    }
                }
                ElseIf ((CMD == 0x0A))
                {
                    If (WMXP)
                    {
                        TWSL = (CPB0 & 0x02)
                        TWRF = (CPB0 & One)
                        TECS = ^^H_EC.WLNS /* \_SB_.PCI0.LPCB.H_EC.WLNS */
                        TECS &= 0xFFFFFFFA
                        TWSL <<= One
                        TECS |= TWSL /* \_SB_.PCI0.LPCB.SNC_.F135.TECS */
                        WMRF = TWRF /* \_SB_.PCI0.LPCB.SNC_.F135.TWRF */
                        TECS |= (WMRF | WLRF) /* \_SB_.PCI0.LPCB.SNC_.F135.TECS */
                        ^^H_EC.WLNS = TECS /* \_SB_.PCI0.LPCB.SNC_.F135.TECS */
                    }
                }
                ElseIf ((CMD == 0x0B))
                {
                    RCB0 = ^^H_EC.WLED /* \_SB_.PCI0.LPCB.H_EC.WLED */
                }
                ElseIf ((CMD == 0x0C))
                {
                    ^^H_EC.WLED = CPB0 /* \_SB_.PCI0.LPCB.SNC_.CPB0 */
                }
                Else
                {
                    Noop
                }

                Return (Zero)
            }

            Scope (^^RP01.WLAN)
            {
                OperationRegion (WLID, PCI_Config, Zero, 0x04)
                Field (WLID, DWordAcc, NoLock, Preserve)
                {
                    VID,    16, 
                    DID,    16
                }
            }

            Method (F155, 0, NotSerialized)
            {
                If ((CMD == Zero))
                {
                    RCB0 = One
                }
                ElseIf ((CMD == One))
                {
                    ^^H_EC.UBCE = CPB0 /* \_SB_.PCI0.LPCB.SNC_.CPB0 */
                    RCB0 = Zero
                }
            }

            Method (F15D, 0, NotSerialized)
            {
                If ((CMD == Zero))
                {
                    RCB0 = ^^H_EC.LORS /* \_SB_.PCI0.LPCB.H_EC.LORS */
                }
                ElseIf ((CMD == One))
                {
                    ^^H_EC.LORS = CPB0 /* \_SB_.PCI0.LPCB.SNC_.CPB0 */
                }
                Else
                {
                    Noop
                }

                Return (Zero)
            }

            Name (WKRS, Zero)
            Method (F13A, 0, NotSerialized)
            {
                If ((CMD == Zero))
                {
                    RCB0 = WKRS /* \_SB_.PCI0.LPCB.SNC_.WKRS */
                }
                ElseIf ((CMD == One))
                {
                    ASTF = CPB0 /* \_SB_.PCI0.LPCB.SNC_.CPB0 */
                }
            }

            Method (F156, 0, NotSerialized)
            {
                If ((CMD == Zero))
                {
                    RCB0 = ^^H_EC.FBDM /* \_SB_.PCI0.LPCB.H_EC.FBDM */
                }
                ElseIf ((CMD == One))
                {
                    ^^H_EC.FBDM = CPB0 /* \_SB_.PCI0.LPCB.SNC_.CPB0 */
                    RCB0 = ^^H_EC.FBRC /* \_SB_.PCI0.LPCB.H_EC.FBRC */
                }
                Else
                {
                    Noop
                }

                Return (Zero)
            }

            Method (HS05, 1, NotSerialized)
            {
                Local0 = (One << 0x0B)
                If ((Arg0 && Local0))
                {
                    WCNG = Zero
                    SCNG = Zero
                }

                Return (One)
            }
        }
    }

    Scope (\)
    {
        OperationRegion (SHEC, SystemIO, 0x2000, 0x08)
        Field (SHEC, ByteAcc, Lock, Preserve)
        {
            SHDP,   8, 
            Offset (0x04), 
            SHCP,   8, 
            Offset (0x08)
        }

        Mutex (LSHE, 0x00)
        Method (SHSC, 1, Serialized)
        {
            While ((SHCP & 0x02))
            {
                Noop
            }

            SHCP = Arg0
            While ((SHCP & 0x02))
            {
                Noop
            }
        }

        Method (SHSD, 1, Serialized)
        {
            While ((SHCP & 0x02))
            {
                Noop
            }

            SHDP = Arg0
            While ((SHCP & 0x02))
            {
                Noop
            }
        }

        Method (SHRD, 0, Serialized)
        {
            While (!(SHCP & One))
            {
                Noop
            }

            Return (SHDP) /* \SHDP */
        }

        Method (RECS, 1, Serialized)
        {
            Acquire (LSHE, 0xFFFF)
            SHSC (0x92)
            SHSD (Arg0)
            Local0 = SHRD ()
            Release (LSHE)
            Return (Local0)
        }

        Method (WECS, 2, Serialized)
        {
            Acquire (LSHE, 0xFFFF)
            SHSC (0x97)
            SHSD (Arg0)
            SHSD (Arg1)
            Release (LSHE)
        }

        Method (SEAW, 1, Serialized)
        {
            Acquire (LSHE, 0xFFFF)
            SHSC (0x9F)
            SHSD (Arg0)
            Release (LSHE)
        }
    }

    Scope (\)
    {
        Name (TRTO, 0x04)
        Name (STRP, 0xF0)
        Mutex (LSTP, 0x00)
        OperationRegion (IO_O, SystemIO, 0x0820, 0x04)
        Field (IO_O, ByteAcc, NoLock, Preserve)
        {
            TRPO,   8
        }
    }

    Method (ISTP, 2, Serialized)
    {
        Acquire (LSTP, 0xFFFF)
        Acquire (LSHE, 0xFFFF)
        STSF = Arg0
        STP0 = Arg1
        TRAP (TRTO, STRP)
        Local0 = STP0 /* \STP0 */
        Release (LSHE)
        Release (LSTP)
        Return (Local0)
    }

    Scope (_SB.PCI0.LPCB)
    {
        Device (WLDS)
        {
            Name (_HID, EisaId ("SNYA008"))  // _HID: Hardware ID
            Method (_STA, 0, NotSerialized)  // _STA: Status
            {
                If ((OSYS >= 0x07DC))
                {
                    Return (0x0F)
                }
                Else
                {
                    Return (Zero)
                }
            }

            Method (GVER, 0, NotSerialized)
            {
                Return (0x0100)
            }

            Method (GWDP, 0, NotSerialized)
            {
                If (One)
                {
                    Local0 = Zero
                }
                Else
                {
                    Local0 = 0x02
                }

                Local0 |= One
                Return (Local0)
            }

            Method (GWDS, 0, NotSerialized)
            {
                Local0 = ((^^H_EC.WLSS & One) << One)
                Local0 |= ^^H_EC.WLED /* \_SB_.PCI0.LPCB.H_EC.WLED */
                Return (Local0)
            }

            Method (SWDS, 2, Serialized)
            {
                Local0 = (Arg1 & One)
                If (Local0)
                {
                    ^^H_EC.WLED = (Arg0 & One)
                }

                Return (Zero)
            }
        }
    }

    Name (_S0, Package (0x04)  // _S0_: S0 System State
    {
        Zero, 
        Zero, 
        Zero, 
        Zero
    })
    If (SS1)
    {
        Name (_S1, Package (0x04)  // _S1_: S1 System State
        {
            One, 
            Zero, 
            Zero, 
            Zero
        })
    }

    If (SS3)
    {
        Name (_S3, Package (0x04)  // _S3_: S3 System State
        {
            0x05, 
            Zero, 
            Zero, 
            Zero
        })
    }

    If (SS4)
    {
        Name (_S4, Package (0x04)  // _S4_: S4 System State
        {
            0x06, 
            Zero, 
            Zero, 
            Zero
        })
    }

    Name (_S5, Package (0x04)  // _S5_: S5 System State
    {
        0x07, 
        Zero, 
        Zero, 
        Zero
    })
}



FACP
----
[000h 0000   4]                    Signature : "FACP"    [Fixed ACPI Description Table (FADT)]
[004h 0004   4]                 Table Length : 0000010C
[008h 0008   1]                     Revision : 05
[009h 0009   1]                     Checksum : 62
[00Ah 0010   6]                       Oem ID : "Sony"
[010h 0016   8]                 Oem Table ID : "VAIO"
[018h 0024   4]                 Oem Revision : 20130313
[01Ch 0028   4]              Asl Compiler ID : "ACPI"
[020h 0032   4]        Asl Compiler Revision : 00040000

[024h 0036   4]                 FACS Address : 9AFBB000
[028h 0040   4]                 DSDT Address : 9AFEF000
[02Ch 0044   1]                        Model : 01
[02Dh 0045   1]                   PM Profile : 02 [Mobile]
[02Eh 0046   2]                SCI Interrupt : 0009
[030h 0048   4]             SMI Command Port : 000000B2
[034h 0052   1]            ACPI Enable Value : A0
[035h 0053   1]           ACPI Disable Value : A1
[036h 0054   1]               S4BIOS Command : 00
[037h 0055   1]              P-State Control : 80
[038h 0056   4]     PM1A Event Block Address : 00000400
[03Ch 0060   4]     PM1B Event Block Address : 00000000
[040h 0064   4]   PM1A Control Block Address : 00000404
[044h 0068   4]   PM1B Control Block Address : 00000000
[048h 0072   4]    PM2 Control Block Address : 00000450
[04Ch 0076   4]       PM Timer Block Address : 00000408
[050h 0080   4]           GPE0 Block Address : 00000420
[054h 0084   4]           GPE1 Block Address : 00000000
[058h 0088   1]       PM1 Event Block Length : 04
[059h 0089   1]     PM1 Control Block Length : 02
[05Ah 0090   1]     PM2 Control Block Length : 01
[05Bh 0091   1]        PM Timer Block Length : 04
[05Ch 0092   1]            GPE0 Block Length : 10
[05Dh 0093   1]            GPE1 Block Length : 00
[05Eh 0094   1]             GPE1 Base Offset : 00
[05Fh 0095   1]                 _CST Support : 85
[060h 0096   2]                   C2 Latency : 0065
[062h 0098   2]                   C3 Latency : 0039
[064h 0100   2]               CPU Cache Size : 0400
[066h 0102   2]           Cache Flush Stride : 0010
[068h 0104   1]            Duty Cycle Offset : 01
[069h 0105   1]             Duty Cycle Width : 03
[06Ah 0106   1]          RTC Day Alarm Index : 0D
[06Bh 0107   1]        RTC Month Alarm Index : 00
[06Ch 0108   1]            RTC Century Index : 00
[06Dh 0109   2]   Boot Flags (decoded below) : 0000
               Legacy Devices Supported (V2) : 0
            8042 Present on ports 60/64 (V2) : 0
                        VGA Not Present (V4) : 0
                      MSI Not Supported (V4) : 0
                PCIe ASPM Not Supported (V4) : 0
                   CMOS RTC Not Present (V5) : 0
[06Fh 0111   1]                     Reserved : 00
[070h 0112   4]        Flags (decoded below) : 000384B5
      WBINVD instruction is operational (V1) : 1
              WBINVD flushes all caches (V1) : 0
                    All CPUs support C1 (V1) : 1
                  C2 works on MP system (V1) : 0
            Control Method Power Button (V1) : 1
            Control Method Sleep Button (V1) : 1
        RTC wake not in fixed reg space (V1) : 0
            RTC can wake system from S4 (V1) : 1
                        32-bit PM Timer (V1) : 0
                      Docking Supported (V1) : 0
               Reset Register Supported (V2) : 1
                            Sealed Case (V3) : 0
                    Headless - No Video (V3) : 0
        Use native instr after SLP_TYPx (V3) : 0
              PCIEXP_WAK Bits Supported (V4) : 0
                     Use Platform Timer (V4) : 1
               RTC_STS valid on S4 wake (V4) : 1
                Remote Power-on capable (V4) : 1
                 Use APIC Cluster Model (V4) : 0
     Use APIC Physical Destination Mode (V4) : 0
                       Hardware Reduced (V5) : 0
                      Low Power S0 Idle (V5) : 0

[074h 0116  12]               Reset Register : [Generic Address Structure]
[074h 0116   1]                     Space ID : 01 [SystemIO]
[075h 0117   1]                    Bit Width : 08
[076h 0118   1]                   Bit Offset : 00
[077h 0119   1]         Encoded Access Width : 01 [Byte Access:8]
[078h 0120   8]                      Address : 00000000000000B2

[080h 0128   1]         Value to cause reset : FB
[081h 0129   2]    ARM Flags (decoded below) : 0000
                              PSCI Compliant : 0
                       Must use HVC for PSCI : 0

[083h 0131   1]          FADT Minor Revision : 00
[084h 0132   8]                 FACS Address : 0000000000000000
[08Ch 0140   8]                 DSDT Address : 000000009AFEF000
[094h 0148  12]             PM1A Event Block : [Generic Address Structure]
[094h 0148   1]                     Space ID : 01 [SystemIO]
[095h 0149   1]                    Bit Width : 20
[096h 0150   1]                   Bit Offset : 00
[097h 0151   1]         Encoded Access Width : 03 [DWord Access:32]
[098h 0152   8]                      Address : 0000000000000400

[0A0h 0160  12]             PM1B Event Block : [Generic Address Structure]
[0A0h 0160   1]                     Space ID : 00 [SystemMemory]
[0A1h 0161   1]                    Bit Width : 00
[0A2h 0162   1]                   Bit Offset : 00
[0A3h 0163   1]         Encoded Access Width : 00 [Undefined/Legacy]
[0A4h 0164   8]                      Address : 0000000000000000

[0ACh 0172  12]           PM1A Control Block : [Generic Address Structure]
[0ACh 0172   1]                     Space ID : 01 [SystemIO]
[0ADh 0173   1]                    Bit Width : 10
[0AEh 0174   1]                   Bit Offset : 00
[0AFh 0175   1]         Encoded Access Width : 02 [Word Access:16]
[0B0h 0176   8]                      Address : 0000000000000404

[0B8h 0184  12]           PM1B Control Block : [Generic Address Structure]
[0B8h 0184   1]                     Space ID : 00 [SystemMemory]
[0B9h 0185   1]                    Bit Width : 00
[0BAh 0186   1]                   Bit Offset : 00
[0BBh 0187   1]         Encoded Access Width : 00 [Undefined/Legacy]
[0BCh 0188   8]                      Address : 0000000000000000

[0C4h 0196  12]            PM2 Control Block : [Generic Address Structure]
[0C4h 0196   1]                     Space ID : 01 [SystemIO]
[0C5h 0197   1]                    Bit Width : 08
[0C6h 0198   1]                   Bit Offset : 00
[0C7h 0199   1]         Encoded Access Width : 01 [Byte Access:8]
[0C8h 0200   8]                      Address : 0000000000000450

[0D0h 0208  12]               PM Timer Block : [Generic Address Structure]
[0D0h 0208   1]                     Space ID : 01 [SystemIO]
[0D1h 0209   1]                    Bit Width : 20
[0D2h 0210   1]                   Bit Offset : 00
[0D3h 0211   1]         Encoded Access Width : 03 [DWord Access:32]
[0D4h 0212   8]                      Address : 0000000000000408

[0DCh 0220  12]                   GPE0 Block : [Generic Address Structure]
[0DCh 0220   1]                     Space ID : 01 [SystemIO]
[0DDh 0221   1]                    Bit Width : 80
[0DEh 0222   1]                   Bit Offset : 00
[0DFh 0223   1]         Encoded Access Width : 00 [Undefined/Legacy]
[0E0h 0224   8]                      Address : 0000000000000420

