                        Device (CAMR)
                        {
                            Name (_ADR, 0x03)  // _ADR: Address
                            Method (_PLD, 0, Serialized)  // _PLD: Physical Location of Device
                            {
                                Name (PLDA, Package (0x01)
                                {
                                    Buffer (0x14)
                                    {
                                        /* 0000 */  0x82, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00,  // ........
                                        /* 0008 */  0x24, 0x1D, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00,  // $.......
                                        /* 0010 */  0xCB, 0x00, 0xA0, 0x00                           // ....
                                    }
                                })
                                Name (PLDB, Package (0x01)
                                {
                                    Buffer (0x14)
                                    {
                                        /* 0000 */  0x82, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00,  // ........
                                        /* 0008 */  0x24, 0x1D, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00,  // $.......
                                        /* 0010 */  0xE7, 0x00, 0xBA, 0x00                           // ....
                                    }
                                })
                                If (((GL00 & 0x04) == 0x04))
                                {
                                    Return (PLDB) /* \_SB_.PCI0.EHC2.HUBN.PR01.PR13.CAMR._PLD.PLDB */
                                }
                                Else
                                {
                                    Return (PLDA) /* \_SB_.PCI0.EHC2.HUBN.PR01.PR13.CAMR._PLD.PLDA */
                                }
                            }
                        }
                    }

                    Device (PR14)
