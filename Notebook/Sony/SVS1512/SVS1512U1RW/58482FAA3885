                If (PICM)
                {
                    Return (AR01) /* \_SB_.PCI0.PEG0.AR01 */
                }

                Return (PR01) /* \_SB_.PCI0.PEG0.PR01 */
            }

            Device (PEGP)
            {
                Name (_ADR, Zero)  // _ADR: Address
                OperationRegion (DGFX, PCI_Config, Zero, 0xF0)
                Field (DGFX, DWordAcc, Lock, Preserve)
