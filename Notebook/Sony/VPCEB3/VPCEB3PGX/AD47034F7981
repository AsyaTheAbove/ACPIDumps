                    Method (SODV, 1, NotSerialized)
                    {
                        \DBGC (0xF6, 0x00, BCEN)
                        If ((DSEN != 0x00))
                        {
                            Return (Ones)
                        }

                        AODV = Arg0
                        If (!(AODV & CADD))
                        {
                            AODV = 0x01
                        }

                        If ((CADD != PADD))
                        {
                            PADD = CADD /* \CADD */
                            Notify (\_SB.PCI0, 0x00) // Bus Check
                            Local0 = (PNHM & 0x000FFFF0)
                            If (((Local0 == 0x000106E0) | (Local0 == 0x000106A0)))
                            {
                                Notify (\_SB.PCI0.PEG3, 0x00) // Bus Check
                            }
                            Else
                            {
                                Notify (\_SB.PCI0.PEGP, 0x00) // Bus Check
                            }

                            Sleep (0x02EE)
                        }

                        \DBGC (0xF6, 0x80, BCEN)
                        Return (Zero)
                    }

                    Mutex (MIDB, 0x00)
