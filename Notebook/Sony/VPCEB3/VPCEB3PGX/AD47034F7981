                        BSTH,   8, 
                        OMFL,   8, 
                        OMFH,   8, 
                        IBMF,   8, 
                        ASSR,   8, 
                        Offset (0x40), 
                        TS1R,   8, 
                        TS1L,   8, 
                        TS2R,   8, 
                        TS2L,   8, 
                        TS3R,   8, 
                        TS3L,   8, 
                        F1FL,   8, 
                        F1FH,   8, 
                        F2FL,   8, 
                        F2FH,   8, 
                        T1U1,   8, 
                        T1U2,   8, 
                        T1U3,   8, 
                        T1U4,   8, 
                        T1U5,   8, 
                        T1U6,   8, 
                        T1U7,   8, 
                        T1D1,   8, 
                        T1D2,   8, 
                        T1D3,   8, 
                        T1L1,   8, 
                        T2R1,   8, 
                        T2U1,   8, 
                        T3L1,   8, 
                        T3L2,   8, 
                        Offset (0x60), 
                        SMBN,   8, 
