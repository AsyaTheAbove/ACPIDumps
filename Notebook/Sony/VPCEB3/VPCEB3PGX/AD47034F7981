            Device (GFX0)
            {
                Name (_ADR, 0x00020000)  // _ADR: Address
                Method (PCPC, 0, NotSerialized)
                {
                    \ECST (0x6F)
                }

                Method (_DOS, 1, NotSerialized)  // _DOS: Disable Output Switching
                {
                    DSEN = (Arg0 & 0x07)
                }

                Method (_DOD, 0, NotSerialized)  // _DOD: Display Output Devices
                {
                    NDID = 0x00
                    If (CondRefOf (IDAB))
                    {
                        IDAB ()
                    }
                    Else
                    {
                        If ((DIDL != Zero))
                        {
                            DID1 = SDDL (DID1)
                        }

                        If ((DDL2 != Zero))
                        {
                            DID2 = SDDL (DID2)
                        }

                        If ((DDL3 != Zero))
                        {
                            DID3 = SDDL (DID3)
                        }

                        If ((DDL4 != Zero))
                        {
                            DID4 = SDDL (DID4)
                        }

                        If ((DDL5 != Zero))
                        {
                            DID5 = SDDL (DID5)
                        }
                    }

                    If ((NDID == 0x01))
                    {
                        Name (TMP1, Package (0x01)
                        {
                            0xFFFFFFFF
                        })
                        TMP1 [0x00] = (0x00010000 | DID1)
                        Return (TMP1) /* \_SB_.PCI0.GFX0._DOD.TMP1 */
                    }

                    If ((NDID == 0x02))
                    {
                        Name (TMP2, Package (0x02)
                        {
                            0xFFFFFFFF, 
                            0xFFFFFFFF
                        })
                        TMP2 [0x00] = (0x00010000 | DID1)
                        TMP2 [0x01] = (0x00010000 | DID2)
                        Return (TMP2) /* \_SB_.PCI0.GFX0._DOD.TMP2 */
                    }

                    If ((NDID == 0x03))
                    {
                        Name (TMP3, Package (0x03)
                        {
                            0xFFFFFFFF, 
                            0xFFFFFFFF, 
                            0xFFFFFFFF
                        })
                        TMP3 [0x00] = (0x00010000 | DID1)
                        TMP3 [0x01] = (0x00010000 | DID2)
                        TMP3 [0x02] = (0x00010000 | DID3)
                        Return (TMP3) /* \_SB_.PCI0.GFX0._DOD.TMP3 */
                    }

                    If ((NDID == 0x04))
                    {
                        Name (TMP4, Package (0x04)
                        {
                            0xFFFFFFFF, 
                            0xFFFFFFFF, 
                            0xFFFFFFFF, 
                            0xFFFFFFFF
                        })
                        TMP4 [0x00] = (0x00010000 | DID1)
                        TMP4 [0x01] = (0x00010000 | DID2)
                        TMP4 [0x02] = (0x00010000 | DID3)
                        TMP4 [0x03] = (0x00010000 | DID4)
                        Return (TMP4) /* \_SB_.PCI0.GFX0._DOD.TMP4 */
                    }

                    If (CondRefOf (NNAB))
                    {
                        Return (NNAB) /* External reference */
                    }
                    ElseIf ((NDID > 0x04))
                    {
                        Name (TMP5, Package (0x05)
                        {
                            0xFFFFFFFF, 
                            0xFFFFFFFF, 
                            0xFFFFFFFF, 
                            0xFFFFFFFF, 
                            0xFFFFFFFF
                        })
                        TMP5 [0x00] = (0x00010000 | DID1)
                        TMP5 [0x01] = (0x00010000 | DID2)
                        TMP5 [0x02] = (0x00010000 | DID3)
                        TMP5 [0x03] = (0x00010000 | DID4)
                        TMP5 [0x04] = (0x00010000 | DID5)
                        Return (TMP5) /* \_SB_.PCI0.GFX0._DOD.TMP5 */
                    }

                    Return (Package (0x01)
                    {
                        0x0400
                    })
                }

                Name (RTL0, Buffer (0x09)
                {
                    /* 0000 */  0x0D, 0x13, 0x1C, 0x29, 0x3B, 0x55, 0x7B, 0xB1,  // ...);U{.
                    /* 0008 */  0xFF                                             // .
                })
                Name (ICL0, Package (0x0B)
                {
                    0x64, 
                    0x64, 
                    0x05, 
                    0x08, 
                    0x0B, 
                    0x10, 
                    0x17, 
                    0x22, 
                    0x30, 
                    0x46, 
                    0x64
                })
                Name (RTL1, Buffer (0x09)
                {
                    /* 0000 */  0x37, 0x3E, 0x4B, 0x5C, 0x6F, 0x86, 0xA0, 0xC3,  // 7>K\o...
                    /* 0008 */  0xFF                                             // .
                })
                Name (ICL1, Package (0x0B)
                {
                    0x64, 
                    0x64, 
                    0x15, 
                    0x18, 
                    0x1D, 
                    0x24, 
                    0x2C, 
                    0x35, 
                    0x3F, 
                    0x4C, 
                    0x64
                })
                Device (DD01)
                {
                    Method (_ADR, 0, Serialized)  // _ADR: Address
                    {
                        If ((DID1 == 0x00))
                        {
                            Return (0x01)
                        }
                        Else
                        {
                            Return ((0xFFFF & DID1))
                        }
                    }

                    Method (_DCS, 0, NotSerialized)  // _DCS: Display Current Status
                    {
                        Return (CDDS (DID1))
                    }

                    Method (_DGS, 0, NotSerialized)  // _DGS: Display Graphics State
                    {
                        Return (NDDS (DID1))
                    }

                    Method (_DSS, 1, NotSerialized)  // _DSS: Device Set State
                    {
                        If (((Arg0 & 0xC0000000) == 0xC0000000))
                        {
                            CSTE = NSTE /* \NSTE */
                        }
                    }
                }

                Device (DD02)
                {
                    Method (_ADR, 0, Serialized)  // _ADR: Address
                    {
                        If ((DID2 == 0x00))
                        {
                            Return (0x02)
                        }
                        Else
                        {
                            Return ((0xFFFF & DID2))
                        }
                    }

                    Method (_DCS, 0, NotSerialized)  // _DCS: Display Current Status
                    {
                        Return (CDDS (DID2))
                    }

                    Method (_DGS, 0, NotSerialized)  // _DGS: Display Graphics State
                    {
                        Return (NDDS (DID2))
                    }

                    Method (_DSS, 1, NotSerialized)  // _DSS: Device Set State
                    {
                        If (((Arg0 & 0xC0000000) == 0xC0000000))
                        {
                            CSTE = NSTE /* \NSTE */
                        }
                    }

                    Method (_BCL, 0, NotSerialized)  // _BCL: Brightness Control Levels
                    {
                        Name (_T_0, Zero)  // _T_x: Emitted by ASL Compiler, x=0-9, A-Z
                        _T_0 = ToInteger (PNID)
                        If ((_T_0 == 0x05))
                        {
                            Return (ICL1) /* \_SB_.PCI0.GFX0.ICL1 */
                        }
                        ElseIf ((_T_0 == 0x06))
                        {
                            Return (ICL1) /* \_SB_.PCI0.GFX0.ICL1 */
                        }
                        ElseIf ((_T_0 == 0x07))
                        {
                            Return (ICL1) /* \_SB_.PCI0.GFX0.ICL1 */
                        }
                        ElseIf ((_T_0 == 0x08))
                        {
                            Return (ICL1) /* \_SB_.PCI0.GFX0.ICL1 */
                        }
                        Else
                        {
                            Return (ICL0) /* \_SB_.PCI0.GFX0.ICL0 */
                        }
                    }

                    Method (_BCM, 1, NotSerialized)  // _BCM: Brightness Control Method
                    {
                        If (((PNID > 0x04) && (PNID < 0x09)))
                        {
                            Local0 = (Match (ICL1, MEQ, Arg0, MTR, 0x00, 0x02) - 0x02)
                            Local0 = DerefOf (RTL1 [Local0])
                        }
                        Else
                        {
                            Local0 = (Match (ICL0, MEQ, Arg0, MTR, 0x00, 0x02) - 0x02)
                            Local0 = DerefOf (RTL0 [Local0])
                        }

                        \_SB.PCI0.GFX0.AINT (0x01, Local0)
                        BRTL = Arg0
                    }

                    Method (_BQC, 0, NotSerialized)  // _BQC: Brightness Query Current
                    {
                        Return (BRTL) /* \BRTL */
                    }
                }

                Device (DD03)
                {
                    Method (_ADR, 0, Serialized)  // _ADR: Address
                    {
                        If ((DID3 == 0x00))
                        {
                            Return (0x03)
                        }
                        Else
                        {
                            Return ((0xFFFF & DID3))
                        }
                    }

                    Method (_DCS, 0, NotSerialized)  // _DCS: Display Current Status
                    {
                        If ((DID3 == 0x00))
                        {
                            Return (0x0B)
                        }
                        Else
                        {
                            Return (CDDS (DID3))
                        }
                    }

                    Method (_DGS, 0, NotSerialized)  // _DGS: Display Graphics State
                    {
                        Return (NDDS (DID3))
                    }

                    Method (_DSS, 1, NotSerialized)  // _DSS: Device Set State
                    {
                        If (((Arg0 & 0xC0000000) == 0xC0000000))
                        {
                            CSTE = NSTE /* \NSTE */
                        }
                    }
                }

                Device (DD04)
