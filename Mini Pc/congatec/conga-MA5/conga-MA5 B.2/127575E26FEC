    External (_SB_.PCI0.XHC_.RHUB, DeviceObj)
    External (_SB_.PCI0.XHC_.RHUB.TPLD, MethodObj)    // 2 Arguments
    External (BID_, IntObj)
    External (ETYC, IntObj)
    External (P8XH, MethodObj)    // 2 Arguments
    External (UBCB, UnknownObj)

    Scope (\_SB)
    {
        Device (UBTC)
        {
            Name (_HID, EisaId ("USBC000"))  // _HID: Hardware ID
            Name (_CID, EisaId ("PNP0CA0"))  // _CID: Compatible ID
            Name (_UID, Zero)  // _UID: Unique ID
            Name (_DDN, "USB Type C")  // _DDN: DOS Device Name
            Name (_ADR, Zero)  // _ADR: Address
            Name (_DEP, Package (0x01)  // _DEP: Dependencies
            {
                \_SB.PCI0.SBRG.H_EC
            })
            Name (CRS, ResourceTemplate ()
            {
                Memory32Fixed (ReadWrite,
                    0x00000000,         // Address Base
                    0x00001000,         // Address Length
                    _Y00)
            })
            Device (CR01)
            {
                Name (_ADR, Zero)  // _ADR: Address
                Method (_PLD, 0, NotSerialized)  // _PLD: Physical Location of Device
                {
                    Return (\_SB.PCI0.XHC.RHUB.TPLD (One, One))
                }
            }

            Method (_CRS, 0, Serialized)  // _CRS: Current Resource Settings
            {
                CreateDWordField (CRS, \_SB.UBTC._Y00._BAS, CBAS)  // _BAS: Base Address
                CBAS = UBCB /* External reference */
                Return (CRS) /* \_SB_.UBTC.CRS_ */
            }

            Method (_STA, 0, NotSerialized)  // _STA: Status
            {
                If ((ETYC == One))
                {
                    Return (0x0F)
                }

                Return (Zero)
            }

            OperationRegion (USBC, SystemMemory, UBCB, 0x38)
            Field (USBC, ByteAcc, Lock, Preserve)
            {
                VER1,   8, 
                VER2,   8, 
                RSV1,   8, 
                RSV2,   8, 
                CCI0,   8, 
                CCI1,   8, 
                CCI2,   8, 
                CCI3,   8, 
                CTL0,   8, 
                CTL1,   8, 
                CTL2,   8, 
                CTL3,   8, 
                CTL4,   8, 
                CTL5,   8, 
                CTL6,   8, 
                CTL7,   8, 
                MGI0,   8, 
                MGI1,   8, 
                MGI2,   8, 
                MGI3,   8, 
                MGI4,   8, 
                MGI5,   8, 
                MGI6,   8, 
                MGI7,   8, 
                MGI8,   8, 
                MGI9,   8, 
                MGIA,   8, 
                MGIB,   8, 
                MGIC,   8, 
                MGID,   8, 
                MGIE,   8, 
                MGIF,   8, 
                MGO0,   8, 
                MGO1,   8, 
                MGO2,   8, 
                MGO3,   8, 
                MGO4,   8, 
                MGO5,   8, 
                MGO6,   8, 
                MGO7,   8, 
                MGO8,   8, 
                MGO9,   8, 
                MGOA,   8, 
                MGOB,   8, 
                MGOC,   8, 
                MGOD,   8, 
                MGOE,   8, 
                MGOF,   8
            }

            Method (_DSM, 4, Serialized)  // _DSM: Device-Specific Method
            {
                If ((Arg0 == ToUUID ("6f8398c2-7ca4-11e4-ad36-631042b5008f") /* Unknown UUID */))
                {
                    Switch (ToInteger (Arg2))
                    {
                        Case (Zero)
                        {
                            Return (Buffer (One)
                            {
                                 0x07                                             // .
                            })
                        }
                        Case (One)
                        {
                            \_SB.PCI0.SBRG.H_EC.ECWT (MGO0, RefOf (\_SB.PCI0.SBRG.H_EC.MGO0))
                            \_SB.PCI0.SBRG.H_EC.ECWT (MGO1, RefOf (\_SB.PCI0.SBRG.H_EC.MGO1))
                            \_SB.PCI0.SBRG.H_EC.ECWT (MGO2, RefOf (\_SB.PCI0.SBRG.H_EC.MGO2))
                            \_SB.PCI0.SBRG.H_EC.ECWT (MGO3, RefOf (\_SB.PCI0.SBRG.H_EC.MGO3))
                            \_SB.PCI0.SBRG.H_EC.ECWT (MGO4, RefOf (\_SB.PCI0.SBRG.H_EC.MGO4))
                            \_SB.PCI0.SBRG.H_EC.ECWT (MGO5, RefOf (\_SB.PCI0.SBRG.H_EC.MGO5))
                            \_SB.PCI0.SBRG.H_EC.ECWT (MGO6, RefOf (\_SB.PCI0.SBRG.H_EC.MGO6))
                            \_SB.PCI0.SBRG.H_EC.ECWT (MGO7, RefOf (\_SB.PCI0.SBRG.H_EC.MGO7))
                            \_SB.PCI0.SBRG.H_EC.ECWT (MGO8, RefOf (\_SB.PCI0.SBRG.H_EC.MGO8))
                            \_SB.PCI0.SBRG.H_EC.ECWT (MGO9, RefOf (\_SB.PCI0.SBRG.H_EC.MGO9))
                            \_SB.PCI0.SBRG.H_EC.ECWT (MGOA, RefOf (\_SB.PCI0.SBRG.H_EC.MGOA))
                            \_SB.PCI0.SBRG.H_EC.ECWT (MGOB, RefOf (\_SB.PCI0.SBRG.H_EC.MGOB))
                            \_SB.PCI0.SBRG.H_EC.ECWT (MGOC, RefOf (\_SB.PCI0.SBRG.H_EC.MGOC))
                            \_SB.PCI0.SBRG.H_EC.ECWT (MGOD, RefOf (\_SB.PCI0.SBRG.H_EC.MGOD))
                            \_SB.PCI0.SBRG.H_EC.ECWT (MGOE, RefOf (\_SB.PCI0.SBRG.H_EC.MGOE))
                            \_SB.PCI0.SBRG.H_EC.ECWT (MGOF, RefOf (\_SB.PCI0.SBRG.H_EC.MGOF))
                            \_SB.PCI0.SBRG.H_EC.ECWR (CTL0, RefOf (\_SB.PCI0.SBRG.H_EC.CTL0))
                            \_SB.PCI0.SBRG.H_EC.ECWR (CTL1, RefOf (\_SB.PCI0.SBRG.H_EC.CTL1))
                            \_SB.PCI0.SBRG.H_EC.ECWR (CTL2, RefOf (\_SB.PCI0.SBRG.H_EC.CTL2))
                            \_SB.PCI0.SBRG.H_EC.ECWR (CTL3, RefOf (\_SB.PCI0.SBRG.H_EC.CTL3))
                            \_SB.PCI0.SBRG.H_EC.ECWT (CTL4, RefOf (\_SB.PCI0.SBRG.H_EC.CTL4))
                            \_SB.PCI0.SBRG.H_EC.ECWT (CTL5, RefOf (\_SB.PCI0.SBRG.H_EC.CTL5))
                            \_SB.PCI0.SBRG.H_EC.ECWT (CTL6, RefOf (\_SB.PCI0.SBRG.H_EC.CTL6))
                            \_SB.PCI0.SBRG.H_EC.ECWT (CTL7, RefOf (\_SB.PCI0.SBRG.H_EC.CTL7))
                            \_SB.PCI0.SBRG.H_EC.ECM1 (0xE0)
                            P8XH (Zero, 0xE0)
                        }
                        Case (0x02)
                        {
                            MGI0 = \_SB.PCI0.SBRG.H_EC.ECRD (RefOf (\_SB.PCI0.SBRG.H_EC.MGI0))
                            MGI1 = \_SB.PCI0.SBRG.H_EC.ECRD (RefOf (\_SB.PCI0.SBRG.H_EC.MGI1))
                            MGI2 = \_SB.PCI0.SBRG.H_EC.ECRD (RefOf (\_SB.PCI0.SBRG.H_EC.MGI2))
                            MGI3 = \_SB.PCI0.SBRG.H_EC.ECRD (RefOf (\_SB.PCI0.SBRG.H_EC.MGI3))
                            MGI4 = \_SB.PCI0.SBRG.H_EC.ECRD (RefOf (\_SB.PCI0.SBRG.H_EC.MGI4))
                            MGI5 = \_SB.PCI0.SBRG.H_EC.ECRD (RefOf (\_SB.PCI0.SBRG.H_EC.MGI5))
                            MGI6 = \_SB.PCI0.SBRG.H_EC.ECRD (RefOf (\_SB.PCI0.SBRG.H_EC.MGI6))
                            MGI7 = \_SB.PCI0.SBRG.H_EC.ECRD (RefOf (\_SB.PCI0.SBRG.H_EC.MGI7))
                            MGI8 = \_SB.PCI0.SBRG.H_EC.ECRD (RefOf (\_SB.PCI0.SBRG.H_EC.MGI8))
                            MGI9 = \_SB.PCI0.SBRG.H_EC.ECRD (RefOf (\_SB.PCI0.SBRG.H_EC.MGI9))
                            MGIA = \_SB.PCI0.SBRG.H_EC.ECRD (RefOf (\_SB.PCI0.SBRG.H_EC.MGIA))
                            MGIB = \_SB.PCI0.SBRG.H_EC.ECRD (RefOf (\_SB.PCI0.SBRG.H_EC.MGIB))
                            MGIC = \_SB.PCI0.SBRG.H_EC.ECRD (RefOf (\_SB.PCI0.SBRG.H_EC.MGIC))
                            MGID = \_SB.PCI0.SBRG.H_EC.ECRD (RefOf (\_SB.PCI0.SBRG.H_EC.MGID))
                            MGIE = \_SB.PCI0.SBRG.H_EC.ECRD (RefOf (\_SB.PCI0.SBRG.H_EC.MGIE))
                            MGIF = \_SB.PCI0.SBRG.H_EC.ECRD (RefOf (\_SB.PCI0.SBRG.H_EC.MGIF))
                            CCI0 = \_SB.PCI0.SBRG.H_EC.ECRD (RefOf (\_SB.PCI0.SBRG.H_EC.CCI0))
                            CCI1 = \_SB.PCI0.SBRG.H_EC.ECRD (RefOf (\_SB.PCI0.SBRG.H_EC.CCI1))
