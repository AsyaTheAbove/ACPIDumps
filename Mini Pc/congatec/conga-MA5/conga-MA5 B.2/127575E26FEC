            Name (MGOF, 0x8F)
            Name (CCI0, 0x90)
            Name (CCI1, 0x91)
            Name (CCI2, 0x92)
            Name (CCI3, 0x93)
            Method (ECRD, 1, Serialized)
            {
                Local0 = DerefOf (Arg0)
                Switch (ToInteger (Local0))
                {
                    Case (0x50)
