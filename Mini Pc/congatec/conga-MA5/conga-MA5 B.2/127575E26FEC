            Method (_STA, 0, NotSerialized)  // _STA: Status
            {
                If (((OSYS >= 0x07DF) || ((OSYS >= 0x07DC) && (
                    S0ID == One))))
                {
                    Return (0x0F)
                }

                Return (Zero)
            }

            Method (_DSM, 4, Serialized)  // _DSM: Device-Specific Method
            {
                ADBG (Concatenate ("PEPY = ", ToHexString (PEPY)))
                ADBG (Concatenate ("PEPC = ", ToHexString (PEPC)))
                ADBG (Concatenate ("OSYS = ", ToHexString (OSYS)))
                If ((Arg0 == ToUUID ("c4eb40a0-6cd2-11e2-bcfd-0800200c9a66") /* Unknown UUID */))
                {
                    If ((Arg2 == Zero))
                    {
                        Return (Buffer (One)
                        {
                             0x7F                                             // .
                        })
                    }

                    If ((\_PR.NLPC == One))
                    {
                        DerefOf (DEVY [Zero]) [One] = One
                        DerefOf (DEVY [One]) [One] = Zero
                        DerefOf (DEVY [0x02]) [One] = Zero
                        DerefOf (DEVY [0x03]) [One] = Zero
                    }

                    If ((\_PR.NLPC == 0x02))
                    {
                        DerefOf (DEVY [Zero]) [One] = One
                        DerefOf (DEVY [One]) [One] = Zero
                        DerefOf (DEVY [0x02]) [One] = One
                        DerefOf (DEVY [0x03]) [One] = Zero
                    }

                    If ((\_PR.NLPC == 0x03))
                    {
                        DerefOf (DEVY [Zero]) [One] = One
                        DerefOf (DEVY [One]) [One] = One
                        DerefOf (DEVY [0x02]) [One] = One
                        DerefOf (DEVY [0x03]) [One] = Zero
                    }

                    If ((\_PR.NLPC == 0x04))
                    {
                        DerefOf (DEVY [Zero]) [One] = One
                        DerefOf (DEVY [One]) [One] = One
                        DerefOf (DEVY [0x02]) [One] = One
                        DerefOf (DEVY [0x03]) [One] = One
                    }

                    If ((ODBG == Zero))
                    {
                        DerefOf (DEVY [0x08]) [One] = One
                    }

                    If ((ODBG == One))
                    {
                        DerefOf (DEVY [0x08]) [One] = Zero
                    }

                    If ((SIOE == Zero))
                    {
                        DerefOf (DEVY [0x16]) [One] = Zero
                    }

                    If ((EMCE == Zero))
                    {
                        DerefOf (DEVY [0x17]) [One] = Zero
                    }

                    If ((SDEN == Zero))
                    {
                        DerefOf (DEVY [0x18]) [One] = Zero
                    }

                    If (^^PCI0.RP02.PXSX.WIST ())
                    {
                        DerefOf (DerefOf (DerefOf (DEVY [0x1B]) [0x02]
                            ) [One]) [One] = 0x03
                        DerefOf (DEVY [0x1B]) [One] = One
                    }

                    If ((Arg2 == One))
                    {
                        If ((S0ID == One))
                        {
                            Return (DEVY) /* \_SB_.PEPD.DEVY */
                        }

                        Return (Package (0x01)
                        {
                            Zero
                        })
                    }

                    If ((Arg2 == 0x02))
                    {
                        Return (BCCD) /* \_SB_.PEPD.BCCD */
                    }

                    If ((Arg2 == 0x03))
                    {
                        ADBG ("Screen Off Notification")
                    }

                    If ((Arg2 == 0x04))
                    {
                        ADBG ("Screen On Notification")
                    }

                    If ((Arg2 == 0x05))
                    {
                        ADBG ("PEP DSM:5")
                        P8XH (Zero, 0xC5)
                        P8XH (One, Zero)
                        If ((((S0ID == One) && (ECLP == One)) && (ECNO == One)))
                        {
                            If ((ECDB == One))
                            {
                                ADBG ("EC Debug")
                                ^^PCI0.SBRG.H_EC.ECWT (One, RefOf (^^PCI0.SBRG.H_EC.DLED))
                            }

                            ^^PCI0.SBRG.H_EC.ECMD (0x2C)
                        }
                    }

                    If ((Arg2 == 0x06))
                    {
                        ADBG ("PEP DSM:6")
                        P8XH (Zero, 0x50)
                        P8XH (One, Zero)
                        If ((((S0ID == One) && (ECLP == One)) && (ECNO == One)))
                        {
                            ^^PCI0.SBRG.H_EC.ECMD (0x2D)
                            If ((ECDB == One))
                            {
                                ADBG ("EC Debug")
                                ^^PCI0.SBRG.H_EC.ECWT (Zero, RefOf (^^PCI0.SBRG.H_EC.DLED))
                            }
                        }
                    }
                }

                Return (One)
            }
        }
    }

    Scope (_GPE)
    {
        Method (_L02, 0, NotSerialized)  // _Lxx: Level-Triggered GPE, xx=0x00-0xFF
        {
            CGPE = Zero
            SGPE = One
            If (CondRefOf (\_TZ.TZ01))
            {
                Notify (\_TZ.TZ01, 0x80) // Thermal Status Change
            }
        }

        Method (_L0D, 0, NotSerialized)  // _Lxx: Level-Triggered GPE, xx=0x00-0xFF
        {
            Notify (\_SB.PCI0.XHC, 0x02) // Device Wake
        }

        Method (_L0E, 0, NotSerialized)  // _Lxx: Level-Triggered GPE, xx=0x00-0xFF
        {
            Notify (\_SB.PCI0.HDAS, 0x02) // Device Wake
        }

        Method (_L0F, 0, NotSerialized)  // _Lxx: Level-Triggered GPE, xx=0x00-0xFF
        {
        }

        Method (_L39, 0, NotSerialized)  // _Lxx: Level-Triggered GPE, xx=0x00-0xFF
        {
            Notify (\_SB.PCI0.SATA.PRT0, 0x02) // Device Wake
        }

        Method (_L03, 0, NotSerialized)  // _Lxx: Level-Triggered GPE, xx=0x00-0xFF
        {
            If ((((BDID == OXHL) || (BDID == LFHL)) || (BDID == JNHL)))
            {
                Local2 = G0S /* \G0S_ */
                Local2 |= 0x01C8
                G0S = Local2
            }
            Else
            {
                RP3S = One
            }

            If ((((BDID == OXHL) || (BDID == LFHL)) || (BDID == JNHL)))
            {
                If ((RP5D == Zero))
                {
                    \_SB.PCI0.RP05.HPME ()
                    Notify (\_SB.PCI0.RP05, 0x02) // Device Wake
                }
            }
            ElseIf ((RP3D == Zero))
            {
                \_SB.PCI0.RP03.HPME ()
                Notify (\_SB.PCI0.RP03, 0x02) // Device Wake
            }
        }

        Method (_L06, 0, NotSerialized)  // _Lxx: Level-Triggered GPE, xx=0x00-0xFF
        {
            If ((((BDID == OXHL) || (BDID == LFHL)) || (BDID == JNHL)))
            {
                Local2 = G0S /* \G0S_ */
                Local2 |= 0x01C8
                G0S = Local2
                If ((RP3D == Zero))
                {
                    \_SB.PCI0.RP03.HPME ()
                    Notify (\_SB.PCI0.RP03, 0x02) // Device Wake
                }
            }
        }

        Method (_L07, 0, NotSerialized)  // _Lxx: Level-Triggered GPE, xx=0x00-0xFF
        {
            RP1S = One
            If ((RP1D == Zero))
            {
                \_SB.PCI0.RP01.HPME ()
                Notify (\_SB.PCI0.RP01, 0x02) // Device Wake
            }
        }

        Method (_L08, 0, NotSerialized)  // _Lxx: Level-Triggered GPE, xx=0x00-0xFF
        {
            RP2S = One
            If ((RP2D == Zero))
            {
                \_SB.PCI0.RP02.HPME ()
                Notify (\_SB.PCI0.RP02, 0x02) // Device Wake
            }
        }

        Method (_L09, 0, NotSerialized)  // _Lxx: Level-Triggered GPE, xx=0x00-0xFF
        {
            Local2 = G0S /* \G0S_ */
            Local2 |= 0x01C8
            G0S = Local2
            If ((RP1D == Zero))
            {
                \_SB.PCI0.RP01.HPME ()
                Notify (\_SB.PCI0.RP01, 0x02) // Device Wake
            }

            If ((RP2D == Zero))
            {
                \_SB.PCI0.RP02.HPME ()
                Notify (\_SB.PCI0.RP02, 0x02) // Device Wake
            }

            If ((RP3D == Zero))
            {
                \_SB.PCI0.RP03.HPME ()
                Notify (\_SB.PCI0.RP03, 0x02) // Device Wake
            }

            If ((RP4D == Zero))
            {
                \_SB.PCI0.RP04.HPME ()
                Notify (\_SB.PCI0.RP04, 0x02) // Device Wake
            }

            If ((RP5D == Zero))
            {
                \_SB.PCI0.RP05.HPME ()
                Notify (\_SB.PCI0.RP05, 0x02) // Device Wake
            }

            If ((RP6D == Zero))
            {
                \_SB.PCI0.RP06.HPME ()
                Notify (\_SB.PCI0.RP06, 0x02) // Device Wake
            }
        }
    }

    Device (_SB.TPM)
    {
        Method (_HID, 0, NotSerialized)  // _HID: Hardware ID
        {
            If (TCMF)
            {
                Return (0x01013469)
            }
            ElseIf ((TTDP == Zero))
            {
                Return (0x310CD041)
            }
            Else
            {
                Return ("MSFT0101")
            }
        }

        OperationRegion (TMMB, SystemMemory, 0xFED40000, 0x5000)
        Field (TMMB, ByteAcc, Lock, Preserve)
        {
            ACC0,   8, 
            Offset (0x08), 
            INTE,   32, 
            INTV,   8, 
            Offset (0x10), 
            INTS,   32, 
            INTF,   32, 
            TSTS,   32, 
            Offset (0x24), 
            FIFO,   32, 
            Offset (0x30), 
            IDTF,   32, 
            Offset (0x4C), 
            SCMD,   32
        }

        Method (_STR, 0, NotSerialized)  // _STR: Description String
        {
            If ((TTDP == Zero))
            {
                Return (Unicode ("TPM 1.2 Device"))
            }
            Else
            {
                Return (Unicode ("TPM 2.0 Device"))
            }
        }

        Name (_UID, One)  // _UID: Unique ID
        Name (CRST, ResourceTemplate ()
        {
            Memory32Fixed (ReadOnly,
                0x00000000,         // Address Base
                0x00001000,         // Address Length
                _Y18)
            Memory32Fixed (ReadOnly,
                0xFED70000,         // Address Base
                0x00001000,         // Address Length
                _Y19)
        })
        Name (CRSD, ResourceTemplate ()
        {
            Memory32Fixed (ReadWrite,
                0xFED40000,         // Address Base
                0x00005000,         // Address Length
                _Y1A)
        })
        Name (CRID, ResourceTemplate ()
        {
            Memory32Fixed (ReadWrite,
                0xFED40000,         // Address Base
                0x00005000,         // Address Length
                _Y1B)
        })
        Name (CREI, ResourceTemplate ()
        {
            Memory32Fixed (ReadWrite,
                0xFED40000,         // Address Base
                0x00005000,         // Address Length
                )
            Interrupt (ResourceConsumer, Level, ActiveLow, Shared, ,, _Y1C)
            {
                0x00000000,
            }
        })
        Method (_CRS, 0, Serialized)  // _CRS: Current Resource Settings
        {
            If ((AMDT == One))
            {
                CreateDWordField (CRST, \_SB.TPM._Y18._BAS, MTFB)  // _BAS: Base Address
                CreateDWordField (CRST, \_SB.TPM._Y18._LEN, LTFB)  // _LEN: Length
                MTFB = TPMB /* \TPMB */
                LTFB = TPBS /* \TPBS */
                CreateDWordField (CRST, \_SB.TPM._Y19._BAS, MTFC)  // _BAS: Base Address
                CreateDWordField (CRST, \_SB.TPM._Y19._LEN, LTFC)  // _LEN: Length
                MTFC = TPMC /* \TPMC */
                LTFC = TPCS /* \TPCS */
                Return (CRST) /* \_SB_.TPM_.CRST */
            }
            Else
            {
                If ((DTPT == One))
                {
                    CreateDWordField (CRSD, \_SB.TPM._Y1A._BAS, MTFE)  // _BAS: Base Address
                    CreateDWordField (CRSD, \_SB.TPM._Y1A._LEN, LTFE)  // _LEN: Length
                    MTFE = 0xFED40000
                    LTFE = 0x5000
                    Return (CRSD) /* \_SB_.TPM_.CRSD */
                }
                ElseIf ((TTPF == One))
                {
                    If ((TMRQ == Zero))
                    {
                        CreateDWordField (CRID, \_SB.TPM._Y1B._BAS, MTFD)  // _BAS: Base Address
                        CreateDWordField (CRID, \_SB.TPM._Y1B._LEN, LTFD)  // _LEN: Length
                        MTFD = 0xFED40000
                        LTFD = 0x5000
                        Return (CRID) /* \_SB_.TPM_.CRID */
                    }
                    Else
                    {
                        CreateDWordField (CREI, \_SB.TPM._Y1C._INT, LIRQ)  // _INT: Interrupts
                        LIRQ = TMRQ /* \TMRQ */
                        Return (CREI) /* \_SB_.TPM_.CREI */
                    }
                }
                ElseIf ((TTPF == Zero))
                {
                    CreateDWordField (CRST, \_SB.TPM._Y19._BAS, MTFF)  // _BAS: Base Address
                    MTFF = FTPM /* \FTPM */
                    Return (CRST) /* \_SB_.TPM_.CRST */
                }

                MTFE = Zero
                LTFE = Zero
                Return (CRID) /* \_SB_.TPM_.CRID */
            }

            Return (CRID) /* \_SB_.TPM_.CRID */
        }

        Method (_SRS, 1, Serialized)  // _SRS: Set Resource Settings
        {
            If ((TMRQ != Zero))
            {
                CreateDWordField (Arg0, 0x11, IRQ0)
                CreateDWordField (CREI, \_SB.TPM._Y1C._INT, LIRQ)  // _INT: Interrupts
                LIRQ = IRQ0 /* \_SB_.TPM_._SRS.IRQ0 */
                TMRQ = IRQ0 /* \_SB_.TPM_._SRS.IRQ0 */
                CreateBitField (Arg0, 0x79, ITRG)
                CreateBitField (CREI, \_SB.TPM._Y1C._HE, LTRG)  // _HE_: High-Edge
                LTRG = ITRG /* \_SB_.TPM_._SRS.ITRG */
                CreateBitField (Arg0, 0x7A, ILVL)
                CreateBitField (CREI, \_SB.TPM._Y1C._LL, LLVL)  // _LL_: Low Level
                LLVL = ILVL /* \_SB_.TPM_._SRS.ILVL */
                If ((((IDTF & 0x0F) == Zero) || ((IDTF & 0x0F
                    ) == 0x0F)))
                {
                    If ((IRQ0 < 0x10))
                    {
                        INTV = (IRQ0 & 0x0F)
                    }

                    If ((ITRG == One))
                    {
                        INTE |= 0x10
                    }
                    Else
                    {
                        INTE &= 0xFFFFFFEF
                    }

                    If ((ILVL == Zero))
                    {
                        INTE |= 0x08
                    }
                    Else
                    {
                        INTE &= 0xFFFFFFF7
                    }
                }
            }
        }

        OperationRegion (CRBD, SystemMemory, TPMM, 0x48)
        Field (CRBD, AnyAcc, NoLock, Preserve)
        {
            Offset (0x04), 
            HERR,   32, 
            Offset (0x40), 
            HCMD,   32, 
            HSTS,   32
        }

        Method (_STA, 0, NotSerialized)  // _STA: Status
        {
            If ((TTDP == Zero))
            {
                If (TPMF)
                {
                    Return (0x0F)
                }

                Return (Zero)
            }
            ElseIf ((TTDP == One))
            {
                If (TPMF)
                {
                    Return (0x0F)
                }

                Return (Zero)
            }

            Return (Zero)
        }

        Method (STRT, 3, Serialized)
        {
            OperationRegion (TPMR, SystemMemory, FTPM, 0x1000)
            Field (TPMR, AnyAcc, NoLock, Preserve)
            {
                Offset (0x04), 
                FERR,   32, 
                Offset (0x0C), 
                BEGN,   32
            }

            Name (TIMR, Zero)
            If ((ToInteger (Arg0) != Zero)) {}
            Switch (ToInteger (Arg1))
            {
                Case (Zero)
                {
                    Return (Buffer (One)
                    {
                         0x03                                             // .
                    })
                }
                Case (One)
                {
                    TIMR = Zero
                    If ((AMDT == One))
                    {
                        While (((BEGN == One) && (TIMR < 0x0200)))
                        {
                            If ((BEGN == One))
                            {
                                Sleep (One)
                                TIMR++
                            }
                        }
                    }
                    ElseIf ((((HSTS & 0x02) | (HSTS & One)
                        ) == 0x03))
                    {
                        HCMD = One
                    }
                    Else
                    {
                        FERR = One
                        BEGN = Zero
                    }

                    Return (Zero)
                }

            }

            Return (One)
        }

        Method (CRYF, 3, Serialized)
        {
            If ((ToInteger (Arg0) != One)) {}
            Switch (ToInteger (Arg1))
            {
                Case (Zero)
                {
                    Return (Buffer (One)
                    {
                         0x03                                             // .
                    })
                }
                Case (One)
                {
                    Name (TPMV, Package (0x02)
                    {
                        One, 
                        Package (0x02)
                        {
                            One, 
                            0x20
                        }
                    })
                    If ((_STA () == Zero))
                    {
                        Return (Package (0x01)
                        {
                            Zero
                        })
                    }

                    Return (TPMV) /* \_SB_.TPM_.CRYF.TPMV */
                }

            }

            Return (Buffer (One)
            {
                 0x00                                             // .
            })
        }
    }

    Scope (_SB.TPM)
    {
        OperationRegion (TSMI, SystemIO, SMIA, 0x02)
        Field (TSMI, WordAcc, NoLock, Preserve)
        {
            SMI,    16
        }

        OperationRegion (ATNV, SystemMemory, PPIM, PPIL)
        Field (ATNV, AnyAcc, NoLock, Preserve)
        {
            RQST,   32, 
            RCNT,   32, 
            ERRO,   32, 
            FLAG,   32, 
            MISC,   32, 
            OPTN,   32, 
            SRSP,   32
        }

        Method (_DSM, 4, Serialized)  // _DSM: Device-Specific Method
        {
            If ((Arg0 == ToUUID ("3dddfaa6-361b-4eb4-a424-8d10089d1653") /* Physical Presence Interface */))
            {
                Switch (ToInteger (Arg2))
                {
                    Case (Zero)
                    {
                        Return (Buffer (0x02)
                        {
                             0xFF, 0x01                                       // ..
                        })
                    }
                    Case (One)
                    {
                        If ((PPIV == Zero))
                        {
                            Return ("1.2")
                        }
                        Else
                        {
                            Return ("1.3")
                        }
                    }
                    Case (0x02)
                    {
                        RQST = DerefOf (Arg3 [Zero])
                        SRSP = Zero
                        FLAG = 0x02
                        TMF1 = OFST /* \OFST */
                        SRSP = Zero
                        SMI = TMF1 /* \TMF1 */
                        Return (SRSP) /* \_SB_.TPM_.SRSP */
                    }
                    Case (0x03)
                    {
                        Name (PPI1, Package (0x02)
                        {
                            Zero, 
                            Zero
                        })
                        PPI1 [One] = RQST /* \_SB_.TPM_.RQST */
                        Return (PPI1) /* \_SB_.TPM_._DSM.PPI1 */
                    }
                    Case (0x04)
                    {
                        Return (TRST) /* \TRST */
                    }
                    Case (0x05)
                    {
                        Name (PPI2, Package (0x03)
                        {
                            Zero, 
                            Zero, 
                            Zero
                        })
                        SRSP = Zero
                        FLAG = 0x05
                        SMI = OFST /* \OFST */
                        PPI2 [One] = RCNT /* \_SB_.TPM_.RCNT */
                        PPI2 [0x02] = ERRO /* \_SB_.TPM_.ERRO */
                        Return (PPI2) /* \_SB_.TPM_._DSM.PPI2 */
                    }
                    Case (0x06)
                    {
                        Return (0x03)
                    }
                    Case (0x07)
                    {
                        RQST = DerefOf (Arg3 [Zero])
                        FLAG = 0x07
                        OPTN = Zero
                        If ((RQST == 0x17))
                        {
                            ToInteger (DerefOf (Arg3 [One]), OPTN) /* \_SB_.TPM_.OPTN */
                        }

                        TMF1 = OFST /* \OFST */
                        SRSP = Zero
                        SMI = TMF1 /* \TMF1 */
                        Return (SRSP) /* \_SB_.TPM_.SRSP */
                    }
                    Case (0x08)
                    {
                        RQST = DerefOf (Arg3 [Zero])
                        FLAG = 0x08
                        TMF1 = OFST /* \OFST */
                        SRSP = Zero
                        SMI = TMF1 /* \TMF1 */
                        Return (SRSP) /* \_SB_.TPM_.SRSP */
                    }
                    Default
                    {
                    }

                }
            }
            ElseIf ((Arg0 == ToUUID ("376054ed-cc13-4675-901c-4756d7f2d45d") /* Unknown UUID */))
            {
                Switch (ToInteger (Arg2))
                {
                    Case (Zero)
                    {
                        Return (Buffer (One)
                        {
                             0x03                                             // .
                        })
                    }
                    Case (One)
                    {
                        RQST = DerefOf (Arg3 [Zero])
                        FLAG = 0x09
                        TMF1 = OFST /* \OFST */
                        SRSP = Zero
                        SMI = TMF1 /* \TMF1 */
                        Return (SRSP) /* \_SB_.TPM_.SRSP */
                    }
                    Default
                    {
                    }

                }
            }

            If ((Arg0 == ToUUID ("cf8e16a5-c1e8-4e25-b712-4f54a96702c8") /* Unknown UUID */))
            {
                Return (CRYF (Arg1, Arg2, Arg3))
            }

            If ((Arg0 == ToUUID ("6bbf6cab-5463-4714-b7cd-f0203c0368d4") /* Unknown UUID */))
            {
                Return (STRT (Arg1, Arg2, Arg3))
            }

            Return (Buffer (One)
            {
                 0x00                                             // .
            })
        }

        Method (TPTS, 1, Serialized)
        {
            Switch (ToInteger (Arg0))
            {
                Case (0x04)
                {
                    RQST = Zero
                    FLAG = 0x09
                    SRSP = Zero
                    SMI = OFST /* \OFST */
                }
                Case (0x05)
                {
                    RQST = Zero
                    FLAG = 0x09
                    SRSP = Zero
                    SMI = OFST /* \OFST */
                }

            }
        }
    }

    Scope (_GPE)
    {
        Method (_L31, 0, Serialized)  // _Lxx: Level-Triggered GPE, xx=0x00-0xFF
        {
            Name (BCSQ, Buffer (0x02) {})
            BCSQ = \_SB.PCI0.BCHC (0x2C, One, 0x02)
            If ((DerefOf (BCSQ [Zero]) == One))
            {
                P8XH (Zero, DerefOf (BCSQ [One]))
                Local0 = DerefOf (BCSQ [One])
                Switch (Local0)
                {
                    Case (One)
                    {
                        If (WDAE)
                        {
                            Fatal (0x01, 0x00000002, Zero)
                        }
                        Else
                        {
                            \_TZ.WDIF = One
                            \_TZ.TZNY (0x80)
                        }
                    }
                    Case (0x02)
                    {
                        \_TZ.TZNY (0x80)
                    }
                    Case (0x04)
                    {
                        If (((\_SB.BATS (Zero) & One) != \_SB.B1PR))
                        {
                            \_SB.B1PR ^= One
                            Notify (\_SB.BAT1, 0x81) // Information Change
                        }
                        Else
                        {
                            Notify (\_SB.BAT1, 0x80) // Status Change
                            Notify (\_PR.CPU0, 0x80) // Performance Capability Change
                            Notify (\_PR.CPU1, 0x80) // Performance Capability Change
                        }
                    }
                    Case (0x05)
                    {
                        If (((\_SB.BATS (One) & One) != \_SB.B2PR))
                        {
                            \_SB.B2PR ^= One
                            Notify (\_SB.BAT2, 0x81) // Information Change
                        }
                        Else
                        {
                            Notify (\_SB.BAT2, 0x80) // Status Change
                            Notify (\_PR.CPU0, 0x80) // Performance Capability Change
                            Notify (\_PR.CPU1, 0x80) // Performance Capability Change
                        }
                    }
                    Case (0x06)
                    {
                        If (((\_SB.BATS (0x02) & One) != \_SB.B3PR))
                        {
                            \_SB.B3PR ^= One
                            Notify (\_SB.BAT3, 0x81) // Information Change
                        }
                        Else
                        {
                            Notify (\_SB.BAT3, 0x80) // Status Change
                            Notify (\_PR.CPU0, 0x80) // Performance Capability Change
                            Notify (\_PR.CPU1, 0x80) // Performance Capability Change
                        }
                    }
                    Case (0x07)
                    {
                        If (((\_SB.BATS (0x03) & One) != \_SB.B4PR))
                        {
                            \_SB.B4PR ^= One
                            Notify (\_SB.BAT4, 0x81) // Information Change
                        }
                        Else
                        {
                            Notify (\_SB.BAT4, 0x80) // Status Change
                            Notify (\_PR.CPU0, 0x80) // Performance Capability Change
                            Notify (\_PR.CPU1, 0x80) // Performance Capability Change
                        }
                    }
                    Case (0x08)
                    {
                        \_SB.PCI0.LPOL = One
                        \_SB.PCI0.LID.LBEV ()
                    }
                    Case (0x09)
                    {
                        \_SB.PCI0.LPOL = Zero
                        \_SB.PCI0.LID.LBEV ()
                    }
                    Case (0x0A)
                    {
                        \_SB.SLPB.SBEV ()
                    }
                    Case (0x0B)
                    {
                        P8XH (Zero, 0x0B)
                        Notify (\_SB.PWRB, 0x80) // Status Change
                    }

                }
            }
        }
    }

    Scope (_SB.PCI0)
    {
        OperationRegion (HCLB, SystemIO, HICB, HICS)
        Field (HCLB, ByteAcc, NoLock, Preserve)
        {
            HCAR,   8, 
            Offset (0x04), 
            HCST,   8
        }

        OperationRegion (HSHB, SystemIO, HISB, HISS)
        Field (HSHB, ByteAcc, NoLock, Preserve)
        {
            HSDA,   504, 
            HSST,   8
        }

        Name (LPOL, One)
        Device (LID)
        {
            Name (_HID, EisaId ("PNP0C0D") /* Lid Device */)  // _HID: Hardware ID
            Method (_STA, 0, NotSerialized)  // _STA: Status
            {
                If ((GE1F == One))
                {
                    Return (0x0F)
                }
                Else
                {
                    Return (Zero)
                }
            }

            Method (_LID, 0, NotSerialized)  // _LID: Lid Status
            {
                Return (LPOL) /* \_SB_.PCI0.LPOL */
            }

            Method (LBEV, 0, NotSerialized)
            {
                Notify (LID, 0x80) // Status Change
            }
        }
    }

    Scope (_SB)
    {
        Name (SLPS, Zero)
        Device (SLPB)
        {
            Name (_HID, EisaId ("PNP0C0E") /* Sleep Button Device */)  // _HID: Hardware ID
            Method (_STA, 0, NotSerialized)  // _STA: Status
            {
                If ((GE2F == One))
                {
                    Return (0x0F)
                }
                Else
                {
                    Return (Zero)
                }
            }

            Method (SBEV, 0, NotSerialized)
            {
                If (SLPS)
                {
                    Notify (SLPB, 0x02) // Device Wake
                }
                Else
                {
                    Notify (SLPB, 0x80) // Status Change
                }
            }
        }
    }

    Scope (_SB.PCI0)
    {
        Method (BCHC, 3, Serialized)
        {
            Name (TBUF, Buffer (0x3F) {})
            Name (RBUF, Buffer (Arg2) {})
            TBUF = Arg0
            Arg1--
            ADBG ("congatec BCHC Entry")
            Local0 = DerefOf (TBUF [Zero])
            ADBG (Concatenate ("BCHC Arg0 =", ToHexString (Local0)))
            Local1 = Zero
            While ((Local1 == Zero))
            {
                Local0 = (HCST & 0x10)
                While ((Local0 == 0x10))
                {
                    Sleep (One)
                    Local0 = (HCST & 0x10)
                }

                Local1 = HCAR /* \_SB_.PCI0.HCAR */
                If ((Local1 == Zero))
                {
                    Local1 = HCAR /* \_SB_.PCI0.HCAR */
                }
                Else
                {
                    Local1 = Zero
                }
            }

            Local0 = HSST /* \_SB_.PCI0.HSST */
            While ((Local0 != Zero))
            {
                Sleep (One)
                Local0 = HSST /* \_SB_.PCI0.HSST */
            }

            Local2 = Zero
            Local0 = DerefOf (TBUF [Local2])
            While ((Local2 < Arg1))
            {
                Local2++
                Local0 ^= DerefOf (TBUF [Local2])
            }

            Arg1++
            TBUF [Arg1] = Local0
            HSDA = TBUF /* \_SB_.PCI0.BCHC.TBUF */
            HSST = Local1
            Local0 = HSST /* \_SB_.PCI0.HSST */
            While ((Local0 != Zero))
            {
                Sleep (One)
                Local0 = HSST /* \_SB_.PCI0.HSST */
            }

            TBUF = HSDA /* \_SB_.PCI0.HSDA */
            Local0 = DerefOf (TBUF [Zero])
            Local2 = Zero
            Local2 ^= Local0
            Local3 = (Local0 & 0xC0)
            If ((Local3 == 0x40))
            {
                If ((Local2 != DerefOf (TBUF [Zero])))
                {
                    RBUF [Zero] = 0xFE
                    RBUF [One] = 0xC3
                }
                Else
                {
                    RBUF [One] = (Local0 & 0x3F)
                    RBUF [Zero] = Zero
                }
            }
            ElseIf ((Local3 == Zero))
            {
                If ((Arg2 <= Local0))
                {
                    RBUF [Zero] = 0xFE
                    RBUF [One] = 0xC0
                }

                Local4 = Zero
                While ((Local4 < Local0))
                {
                    Local4++
                    Local2 ^= DerefOf (TBUF [Local4])
                }

                Local4++
                If ((Local2 == DerefOf (TBUF [Local4])))
                {
                    Local4 = Zero
                    RBUF [Local4] = DerefOf (TBUF [Local4])
                    While ((Local4 < Local0))
                    {
                        Local4++
                        RBUF [Local4] = DerefOf (TBUF [Local4])
                    }
                }
                Else
                {
                    RBUF [Zero] = 0xFE
                    RBUF [One] = 0xC3
                }
            }
            Else
            {
                RBUF [Zero] = 0xFE
                RBUF [One] = Local0
            }

            HCAR = Local1
            Local0 = DerefOf (RBUF [Zero])
            ADBG (Concatenate ("BCHC RBUF 0=", ToHexString (Local0)))
            Local0 = DerefOf (RBUF [One])
            ADBG (Concatenate ("BCHC RBUF 1=", ToHexString (Local0)))
            ADBG ("congatec BCHC Exit")
            Return (RBUF) /* \_SB_.PCI0.BCHC.RBUF */
        }
    }

    Name (BCIS, One)
    Scope (_SB.PCI0)
    {
        Method (IBGS, 1, NotSerialized)
        {
            Name (GSBF, Buffer (0x02) {})
            GSBF = BCHC ((0x48 | Arg0), One, 0x02)
            If ((DerefOf (GSBF [Zero]) == Zero))
            {
                Local0 = DerefOf (GSBF [One])
                Return (Local0)
            }
            Else
            {
                Return (0xFE)
            }
        }

        Method (BCIT, 6, Serialized)
        {
            Name (RDBF, Buffer (0x21) {})
            Name (WRBF, Buffer ((Arg1 + 0x03)) {})
            CreateByteField (WRBF, Zero, CMDB)
            CreateByteField (WRBF, One, WCNT)
            CreateByteField (WRBF, 0x02, RCNT)
            If ((Arg1 != Zero))
            {
                CreateField (WRBF, 0x18, (Arg1 * 0x08), WBUF)
            }

            CMDB = (0x40 | Arg0)
            WCNT = (Arg1 | Arg3)
            RCNT = (Arg2 | Arg4)
            If ((Arg1 != Zero))
            {
                WBUF = Arg5
            }

            RDBF = BCHC (WRBF, (Arg1 + 0x03), 0x02)
            If ((DerefOf (RDBF [Zero]) != 0xFE))
            {
                Local0 = 0x02
                While ((Local0 == 0x02))
                {
                    Local0 = IBGS (Arg0)
                }

                If ((Local0 == 0xFE))
                {
                    RDBF [Zero] = Local0
                }
                ElseIf (Arg2)
                {
                    RDBF = BCHC ((0x50 | Arg0), One, (Arg2 + One))
                    If ((Arg2 != DerefOf (RDBF [Zero])))
                    {
                        RDBF [Zero] = 0xFE
                    }
                }
            }

            Return (RDBF) /* \_SB_.PCI0.BCIT.RDBF */
        }

        Method (BCIX, 4, Serialized)
        {
            Name (RDBF, Buffer (0x21) {})
            Name (WRBF, Buffer ((Arg1 + 0x03)) {})
            CreateByteField (WRBF, Zero, CMDB)
            CreateByteField (WRBF, One, WCNT)
            CreateByteField (WRBF, 0x02, RCNT)
            If ((Arg1 != Zero))
            {
                CreateField (WRBF, 0x18, (Arg1 * 0x08), WBUF)
            }

            If ((BCIS == One))
            {
                CMDB = (0x68 | Arg0)
                WCNT = Arg1
                RCNT = Arg2
                If ((Arg1 != Zero))
                {
                    WBUF = Arg3
                }

                RDBF = BCHC (WRBF, (Arg1 + 0x03), 0x02)
                If ((DerefOf (RDBF [Zero]) != 0xFE))
                {
                    Local0 = 0x02
                    While ((Local0 == 0x02))
                    {
                        Local0 = IBGS (Arg0)
                    }

                    If ((Local0 == 0xFE))
                    {
                        RDBF [Zero] = Local0
                    }
                    ElseIf (Arg2)
                    {
                        RDBF = BCHC ((0x50 | Arg0), One, (Arg2 + One))
                    }
                }
                ElseIf ((DerefOf (RDBF [One]) == 0xC1))
                {
                    BCIS = Zero
                }
            }
            Else
            {
                RDBF [Zero] = 0xFE
                RDBF [One] = 0xC1
            }

            Return (RDBF) /* \_SB_.PCI0.BCIX.RDBF */
        }
    }

    Mutex (MUCG, 0x00)
    Name (BSBN, 0xF0)
    Scope (_SB)
    {
        Name (NOSB, Zero)
        Name (B1PR, Zero)
        Name (B2PR, Zero)
        Name (B3PR, Zero)
        Name (B4PR, Zero)
        Name (MAXL, 0x0E)
        Name (DDDD, Zero)
        Name (BF33, Buffer (0x21) {})
        CreateByteField (BF33, Zero, TSTA)
        CreateByteField (BF33, One, NOBY)
        CreateByteField (BF33, 0x02, DBY2)
        CreateWordField (BF33, One, RWRD)
        CreateField (BF33, 0x08, 0x0100, RBUF)
        Device (ADP1)
        {
            Method (_INI, 0, NotSerialized)  // _INI: Initialize
            {
                Name (BSTS, Buffer (0x02) {})
                BSTS = ^^PCI0.BCHC (0x3C, One, 0x02)
                If ((DerefOf (BSTS [Zero]) == One))
                {
                    Local0 = DerefOf (BSTS [One])
                    If (((Local0 & 0x08) == 0x08))
                    {
                        BSBN = 0x02
                    }
                    Else
                    {
                        BSBN = Zero
                    }
                }
                Else
                {
                    BSBN = Zero
                }

                BF33 = ^^PCI0.BCIX (BSBN, 0x02, 0x02, Buffer (0x02)
                        {
                             0x16, 0x44                                       // .D
                        })
                If ((TSTA != 0xFE))
                {
                    Local0 = DerefOf (BF33 [One])
                    NOSB = (Local0 & 0x0F)
                }
                Else
                {
                    BF33 = ^^PCI0.BCIT (BSBN, 0x02, Zero, 0x80, Zero, Buffer (0x02)
                            {
                                 0x16, 0x44                                       // .D
                            })
                    If ((DerefOf (BF33 [Zero]) != 0xFE))
                    {
                        BF33 = ^^PCI0.BCIT (BSBN, One, 0x02, 0xC0, Zero, 0x17)
                        If ((DerefOf (BF33 [Zero]) != 0xFE))
                        {
                            Local0 = DerefOf (BF33 [One])
                            NOSB = (Local0 & 0x0F)
                        }
                    }
                }

                P8XH (Zero, NOSB)
            }

            Name (_HID, "ACPI0003" /* Power Source Device */)  // _HID: Hardware ID
            Method (_PSR, 0, NotSerialized)  // _PSR: Power Source
            {
                If ((NOSB == Zero))
                {
                    Return (One)
                }

                If ((BATS (Zero) & 0x40))
                {
                    Return (One)
                }
                Else
                {
                    Return (Zero)
                }
            }

            Name (_PCL, Package (0x01)  // _PCL: Power Consumer List
            {
                _SB
            })
        }

        Device (BAT1)
        {
            Name (_HID, EisaId ("PNP0C0A") /* Control Method Battery */)  // _HID: Hardware ID
            Name (_UID, One)  // _UID: Unique ID
            Method (_STA, 0, NotSerialized)  // _STA: Status
            {
                If ((NOSB & One))
                {
                    If ((BATS (Zero) & One))
                    {
                        Return (0x1F)
                    }
                    Else
                    {
                        Return (0x0F)
                    }
                }
                Else
                {
                    Return (Zero)
                }
            }

            Method (_BIF, 0, NotSerialized)  // _BIF: Battery Information
            {
                Return (CBIF (One))
            }

            Method (_BST, 0, NotSerialized)  // _BST: Battery Status
            {
                Return (CBST (One))
            }

            Name (_PCL, Package (0x01)  // _PCL: Power Consumer List
            {
                _SB
            })
        }

        Device (BAT2)
        {
            Name (_HID, EisaId ("PNP0C0A") /* Control Method Battery */)  // _HID: Hardware ID
            Name (_UID, 0x02)  // _UID: Unique ID
            Method (_STA, 0, NotSerialized)  // _STA: Status
            {
                If ((NOSB & 0x02))
                {
                    If ((BATS (One) & One))
                    {
                        Return (0x1F)
                    }
                    Else
                    {
                        Return (0x0F)
                    }
                }
                Else
                {
                    Return (Zero)
                }
            }

            Method (_BIF, 0, NotSerialized)  // _BIF: Battery Information
            {
                Return (CBIF (0x02))
            }

            Method (_BST, 0, NotSerialized)  // _BST: Battery Status
            {
                Return (CBST (0x02))
            }

            Name (_PCL, Package (0x01)  // _PCL: Power Consumer List
            {
                _SB
            })
        }

        Device (BAT3)
        {
            Name (_HID, EisaId ("PNP0C0A") /* Control Method Battery */)  // _HID: Hardware ID
            Name (_UID, 0x03)  // _UID: Unique ID
            Method (_STA, 0, NotSerialized)  // _STA: Status
            {
                If ((NOSB & 0x04))
                {
                    If ((BATS (0x02) & One))
                    {
                        Return (0x1F)
                    }
                    Else
                    {
                        Return (0x0F)
                    }
                }
                Else
                {
                    Return (Zero)
                }
            }

            Method (_BIF, 0, NotSerialized)  // _BIF: Battery Information
            {
                Return (CBIF (0x03))
            }

            Method (_BST, 0, NotSerialized)  // _BST: Battery Status
            {
                Return (CBST (0x03))
            }

            Name (_PCL, Package (0x01)  // _PCL: Power Consumer List
            {
                _SB
            })
        }

        Device (BAT4)
        {
            Name (_HID, EisaId ("PNP0C0A") /* Control Method Battery */)  // _HID: Hardware ID
            Name (_UID, 0x04)  // _UID: Unique ID
            Method (_STA, 0, NotSerialized)  // _STA: Status
            {
                If ((NOSB & 0x08))
                {
                    If ((BATS (0x03) & One))
                    {
                        Return (0x1F)
                    }
                    Else
                    {
                        Return (0x0F)
                    }
                }
                Else
                {
                    Return (Zero)
                }
            }

            Method (_BIF, 0, NotSerialized)  // _BIF: Battery Information
            {
                Return (CBIF (0x04))
            }

            Method (_BST, 0, NotSerialized)  // _BST: Battery Status
            {
                Return (CBST (0x04))
            }

            Name (_PCL, Package (0x01)  // _PCL: Power Consumer List
            {
                _SB
            })
        }

        Method (BATS, 1, Serialized)
        {
            Name (BSTS, Buffer (0x02) {})
            BSTS = ^PCI0.BCHC ((0x3C | Arg0), One, 0x02)
            If ((DerefOf (BSTS [Zero]) == One))
            {
                Local0 = DerefOf (BSTS [One])
            }
            Else
            {
                Local0 = Zero
            }

            Return (Local0)
        }

        Method (SELB, 1, Serialized)
        {
            Name (TBUF, Buffer (0x04)
            {
                 0x16, 0x41, 0x00, 0x00                           // .A..
            })
            Local0 = One
            Local0 <<= (Arg0 + 0x03)
            TBUF [0x03] = Local0
            BF33 = ^PCI0.BCIT (BSBN, 0x04, Zero, 0xC0, Zero, TBUF)
            Local0 = DerefOf (BF33 [Zero])
            Return (Local0)
        }

        Method (CBIF, 1, Serialized)
        {
            Acquire (MUCG, 0xFFFF)
            Name (BIFP, Package (0x0D)
            {
                One, 
                0xFFFFFFFF, 
                0xFFFFFFFF, 
                One, 
                0xFFFFFFFF, 
                Zero, 
                Zero, 
                One, 
                One, 
                "CMBDevice", 
                "0000", 
                "CHEM", 
                "Manufacturer"
            })
            BF33 = SELB (Arg0)
            If ((TSTA != 0xFE))
            {
                BF33 = ^PCI0.BCIX (BSBN, 0x02, 0x0E, Buffer (0x02)
                        {
                             0x17, 0x20                                       // . 
                        })
                If ((TSTA != 0xFE))
                {
                    BIFP [0x0C] = RBUF /* \_SB_.RBUF */
                }
                ElseIf ((DerefOf (BF33 [One]) == 0xC1))
                {
                    BF33 = ^PCI0.BCIT (BSBN, 0x02, Zero, 0x80, Zero, Buffer (0x02)
                            {
                                 0x16, 0x20                                       // . 
                            })
                    If ((TSTA != 0xFE))
                    {
                        BF33 = ^PCI0.BCIT (BSBN, One, One, 0x80, 0x80, 0x17)
                        If (((TSTA != 0xFE) && (NOBY <= 0x20)))
                        {
                            If ((NOBY > MAXL))
                            {
                                NOBY = MAXL /* \_SB_.MAXL */
                            }

                            BF33 = ^PCI0.BCIT (BSBN, Zero, NOBY, 0x40, Zero, Zero)
                            If ((TSTA != 0xFE))
                            {
                                BIFP [0x0C] = RBUF /* \_SB_.RBUF */
                            }
                        }
                    }
                }

                BF33 = ^PCI0.BCIX (BSBN, 0x02, 0x0E, Buffer (0x02)
                        {
                             0x17, 0x21                                       // .!
                        })
                If ((TSTA != 0xFE))
                {
                    BIFP [0x09] = RBUF /* \_SB_.RBUF */
                }
                ElseIf ((DerefOf (BF33 [One]) == 0xC1))
                {
                    BF33 = ^PCI0.BCIT (BSBN, 0x02, Zero, 0x80, Zero, Buffer (0x02)
                            {
                                 0x16, 0x21                                       // .!
                            })
                    If ((TSTA != 0xFE))
                    {
                        BF33 = ^PCI0.BCIT (BSBN, One, One, 0x80, 0x80, 0x17)
                        If (((TSTA != 0xFE) && (NOBY <= 0x20)))
                        {
                            If ((NOBY > MAXL))
                            {
                                NOBY = MAXL /* \_SB_.MAXL */
                            }

                            BF33 = ^PCI0.BCIT (BSBN, Zero, NOBY, 0x40, Zero, Zero)
                            If ((TSTA != 0xFE))
                            {
                                BIFP [0x09] = RBUF /* \_SB_.RBUF */
                            }
                        }
                    }
                }

                BF33 = ^PCI0.BCIX (BSBN, 0x02, 0x0E, Buffer (0x02)
                        {
                             0x17, 0x22                                       // ."
                        })
                If ((TSTA != 0xFE))
                {
                    BIFP [0x0B] = RBUF /* \_SB_.RBUF */
                }
                ElseIf ((DerefOf (BF33 [One]) == 0xC1))
                {
                    BF33 = ^PCI0.BCIT (BSBN, 0x02, Zero, 0x80, Zero, Buffer (0x02)
                            {
                                 0x16, 0x22                                       // ."
                            })
                    If ((TSTA != 0xFE))
                    {
                        BF33 = ^PCI0.BCIT (BSBN, One, One, 0x80, 0x80, 0x17)
                        If (((TSTA != 0xFE) && (NOBY <= 0x20)))
                        {
                            If ((NOBY > MAXL))
                            {
                                NOBY = MAXL /* \_SB_.MAXL */
                            }

                            BF33 = ^PCI0.BCIT (BSBN, Zero, NOBY, 0x40, Zero, Zero)
                            If ((TSTA != 0xFE))
                            {
                                BIFP [0x0B] = RBUF /* \_SB_.RBUF */
                            }
                        }
                    }
                }

                BF33 = ^PCI0.BCIT (BSBN, 0x02, Zero, 0x80, Zero, Buffer (0x02)
                        {
                             0x16, 0x1C                                       // ..
                        })
                If ((TSTA != 0xFE))
                {
                    BF33 = ^PCI0.BCIT (BSBN, One, 0x02, 0xC0, Zero, 0x17)
                    If ((TSTA != 0xFE))
                    {
                        BIFP [0x0A] = DC2S (RWRD)
                    }
                }

                BF33 = ^PCI0.BCIT (BSBN, 0x02, Zero, 0x80, Zero, Buffer (0x02)
                        {
                             0x16, 0x03                                       // ..
                        })
                If ((TSTA != 0xFE))
                {
                    BF33 = ^PCI0.BCIT (BSBN, One, 0x02, 0xC0, Zero, 0x17)
                    If ((DerefOf (BF33 [Zero]) != 0xFE))
                    {
                        If ((RWRD & 0x8000))
                        {
                            BIFP [Zero] = Zero
                        }
                        Else
                        {
                            BIFP [Zero] = One
                        }
                    }
                }

                BF33 = ^PCI0.BCIT (BSBN, 0x02, Zero, 0x80, Zero, Buffer (0x02)
                        {
                             0x16, 0x10                                       // ..
                        })
                If ((TSTA != 0xFE))
                {
                    BF33 = ^PCI0.BCIT (BSBN, One, 0x02, 0xC0, Zero, 0x17)
                    If ((DerefOf (BF33 [Zero]) != 0xFE))
                    {
                        BIFP [0x02] = RWRD /* \_SB_.RWRD */
                    }
                }

                BF33 = ^PCI0.BCIT (BSBN, 0x02, Zero, 0x80, Zero, Buffer (0x02)
                        {
                             0x16, 0x18                                       // ..
                        })
                If ((TSTA != 0xFE))
                {
                    BF33 = ^PCI0.BCIT (BSBN, One, 0x02, 0xC0, Zero, 0x17)
                    If ((DerefOf (BF33 [Zero]) != 0xFE))
                    {
                        BIFP [One] = RWRD /* \_SB_.RWRD */
                    }
                }

                BF33 = ^PCI0.BCIT (BSBN, 0x02, Zero, 0x80, Zero, Buffer (0x02)
                        {
                             0x16, 0x19                                       // ..
                        })
                If ((TSTA != 0xFE))
                {
                    BF33 = ^PCI0.BCIT (BSBN, One, 0x02, 0xC0, Zero, 0x17)
                    If ((DerefOf (BF33 [Zero]) != 0xFE))
                    {
                        BIFP [0x04] = RWRD /* \_SB_.RWRD */
                    }
                }

                BF33 = ^PCI0.BCIT (BSBN, 0x02, Zero, 0x80, Zero, Buffer (0x02)
                        {
                             0x16, 0x01                                       // ..
                        })
                If ((TSTA != 0xFE))
                {
                    BF33 = ^PCI0.BCIT (BSBN, One, 0x02, 0xC0, Zero, 0x17)
                    If ((DerefOf (BF33 [Zero]) != 0xFE))
                    {
                        BIFP [0x05] = RWRD /* \_SB_.RWRD */
                    }
                }
            }

            Release (MUCG)
            Return (BIFP) /* \_SB_.CBIF.BIFP */
        }

        Name (BPKG, Package (0x04)
        {
            Zero, 
            0xFFFFFFFF, 
            0xFFFFFFFF, 
            0xFFFFFFFF
        })
        Method (CBST, 1, Serialized)
        {
            Acquire (MUCG, 0xFFFF)
            Local0 = BATS ((Arg0 - One))
            Local1 = Zero
            If ((Local0 & 0x04))
            {
                Local1 |= One
            }

            If ((Local0 & 0x02))
            {
                Local1 |= 0x02
            }

            BPKG [Zero] = Local1
            BF33 = SELB (Arg0)
            If ((TSTA != 0xFE))
            {
                BF33 = ^PCI0.BCIT (BSBN, 0x02, Zero, 0x80, Zero, Buffer (0x02)
                        {
                             0x16, 0x09                                       // ..
                        })
                If ((TSTA != 0xFE))
                {
                    BF33 = ^PCI0.BCIT (BSBN, One, 0x02, 0xC0, Zero, 0x17)
                    If ((TSTA != 0xFE))
                    {
                        If ((RWRD == 0xFFFF))
                        {
                            BPKG [0x03] = 0xFFFFFFFF
                        }
                        Else
                        {
                            BPKG [0x03] = RWRD /* \_SB_.RWRD */
                        }
                    }
                }

                BF33 = ^PCI0.BCIT (BSBN, 0x02, Zero, 0x80, Zero, Buffer (0x02)
                        {
                             0x16, 0x0F                                       // ..
                        })
                If ((TSTA != 0xFE))
                {
                    BF33 = ^PCI0.BCIT (BSBN, One, 0x02, 0xC0, Zero, 0x17)
                    If ((TSTA != 0xFE))
                    {
                        If ((RWRD == 0xFFFF))
                        {
                            BPKG [0x02] = 0xFFFFFFFF
                        }
                        Else
                        {
                            BPKG [0x02] = RWRD /* \_SB_.RWRD */
                        }
                    }
                }

                BF33 = ^PCI0.BCIT (BSBN, 0x02, Zero, 0x80, Zero, Buffer (0x02)
                        {
                             0x16, 0x0A                                       // ..
                        })
                If ((TSTA != 0xFE))
                {
                    BF33 = ^PCI0.BCIT (BSBN, One, 0x02, 0xC0, Zero, 0x17)
                    If ((TSTA != 0xFE))
                    {
                        If ((RWRD & 0x8000))
                        {
                            Local0 = (RWRD & 0x7FFF)
                            Local0 = ~Local0
                            Local0++
                            Local0 &= 0x7FFF
                            If ((Local0 == One))
                            {
                                BPKG [One] = 0xFFFFFFFF
                            }
                            Else
                            {
                                BPKG [One] = Local0
                            }
                        }
                        Else
                        {
                            BPKG [One] = RWRD /* \_SB_.RWRD */
                        }
                    }
                }
            }

            Release (MUCG)
            Return (BPKG) /* \_SB_.BPKG */
        }

        Method (CBSS, 2, Serialized)
        {
            Name (TBU1, Buffer (0x04)
            {
                 0x16, 0x3D, 0x00, 0x00                           // .=..
            })
            If ((Arg0 == One))
            {
                BF33 = ^PCI0.BCIT (BSBN, 0x02, Zero, 0x80, Zero, Buffer (0x02)
                        {
                             0x16, 0x3D                                       // .=
                        })
                If ((TSTA != 0xFE))
                {
                    BF33 = ^PCI0.BCIT (BSBN, One, 0x02, 0xC0, Zero, 0x17)
                    If ((TSTA != 0xFE))
                    {
                        If (Arg1)
                        {
                            TBU1 [0x03] = (DBY2 & 0xFB)
                        }
                        Else
                        {
                            TBU1 [0x03] = (DBY2 | 0x04)
                        }

                        TBU1 [0x02] = NOBY /* \_SB_.NOBY */
                        BF33 = ^PCI0.BCIT (BSBN, 0x04, Zero, 0xC0, Zero, TBU1)
                    }
                }
            }

            If ((Arg0 == 0x03))
            {
                If ((Arg1 == Zero))
                {
                    BF33 = ^PCI0.BCIT (BSBN, 0x02, Zero, 0x80, Zero, Buffer (0x02)
                            {
                                 0x16, 0x3D                                       // .=
                            })
                    If ((TSTA != 0xFE))
                    {
                        BF33 = ^PCI0.BCIT (BSBN, One, 0x02, 0xC0, Zero, 0x17)
                        If ((TSTA != 0xFE))
                        {
                            TBU1 [0x03] = (DBY2 | 0x02)
                        }

                        TBU1 [0x02] = NOBY /* \_SB_.NOBY */
                        BF33 = ^PCI0.BCIT (BSBN, 0x04, Zero, 0xC0, Zero, TBU1)
                    }
                }
            }
        }

        Method (DC2S, 1, NotSerialized)
        {
            Name (STR1, Buffer (0x06)
            {
                "00000"
            })
            Local1 = Arg0
            Local3 = 0x04
            While ((Local1 > Zero))
            {
                Local0 = Zero
                Divide (Local1, 0x0A, Local0, Local1)
                Local0 &= 0x0F
                Local0 |= 0x30
                STR1 [Local3] = Local0
                Local3--
            }

            Return (STR1) /* \_SB_.DC2S.STR1 */
        }
    }

    Scope (_SB.PCI0.SBRG)
    {
        Device (LPCC)
        {
            Name (_HID, EisaId ("PNP0C02") /* PNP Motherboard Resources */)  // _HID: Hardware ID
            Name (_UID, 0x0678)  // _UID: Unique ID
            Name (CRS, ResourceTemplate ()
            {
                IO (Decode16,
                    0x0000,             // Range Minimum
                    0x0000,             // Range Maximum
                    0x00,               // Alignment
                    0x00,               // Length
                    _Y1D)
                IO (Decode16,
                    0x0000,             // Range Minimum
                    0x0000,             // Range Maximum
                    0x00,               // Alignment
                    0x00,               // Length
                    _Y1E)
                IO (Decode16,
                    0x0000,             // Range Minimum
                    0x0000,             // Range Maximum
                    0x00,               // Alignment
                    0x00,               // Length
                    _Y1F)
                IO (Decode16,
                    0x0000,             // Range Minimum
                    0x0000,             // Range Maximum
                    0x00,               // Alignment
                    0x00,               // Length
                    _Y20)
                IO (Decode16,
                    0x0000,             // Range Minimum
                    0x0000,             // Range Maximum
                    0x00,               // Alignment
                    0x00,               // Length
                    _Y21)
            })
            Method (_CRS, 0, NotSerialized)  // _CRS: Current Resource Settings
            {
                CreateWordField (CRS, \_SB.PCI0.SBRG.LPCC._Y1D._MIN, LDB1)  // _MIN: Minimum Base Address
                CreateWordField (CRS, \_SB.PCI0.SBRG.LPCC._Y1D._MAX, LDE1)  // _MAX: Maximum Base Address
                CreateByteField (CRS, \_SB.PCI0.SBRG.LPCC._Y1D._LEN, LDL1)  // _LEN: Length
                CreateWordField (CRS, \_SB.PCI0.SBRG.LPCC._Y1E._MIN, LDB2)  // _MIN: Minimum Base Address
                CreateWordField (CRS, \_SB.PCI0.SBRG.LPCC._Y1E._MAX, LDE2)  // _MAX: Maximum Base Address
                CreateByteField (CRS, \_SB.PCI0.SBRG.LPCC._Y1E._LEN, LDL2)  // _LEN: Length
                CreateWordField (CRS, \_SB.PCI0.SBRG.LPCC._Y1F._MIN, LDB3)  // _MIN: Minimum Base Address
                CreateWordField (CRS, \_SB.PCI0.SBRG.LPCC._Y1F._MAX, LDE3)  // _MAX: Maximum Base Address
                CreateByteField (CRS, \_SB.PCI0.SBRG.LPCC._Y1F._LEN, LDL3)  // _LEN: Length
                CreateWordField (CRS, \_SB.PCI0.SBRG.LPCC._Y20._MIN, LDB4)  // _MIN: Minimum Base Address
                CreateWordField (CRS, \_SB.PCI0.SBRG.LPCC._Y20._MAX, LDE4)  // _MAX: Maximum Base Address
                CreateByteField (CRS, \_SB.PCI0.SBRG.LPCC._Y20._LEN, LDL4)  // _LEN: Length
                CreateWordField (CRS, \_SB.PCI0.SBRG.LPCC._Y21._MIN, LGPB)  // _MIN: Minimum Base Address
                CreateWordField (CRS, \_SB.PCI0.SBRG.LPCC._Y21._MAX, LGPE)  // _MAX: Maximum Base Address
                CreateByteField (CRS, \_SB.PCI0.SBRG.LPCC._Y21._LEN, LGPL)  // _LEN: Length
                LDB1 = LR1B /* \LR1B */
                LDE1 = LR1B /* \LR1B */
                LDL1 = LR1L /* \LR1L */
                LDB2 = LR2B /* \LR2B */
                LDE2 = LR2B /* \LR2B */
                LDL2 = LR2L /* \LR2L */
                LDB3 = LR3B /* \LR3B */
                LDE3 = LR3B /* \LR3B */
                LDL3 = LR3L /* \LR3L */
                LDB4 = LR4B /* \LR4B */
                LDE4 = LR4B /* \LR4B */
                LDL4 = LR4L /* \LR4L */
                If (GPDE)
                {
                    LGPB = 0x0200
                    LGPE = 0x0200
                    LGPL = 0x10
                }

                Return (CRS) /* \_SB_.PCI0.SBRG.LPCC.CRS_ */
            }
        }
    }
}



