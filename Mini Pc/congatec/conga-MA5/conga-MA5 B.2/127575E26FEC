                    CreateWordField (CRS1, \_SB.PCI0.SBRG.SIO1._Y0B._MIN, IO11)  // _MIN: Minimum Base Address
                    CreateWordField (CRS1, \_SB.PCI0.SBRG.SIO1._Y0B._MAX, IO12)  // _MAX: Maximum Base Address
