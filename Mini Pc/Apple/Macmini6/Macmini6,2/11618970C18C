                    }

                    Return (PR06 ())
                }
            }

            Scope (\_SB.PCI0.RP01)
            {
                OperationRegion (A1E0, PCI_Config, 0x00, 0x60)
                Field (A1E0, ByteAcc, NoLock, Preserve)
                {
                    Offset (0x04), 
                    BMIE,   3, 
                    Offset (0x19), 
                    SECB,   8, 
                    SBBN,   8, 
                    Offset (0x1E), 
                        ,   13, 
                    MABT,   1, 
                    Offset (0x4A), 
                        ,   5, 
                    TPEN,   1, 
                    Offset (0x50), 
                        ,   4, 
                    LDIS,   1, 
                        ,   24, 
                    LACT,   1
                }

                Method (_BBN, 0, NotSerialized)  // _BBN: BIOS Bus Number
                {
                    If (((BMIE == 0x00) && (SECB == 0xFF)))
                    {
                        Return (SNBS) /* \_SB_.PCI0.RP01.SNBS */
                    }
                    Else
                    {
                        Return (SECB) /* \_SB_.PCI0.RP01.SECB */
                    }
                }

                Method (_STA, 0, NotSerialized)  // _STA: Status
                {
                    Return (0x0F)
                }

                Name (BMIS, 0x00)
                Name (SNBS, 0x00)
                Name (SOBS, 0x00)
                Name (LPFL, 0x00)
                Method (C4PU, 0, Serialized)
                {
                    SBBN = SOBS /* \_SB_.PCI0.RP01.SOBS */
                    SECB = SNBS /* \_SB_.PCI0.RP01.SNBS */
                    BMIE = BMIS /* \_SB_.PCI0.RP01.BMIS */
                    LDIS = 0x00
                    Local0 = 0x00
                    LPFL = (LPFL | 0x20)
                    GP49 = 0x00
                    While (0x01)
                    {
                        Sleep (0x05)
                        Sleep (0x64)
                        Local1 = 0x00
                        Local2 = (Timer + 0x00989680)
                        While ((Timer <= Local2))
                        {
                            If (((LACT == 0x01) && ((\_SB.PCI0.RP01.GIGE.AVND != 0xFFFF) && (
                                \_SB.PCI0.RP01.SDXC.AVND != 0xFFFF))))
                            {
                                Local1 = 0x01
                                Break
                            }

                            Sleep (0x0A)
                        }

                        If ((Local1 == 0x01))
                        {
                            LPFL = (LPFL & ~0x20)
                            MABT = 0x01
                            Break
                        }

                        If ((Local0 == 0x04))
                        {
                            LPFL = (LPFL | (0x20 | 0x80))
                            Break
                        }

                        Local0++
                        GP49 = 0x01
                        Sleep (0x64)
                        GP49 = 0x00
                    }

                    Return (Zero)
                }

                Method (C4PD, 0, Serialized)
                {
                    LPFL = (LPFL | 0x10)
                    BMIS = BMIE /* \_SB_.PCI0.RP01.BMIE */
                    SNBS = SECB /* \_SB_.PCI0.RP01.SECB */
                    SOBS = SBBN /* \_SB_.PCI0.RP01.SBBN */
                    BMIE = 0x00
                    SECB = 0xFF
                    SBBN = 0xFE
                    Local0 = TPEN /* \_SB_.PCI0.RP01.TPEN */
                    LDIS = 0x01
                    Local0 = (Timer + 0x00989680)
                    While ((Timer <= Local0))
                    {
                        If ((LACT == 0x00))
                        {
                            LPFL = (LPFL & ~0x10)
                            Break
                        }

                        Sleep (0x0A)
                    }

                    If ((LPFL & 0x10))
                    {
                        LPFL = (LPFL | (0x10 | 0x40))
                    }

                    GP49 = 0x01
                    Sleep (0x32)
                }

                Method (C4LP, 0, Serialized)
                {
                    If (!OSDW ())
                    {
                        Return (Zero)
                    }

                    If ((((LPFL & 0x04) == 0x00) && ((LPFL & 0x03
                        ) == 0x03)))
                    {
                        LPFL = (LPFL & ~0x10)
                        C4PD ()
                        LPFL = (LPFL | 0x04)
                        Return (Zero)
                    }

                    If ((((LPFL & 0x04) == 0x04) && ((LPFL & 
                        0x03) != 0x03)))
                    {
                        LPFL = (LPFL & ~0x20)
                        C4PU ()
                        LPFL = (LPFL & ~0x04)
                        Return (Zero)
                    }

                    Return (Zero)
                }

                Device (GIGE)
                {
                    Name (_ADR, 0x00)  // _ADR: Address
                    OperationRegion (ARE0, PCI_Config, 0x00, 0x04)
                    Field (ARE0, ByteAcc, NoLock, Preserve)
                    {
                        AVND,   16
                    }

                    Method (_STA, 0, NotSerialized)  // _STA: Status
                    {
                        Return (0x0F)
                    }

                    Method (_PRW, 0, NotSerialized)  // _PRW: Power Resources for Wake
                    {
                        If (OSDW ())
                        {
                            Return (Package (0x02)
                            {
                                0x09, 
                                0x04
                            })
                        }
                        Else
                        {
                            Return (Package (0x02)
                            {
                                0x09, 
                                0x03
                            })
                        }
                    }

                    Method (_RMV, 0, NotSerialized)  // _RMV: Removal Status
                    {
                        Return (0x00)
                    }

                    Method (_PSW, 1, NotSerialized)  // _PSW: Power State Wake
                    {
                        If (Arg0)
                        {
                            GP12 = 0x01
                        }
                        Else
                        {
                            GP12 = 0x00
                        }
                    }

                    Name (_GPE, 0x1D)  // _GPE: General Purpose Events
                    Method (ELPR, 1, Serialized)
                    {
                        If (Arg0)
                        {
                            LPFL = (LPFL | 0x01)
                        }
                        Else
                        {
                            LPFL = (LPFL & ~0x01)
                        }

                        C4LP ()
                    }

                    Method (EDET, 0, NotSerialized)
                    {
                        Return (GP13) /* \GP13 */
                    }
                }

                Device (SDXC)
                {
                    Name (_ADR, 0x01)  // _ADR: Address
                    Method (_PRW, 0, NotSerialized)  // _PRW: Power Resources for Wake
                    {
                        If (OSDW ())
                        {
                            Return (Package (0x02)
                            {
                                0x1E, 
