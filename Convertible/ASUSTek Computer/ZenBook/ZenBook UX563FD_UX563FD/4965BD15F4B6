            {
                If ((S2DE == One))
                {
                    Notify (\_SB.PCI0.LPCB.EC0.SEN2, 0x90) // Device-Specific
                }
            }

            If ((Local0 & 0x02))
            {
                If ((S2DE == One))
                {
                    Notify (\_SB.PCI0.LPCB.EC0.SEN2, 0x90) // Device-Specific
                }
            }

            \_SB.PCI0.LPCB.EC0.WP2E (0x78, Zero)
        }

        Method (_QE5, 0, NotSerialized)  // _Qxx: EC Query, xx=0x00-0xFF
        {
