            ADBG ("PR16 DEP Call")
            If (((S0ID == One) || (OSYS >= 0x07DF)))
            {
                ADBG ("PR16 DEP")
                Return (Package (0x01)
                {
                    PEPD
                })
            }
            Else
            {
                ADBG ("PR16 DEP NULL")
                Return (Package (0x00) {})
            }
        }
    }

