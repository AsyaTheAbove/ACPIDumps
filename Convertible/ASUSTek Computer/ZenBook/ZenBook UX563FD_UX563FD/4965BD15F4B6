        }

        Method (UVTH, 1, Serialized)
        {
            If ((\_SB.PCI0.LPCB.H_EC.ECAV == One))
            {
                Local0 = Arg0
                \_SB.PCI0.LPCB.H_EC.ECWT (Arg0, RefOf (\_SB.PCI0.LPCB.H_EC.UVTH))
