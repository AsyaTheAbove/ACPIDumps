                Method (_STA, 0, NotSerialized)  // _STA: Status
                {
                    If ((\_SB.PCI0.PATA.VID != 0x1106))
                    {
                        Return (0x00)
                    }
                    Else
                    {
                        PMEX ()
                        If ((\_SB.PCI0.PATA.CMDR == 0x00))
                        {
                            Return (0x0D)
                        }
                        Else
                        {
                            Return (0x0F)
                        }
                    }
                }

                Method (_REG, 2, NotSerialized)  // _REG: Region Availability
                {
                    If ((Arg0 == 0x02))
                    {
                        REGF = Arg1
                    }
                }

                Name (TIM0, Package (0x07)
                {
                    Package (0x05)
                    {
                        0x78, 
                        0xB4, 
                        0xF0, 
                        0x017F, 
