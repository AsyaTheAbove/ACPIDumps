    Method (SPKR, 1, NotSerialized)
    {
        Local0 = S1B /* \S1B_ */
        CTRL = 0xB6
        CNT2 = 0x55
        CNT2 = 0x03
        Local2 = Arg0
        While ((Local2 > 0x00))
        {
            S1B |= 0x03
            Local3 = 0x5FFF
            While ((Local3 > 0x00))
            {
                Local3--
            }

            S1B &= 0xFC
            Local3 = 0x0EFF
            While ((Local3 > 0x00))
            {
                Local3--
            }

            Local2--
        }

        S1B = Local0
    }

    Scope (\)
    {
        Name (PICF, 0x00)
        Method (_PIC, 1, NotSerialized)  // _PIC: Interrupt Model
        {
            PICF = Arg0
        }
