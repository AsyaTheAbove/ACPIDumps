                }

                Method (_DIS, 0, NotSerialized)  // _DIS: Disable Device
                {
                    PIRG = Zero
                }

                Method (_CRS, 0, NotSerialized)  // _CRS: Current Resource Settings
                {
                    Name (BUFG, ResourceTemplate ()
                    {
                        IRQ (Level, ActiveLow, Shared, _Y09)
                            {}
                    })
                    CreateByteField (BUFG, \_SB.PCI0.LNK0._CRS._Y09._INT, IRG1)  // _INT: Interrupts
                    CreateByteField (BUFG, 0x02, IRG2)
                    IRG1 = Zero
                    IRG2 = Zero
                    Local0 = PIRG /* \PIRG */
                    If ((Local0 > 0x00))
                    {
                        If ((Local0 > 0x07))
                        {
                            Local0 -= 0x08
                            IRG2 = (One << Local0)
                        }
                        Else
                        {
                            Local1 = 0x01
                            IRG1 = (Local1 << Local0)
                        }
                    }

                    Return (BUFG) /* \_SB_.PCI0.LNK0._CRS.BUFG */
                }

                Method (_SRS, 1, NotSerialized)  // _SRS: Set Resource Settings
                {
                    CreateByteField (Arg0, 0x01, IRG1)
                    CreateByteField (Arg0, 0x02, IRG2)
                    If ((IRG2 > Zero))
                    {
                        FindSetLeftBit (IRG2, Local0)
                        Local0--
                        Local0 += 0x08
                    }
                    Else
                    {
                        FindSetLeftBit (IRG1, Local0)
                        Local0--
                    }

                    ENR8 = One
                    POLG = Zero
                    PIRG = Local0
                }
            }

            Device (LNK1)
