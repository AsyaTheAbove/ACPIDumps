                            {}
                    })
                    CreateByteField (BUFA, \_SB.PCI0.LNKB._CRS._Y04._INT, IRA1)  // _INT: Interrupts
                    CreateByteField (BUFA, 0x02, IRA2)
                    Local3 = 0x00
                    Local4 = 0x00
                    Local1 = (PIBC & 0x0F)
                    If ((Local1 != 0x00))
                    {
                        If ((Local1 > 0x07))
                        {
                            Local2 = (Local1 - 0x08)
                            Local4 = (One << Local2)
                        }
                        ElseIf ((Local1 > 0x00))
                        {
                            Local3 = (One << Local1)
                        }

                        IRA1 = Local3
                        IRA2 = Local4
                    }

                    Return (BUFA) /* \_SB_.PCI0.LNKB._CRS.BUFA */
                }

                Method (_SRS, 1, NotSerialized)  // _SRS: Set Resource Settings
                {
                    CreateByteField (Arg0, 0x01, IRA1)
                    CreateByteField (Arg0, 0x02, IRA2)
                    Local0 = (IRA2 << 0x08)
                    Local0 |= IRA1 /* \_SB_.PCI0.LNKB._SRS.IRA1 */
                    Local1 = 0x00
                    Local0 >>= 0x01
                    While ((Local0 > 0x00))
                    {
                        Local1++
                        Local0 >>= 0x01
                    }

                    If ((OSFL == 0x02))
                    {
                        If (ENR8)
                        {
                            PIRF = Local1
                        }
                    }

                    PIBC &= 0xF0
                    PIBC |= Local1
                }
            }

            Device (LNKC)
            {
                Name (_HID, EisaId ("PNP0C0F") /* PCI Interrupt Link Device */)  // _HID: Hardware ID
                Name (_UID, 0x03)  // _UID: Unique ID
                Method (_STA, 0, NotSerialized)  // _STA: Status
                {
                    Local0 = (PIBC & 0xF0)
                    If ((Local0 == 0x00))
                    {
                        Return (0x09)
                    }
                    Else
                    {
                        Return (0x0B)
                    }
