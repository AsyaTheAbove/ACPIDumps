                        0x00, 
                        0x16
                    }, 

                    Package (0x04)
                    {
                        0x0007FFFF, 
                        0x03, 
                        0x00, 
                        0x17
                    }
                })
                OperationRegion (DFSR, SystemMemory, 0x000F7A82, 0x01)
                Field (DFSR, DWordAcc, NoLock, Preserve)
                {
                    DFSN,   8
                }

                Method (_PRT, 0, NotSerialized)  // _PRT: PCI Routing Table
                {
                    Local1 = DFSN /* \_SB_.PCI0.P2PB.DFSN */
                    If ((Local1 != 0xFF))
                    {
                        Local0 = Zero
                        Local4 = One
                        While (Local4)
                        {
                            Local2 = DerefOf (DerefOf (PICM [Local0]) [0x00])
                            Local0++
                            Local2 >>= 0x10
                            If ((Local2 == Local1))
                            {
                                Local3 = 0x000FFFFF
                                DerefOf (PICM [Local0]) [0x00] = Local3
                                DerefOf (PICM [Local0]) [0x01] = Zero
                                Local4 = Zero
                            }

                            Local2 = SizeOf (PICM)
                            If ((Local0 >= Local2))
                            {
                                Local4 = Zero
                            }
                        }

                        Local0 = Zero
                        Local4 = One
                        While (Local4)
                        {
                            Local2 = DerefOf (DerefOf (APIC [Local0]) [0x00])
                            Local0++
                            Local2 >>= 0x10
                            If ((Local2 == Local1))
                            {
                                Local3 = 0x000FFFFF
                                DerefOf (APIC [Local0]) [0x00] = Local3
                                DerefOf (APIC [Local0]) [0x01] = Zero
                                Local4 = Zero
                            }

                            Local2 = SizeOf (APIC)
                            If ((Local0 >= Local2))
                            {
                                Local4 = Zero
                            }
                        }
                    }

                    If (!PICF)
                    {
                        Return (PICM) /* \_SB_.PCI0.P2PB.PICM */
                    }
                    Else
                    {
                        Return (APIC) /* \_SB_.PCI0.P2PB.APIC */
                    }
                }
            }

