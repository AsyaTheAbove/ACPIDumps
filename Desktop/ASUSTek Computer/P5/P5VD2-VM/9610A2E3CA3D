                    Offset (0x04), 
                    CMDR,   3, 
                    Offset (0x3C), 
                    U3IR,   4, 
                    Offset (0x84), 
                    ECDX,   2
                }

                Method (_STA, 0, NotSerialized)  // _STA: Status
                {
                    If ((\_SB.PCI0.USB1.VID != 0x1106))
                    {
                        Return (0x00)
                    }
                    ElseIf ((\_SB.PCI0.USB1.CMDR == 0x00))
                    {
                        Return (0x0D)
                    }
                    Else
                    {
                        Return (0x0F)
                    }
                }
