    0050: AF B4 9F 68 82 23 EC 50 40 5A 73 7F EC E4 07 CB  // ...h.#.P@Zs.....
    0060: DC 25 1A 9C E3 E3 66 11 E0 A5 98 06 C5 80 0A FA  // .%....f.........
    0070: 42 93 86 98 E7 D5 1B D4 D7 3A A4 0B EE E2 7D BE  // B........:....}.
    0080: 5F 5B 15 0C AB D0 21 DE BF E9 B5 6E A4 57 B9 8C  // _[....!....n.W..
    0090: 0C D2 BA 3A 69 30 76 94 71 A2 64 D7 4C D8 85 BF  // ...:i0v.q.d.L...
    00A0: DF A5 6A C8 DC 45 D5 4D 8C B8 8C 05 2F FC 2E 23  // ..j..E.M..../..#
    00B0: C4 29 C5 6F 3F 29 6C 6D 57 79 0E B6 75 ED 21 95  // .).o?)lmWy..u.!.
    00C0: 01 00 00 00 B6 00 00 00 00 00 02 00 44 45 4C 4C  // ............DELL
    00D0: 20 20 46 58 30 39 20 20 20 00 57 49 4E 44 4F 57  //   FX09   .WINDOW
    00E0: 53 20 00 00 00 00 00 00 00 00 00 00 00 00 00 00  // S ..............
    00F0: 00 00 00 00 00 00 76 C9 FF BD 36 EF AD 55 34 02  // ......v...6..U4.
    0100: F0 0E 8F 66 61 D1 3B 53 B5 3E 74 FC 17 FF 99 46  // ...fa.;S.>t....F
    0110: DD D5 90 37 F6 A7 B1 FB B0 94 00 EA 69 7E 60 2D  // ...7........i~`-
    0120: F2 03 0E 9F 0F 94 09 17 25 C4 A2 72 CF 94 D4 EA  // ........%..r....
    0130: CF B4 7F C4 2D B5 53 D9 C4 47 29 FE 98 F8 4A 20  // ....-.S..G)...J 
    0140: 75 89 14 2B D2 FE 8C BF 4F 11 08 20 00 64 EB E9  // u..+....O.. .d..
    0150: 91 3A 23 4D 84 5A E5 52 0B 83 DB 52 EF DA 64 60  // .:#M.Z.R...R..d`
    0160: FF 9E B9 50 C6 DD B9 21 64 93 FA 85 53 71 29 0A  // ...P...!d...Sq).
    0170: 65 2F 37 AD 11 24                                // e/7..$


SSDT1
-----
DefinitionBlock ("", "SSDT", 1, "PmRef", "CpuPm", 0x00003000)
{
    External (_PR_.CPU0, DeviceObj)
    External (_PR_.CPU1, DeviceObj)
    External (_PR_.CPU2, DeviceObj)
    External (_PR_.CPU3, DeviceObj)

    Scope (\)
    {
        Name (SSDT, Package (0x1E)
        {
            "CPU0IST ", 
            0xCF5E7680, 
            0x0000026C, 
            "CPU1IST ", 
            0xCF5E7B40, 
            0x00000152, 
            "CPU0CST ", 
            0x00000000, 
            0xF000E819, 
            "CPU1CST ", 
            0x00000000, 
            0xF000E819, 
            "CPU2IST ", 
            0x00000000, 
            0xF000E819, 
            "CPU3IST ", 
            0x00000000, 
            0xF000E819, 
            "CPU4IST ", 
            0x00000000, 
            0xF000E819, 
            "CPU5IST ", 
            0x00000000, 
            0xF000E819, 
            "CPU6IST ", 
            0x00000000, 
            0xF000E819, 
            "CPU7IST ", 
            0x00000000, 
            0xF000E819
        })
        Name (CFGD, 0x420583F2)
        Name (\PDC0, 0x80000000)
        Name (\PDC1, 0x80000000)
        Name (\PDC2, 0x80000000)
        Name (\PDC3, 0x80000000)
        Name (\PDC4, 0x80000000)
        Name (\PDC5, 0x80000000)
        Name (\PDC6, 0x80000000)
        Name (\PDC7, 0x80000000)
        Name (\SDTL, 0x00)
    }

    Scope (\_PR.CPU0)
    {
        Name (HI0, 0x00)
        Name (HC0, 0x00)
        Name (TLD0, 0x00)
        Method (_PDC, 1, NotSerialized)  // _PDC: Processor Driver Capabilities
        {
            CreateDWordField (Arg0, 0x08, CAP0)
            PDC0 = CAP0 /* \_PR_.CPU0._PDC.CAP0 */
            If ((TLD0 == 0x00))
            {
                If (((PDC0 & 0x0A) == 0x0A))
                {
                    If ((CFGD & 0x02))
                    {
                        OperationRegion (IST0, SystemMemory, DerefOf (SSDT [0x01]), DerefOf (SSDT [0x02]))
                        Load (IST0, HI0) /* \_PR_.CPU0.HI0_ */
                    }

                    TLD0 = 0x01
                }
            }
        }
    }

    Scope (\_PR.CPU1)
    {
        Name (HI1, 0x00)
        Name (HC1, 0x00)
        Name (TLD1, 0x00)
        Method (_PDC, 1, NotSerialized)  // _PDC: Processor Driver Capabilities
        {
            CreateDWordField (Arg0, 0x08, CAP1)
            PDC1 = CAP1 /* \_PR_.CPU1._PDC.CAP1 */
            If ((TLD1 == 0x00))
            {
                If (((PDC1 & 0x0A) == 0x0A))
                {
                    If ((CFGD & 0x02))
                    {
                        OperationRegion (IST1, SystemMemory, DerefOf (SSDT [0x04]), DerefOf (SSDT [0x05]))
                        Load (IST1, HI1) /* \_PR_.CPU1.HI1_ */
                    }

                    TLD1 = 0x01
                }
            }
        }
    }

    Scope (\_PR.CPU2)
    {
        Name (HI2, 0x00)
        Name (HC2, 0x00)
        Name (TLD2, 0x00)
        Method (_PDC, 1, NotSerialized)  // _PDC: Processor Driver Capabilities
        {
            CreateDWordField (Arg0, 0x08, CAP2)
            PDC2 = CAP2 /* \_PR_.CPU2._PDC.CAP2 */
            If ((TLD2 == 0x00))
            {
                If (((PDC2 & 0x0A) == 0x0A))
                {
                    If ((CFGD & 0x02))
                    {
                        OperationRegion (IST2, SystemMemory, DerefOf (SSDT [0x0D]), DerefOf (SSDT [0x0E]))
                        Load (IST2, HI2) /* \_PR_.CPU2.HI2_ */
                    }

                    TLD2 = 0x01
                }
            }
        }
    }

    Scope (\_PR.CPU3)
    {
        Name (HI3, 0x00)
        Name (HC3, 0x00)
        Name (TLD3, 0x00)
        Method (_PDC, 1, NotSerialized)  // _PDC: Processor Driver Capabilities
        {
            CreateDWordField (Arg0, 0x08, CAP3)
            PDC3 = CAP3 /* \_PR_.CPU3._PDC.CAP3 */
            If ((TLD3 == 0x00))
            {
                If (((PDC3 & 0x0A) == 0x0A))
                {
                    If ((CFGD & 0x02))
                    {
                        OperationRegion (IST3, SystemMemory, DerefOf (SSDT [0x10]), DerefOf (SSDT [0x11]))
                        Load (IST3, HI3) /* \_PR_.CPU3.HI3_ */
                    }

                    TLD3 = 0x01
                }
            }
        }
    }
}



SSDT2
-----
DefinitionBlock ("", "SSDT", 1, "PmRef", "Cpu0Ist", 0x00003000)
{
    External (_PR_.CPU0, DeviceObj)
    External (CFGD, UnknownObj)

    Scope (\_PR.CPU0)
    {
        Method (_PPC, 0, NotSerialized)  // _PPC: Performance Present Capabilities
        {
            Return (0x00)
        }

        Method (_PCT, 0, NotSerialized)  // _PCT: Performance Control
        {
            If (((CFGD & 0x00060000) == 0x00020000))
            {
                Return (Package (0x02)
                {
                    ResourceTemplate ()
                    {
                        Register (SystemIO, 
                            0x10,               // Bit Width
                            0x00,               // Bit Offset
                            0x0000000000000880, // Address
                            ,)
                    }, 

                    ResourceTemplate ()
                    {
                        Register (SystemIO, 
                            0x10,               // Bit Width
                            0x00,               // Bit Offset
                            0x0000000000000882, // Address
                            ,)
                    }
                })
            }

            If (((CFGD & 0x00060000) == 0x00040000))
            {
                Return (Package (0x02)
                {
                    ResourceTemplate ()
                    {
                        Register (FFixedHW, 
                            0x00,               // Bit Width
                            0x00,               // Bit Offset
                            0x0000000000000000, // Address
                            ,)
                    }, 

                    ResourceTemplate ()
                    {
                        Register (FFixedHW, 
                            0x00,               // Bit Width
                            0x00,               // Bit Offset
                            0x0000000000000000, // Address
                            ,)
                    }
                })
            }

            If (((CFGD & 0x4000) || (CFGD & 0x00010000)))
            {
                Return (Package (0x02)
                {
                    ResourceTemplate ()
                    {
                        Register (FFixedHW, 
                            0x00,               // Bit Width
                            0x00,               // Bit Offset
                            0x0000000000000000, // Address
                            ,)
                    }, 

                    ResourceTemplate ()
                    {
                        Register (FFixedHW, 
                            0x00,               // Bit Width
                            0x00,               // Bit Offset
                            0x0000000000000000, // Address
                            ,)
                    }
                })
            }

            Return (Package (0x02)
            {
                ResourceTemplate ()
                {
                    Register (SystemIO, 
                        0x10,               // Bit Width
                        0x00,               // Bit Offset
                        0x0000000000000880, // Address
                        ,)
                }, 

                ResourceTemplate ()
                {
                    Register (SystemIO, 
                        0x10,               // Bit Width
                        0x00,               // Bit Offset
                        0x0000000000000882, // Address
                        ,)
                }
            })
        }

        Method (_PSS, 0, NotSerialized)  // _PSS: Performance Supported States
        {
            If (((CFGD & 0x00060000) == 0x00020000))
            {
                Return (SPSS) /* \_PR_.CPU0.SPSS */
            }

            If (((CFGD & 0x00060000) == 0x00040000))
            {
                Return (NPSS) /* \_PR_.CPU0.NPSS */
            }

            If (((CFGD & 0x4000) || (CFGD & 0x00010000)))
            {
                Return (NPSS) /* \_PR_.CPU0.NPSS */
            }

            Return (SPSS) /* \_PR_.CPU0.SPSS */
        }

        Name (SPSS, Package (0x03)
        {
            Package (0x06)
            {
                0x00000898, 
                0x000157C0, 
                0x000000A0, 
                0x0000000A, 
                0x00000036, 
                0x00000000
            }, 

            Package (0x06)
            {
                0x00000640, 
                0x000105B8, 
                0x000000A0, 
                0x0000000A, 
                0x00000136, 
                0x00000001
            }, 

            Package (0x06)
            {
                0x000004B0, 
                0x0000D6D8, 
                0x000000A0, 
                0x0000000A, 
                0x00000236, 
                0x00000002
            }
        })
        Name (NPSS, Package (0x03)
        {
            Package (0x06)
            {
                0x00000898, 
                0x000157C0, 
                0x0000000A, 
                0x0000000A, 
                0x00000B27, 
                0x00000B27
            }, 

            Package (0x06)
            {
                0x00000640, 
                0x000105B8, 
                0x0000000A, 
                0x0000000A, 
                0x00000821, 
                0x00000821
            }, 

            Package (0x06)
            {
                0x000004B0, 
                0x0000D6D8, 
                0x0000000A, 
                0x0000000A, 
                0x0000061D, 
                0x0000061D
            }
        })
    }
}



