    Name (WOLA, One)
    Scope (_SB.PCI0.SBRG)
    {
        Method (RGPM, 0, NotSerialized)
        {
            Local0 = ^^SMBS.GPXC /* \_SB_.PCI0.SMBS.GPXC */
            Local0 &= 0xFFFFFFFFFFFFFF3F
            ^^SMBS.GPXC = Local0
            Local1 = ^^SMBS.CLGP /* \_SB_.PCI0.SMBS.CLGP */
            Return (Local1)
        }
    }

    Scope (_GPE)
    {
        Name (WLAN, One)
        Name (WLST, One)
        Method (_L1A, 0, NotSerialized)  // _Lxx: Level-Triggered GPE
        {
            If (WOLA)
            {
                Notify (\_SB.PCI0.BR14, 0x02) // Device Wake
                Notify (\_SB.PCI0.PCE5, 0x02) // Device Wake
                Notify (\_SB.PCI0.PCE6, 0x02) // Device Wake
                Notify (\_SB.PWRB, 0x02) // Device Wake
            }
        }
    }

    Scope (_SB.PCI0.BR14)
    {
        Method (_PRW, 0, NotSerialized)  // _PRW: Power Resources for Wake
        {
            If (WOLA)
            {
                Return (GPRW (0x1A, 0x04))
            }
            Else
            {
                Return (GPRW (0x1A, One))
            }
        }
    }

    Scope (_SB.PCI0.PCE5)
