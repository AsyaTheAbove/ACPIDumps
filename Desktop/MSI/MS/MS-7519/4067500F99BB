            Device (P0P4)
            {
                Name (_ADR, 0x001C0000)  // _ADR: Address
                Method (_PRW, 0, NotSerialized)  // _PRW: Power Resources for Wake
                {
                    Return (GPRW (0x09, 0x04))
                }

                Method (_PRT, 0, NotSerialized)  // _PRT: PCI Routing Table
                {
                    If (PICM)
                    {
                        Return (AR04) /* \_SB_.AR04 */
                    }

                    Return (PR04) /* \_SB_.PR04 */
                }

                Device (JMB0)
                {
                    Name (_ADR, Zero)  // _ADR: Address
                    OperationRegion (CF40, PCI_Config, 0x40, 0x04)
                    Field (CF40, ByteAcc, NoLock, Preserve)
                    {
                        CHE0,   1, 
                        MULT,   1, 
                            ,   1, 
                        CAB0,   1, 
                        CHE1,   1, 
                        Offset (0x01), 
                        AHEN,   1, 
                            ,   3, 
                        PRT0,   1, 
                        AHM0,   1, 
                        PRT1,   1, 
                        AHM1,   1, 
                        CF42,   6, 
                        SWAP,   1, 
                        PATA,   1, 
                            ,   6, 
                        WTEN,   1, 
                        Offset (0x04)
                    }

                    Name (PIOT, Package (0x05)
                    {
                        0x0258, 
                        0x0186, 
                        0x014A, 
                        0xB4, 
                        0x78
                    })
                    Name (UDMA, Package (0x07)
                    {
                        0x78, 
                        0x50, 
                        0x3C, 
                        0x28, 
                        0x1E, 
                        0x14, 
                        0x0F
                    })
                    Name (MDMA, Package (0x03)
                    {
                        0x01E0, 
                        0x96, 
                        0x78
                    })
                    Name (IDEB, Buffer (0x14){})
                    CreateDWordField (IDEB, Zero, GTM0)
                    CreateDWordField (IDEB, 0x04, GTM1)
                    CreateDWordField (IDEB, 0x08, GTM2)
                    CreateDWordField (IDEB, 0x0C, GTM3)
                    CreateDWordField (IDEB, 0x10, GTM4)
                    Name (PIO0, 0x04)
                    Name (DMA0, 0x06)
                    Name (MDA0, 0x02)
                    Name (PIO1, 0x04)
                    Name (DMA1, 0x06)
                    Name (MDA1, 0x02)
                    Name (PIO2, 0x04)
                    Name (DMA2, 0x06)
                    Name (MDA2, 0x02)
                    Name (PIO3, 0x04)
                    Name (DMA3, 0x06)
                    Name (MDA3, 0x02)
                    Name (FLGP, 0x1F)
                    Name (FLGS, 0x1F)
                    Device (IDE0)
                    {
                        Name (_ADR, Zero)  // _ADR: Address
                        Method (_GTM, 0, NotSerialized)  // _GTM: Get Timing Mode
                        {
                            Local0 = DerefOf (PIOT [PIO0])
                            Local2 = DerefOf (PIOT [PIO1])
                            Local4 = 0x1A
                            If ((FLGP && One))
                            {
                                If (!MULT)
                                {
                                    If ((CAB0 && SWAP))
                                    {
                                        DMA0 = 0x02
                                    }
                                }

                                Local1 = DerefOf (UDMA [DMA0])
                                Local4 |= One
                            }
                            Else
                            {
                                Local1 = DerefOf (MDMA [MDA0])
                            }

                            If ((FLGP && 0x04))
                            {
                                If ((CAB0 && SWAP))
                                {
                                    DMA1 = 0x02
                                }

                                Local3 = DerefOf (UDMA [DMA1])
                                Local4 |= 0x04
                            }
                            Else
                            {
                                Local3 = DerefOf (MDMA [MDA1])
                            }

                            GTM0 = Local0
                            GTM1 = Local1
                            GTM2 = Local2
                            GTM3 = Local3
                            GTM4 = Local4
                            Return (IDEB) /* \_SB_.PCI0.P0P4.JMB0.IDEB */
                        }

                        Method (_STM, 3, NotSerialized)  // _STM: Set Timing Mode
                        {
                            IDEB = Arg0
                            Local0 = GTM0 /* \_SB_.PCI0.P0P4.JMB0.GTM0 */
                            Local1 = GTM1 /* \_SB_.PCI0.P0P4.JMB0.GTM1 */
                            Local2 = GTM2 /* \_SB_.PCI0.P0P4.JMB0.GTM2 */
                            Local3 = GTM3 /* \_SB_.PCI0.P0P4.JMB0.GTM3 */
                            Local4 = GTM4 /* \_SB_.PCI0.P0P4.JMB0.GTM4 */
                            FLGP = Local4
                            If (((Local0 != Ones) && (Local0 != Zero)))
                            {
                                PIO0 = Match (PIOT, MEQ, Local0, MTR, Zero, Zero)
                            }

                            If (((Local1 != Ones) && (Local1 != Zero)))
                            {
                                If ((FLGP && One))
                                {
                                    DMA0 = Match (UDMA, MEQ, Local1, MTR, Zero, Zero)
                                }
                                Else
                                {
                                    MDA0 = Match (MDMA, MEQ, Local1, MTR, Zero, Zero)
                                }
                            }

                            If (((Local2 != Ones) && (Local2 != Zero)))
                            {
                                PIO1 = Match (PIOT, MEQ, Local2, MTR, Zero, Zero)
                            }

                            If (((Local3 != Ones) && (Local3 != Zero)))
                            {
                                If ((FLGP && 0x04))
                                {
                                    DMA1 = Match (UDMA, MEQ, Local3, MTR, Zero, Zero)
                                }
                                Else
                                {
                                    MDA1 = Match (MDMA, MEQ, Local3, MTR, Zero, Zero)
                                }
                            }
                        }

                        Device (DRV0)
                        {
                            Name (_ADR, Zero)  // _ADR: Address
                            Method (_GTF, 0, NotSerialized)  // _GTF: Get Task File
                            {
                                Local0 = Buffer (0x07)
                                    {
                                         0x03, 0x00, 0x00, 0x00, 0x00, 0xA0, 0xEF         // .......
                                    }
                                Local1 = Buffer (0x07)
                                    {
                                         0x03, 0x00, 0x00, 0x00, 0x00, 0xA0, 0xEF         // .......
                                    }
                                CreateByteField (Local0, One, PIOM)
                                CreateByteField (Local1, One, DMAM)
                                PIOM = PIO0 /* \_SB_.PCI0.P0P4.JMB0.PIO0 */
                                PIOM |= 0x08
                                If ((FLGP && One))
                                {
                                    DMAM = DMA0 /* \_SB_.PCI0.P0P4.JMB0.DMA0 */
                                    DMAM |= 0x40
                                }
                                Else
                                {
                                    DMAM = MDA0 /* \_SB_.PCI0.P0P4.JMB0.MDA0 */
                                    DMAM |= 0x20
                                }

                                Concatenate (Local0, Local1, Local2)
                                Return (Local2)
                            }
                        }

                        Device (DRV1)
                        {
                            Name (_ADR, One)  // _ADR: Address
                            Method (_GTF, 0, NotSerialized)  // _GTF: Get Task File
                            {
                                Local0 = Buffer (0x07)
                                    {
                                         0x03, 0x00, 0x00, 0x00, 0x00, 0xB0, 0xEF         // .......
                                    }
                                Local1 = Buffer (0x07)
                                    {
                                         0x03, 0x00, 0x00, 0x00, 0x00, 0xB0, 0xEF         // .......
                                    }
                                CreateByteField (Local0, One, PIOM)
                                CreateByteField (Local1, One, DMAM)
                                PIOM = PIO1 /* \_SB_.PCI0.P0P4.JMB0.PIO1 */
                                PIOM |= 0x08
                                If ((FLGP && 0x04))
                                {
                                    DMAM = DMA1 /* \_SB_.PCI0.P0P4.JMB0.DMA1 */
                                    DMAM |= 0x40
                                }
                                Else
                                {
                                    DMAM = MDA1 /* \_SB_.PCI0.P0P4.JMB0.MDA1 */
                                    DMAM |= 0x20
                                }

                                Concatenate (Local0, Local1, Local2)
                                Return (Local2)
                            }
                        }
                    }

                    Device (IDE1)
                    {
                        Name (_ADR, One)  // _ADR: Address
                        Method (_GTM, 0, NotSerialized)  // _GTM: Get Timing Mode
                        {
                            Local0 = DerefOf (PIOT [PIO2])
                            Local2 = DerefOf (PIOT [PIO3])
                            Local4 = 0x1A
                            If ((FLGS && One))
                            {
                                If (!MULT)
                                {
                                    If ((CAB0 && !SWAP))
                                    {
                                        DMA2 = 0x02
                                    }
                                }

                                Local1 = DerefOf (UDMA [DMA2])
                                Local4 |= One
                            }
                            Else
                            {
                                Local1 = DerefOf (MDMA [MDA2])
                            }

                            If ((FLGS && 0x04))
                            {
                                If ((CAB0 && !SWAP))
                                {
                                    DMA3 = 0x02
                                }

                                Local3 = DerefOf (UDMA [DMA3])
                                Local4 |= 0x04
                            }
                            Else
                            {
                                Local3 = DerefOf (MDMA [MDA3])
                            }

                            GTM0 = Local0
                            GTM1 = Local1
                            GTM2 = Local2
                            GTM3 = Local3
                            GTM4 = Local4
                            Return (IDEB) /* \_SB_.PCI0.P0P4.JMB0.IDEB */
                        }

                        Method (_STM, 3, NotSerialized)  // _STM: Set Timing Mode
                        {
                            IDEB = Arg0
                            Local0 = GTM0 /* \_SB_.PCI0.P0P4.JMB0.GTM0 */
                            Local1 = GTM1 /* \_SB_.PCI0.P0P4.JMB0.GTM1 */
                            Local2 = GTM2 /* \_SB_.PCI0.P0P4.JMB0.GTM2 */
                            Local3 = GTM3 /* \_SB_.PCI0.P0P4.JMB0.GTM3 */
                            Local4 = GTM4 /* \_SB_.PCI0.P0P4.JMB0.GTM4 */
                            FLGS = Local4
                            If (((Local0 != Ones) && (Local0 != Zero)))
                            {
                                PIO2 = Match (PIOT, MEQ, Local0, MTR, Zero, Zero)
                            }

                            If (((Local1 != Ones) && (Local1 != Zero)))
                            {
                                If ((FLGS && One))
                                {
                                    DMA2 = Match (UDMA, MEQ, Local1, MTR, Zero, Zero)
                                }
                                Else
                                {
                                    MDA2 = Match (MDMA, MEQ, Local1, MTR, Zero, Zero)
                                }
                            }

                            If (((Local2 != Ones) && (Local2 != Zero)))
                            {
                                PIO3 = Match (PIOT, MEQ, Local2, MTR, Zero, Zero)
                            }

                            If (((Local3 != Ones) && (Local3 != Zero)))
                            {
                                If ((FLGS && 0x04))
                                {
                                    DMA3 = Match (UDMA, MEQ, Local3, MTR, Zero, Zero)
                                }
                                Else
                                {
                                    MDA3 = Match (MDMA, MEQ, Local3, MTR, Zero, Zero)
                                }
                            }
                        }

                        Device (DRV0)
                        {
                            Name (_ADR, Zero)  // _ADR: Address
                            Method (_GTF, 0, NotSerialized)  // _GTF: Get Task File
                            {
                                Local0 = Buffer (0x07)
                                    {
                                         0x03, 0x00, 0x00, 0x00, 0x00, 0xA0, 0xEF         // .......
                                    }
                                Local1 = Buffer (0x07)
                                    {
                                         0x03, 0x00, 0x00, 0x00, 0x00, 0xA0, 0xEF         // .......
                                    }
                                CreateByteField (Local0, One, PIOM)
                                CreateByteField (Local1, One, DMAM)
                                PIOM = PIO2 /* \_SB_.PCI0.P0P4.JMB0.PIO2 */
                                PIOM |= 0x08
                                If ((FLGS && One))
                                {
                                    DMAM = DMA2 /* \_SB_.PCI0.P0P4.JMB0.DMA2 */
                                    DMAM |= 0x40
                                }
                                Else
                                {
                                    DMAM = MDA2 /* \_SB_.PCI0.P0P4.JMB0.MDA2 */
                                    DMAM |= 0x20
                                }

                                Concatenate (Local0, Local1, Local2)
                                Return (Local2)
                            }
                        }

