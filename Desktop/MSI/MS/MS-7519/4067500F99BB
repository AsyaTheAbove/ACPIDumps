                {
                    SLPS = Zero
                    PS1E = Zero
                    If (((Arg0 == One) && RTCS)){}
                    ElseIf (((Arg0 == 0x03) && BRTC)){}
                    Else
                    {
                        Notify (PWRB, 0x02) // Device Wake
                    }
                }

