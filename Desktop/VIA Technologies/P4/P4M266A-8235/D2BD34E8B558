                    CreateByteField (BUF2, 0x03, IOHI)
                    CreateByteField (BUF2, \_SB.PCI0.UAR2._CRS._Y16._MAX, IORL)  // _MAX: Maximum Base Address
                    CreateByteField (BUF2, 0x05, IORH)
