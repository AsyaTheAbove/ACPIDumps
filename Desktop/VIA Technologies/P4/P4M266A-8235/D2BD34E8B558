                            Local2 = (Local1 - 0x08)
                            Local4 = (One << Local2)
                        }
                        ElseIf ((Local1 > 0x00))
                        {
                            Local3 = (One << Local1)
                        }

                        IRA1 = Local3
                        IRA2 = Local4
                    }

                    Return (BUFA) /* \_SB_.PCI0.LNKB._CRS.BUFA */
                }

                Method (_SRS, 1, NotSerialized)  // _SRS: Set Resource Settings
                {
                    CreateByteField (Arg0, 0x01, IRA1)
                    CreateByteField (Arg0, 0x02, IRA2)
                    Local0 = (IRA2 << 0x08)
                    Local0 |= IRA1 /* \_SB_.PCI0.LNKB._SRS.IRA1 */
                    Local1 = 0x00
                    Local0 >>= 0x01
                    While ((Local0 > 0x00))
                    {
                        Local1++
                        Local0 >>= 0x01
                    }

                    PIBC &= 0xF0
                    PIBC |= Local1
                }
            }

