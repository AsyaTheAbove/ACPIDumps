            Device (UAR2)
            {
                Method (_HID, 0, NotSerialized)  // _HID: Hardware ID
                {
                    ENFG ()
                    LDN = 0x02
                    Local0 = (OPT2 & 0x07)
                    If ((Local0 == 0x04))
                    {
                        Return (0x05878526)
                    }
                    Else
                    {
                        Return (0x0105D041)
                    }

                    EXFG ()
                }

                Name (_UID, 0x02)  // _UID: Unique ID
                Method (_STA, 0, NotSerialized)  // _STA: Status
                {
                    ENFG ()
                    LDN = 0x02
                    Local0 = (OPT2 & 0x07)
                    If ((Local0 != 0x01))
                    {
                        If (ACTR)
                        {
                            EXFG ()
                            Return (0x0F)
                        }
                        ElseIf ((IOAH || IOAL))
                        {
                            EXFG ()
                            Return (0x0D)
                        }
                        Else
                        {
                            EXFG ()
                            Return (0x00)
                        }
                    }
                    Else
                    {
                        EXFG ()
                        Return (0x00)
                    }
                }

                Method (_DIS, 0, NotSerialized)  // _DIS: Disable Device
                {
                    ENFG ()
                    LDN = 0x02
                    ACTR = Zero
                    EXFG ()
                    DISD (0x01)
                }

