                    CreateByteField (Arg0, 0x14, DMAC)
                    ENFG ()
                    LDN = 0x03
                    ACTR = One
                    IOAL = IOLO /* \_SB_.PCI0.ECP1._SRS.IOLO */
                    IOAH = IOHI /* \_SB_.PCI0.ECP1._SRS.IOHI */
                    FindSetLeftBit (IRQW, Local0)
                    Local0 -= 0x01
                    INTR = Local0
                    FindSetLeftBit (DMAC, Local1)
                    Local0 = DMCH /* \DMCH */
                    DMCH = (Local1 - 0x01)
                    SLDM (Local0, DMCH)
                    EXFG ()
                    CKIO (IOAD, 0x02)
                }
            }

