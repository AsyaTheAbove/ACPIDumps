                            CreateWordField (CRS, \_SB.PCI0.SBRG.SIO1._Y19._MAX, GP31)  // _MAX: Maximum Base Address
                            CreateByteField (CRS, \_SB.PCI0.SBRG.SIO1._Y19._LEN, GPL3)  // _LEN: Length
                            GP30 = IO2B /* \IO2B */
